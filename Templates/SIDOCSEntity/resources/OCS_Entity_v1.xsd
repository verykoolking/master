	<xsd:complexType name="ArrayOfOCSComponentProdOfferPriceType">
		<xsd:sequence>
			<xsd:element name="item" type="tns:OCSComponentProdOfferPriceType" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OCSComponentProdOfferPriceType">
		<xsd:complexContent>
			<xsd:extension base="pop:ComponentProdOfferPrice">
				<xsd:sequence>
					<!-- identifiers inherited from EntityBase
          <xsd:element name="identifier" type="EntityIdentifier" minOccurs="1" maxOccurs="1"/>
          -->
					<!-- identifiers inherited from ProductOfferingPrice
          <xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="validFor" type="base:TimePeriod" minOccurs="0" maxOccurs="1">
          -->
					<!-- identifiers inherited from ComponentProdOfferPrice
          <xsd:element name="unitOfMeasure" type="base:Quantity" minOccurs="0" maxOccurs="1">
          <xsd:element name="price" type="base:Money" minOccurs="0" maxOccurs="1">
          -->
					<xsd:element name="oCSPrice" type="tns:MonetaryType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="oCSProdOfferPriceCharacteristic" type="tns:ArrayOfCharacteristicValueType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="relations" type="tns:ArrayOfRelationType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrayOfOCSCustomerFacingServiceSpecType">
		<xsd:sequence>
			<xsd:element name="item" type="tns:OCSCustomerFacingServiceSpecType" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OCSCustomerFacingServiceSpecType">
		<xsd:complexContent>
			<xsd:extension base="cfs:CustomerFacingServiceSpec">
				<xsd:sequence>
					<!-- identifiers inherited from EntityBase
          <xsd:element name="identifier" type="EntityIdentifier" minOccurs="1" maxOccurs="1"/>
          -->
					<!-- identifiers inherited from ServiceSpecification
          -->
					<!-- identifiers inherited from CustomerFacingServiceSpec
          -->
					<xsd:element name="type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>
								<p>A word, term, or phrase by which the CustomerFacingServiceSpec is known and distinguished from other CustomerFacingServiceSpec</p>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>
								<p>A narrative that explains what the customer facing service specification is</p>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="oCSServiceSpecCharacteristic" type="tns:ArrayOfCharacteristicValueType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="relations" type="tns:ArrayOfRelationType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrayOfOCSProductType">
		<xsd:sequence>
			<xsd:element name="item" type="tns:OCSProductType" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OCSProductType">
		<xsd:complexContent>
			<xsd:extension base="pr:Product">
				<xsd:sequence>
					<!-- identifiers inherited from EntityBase
          <xsd:element name="identifier" type="EntityIdentifier" minOccurs="1" maxOccurs="1"/>
          -->
					<!-- identifiers inherited from Product
          <xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="productStatus" type="xsd:string" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="productSerialNumber" type="xsd:string" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="validFor" type="base:TimePeriod" minOccurs="0" maxOccurs="1"/>
          -->
					<xsd:element name="type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="oCSProductCharacteristicValue" type="tns:ArrayOfCharacteristicValueType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="relations" type="tns:ArrayOfRelationType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrayOfOCSComponentProdPriceType">
		<xsd:sequence>
			<xsd:element name="item" type="tns:OCSComponentProdPriceType" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OCSComponentProdPriceType">
		<xsd:complexContent>
			<xsd:extension base="pp:ComponentProdPrice">
				<xsd:sequence>
					<!-- identifiers inherited from EntityBase
          <xsd:element name="identifier" type="EntityIdentifier" minOccurs="1" maxOccurs="1"/>
          -->
					<!-- attributes inherited from ProductPrice
          <xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="validFor" type="base:TimePeriod" minOccurs="0" maxOccurs="1"/>
          -->
					<!-- identifiers inherited from ComponentProdPrice
          <xsd:element name="unitOfMeasure" type="base:Quantity" minOccurs="0" maxOccurs="1">
          <xsd:element name="price" type="base:Money" minOccurs="0" maxOccurs="1">
          -->
					<xsd:element name="oCSPrice" type="tns:MonetaryType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="oCSProdPriceCharacteristic" type="tns:ArrayOfCharacteristicValueType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="relations" type="tns:ArrayOfRelationType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrayOfOCSCustomerFacingServiceType">
		<xsd:sequence>
			<xsd:element name="item" type="tns:OCSCustomerFacingServiceType" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OCSCustomerFacingServiceType">
		<xsd:complexContent>
			<xsd:extension base="ser:CustomerFacingService">
				<xsd:sequence>
					<!-- identifiers inherited from EntityBase
          <xsd:element name="identifier" type="EntityIdentifier" minOccurs="1" maxOccurs="1"/>
          -->
					<!-- identifiers inherited from ServiceSpecification
          -->
					<!-- identifiers inherited from CustomerFacingServiceSpec
          -->
					<xsd:element name="type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="oCSServiceCharacteristicValue" type="tns:ArrayOfCharacteristicValueType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="relations" type="tns:ArrayOfRelationType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
