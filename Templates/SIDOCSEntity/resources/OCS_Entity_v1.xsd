		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OCSProductSpecificationType">
		<xsd:complexContent>
			<xsd:extension base="ps:ProductSpecification">
				<xsd:sequence>
					<!-- identifiers inherited from EntityBase
          <xsd:element name="identifier" type="EntityIdentifier" minOccurs="1" maxOccurs="1"/>
          -->
					<!-- identifiers inherited from ProductSpecification
          <xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="productNumber" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="brand" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="validFor" type="base:TimePeriod" minOccurs="0" maxOccurs="1">
          <xsd:element name="lifecycleStatus" type="xsd:string" minOccurs="0" maxOccurs="1">
          -->
					<xsd:element name="type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="oCSProductSpecCharacteristic" type="tns:ArrayOfCharacteristicValueType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="relations" type="tns:ArrayOfRelationType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
