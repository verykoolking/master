	<xsd:import namespace="http://www.tmforum.org/xml/tip/internal/entity" schemaLocation="internal_entity.xsd"/>
	-->
	<!-- Element root that should be removed here, just for examples -->
	<xsd:element name="oCSEntityList" type="tns:ArrayOfOCSEntityType"/>
	<!-- Complex type definitions -->
	<xsd:complexType name="ArrayOfOCSProductOfferingType">
		<xsd:sequence>
			<xsd:element name="item" type="tns:OCSProductOfferingType" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OCSProductOfferingType">
		<xsd:complexContent>
			<xsd:extension base="po:ProductOffering">
				<xsd:sequence>
					<!-- identifiers inherited from EntityBase
          <xsd:element name="identifier" type="EntityIdentifier" minOccurs="1" maxOccurs="1"/>
          -->
					<!-- identifiers inherited from ProductOffering
          <xsd:element name="id" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="validFor" type="base:TimePeriod" minOccurs="0" maxOccurs="1">
          <xsd:element name="status" type="xsd:string" minOccurs="0" maxOccurs="1">
          -->
					<xsd:element name="type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="oCSProductSpecCharValueUse" type="tns:ArrayOfCharacteristicValueType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="relations" type="tns:ArrayOfRelationType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrayOfOCSBundledProductOfferingType">
		<xsd:sequence>
			<xsd:element name="item" type="tns:OCSBundledProductOfferingType" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OCSBundledProductOfferingType">
		<xsd:complexContent>
			<xsd:extension base="po:BundledProductOffering">
				<xsd:sequence>
					<!-- identifiers inherited from EntityBase
          <xsd:element name="identifier" type="EntityIdentifier" minOccurs="1" maxOccurs="1"/>
          -->
					<!-- identifiers inherited from ProductOffering
          <xsd:element name="id" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
          <xsd:element name="validFor" type="base:TimePeriod" minOccurs="0" maxOccurs="1">
          <xsd:element name="status" type="xsd:string" minOccurs="0" maxOccurs="1">
          -->
					<xsd:element name="type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="oCSProductSpecCharValueUse" type="tns:ArrayOfCharacteristicValueType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="relations" type="tns:ArrayOfRelationType" minOccurs="0" maxOccurs="1"/>
