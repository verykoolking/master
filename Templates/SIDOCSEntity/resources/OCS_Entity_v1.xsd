					<xsd:element name="type" type="xsd:string" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="oCSPriceEventCharacteristic" type="tns:ArrayOfCharacteristicValueType" minOccurs="0" maxOccurs="1"/>
