	</xsd:complexType>
	<xsd:element name="QueryEntityResponse" type="QueryEntityResponseType"/>
	<xsd:complexType name="QueryEntityResponseType">
		<xsd:sequence>
			<xsd:element name="queryCommonInfoResult" type="oo:QueryCommonInfoResultType"/>
			<xsd:element name="oCSEntityList" type="oe:ArrayOfOCSEntityType"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
