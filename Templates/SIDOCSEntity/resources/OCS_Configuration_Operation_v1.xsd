<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.telefonica.com/schemas/SOAP/OCSConfigurationOperation/v1/types" xmlns:oo="http://www.telefonica.com/schemas/SOAP/OCSOperation/v1/types" xmlns:oe="http://www.telefonica.com/schemas/SOAP/OCSEntity/v1/types" targetNamespace="http://www.telefonica.com/schemas/SOAP/OCSConfigurationOperation/v1/types" elementFormDefault="qualified">
	<xsd:import namespace="http://www.telefonica.com/schemas/SOAP/OCSOperation/v1/types" schemaLocation="OCS_Operation_v1.xsd"/>
	<xsd:import namespace="http://www.telefonica.com/schemas/SOAP/OCSEntity/v1/types" schemaLocation="OCS_Entity_v1.xsd"/>
	
<!-- <xsd:annotation><xsd:documentation><p>

	XSD FOR OCS CONFIGURATION OPERATION
	=========================================================================

	Last changes (2014-07-10)
	- oCSEntityStatus element  changes to oCSEntityStatusList (QueryEntityStatusResponseType).
	- ConfigurationStatusType removed
	- <xsd:element name="configurationStatus" type="ConfigurationStatusType"/> changed to 
	  <xsd:element name="configurationStatus" type="oe:ConfigurationStatusType"/>
    - Telefonica namespaces URI corrected
	
	</p></xsd:documentation></xsd:annotation> -->
	
	<!-- main type definition -->
	<xsd:element name="StartConfiguration" type="StartConfigurationType"/>
	<xsd:complexType name="StartConfigurationType">
		<xsd:sequence>
			<xsd:element name="sessionCommonInfo" type="oo:SessionCommonInfoType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="StartConfigurationResponse" type="StartConfigurationResponseType"/>
	<xsd:complexType name="StartConfigurationResponseType">
		<xsd:sequence>
			<xsd:element name="sessionCommonInfoResult" type="oo:SessionCommonInfoResultType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="EndConfiguration" type="EndConfigurationType"/>
	<xsd:complexType name="EndConfigurationType">
		<xsd:sequence>
			<xsd:element name="sessionCommonInfo" type="oo:SessionCommonInfoType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="EndConfigurationResponse" type="EndConfigurationResponseType"/>
	<xsd:complexType name="EndConfigurationResponseType">
		<xsd:sequence>
			<xsd:element name="sessionCommonInfoResult" type="oo:SessionCommonInfoResultType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="QueryConfigurationStatus" type="QueryConfigurationStatusType"/>
	<xsd:complexType name="QueryConfigurationStatusType">
		<xsd:sequence>
			<xsd:element name="queryCommonInfo" type="oo:QueryCommonInfoType"/>
			<xsd:element name="sessionId" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="QueryConfigurationStatusResponse" type="QueryConfigurationStatusResponseType"/>
	<xsd:complexType name="QueryConfigurationStatusResponseType">
		<xsd:sequence>
			<xsd:element name="queryCommonInfoResult" type="oo:QueryCommonInfoResultType"/>
			<xsd:element name="configurationStatus" type="oe:ConfigurationStatusType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="SendNotification" type="SendNotificationType"/>
