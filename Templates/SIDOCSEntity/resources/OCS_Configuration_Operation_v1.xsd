		<xsd:sequence>
			<xsd:element name="queryCommonInfo" type="oo:QueryCommonInfoType"/>
			<xsd:element name="oCSEntity" type="oe:OCSEntityType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="QueryEntityStatusResponse" type="QueryEntityStatusResponseType"/>
	<xsd:complexType name="QueryEntityStatusResponseType">
		<xsd:sequence>
			<xsd:element name="queryCommonInfoResult" type="oo:QueryCommonInfoResultType"/>
			<xsd:element name="oCSEntityStatusList" type="oe:ArrayOfOCSEntityStatusType"/>
		</xsd:sequence>
	</xsd:complexType>
