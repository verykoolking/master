   <xsd:complexType name="FileDescriptor">
      <xsd:annotation>
         <xsd:documentation>
            <p>
               <p>Provides information on file(s) being transferred.</p>
            </p>
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="fileName" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
                  <p>
                     <p>Provides the file name(s) of the file being transferred.</p>
                     <p>It is possible for the file name to contain a wildcard allowing specifying multiple files with one descriptor. However, the readyTime and expirationTime for all the files matching the wildcarded name should be identical. </p>
                  </p>
                  <p>This element is generated from an attribute.</p>
                  <p>This attribute  is  mandatory</p>
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="fileLocation" type="xsd:anyURI" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
                  <p>
                     <p>URI indicating the location of the file(s) being produced. The file location does not include the file name.</p>
                  </p>
                  <p>This element is generated from an attribute.</p>
                  <p>This attribute  is  mandatory</p>
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="readyTime" type="xsd:dateTime" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
                  <p>
                     <p>Indicates the time at which the file(s) is ready for data transfer. </p>
                     <p>The file(s) should not be changed after this time. </p>
                  </p>
                  <p>This element is generated from an attribute.</p>
                  <p>This attribute  is  mandatory</p>
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="expirationTime" type="xsd:dateTime" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
                  <p>
                     <p>Indicates the time beyond which the file(s) may be deleted by the OS which made it available. </p>
                     <p>By default, the expirationTime should be the readyTime + the retention period. </p>
                  </p>
                  <p>This element is generated from an attribute.</p>
                  <p>This attribute  is  optional</p>
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
