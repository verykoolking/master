   <xsd:element name="businessInteractionItemPrice" type="tns:BusinessInteractionItemPrice"/>
   <xsd:complexType name="BusinessInteractionItemPrice">
      <xsd:annotation>
         <xsd:documentation>
            <p>An amount associated with an BusinessInteractionItem that is valued by the associated ProductOfferingPrice.</p>
            <p>This association class  is  optional</p>
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="quantity" type="base:Quantity" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
                  <p>The portion of the Business Interaction Item quantity that is valued by a price.</p>
                  <p>This element is generated from an attribute.</p>
                  <p>This attribute  is  optional</p>
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="price" type="base:Money" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
                  <p>A one-time price, not included as an available product offering price, associated with a business interaction item.</p>
                  <p>This element is generated from an attribute.</p>
                  <p>This attribute  is  optional</p>
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
                  <p>Narrative that explains the price that can include a reason why a price was overridden.</p>
                  <p>This element is generated from an attribute.</p>
                  <p>This attribute  is  optional</p>
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
