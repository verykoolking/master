   <xsd:complexType name="ArrayOfBusinessInteractionItemPrice">
      <xsd:sequence>
         <xsd:element name="item" type="tns:BusinessInteractionItemPrice" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
