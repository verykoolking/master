    <xsd:complexType name="CardIdType">
        <xsd:sequence>
            <xsd:element ref="cardClassId" minOccurs="0" />
            <xsd:element ref="cardSubClassId" minOccurs="0" />
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="cardId" type="CardIdType" />

    <xsd:complexType name="HomeZoneLocationNumberInfoListType">
        <xsd:sequence>
            <xsd:element ref="locationNumberId" maxOccurs="unbounded" minOccurs="1"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="homeZoneLocationNumberInfoList" type="HomeZoneLocationNumberInfoListType" />

    <xsd:complexType name="LifeCycleStatusType">
        <xsd:sequence>
            <xsd:element ref="subscriberState" />
