            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="lifeCycleStatus" type="LifeCycleStatusType" />
    <xsd:element name="nextState" type="LifeCycleStatusType" />

    <xsd:complexType name="NetworkCardInfoType">
