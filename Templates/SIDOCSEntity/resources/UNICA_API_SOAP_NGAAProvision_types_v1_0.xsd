        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="servicePlansInfoList" type="ServicePlansInfoListType" />
    <xsd:element name="allowedPlansInfoList" type="ServicePlansInfoListType"/>

    <xsd:complexType name="SMSSubscriptionsInfoType">
