<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.telefonica.com/schemas/UNICA/SOAP/NGAAProvision/v1/types" 
xmlns="http://www.telefonica.com/schemas/UNICA/SOAP/NGAAProvision/v1/types" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:uct="http://www.telefonica.com/schemas/UNICA/SOAP/common/v2" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:import namespace="http://www.telefonica.com/schemas/UNICA/SOAP/common/v2" schemaLocation="UNICA_API_SOAP_common_types_v2_1.xsd"/>

    
<!-- ************************************* Auxiliar ************************************* -->

    <xsd:element name="userCode" type="xsd:string"/>
                
    <xsd:complexType name="UnitValueType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:long">
                <xsd:attribute name="exponent" type="xsd:int"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

<!-- ************************************* Coin ************************************* -->

    <xsd:complexType name="MonetaryType">
        <xsd:simpleContent>
            <xsd:extension base="UnitValueType">
<!-- Future compatibility -->
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:simpleType name="CurrencyCodeType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="currencyCode" type="CurrencyCodeType"/>

<!-- ************************************* Operation ************************************* -->

    <xsd:element name="appId" type="xsd:string"/>
    <xsd:element name="numRegs" type="xsd:int"/>
    <xsd:element name="opCost" type="MonetaryType"/>
    <xsd:element name="opExecutionDate" type="xsd:dateTime"/>
    <xsd:element name="opId" type="xsd:string"/>
    <xsd:element name="reference" type="xsd:string"/>
    <xsd:element name="token" type="xsd:string"/>

    <xsd:complexType name="OpSubIdListType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="1" ref="opId"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="opSubIdList" type="OpSubIdListType"/>

    <xsd:complexType name="OpReferenceType">
        <xsd:sequence>
            <xsd:element ref="opId"/>
            <xsd:element minOccurs="0" ref="opSubIdList"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="opReference" type="OpReferenceType"/>

    <xsd:complexType name="AppInfoType">
        <xsd:sequence>
            <xsd:element ref="appId"/>
            <xsd:element ref="reference"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="appInfo" type="AppInfoType"/>

    <xsd:complexType name="ActionCommonInfoType">
        <xsd:sequence>
            <xsd:element ref="appInfo"/>
            <xsd:element ref="userCode"/>
            <xsd:element minOccurs="0" ref="opCost"/>
            <xsd:element minOccurs="0" ref="currencyCode"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="actionCommonInfo" type="ActionCommonInfoType"/>

    <xsd:complexType name="OpResultInfoType">
        <xsd:sequence>
            <xsd:element ref="opReference"/>
            <xsd:element ref="opExecutionDate"/>
            <xsd:element ref="opCost"/>
            <xsd:element minOccurs="0" ref="balanceActivityInfo"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="AccOpResultInfoType">
        <xsd:complexContent>
            <xsd:extension base="OpResultInfoType">
                <xsd:sequence>
<!-- Future compatibility -->
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="result" type="AccOpResultInfoType"/>

    <xsd:complexType name="PagingInfoType">
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="token"/>
            <xsd:element ref="numRegs"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="pagingInfo" type="PagingInfoType"/>

    <xsd:complexType name="QueryCommonInfoType">
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="currencyCode"/>
            <xsd:element minOccurs="0" ref="pagingInfo"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="queryCommonInfo" type="QueryCommonInfoType"/>

<!-- ************************************* Time ************************************* -->

    <xsd:element name="date" type="xsd:dateTime"/>
    <xsd:element name="specialTime" type="xsd:string"/>

    <xsd:complexType name="TimeOffsetType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:duration">
                <xsd:attribute name="timeRoundUnit" type="xsd:string"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:element name="timeOffset" type="TimeOffsetType"/>
    
    <xsd:complexType name="TimeType">
        <xsd:choice>
            <xsd:element ref="date"/>
            <xsd:element ref="timeOffset"/>
            <xsd:element ref="specialTime"/>
        </xsd:choice>
    </xsd:complexType>

    <xsd:element name="startTime" type="TimeType"/>
    <xsd:element name="endTime" type="TimeType"/>

    <xsd:complexType name="TimeIntervalType">
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="startTime"/>
            <xsd:element minOccurs="0" ref="endTime"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="timeInterval" type="TimeIntervalType"/>
    
<!-- ************************************* Subscriber Account ************************************* -->

<!-- Subscriber Identification -->
        
    <xsd:element name="anyUri" type="xsd:anyURI"/>
    <xsd:element name="e164Number" type="uct:E164Type"/>
    <xsd:element name="IMSI" type="uct:ImsiType"/>
    <xsd:element name="ipAddress" type="uct:IpAddressType"/>
    <xsd:element name="otherId" type="uct:OtherIdType"/>
    <xsd:element name="sipUri" type="uct:SipUriType"/>
    <xsd:element name="userName" type="xsd:string"/>
        
    <xsd:complexType name="SubscriberIdType">
        <xsd:choice>
            <xsd:element ref="e164Number"/>
            <xsd:element ref="IMSI"/>
            <xsd:element ref="sipUri"/>
            <xsd:element ref="anyUri"/>
            <xsd:element ref="otherId"/>
            <xsd:element ref="userName"/>
            <xsd:element ref="ipAddress"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:element name="subscriberId" type="SubscriberIdType"/>

    <xsd:simpleType name="ServiceClassType">
            <xsd:restriction base="xsd:string">
                <xsd:pattern value="[1-5]" />
            </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="serviceClass" type="ServiceClassType" />

<!-- ************************************* Account Management **************************************** -->

    <xsd:simpleType name="ICCNumberType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{1,19}" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:element name="accountProfileId" type="xsd:string" />
    <xsd:element name="actionCauseCode" type="xsd:string" />
    <xsd:element name="activationDate" type="xsd:dateTime" />
    <xsd:element name="availableBalanceFlag" type="uct:FlagType"/>
    <xsd:element name="billingCycleCode" type="xsd:string" />
    <xsd:element name="cancellableFlag" type="uct:FlagType" />
    <xsd:element name="cancellationFlag" type="uct:FlagType" />
    <xsd:element name="cardClassId" type="xsd:string" />
    <xsd:element name="cardSubClassId" type="xsd:string" />
    <xsd:element name="cardProfileId" type="xsd:string" />
    <xsd:element name="cardTypeId" type="xsd:string" />
    <xsd:element name="categoryCode" type="xsd:string" />
    <xsd:element name="causeCode" type="xsd:string" />
    <xsd:element name="chargingModality" type="xsd:int" />
    <xsd:element name="cycleSpendControlFlag" type="uct:FlagType" />
    <xsd:element name="deactivateEndDate" type="xsd:dateTime"/>
    <xsd:element name="deactivateStartDate" type="xsd:dateTime"/>
    <xsd:element name="destinationNetworkCode" type="xsd:string"/>
    <xsd:element name="dischargeDate" type="xsd:dateTime"/>
    <xsd:element name="dischargeReason" type="xsd:string"/>
    <xsd:element name="expirationNumDays" type="UnitValueType"/>
    <xsd:element name="expirationThreshold" type="xsd:int"/>
    <xsd:element name="genericRatingParameter" type="xsd:string" />
    <xsd:element name="geographicArea" type="xsd:string"/>
    <xsd:element name="hlrId" type="xsd:string" />
    <xsd:element name="iccNumber" type="ICCNumberType" />
    <xsd:element name="IMEI" type="uct:ImeiType"/>        
    <xsd:element name="immediateFlag" type="uct:FlagType" />    
    <xsd:element name="incompatibleOperationsFlag" type="uct:FlagType"/>
    <xsd:element name="initialBalance" type="MonetaryType" />
    <xsd:element name="invoiceAccount" type="xsd:string"/>
    <xsd:element name="keyCode" type="xsd:string"/>
    <xsd:element name="langCode" type="xsd:string"/>
    <xsd:element name="limitEventFlag" type="uct:FlagType"/>
    <xsd:element name="lifeCycleStatusDate" type="xsd:dateTime"/>
    <xsd:element name="locationNumberId" type="xsd:string"/>
    <xsd:element name="maximum" type="MonetaryType" />
    <xsd:element name="messageNotificationEventFlag" type="uct:FlagType" />    
    <xsd:element name="modifiableFlag" type="uct:FlagType"/>
    <xsd:element name="negativeBalanceUsageFlag" type="uct:FlagType"/>
