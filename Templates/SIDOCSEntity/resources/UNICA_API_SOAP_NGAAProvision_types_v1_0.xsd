    <xsd:element name="subscriberSubState" type="xsd:string"/>
    <xsd:element name="subscriptionArea" type="xsd:string"/>
    <xsd:element name="suspensionIdCode" type="xsd:string"/>
    <xsd:element name="suspensionTime" type="xsd:duration" />
    <xsd:element name="tariffPlanId" type="xsd:string" />
    <xsd:element name="taxCode" type="xsd:string" />
    <xsd:element name="technologyId" type="xsd:string" />
    <xsd:element name="threshold" type="UnitValueType"/>
