            <xsd:element ref="smsSubscriptionFlag" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="smsSubscriptionsInfo" type="SMSSubscriptionsInfoType" />
    
