        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="networkCardInfo" type="NetworkCardInfoType" />

    <xsd:complexType name="SegmentInfoType">
        <xsd:sequence>
            <xsd:element ref="segmentId" />
