    <xsd:element name="saleModality3" type="xsd:string"/>
    <xsd:element name="saleModality4" type="xsd:string"/>
    <xsd:element name="segmentId" type="xsd:string"/>
    <xsd:element name="segmentReference" type="xsd:string"/>
