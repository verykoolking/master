        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="segmentInfoList" type="SegmentInfoListType" />

    <xsd:complexType name="ServicePlansBasicInfoType">
        <xsd:sequence>
            <xsd:element ref="serviceClass" minOccurs="0"/>
            <xsd:element ref="tariffPlanId" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="servicePlansBasicInfo" type="ServicePlansBasicInfoType" />

    <xsd:complexType name="ServicePlansBasicInfoListType">
        <xsd:sequence>
            <xsd:element ref="servicePlansBasicInfo" maxOccurs="unbounded" minOccurs="1"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="servicePlansBasicInfoList" type="ServicePlansBasicInfoListType" />

    <xsd:complexType name="ServicePlansInfoType">
        <xsd:sequence>
            <xsd:element ref="tariffPlanId"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="servicePlansInfo" type="ServicePlansInfoType" />

