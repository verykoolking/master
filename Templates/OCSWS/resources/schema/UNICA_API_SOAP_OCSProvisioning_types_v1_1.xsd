<?xml version="1.0" encoding="UTF-8"?>

<!--  PA7 -->

<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.telefonica.com/schemas/UNICA/SOAP/OCSProvisioning/v1/types"
    xmlns="http://www.telefonica.com/schemas/UNICA/SOAP/OCSProvisioning/v1/types"
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:uct="http://www.telefonica.com/schemas/UNICA/SOAP/common/v2"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://www.telefonica.com/schemas/UNICA/SOAP/common/v2"
        schemaLocation="UNICA_API_SOAP_common_types_v2_1.xsd" />

    
<!-- ************************************* Auxiliar ************************************* -->

    <xsd:element name="userCode" type="xsd:string"/>
                
    <xsd:complexType name="UnitValueType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:long">
                <xsd:attribute name="exponent" type="xsd:int"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

<!-- ************************************* Coin ************************************* -->

    <xsd:complexType name="MonetaryType">
        <xsd:simpleContent>
            <xsd:extension base="UnitValueType">
<!-- Future compatibility -->
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:simpleType name="CurrencyCodeType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="currencyCode" type="CurrencyCodeType"/>

<!-- ************************************* Operation ************************************* -->

    <xsd:element name="appId" type="xsd:string"/>
    <xsd:element name="numRegs" type="xsd:int"/>
    <xsd:element name="opCost" type="MonetaryType"/>
    <xsd:element name="opExecutionDate" type="xsd:dateTime"/>
    <xsd:element name="opId" type="xsd:string"/>
    <xsd:element name="reference" type="xsd:string"/>
    <xsd:element name="token" type="xsd:string"/>

    <xsd:complexType name="OpSubIdListType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="1" ref="opId"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="opSubIdList" type="OpSubIdListType"/>

    <xsd:complexType name="OpReferenceType">
        <xsd:sequence>
            <xsd:element ref="opId"/>
            <xsd:element minOccurs="0" ref="opSubIdList"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="opReference" type="OpReferenceType"/>

    <xsd:complexType name="AppInfoType">
        <xsd:sequence>
            <xsd:element ref="appId"/>
            <xsd:element ref="reference"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="appInfo" type="AppInfoType"/>

    <xsd:complexType name="ActionCommonInfoType">
        <xsd:sequence>
            <xsd:element ref="appInfo"/>
            <xsd:element ref="userCode"/>
            <xsd:element minOccurs="0" ref="opCost"/>
            <xsd:element minOccurs="0" ref="currencyCode"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="actionCommonInfo" type="ActionCommonInfoType"/>

    <xsd:complexType name="OpResultInfoType">
        <xsd:sequence>
            <xsd:element ref="opReference"/>
            <xsd:element ref="opExecutionDate"/>
            <xsd:element ref="opCost"/>
            <xsd:element minOccurs="0" ref="balanceActivityInfo"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="AccOpResultInfoType">
        <xsd:complexContent>
            <xsd:extension base="OpResultInfoType">
                <xsd:sequence>
<!-- Future compatibility -->
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="result" type="AccOpResultInfoType"/>

    <xsd:complexType name="PagingInfoType">
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="token"/>
            <xsd:element ref="numRegs"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="pagingInfo" type="PagingInfoType"/>

    <xsd:complexType name="QueryCommonInfoType">
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="currencyCode"/>
            <xsd:element minOccurs="0" ref="pagingInfo"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="queryCommonInfo" type="QueryCommonInfoType"/>

<!-- ************************************* Time ************************************* -->

    <xsd:element name="date" type="xsd:dateTime"/>
    <xsd:element name="specialTime" type="xsd:string"/>

    <xsd:complexType name="TimeOffsetType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:duration">
                <xsd:attribute name="timeRoundUnit" type="xsd:string"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:element name="timeOffset" type="TimeOffsetType"/>
    
    <xsd:complexType name="TimeType">
        <xsd:choice>
            <xsd:element ref="date"/>
            <xsd:element ref="timeOffset"/>
            <xsd:element ref="specialTime"/>
        </xsd:choice>
    </xsd:complexType>

    <xsd:element name="startTime" type="TimeType"/>
    <xsd:element name="endTime" type="TimeType"/>

    <xsd:complexType name="TimeIntervalType">
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="startTime"/>
            <xsd:element minOccurs="0" ref="endTime"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="timeInterval" type="TimeIntervalType"/>
    
<!-- ************************************* Subscriber Account ************************************* -->

<!-- Subscriber Identification -->
        
    <xsd:element name="anyUri" type="xsd:anyURI"/>
    <xsd:element name="e164Number" type="uct:E164Type"/>
    <xsd:element name="IMSI" type="uct:ImsiType"/>
    <xsd:element name="ipAddress" type="uct:IpAddressType"/>
    <xsd:element name="otherId" type="uct:OtherIdType"/>
    <xsd:element name="sipUri" type="uct:SipUriType"/>
    <xsd:element name="userName" type="xsd:string"/>
        
    <xsd:complexType name="SubscriberIdType">
        <xsd:choice>
            <xsd:element ref="e164Number"/>
            <xsd:element ref="IMSI"/>
            <xsd:element ref="sipUri"/>
            <xsd:element ref="anyUri"/>
            <xsd:element ref="otherId"/>
            <xsd:element ref="userName"/>
            <xsd:element ref="ipAddress"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:element name="subscriberId" type="SubscriberIdType"/>

    <xsd:simpleType name="ServiceClassType">
            <xsd:restriction base="xsd:string">
                <xsd:pattern value="[1-5]" />
            </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="serviceClass" type="ServiceClassType" />

    <xsd:complexType name="ServiceClassListType">
        <xsd:sequence>
            <xsd:element ref="serviceClass" maxOccurs="unbounded" minOccurs="1"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="serviceClassList" type="ServiceClassListType" />

<!-- ************************************* Account Activity **************************************** -->

    <xsd:element name="areaCode" type="xsd:string"/>
    <xsd:element name="callDate" type="xsd:dateTime"/>
    <xsd:element name="callDescription" type="xsd:string"/>
    <xsd:element name="callDiscount" type="xsd:int"/>
    <xsd:element name="callDuration" type="xsd:duration"/>
    <xsd:element name="callingCardSubscriberId" type="SubscriberIdType"/>
    <xsd:element name="callTypeCode" type="xsd:string"/>
    <xsd:element name="cellCode" type="xsd:string"/>
    <xsd:element name="contentDate" type="xsd:dateTime"/>
    <xsd:element name="contentDescription" type="xsd:string"/>
    <xsd:element name="contentDuration" type="xsd:duration"/>
    <xsd:element name="contentTypeCode" type="xsd:string"/>
    <xsd:element name="destinationPrefixMaskFilter" type="xsd:string"/>
    <xsd:element name="destinationTypeCode" type="xsd:string"/>
    <xsd:element name="downloadData" type="xsd:string"/>
    <xsd:element name="homeZoneOriginatingFlag" type="uct:FlagType"/>
    <xsd:element name="longDistanceCallCost" type="MonetaryType"/>
    <xsd:element name="longDistanceCallTax" type="xsd:int"/>
    <xsd:element name="longDistanceNonDiscountedCallCost" type="MonetaryType"/>
    <xsd:element name="messageCost" type="MonetaryType"/>
    <xsd:element name="messageDate" type="xsd:dateTime"/>
    <xsd:element name="messageDescription" type="xsd:string"/>
    <xsd:element name="messageTax" type="xsd:int"/>
    <xsd:element name="messageTypeCode" type="xsd:string"/>
    <xsd:element name="minAmountFilter" type="MonetaryType"/>
    <xsd:element name="nonDiscountedMessageCost" type="MonetaryType"/>
    <xsd:element name="nonDiscountedSessionCost" type="MonetaryType"/>
    <xsd:element name="onCallChangedTariffFlag" type="uct:FlagType"/>
    <xsd:element name="onAirCallCost" type="MonetaryType"/>
    <xsd:element name="onAirCallTax" type="xsd:int"/>
    <xsd:element name="onAirNonDiscountedCallCost" type="MonetaryType"/>
    <xsd:element name="operatorLD" type="xsd:string"/>
    <xsd:element name="provinceCode" type="xsd:string"/>
    <xsd:element name="regionCode" type="xsd:string"/>
    <xsd:element name="roamingOperator" type="xsd:string"/>
    <xsd:element name="sessionCost" type="MonetaryType"/>
    <xsd:element name="sessionDate" type="xsd:dateTime"/>
    <xsd:element name="sessionDescription" type="xsd:string"/>
    <xsd:element name="sessionDuration" type="xsd:duration"/>
    <xsd:element name="sessionTax" type="xsd:int"/>
    <xsd:element name="sessionTypeCode" type="xsd:string"/>
    <xsd:element name="subscriberTaxValue" type="xsd:int"/>
    <xsd:element name="totalCallCost" type="MonetaryType"/>
    <xsd:element name="totalCallDuration" type="xsd:duration"/>
    <xsd:element name="totalNonDiscountedCallCost" type="MonetaryType"/>
    <xsd:element name="totalSessionDuration" type="xsd:duration"/>
    <xsd:element name="uploadData" type="xsd:string"/>

    <xsd:element name="destinationSubscriberInfo" type="DestinationMsgSubscriberInfoType"/>

    
    <xsd:simpleType name="CallCategoryType">
            <xsd:restriction base="xsd:string">
            </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="callCategory" type="CallCategoryType"/>

    <xsd:complexType name="CallChargingInfoType">
        <xsd:sequence>
            <xsd:element ref="subscriberTaxValue" minOccurs="0"/>
            <xsd:element ref="onAirCallTax" minOccurs="0"/>
            <xsd:element ref="longDistanceCallTax" minOccurs="0"/>
            <xsd:element ref="callDiscount" minOccurs="0"/>
            <xsd:element ref="totalCallCost" minOccurs="0"/>
            <xsd:element ref="onAirCallCost" minOccurs="0"/>
            <xsd:element ref="longDistanceCallCost" minOccurs="0"/>
            <xsd:element ref="totalNonDiscountedCallCost" minOccurs="0"/>
            <xsd:element ref="onAirNonDiscountedCallCost" minOccurs="0"/>
            <xsd:element ref="longDistanceNonDiscountedCallCost" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="callChargingInfo" type="CallChargingInfoType" />

    <xsd:complexType name="MessageChargingInfoType">
        <xsd:sequence>
            <xsd:element ref="subscriberTaxValue" minOccurs="0"/>
