            <xsd:element ref="messageCost" minOccurs="0"/>
            <xsd:element ref="nonDiscountedMessageCost" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="messageChargingInfo" type="MessageChargingInfoType" />

    <xsd:complexType name="SessionChargingInfoType">
        <xsd:sequence>
            <xsd:element ref="subscriberTaxValue" minOccurs="0"/>
            <xsd:element ref="sessionTax" minOccurs="0"/>
            <xsd:element ref="sessionCost" minOccurs="0"/>
            <xsd:element ref="nonDiscountedSessionCost" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="sessionChargingInfo" type="SessionChargingInfoType" />

    <xsd:complexType name="CalledSubscriberInfoType">
        <xsd:sequence>
            <xsd:element ref="subscriberId"/>
            <xsd:element ref="subscriberLocationInfo" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="calledSubscriberInfo" type="CalledSubscriberInfoType" />

    <xsd:complexType name="CallExtraInfoType">
        <xsd:sequence>
            <xsd:element ref="callingCardSubscriberId" minOccurs="0"/>
            <xsd:element ref="homeZoneOriginatingFlag"/>
            <xsd:element ref="onCallChangedTariffFlag"/>
            <xsd:element ref="roamingOperator" minOccurs="0"/>
            <xsd:element ref="operatorLD" minOccurs="0"/>
            <xsd:element ref="cellCode" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="callExtraInfo" type="CallExtraInfoType" />

    <xsd:complexType name="MessageExtraInfoType">
        <xsd:sequence>
            <xsd:element ref="roamingOperator" minOccurs="0"/>
            <xsd:element ref="operatorLD" minOccurs="0"/>
            <xsd:element ref="cellCode" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="messageExtraInfo" type="MessageExtraInfoType" />

    <xsd:complexType name="SessionExtraInfoType">
        <xsd:sequence>
            <xsd:element ref="roamingOperator" minOccurs="0"/>
