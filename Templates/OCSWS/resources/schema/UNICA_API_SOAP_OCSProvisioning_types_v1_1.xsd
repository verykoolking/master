            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="sessionExtraInfo" type="SessionExtraInfoType" />

    <xsd:simpleType name="ContentCategoryType">
            <xsd:restriction base="xsd:string">
            </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="contentCategory" type="ContentCategoryType"/>

    <xsd:simpleType name="DataSessionCategoryType">
            <xsd:restriction base="xsd:string">
            </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="dataSessionCategory" type="DataSessionCategoryType"/>

    <xsd:complexType name="LocationInfoType">
