        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="subscriberLocationInfo" type="LocationInfoType"/>
    
    <xsd:complexType name="DestinationMsgSubscriberInfoType">
        <xsd:sequence>
            <xsd:element ref="subscriberId"/>
            <xsd:element ref="subscriberLocationInfo" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="extension" type="uct:ExtensionType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="destinationMsgSubscriberInfo" type="DestinationMsgSubscriberInfoType" />

    <xsd:simpleType name="MessageCategoryType">
            <xsd:restriction base="xsd:string">
            </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="messageCategory" type="MessageCategoryType"/>

    <xsd:complexType name="CallInfoType">
        <xsd:sequence>
            <xsd:element ref="callCategory" minOccurs="0"/>
            <xsd:element ref="callTypeCode" minOccurs="0"/>
