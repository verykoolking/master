<OCSEntityType>
		<oCSProductSpecification>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductSpecification: name; description; productNumber; validFor; lifeCycleStatus -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>BoltOnType_TC</scheme>
				</dn>
				<entityType>BoltOnType</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>BoltOnType</type>
			<oCSProductSpecCharacteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Bolt On Type Code </annotation> -->
				<characteristic  type="xsd:string" maxLength="4">
					<name>BoltOnTypeCode</name>
					<value>BTTC</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Bolt On Type Description</annotation> -->
				<characteristic  type="xsd:string" maxLength="240">
					<name>BoltOnTypeDescription</name>
					<value>Test Description</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Bolt On Type Modality</annotation> -->
				<characteristic  type="xsd:string" enumValues="BD;CR;XN;DT;ME;MI;PH;MC;MG;GB">
					<name>BoltOnTypeModality</name>
					<value>CR</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Bolt On Type ID </annotation> -->
				<characteristic  type="xsd:long" minValue="1" maxValue="9999">
					<name>BoltOnTypeID</name>
					<value>1234</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Consumption Rate</annotation> -->
				<characteristic  type="xsd:string" enumValues="BASE_RATE;BO_RATE;SPECIAL_RATE" defaultValue="BASE_RATE">
					<name>ConsumptionRate</name>
				</characteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>EventTechnicalSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Threshold Event</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>EventThresholdActivation</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Exhausted Counter Event</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>EventActivationByExhaustedCnt</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>First use Event</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>EventFirstUse</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>First use after renewal Event</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>EventFirstUseAfterRenewal</name>
						</characteristic>

					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>BoltOnGenericSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Accumulative Bolt On Type with Expired Remainder</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>AccumulativeWithExpiredRemainder</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Cancellable Bolt On Type</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>CancellableBoltOnType</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Bolt on type can be subscribed in subscriber's suspended status</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>CanBeSubscribedInSuspendedStatus</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Bolt on type with deferred validity</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>DeferredValidity</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Multiple instances</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>MultipleInstances</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Bolt On must be particular</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>BoltOnMustBeParticular</name>
							<value>true</value>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Share with authorized subscribers</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>ShareWithAuthorizedSubscribers</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Generation of Expiration Notice before the Bolt On Type expires</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>GenerationOfExpirationNotice</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Number of ticks (before the expiration of the Bolt On Type) to execute the bolt on expiration notification process</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="999999">
							<name>TicksPreviousNotification</name>
						</characteristic>

						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>DoesNotApplyToLongDistance</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Doesn' t Apply Air Time</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>DoesNotApplyToAirTime</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Highest Priority for 100% Reduction</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>HighestPriorityFor100Reduction</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Not applied if credit is 0</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>NotAppliedIfCreditIs0</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Restrictive</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>Restrictive</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Freeze rate units</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>FreezeRateUnits</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Exclude in queries from IVR </annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>ExcludedInQueriesFromIVR</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Applied after additional balance</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>AppliedAfterAdditionalBalance</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>X/N for events</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>X_NForEvents</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>X/N multisession</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>X_NMultisession</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Strict Notification</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>StrictNotification</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Dynamic Rating</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>DynamicRating</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Used for IVR balance queries</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>BalanceQueryIVR</name>
						</characteristic>

					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>GeneralBoltOnValuesSpec</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>General from bolton</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>GeneralForBoltOn</name>
							<value>false</value>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Bolt On Value</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="999999999999999" defaultValue="0">
							<name>BoltOnValue</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Apply % reduction in long distance</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>ApplyReductionInLongDistance</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>                                                                                                                                                                                                                                                               </annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="999999999999999">
							<name>PercentageReductionInLD</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Units of a complete cycle (seconds/bytes)</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="999999999999999" defaultValue="0">
							<name>UnitsOfACompleteCycle</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Last free units in the cycle (seconds/bytes)</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="999999999999999" defaultValue="0">
							<name>LastFreeUnitsInTheCycle</name>
						</characteristic>

					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>GeneralStartEndDateSpec</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>General from bolton</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>GeneralForBoltOn</name>
							<value>false</value>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Start date</annotation> -->
						<characteristic  type="xsd:dateTime">
							<name>StartDateTime</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>End date</annotation> -->
						<characteristic  type="xsd:dateTime">
							<name>EndDateTime</name>
						</characteristic>

					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>SessionSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Bolt on duration</annotation> -->
						<characteristic  type="xsd:string" enumValues="TOTAL;NONE;PARTIAL" defaultValue="NONE">
							<name>BoltOnDuration</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Maximum Number of Sessions</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="99999">
							<name>MaximumNumberOfSessions</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Maximum of units per session</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="9999999999">
							<name>MaximumOfUnitsPerSession</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Sessions Threshold</annotation> -->
						<characteristic  type="xsd:long" minValue="0" defaultValue="0">
							<name>SessionsThreshold</name>
						</characteristic>
						<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
							<name>SessionTechnicalSpec</name>
							<item>
								<!-- <xsd:annotation> <xsd:documentation><p>IVR interfaces</annotation> -->
								<characteristic  type="xsd:boolean" defaultValue="0">
									<name>IVR_Interfaces</name>
									<value>false</value>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>MS interfaces</annotation> -->
								<characteristic  type="xsd:boolean" defaultValue="0">
									<name>MS_Interfaces</name>
									<value>false</value>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>CDRs</annotation> -->
								<characteristic  type="xsd:boolean" defaultValue="0">
									<name>CDRs</name>
									<value>false</value>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>Exhausted Sessions Event</annotation> -->
								<characteristic  type="xsd:boolean" defaultValue="0">
									<name>ExhaustedSessionsEvent</name>
									<value>false</value>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>Sessions  Threshold Event</annotation> -->
								<characteristic  type="xsd:boolean" defaultValue="0">
									<name>SessionsThresholdEvent</name>
									<value>false</value>
								</characteristic>

							</item>
						</complexCharacteristic>

					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="0" maxOccurs="1" individualItemUpdate="false">
					<name>HappyBoltOnSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Period established by provision</annotation> -->
						<characteristic  type="xsd:boolean">
							<name>GeneralPeriod</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Rate/Reduction Coefficient</annotation> -->
						<characteristic  type="xsd:string" enumValues="RATE;CR" defaultValue="CR">
							<name>RateOrReductionSwitch</name>
						</characteristic>
						<complexCharacteristic minOccurs="0" maxOccurs="1" individualItemUpdate="false">
							<name>ReductionCoefficient</name>
							<item>
								<!-- <xsd:annotation> <xsd:documentation><p>Franchised Bolt On Value</annotation> -->
								<characteristic  type="xsd:long" minValue="0" maxValue="1000" defaultValue="0">
									<name>ReductionValue</name>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>Setting % reduction in long distance</annotation> -->
								<characteristic  type="xsd:boolean" defaultValue="0">
									<name>ApplyReductionInLongDistance</name>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>                                                                                                                                                                                                                                                               </annotation> -->
								<characteristic  type="xsd:long" minValue="0" maxValue="1000">
									<name>LongDistanceReductionValue</name>
								</characteristic>

							</item>
						</complexCharacteristic>
						<complexCharacteristic minOccurs="0" individualItemUpdate="true">
							<name>RateSpecification</name>
							<!--<item operation="A">

								<relations>-->
									<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
									<!--<relation name="TemporalAppCondition" oCSTargetEntityType="TemporalAppCondition_TC" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
										<item>
											<target>
												<context>
													<valuePairs/>
													<scheme>String</scheme>
												</context>
												<dn>
													<valuePairs/>
													<scheme>TemporalAppCondition_TC</scheme>
												</dn>
												<entityType>TemporalAppCondition</entityType>
											</target>
										</item>
									</relation>-->
									<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
									<!--<relation name="Rate" oCSTargetEntityType="Rate_TC" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
										<item>
											<target>
												<context>
													<valuePairs/>
													<scheme>String</scheme>
												</context>
												<dn>
													<valuePairs/>
													<scheme>Rate_TC</scheme>
												</dn>
												<entityType>Rate</entityType>
											</target>
										</item>
									</relation>
								</relations>
							</item>-->
						</complexCharacteristic>
						<complexCharacteristic minOccurs="0" individualItemUpdate="true">
							<name>HappyBoltOnPeriod</name>
							<item operation="A">
								<!-- <xsd:annotation> <xsd:documentation><p>Year</annotation> -->
								<characteristic  type="xsd:long" minValue="1990" maxValue="9999" defaultValue="1990">
									<name>Year</name>
									<value></value>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>Month </annotation> -->
								<characteristic  type="xsd:string" maxLength="12" defaultValue="00000000000">
									<name>Month</name>
									<value></value>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>Day</annotation> -->
								<characteristic  type="xsd:string" maxLength="31" defaultValue="0000000000000000000000000000000">
									<name>Day</name>
									<value></value>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>Hour</annotation> -->
								<characteristic  type="xsd:string" maxLength="24" defaultValue="000000000000000000000000">
									<name>Hour</name>
									<value></value>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>Week Day</annotation> -->
								<characteristic  type="xsd:string" maxLength="7" defaultValue="0000000">
									<name>WeekDay</name>
									<value></value>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>Duration</annotation> -->
								<characteristic  type="xsd:long" minValue="0" maxValue="9999" defaultValue="0">
									<name>Duration</name>
									<value></value>
								</characteristic>

							</item>
						</complexCharacteristic>

					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>UCRelatedSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Valid Bolt On for Usage Control</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>ValidBoltOnForUsageControl</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Validity (enum + text field)</annotation> -->
						<characteristic  type="xsd:string" enumValues="DAYS;MONTHS;END_OF_DAY;END_OF_MONTH;MINUTES">
							<name>TypeOfPeriod</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="99999999">
							<name>Period</name>
						</characteristic>

					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" individualItemUpdate="true">
					<name>ThresholdSpecification</name>
					<item operation="A">
						<!-- <xsd:annotation> <xsd:documentation><p>It identifies if threshold for Value or for Renewal Counter</annotation> -->
						<characteristic  type="xsd:string" enumValues="VALUE_COUNTER;RENEWAL_COUNTER">
							<name>ThresholdType</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Value</annotation> -->
						<characteristic  type="xsd:long" minValue="0">
							<name>ThresholdValue</name>
						</characteristic>

					</item>
				</complexCharacteristic>
			</oCSProductSpecCharacteristic>
			<relations>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="ApplicabilityCondition" oCSTargetEntityType="ApplicabilityCondition_TC" minOccurs="0" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>Testing</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>ApplicabilityCondition_TC</scheme>
							</dn>
							<entityType>ApplicabilityCondition</entityType>
						</target>
					</item>
				</relation>
			</relations>
		</oCSProductSpecification>
</OCSEntityType>
