<OCSEntityType>
		<oCSProductSpecification>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductSpecification: name; description; productNumber; validFor; lifeCycleStatus -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>CALifeCycleStatus_T1</scheme>
				</dn>
				<entityType>CALifeCycleStatus</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>CALifeCycleStatus</type>
			<oCSProductSpecCharacteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<characteristic  type="xsd:long" minValue="0" maxValue="99999">
					<name>StatusId</name>
					<value>1234</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Status Description</annotation> -->
				<characteristic  type="xsd:string" maxLength="240">
					<name>StatusDescription</name>
					<value>StatusDescription</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Inactivity status</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>InactivityStatus</name>
					<value>0</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Time of permanence in the status (hours):</annotation> -->
				<characteristic  type="xsd:long" minValue="0" maxValue="99999">
					<name>TimeOfPermanenceInTheStatus</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>The activation is retried periodically, for a maximum period of time</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>PeriodicRetries</name>
					<value>0</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Number of hours between attempts</annotation> -->
				<characteristic  type="xsd:long" minValue="0" maxValue="99999">
					<name>NumberOfHoursBetweenAttempts</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>The reactivation date is modified after a successful attempt</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>ModifyRenewalDateAfterRetryOK</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Number of recurrent activation phases during which it is retried</annotation> -->
				<characteristic  type="xsd:long" minValue="0" maxValue="99999">
					<name>NumOfRecurrentActivationPhases</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>When this status is set, it will be notified via ASN1</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>ChangeOfStatusNotifiedViaASN1</name>
					<value>0</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>When a change of reactivation date takes place, it will be notified via ASN1</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>ChangeOfReactDateNotifiedViaASN1</name>
					<value>0</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>The recurrent activation is retried after a Charges Manager operation</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>AfterOperationRetries</name>
					<value>0</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Outstanding executions are recovered and they will be charged when subscriber has balance again</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>OutstandingExecutionsAreRecovere</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Maximum number of delays</annotation> -->
				<characteristic  type="xsd:long" minValue="0" maxValue="999">
					<name>MaximumNumberOfDelays</name>
					<value>123</value>
				</characteristic>
			</oCSProductSpecCharacteristic>
		</oCSProductSpecification>
</OCSEntityType>
