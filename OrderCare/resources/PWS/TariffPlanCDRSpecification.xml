<OCSEntityType>
		<oCSProductSpecification>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductSpecification: name; description; productNumber; validFor; lifeCycleStatus -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>TariffPlanCDRSpecification_TC</scheme>
				</dn>
				<entityType>TariffPlanCDRSpecification</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>TariffPlanCDRSpecification</type>
			<oCSProductSpecCharacteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Generate CDR to a different file</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>DifferentFile</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Activate partial CDRs by stretch</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>PartialCDR</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Length of time of stretch (in seconds)</annotation> -->
				<characteristic  type="xsd:long">
					<name>StretchTime</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Generate CDR with operation information without associated cost</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>CdrWithOperationInformation</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>The CDRs of this Plan will be processed in the Balance Control node</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>BalanceControlNode</name>
				</characteristic>
			</oCSProductSpecCharacteristic>
		</oCSProductSpecification>
</OCSEntityType>
