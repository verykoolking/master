<OCSEntityType>
		<oCSProductSpecification>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductSpecification: name; description; productNumber; validFor; lifeCycleStatus -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>UsageControlThresholdSetSpec_TC</scheme>
				</dn>
				<entityType>UsageControlThresholdSetSpecification</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>UsageControlThresholdSetSpecification</type>
			<oCSProductSpecCharacteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Threshold Id</annotation> -->
				<characteristic  type="xsd:long" minValue="0" maxValue="99">
					<name>ThresholdSetIdentifier</name>
					<value>1</value>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Description</annotation> -->
				<characteristic  type="xsd:string" maxLength="50">
					<name>Description</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Threshold values as percentage</annotation> -->
				<complexCharacteristic minOccurs="0" maxOccurs="5" isOrdered="true" individualItemUpdate="false">
					<name>SetIsAnOrderedListOf</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Cyclic (for T1)</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>Cyclic</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Threshold 1</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="9223372036854775807">
							<name>UsageThresholdAmount</name>
						</characteristic>

					</item>
				</complexCharacteristic>
			</oCSProductSpecCharacteristic>
		</oCSProductSpecification>
</OCSEntityType>
