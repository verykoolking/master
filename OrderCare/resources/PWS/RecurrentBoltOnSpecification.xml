<OCSEntityType>
		<oCSProductSpecification>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductSpecification: name; description; productNumber; validFor; lifeCycleStatus -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>RecurrentBoltOnSpecification_T</scheme>
				</dn>
				<entityType>RecurrentBoltOnSpecification</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>RecurrentBoltOnSpecification</type>
			<oCSProductSpecCharacteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<characteristic  type="xsd:any">
					<name>RecurrentBoltOnSpecification</name>
				</characteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>CABOCycleChngeSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Properties Bolt on</annotation> -->
						<characteristic  type="xsd:any">
							<name>CABOCycleChngeSpecification</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Awarded Amount during a cycle change</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="9223372036854769664" isOverride="true">
							<name>boltonAmount</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Number of sessions during a cycle change</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="99999" isOverride="true">
							<name>boltonMaxNumberOfSessions</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>% reduction in long distance during a cycle change</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="9999" isOverride="true">
							<name>boltonLD</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>It is a percentage of the awarded amount during a cycle change

</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0" isOverride="true">
							<name>calcBoltonAmountCode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>It is a percentage of % reduction in long distance during a cycle change</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0" isOverride="true">
							<name>calcBoltonLDCode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>It is a percentage of the cost of the recurrent bolton provision during a cycle change
</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0" isOverride="true">
							<name>calcBoltonCostCode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>It is a percentage of the number of sessions during a cycle change</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0" isOverride="true">
							<name>calcMaxNumberOfSessionsCode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>It is a percentage of the maximum number of units per sessions during a cycle change</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>PercenMaxNumberUnitsPerSessions</name>
						</characteristic>

					</item>
				</complexCharacteristic>
			</oCSProductSpecCharacteristic>
			<relations>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="CyclicActionSpecification" oCSTargetEntityType="CyclicActionSpecification" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>CyclicActionSpecification</scheme>
							</dn>
							<entityType>CyclicActionSpecification</entityType>
						</target>
					</item>
				</relation>
			</relations>
		</oCSProductSpecification>
</OCSEntityType>
