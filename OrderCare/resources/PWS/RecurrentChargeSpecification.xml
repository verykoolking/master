<OCSEntityType>
		<oCSProductSpecification>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductSpecification: name; description; productNumber; validFor; lifeCycleStatus -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>RecurrentChargeSpecification_T</scheme>
				</dn>
				<entityType>RecurrentChargeSpecification</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>RecurrentChargeSpecification</type>
			<oCSProductSpecCharacteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>A Charge</annotation> -->
				<characteristic  type="xsd:any">
					<name>RecurrentChargeSpecification</name>
				</characteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>CACHCycleChngeSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Properties Charge</annotation> -->
						<characteristic  type="xsd:any">
							<name>CACHCycleChngeSpecification</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Allow partial charge during a cycle change</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0" isOverride="true">
							<name>totalFlag</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>It is a percentage of the amount of the recurrent charge during a cycle change
</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0" isOverride="true">
							<name>calcChargingAmountCode</name>
						</characteristic>

					</item>
				</complexCharacteristic>
			</oCSProductSpecCharacteristic>
			<relations>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="CyclicActionSpecification" oCSTargetEntityType="CyclicActionSpecification" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>CyclicActionSpecification</scheme>
							</dn>
							<entityType>CyclicActionSpecification</entityType>
						</target>
					</item>
				</relation>
			</relations>
		</oCSProductSpecification>
</OCSEntityType>
