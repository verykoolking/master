<OCSEntityType>
		<oCSProductSpecification>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductSpecification: name; description; productNumber; validFor; lifeCycleStatus -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>TribeSpecification_T</scheme>
				</dn>
				<entityType>TribeSpecification</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>TribeSpecification</type>
			<oCSProductSpecCharacteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Priority</annotation> -->
				<characteristic  type="xsd:long" minValue="0" maxValue="9999">
					<name>TribePriority</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Maximum members in the tribe</annotation> -->
				<characteristic  type="xsd:long" minValue="2" maxValue="99999999">
					<name>MaxMembersInTheTribe</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Maximum external members in the Tribe</annotation> -->
				<characteristic  type="xsd:long" minValue="0" maxValue="99999999" defaultValue="0">
					<name>MaxExternalMembersTribe</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Code</annotation> -->
				<characteristic  type="xsd:string" maxLength="4">
					<name>RegExCode</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Description</annotation> -->
				<characteristic  type="xsd:string" maxLength="40">
					<name>RegExDescription</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>String</annotation> -->
				<characteristic  type="xsd:string" maxLength="128">
					<name>RegExString</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Imported subscribers are allowed to pass from external members to owned members</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="1">
					<name>ImpSubsAllFromExToOwn</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Exported subscribers are allowed to pass from owned members to external members</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="1">
					<name>ExpSubsAllFromOwnToExt</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>Infinite Date</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>InfiniteDate</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>    Start Date</annotation> -->
				<characteristic  type="xsd:dateTime" defaultValue="01/01/1999">
					<name>StartDate</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>    End Date</annotation> -->
				<characteristic  type="xsd:dateTime" defaultValue="01/01/1999">
					<name>EndDate</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>End Allowed Subscription Date</annotation> -->
				<characteristic  type="xsd:dateTime">
					<name>EndAllowedSubscriptionDate</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>    Voice Tribe</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>VoiceTribe</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>    SMS Tribe</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>SMSTribe</name>
				</characteristic>
			</oCSProductSpecCharacteristic>
		</oCSProductSpecification>
</OCSEntityType>
