<OCSEntityType>
		<oCSProductSpecification>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductSpecification: name; description; productNumber; validFor; lifeCycleStatus -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>PromotionalProfileSpecification_T</scheme>
				</dn>
				<entityType>PromotionalProfileSpecification</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>PromotionalProfileSpecification</type>
			<oCSProductSpecCharacteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<characteristic  type="xsd:any">
					<name>PromotionalProfileSpecification</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>      The profile will apply to all promotions existing in the system with the profile filter configured</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>ApplyToAllPromotionsWithProfile</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>      Allow Overlap</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>AllowOverlap</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>      Validity period</annotation> -->
				<characteristic  type="xsd:long" maxValue="999999999">
					<name>ValidityPeriod</name>
				</characteristic>
			</oCSProductSpecCharacteristic>
		</oCSProductSpecification>
</OCSEntityType>
