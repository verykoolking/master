<OCSEntityType>
		<oCSProductOffering>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductOffering: id; name; description; validFor; status -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>TypeOfService_T</scheme>
				</dn>
				<entityType>TypeOfService</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>TypeOfService</type>
			<oCSProductSpecCharValueUse>
				<!-- <xsd:annotation> <xsd:documentation><p>Service</annotation> -->
				<characteristic  type="xsd:string" enumValues="1 - Voice Calls;2 - SMS/MMS;3 - Data;4 - Contents;5 - Other Services">
					<name>ServiceID</name>
				</characteristic>
			</oCSProductSpecCharValueUse>
			<relations>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="ServiceTariffPlanAllowedPer" oCSTargetEntityType="ServiceTariffPlan" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>ServiceTariffPlan</scheme>
							</dn>
							<entityType>ServiceTariffPlanAllowedPer</entityType>
						</target>
					</item>
				</relation>
			</relations>
		</oCSProductOffering>
</OCSEntityType>
