<OCSEntityType>
		<oCSProductSpecification>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductSpecification: name; description; productNumber; validFor; lifeCycleStatus -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>TariffPlanSpecification_TC</scheme>
				</dn>
				<entityType>TariffPlanSpecification</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>TariffPlanSpecification</type>
			<oCSProductSpecCharacteristic>
				<complexCharacteristic minOccurs="0" maxOccurs="1" individualItemUpdate="false">
					<name>BulkLoadFile</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Bulk Load File Code</annotation> -->
						<characteristic  type="xsd:string" maxLength="30">
							<name>BulkLoadFileCode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Default Bulk Load File</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>Default</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>OCSRE Profile</annotation> -->
						<characteristic  type="xsd:long" maxValue="9999999999">
							<name>ReSubscriberProfileCode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Sale Modality</annotation> -->
						<characteristic  type="xsd:string" maxLength="2">
							<name>SaleModalityCode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Subscription Type</annotation> -->
						<characteristic  type="xsd:string" maxLength="3">
							<name>SubscriptionType</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Prepay Substatus after Subscription</annotation> -->
						<characteristic  type="xsd:string" maxLength="1">
							<name>SubstatusCode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Initial Amount</annotation> -->
						<characteristic  type="xsd:long">
							<name>InitialAmount</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Amount of the threshold to make a change of status</annotation> -->
						<characteristic  type="xsd:long">
							<name>ThresholdAmount</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Credit limit</annotation> -->
						<characteristic  type="xsd:long">
							<name>CreditLimit</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Upper limit amount int he subscription</annotation> -->
						<characteristic  type="xsd:long">
							<name>UpperLimit</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Unblock Flag</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>UnblokFlag</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>   Activation Flag</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>ActivationFlag</name>
						</characteristic>

						<relations>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="TariffPlan" oCSTargetEntityType="TariffPlan_TC" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>TariffPlan_TC</scheme>
										</dn>
										<entityType>TariffPlan</entityType>
									</target>
								</item>
							</relation>
						</relations>
					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>TariffPlanGeneralParametersSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>IVR Code</annotation> -->
						<characteristic  type="xsd:long">
							<name>IVRCode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Subscriber type</annotation> -->
						<characteristic  type="xsd:string" enumValues="0;1;2">
							<name>SubscriberType</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Calling Card</annotation> -->
						<characteristic  type="xsd:string" enumValues="N;W;B;R">
							<name>CallingCardType</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Distance Id.</annotation> -->
						<characteristic  type="xsd:string" enumValues="N;S;M">
							<name>DistanceId</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Tariff Plan with unlimited consumption limits</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>UnlimitedConsumption</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Sending SMS allowed</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="1">
							<name>AllowedSendingAdminSMS</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Accept SOS Top Ups</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>AcceptSOSTopUps</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>    Number of Tops Ups</annotation> -->
						<characteristic  type="xsd:long" maxValue="999">
							<name>NumberOfTopUps</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Tariff Plan for personal call to corporate subscribers</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>AllowedPersonalCallsToCorpSubscr</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Apply maximum balance</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>ApplyMaximumBalance</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Activate OFCS log</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>OfflineLog</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Subscriber's cycle day is automatically calculated</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>CycleDayAutomaticCalculate</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Day of the month</annotation> -->
						<characteristic  type="xsd:long">
							<name>CalculationDay</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Billing day</annotation> -->
						<characteristic  type="xsd:long">
							<name>ApplyCycleDay</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Subscriber's cycle day</annotation> -->
						<characteristic  type="xsd:long">
							<name>SubscriberCycleDay</name>
						</characteristic>

					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>TariffPlanRestrictionSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>       Minimum Balance</annotation> -->
						<characteristic  type="xsd:long">
							<name>MinimumBalance</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>       Simultaneous calls</annotation> -->
						<characteristic  type="xsd:long" defaultValue="10">
							<name>SimultaneousCalls</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>       Fixed Wireless</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>FixedWireless</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>       maximum number of SMS reserves</annotation> -->
						<characteristic  type="xsd:long" defaultValue="10">
							<name>SimultaneousSMS</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>       The tariff plan does not allow that subcribers send SMS</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>SendSMSNotAllowed</name>
						</characteristic>

					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<name>TariffPlanNotificationSpecification</name>
					<item>
						<!-- <xsd:annotation> <xsd:documentation><p>Send notification by charge in wallets</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>SendNotificatioByChargeInWallets</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Send notification by charge in bolton types</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>SendNotificationByChargeInBOT</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Give priority to Plan configuration</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>TariffPlanConfigurationPriority</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>        Balance</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>Balance</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>        expiration</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>Expiration</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>        Cost</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>Cost</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>        After call end</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>AfterCallEnd</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>        after sms sending</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>AfterSmsSending</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>        After data session end</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>AfterDataSessionEnd</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>        after sms receiving</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>AfterSmsReceiving</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>        After administrative operation</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>AfterAdminOperation</name>
						</characteristic>

						<relations>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="SetOfWallets" oCSTargetEntityType="SetOfWallets_TC" minOccurs="0" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>SetOfWallets_TC</scheme>
										</dn>
										<entityType>SetOfWallets</entityType>
									</target>
								</item>
							</relation>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="SetOfBoltOnTypes" oCSTargetEntityType="SetOfBoltOnTypes_TC" minOccurs="0" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>SetOfBoltOnTypes_TC</scheme>
										</dn>
										<entityType>SetOfBoltOnTypes</entityType>
									</target>
								</item>
							</relation>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="BoltOnTypeGroup" oCSTargetEntityType="BoltOnTypeGroup_TC" minOccurs="0" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>BoltOnTypeGroup_TC</scheme>
										</dn>
										<entityType>BoltOnTypeGroup</entityType>
									</target>
								</item>
							</relation>
						</relations>
					</item>
				</complexCharacteristic>
			</oCSProductSpecCharacteristic>
			<relations>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="LifeCycle" oCSTargetEntityType="LifeCycle_TC" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>LifeCycle_TC</scheme>
							</dn>
							<entityType>LifeCycle</entityType>
						</target>
					</item>
				</relation>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="ChargingMode" oCSTargetEntityType="ChargingMode_TC" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>ChargingMode_TC</scheme>
							</dn>
							<entityType>ChargingMode</entityType>
						</target>
					</item>
				</relation>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="TariffPlanCDRSpecification" oCSTargetEntityType="TariffPlanCDRSpecification_TC" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>TariffPlanCDRSpecification_TC</scheme>
							</dn>
							<entityType>TariffPlanCDRSpecification</entityType>
						</target>
					</item>
				</relation>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="TariffPlanGroupForBalanceTrans" oCSTargetEntityType="TariffPlanGroupForBalanceTransfer_TC" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>TariffPlanGroupForBalanceTransfer_TC</scheme>
							</dn>
							<entityType>TariffPlanGroupForBalanceTrans</entityType>
						</target>
					</item>
				</relation>
			</relations>
		</oCSProductSpecification>
</OCSEntityType>
