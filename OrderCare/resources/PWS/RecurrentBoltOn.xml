<OCSEntityType>
		<oCSProductOffering>
			<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
			<!-- Attributes inherited from SID EntityBase: identifier -->
			<!-- Attributes inherited from SID ProductOffering: id; name; description; validFor; status -->
			<!-- Compulsory SID attributes -->
			<identifier>
				<context>
					<valuePairs/>
					<scheme>Testing</scheme>
				</context>
				<dn>
					<valuePairs/>
					<scheme>RecurrentBoltOn_T</scheme>
				</dn>
				<entityType>RecurrentBoltOn</entityType>
			</identifier>
			<!-- NGAA attributes -->
			<type>RecurrentBoltOn</type>
			<oCSProductSpecCharValueUse>
				<!-- <xsd:annotation> <xsd:documentation><p>Large Code of Cyclic Action</annotation> -->
				<characteristic  type="xsd:string" maxLength="16">
					<name>LargeCodeOfCyclicAction</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>    Grace Period</annotation> -->
				<characteristic  type="xsd:long" minValue="0" maxValue="99999" defaultValue="0">
					<name>GracePeriod</name>
				</characteristic>
				<!-- <xsd:annotation> <xsd:documentation><p>        Tax applies</annotation> -->
				<characteristic  type="xsd:boolean" defaultValue="0">
					<name>TaxApplies</name>
				</characteristic>
				<complexCharacteristic minOccurs="0" individualItemUpdate="true">
					<name>RecurrentBoltOnRenewalPhase</name>
					<item operation="A">
						<!-- <xsd:annotation> <xsd:documentation><p>        Phase Number</annotation> -->
						<characteristic  type="xsd:long" minValue="0" maxValue="9999999">
							<name>PhaseNumber</name>
						</characteristic>
						<complexCharacteristic minOccurs="1" maxOccurs="1" individualItemUpdate="false">
							<name>CyclicBoltOnAwarding</name>
							<item>
								<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
								<characteristic  type="xsd:any">
									<name>CyclicBoltOnAwarding</name>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>        Subsized Amount</annotation> -->
								<characteristic  type="xsd:long" minValue="0" isOverride="true">
									<name>boltonAmount</name>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>        % reduction in long distance</annotation> -->
								<characteristic  type="xsd:long" minValue="0" isOverride="true">
									<name>boltonLD</name>
								</characteristic>
								<!-- <xsd:annotation> <xsd:documentation><p>        Number of sessions</annotation> -->
								<characteristic  type="xsd:long" minValue="0" maxValue="99999" isOverride="true">
									<name>boltonMaxNumberOfSessions</name>
								</characteristic>

							</item>
						</complexCharacteristic>

						<relations>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="ChargingMode" oCSTargetEntityType="ChargingMode" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>ChargingMode</scheme>
										</dn>
										<entityType>ChargingMode</entityType>
									</target>
								</item>
							</relation>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="Price" oCSTargetEntityType="Price" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>Price</scheme>
										</dn>
										<entityType>Price</entityType>
									</target>
								</item>
							</relation>
						</relations>
					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="1" maxOccurs="unbounded" individualItemUpdate="true">
					<name>PriceGuiding</name>
					<item operation="A">
						<!-- <xsd:annotation> <xsd:documentation><p>Mode</annotation> -->
						<characteristic  type="xsd:string" enumValues="E;C">
							<name>Mode</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>User Profile</annotation> -->
						<characteristic  type="xsd:string" enumValues="*;PROM ;SIM ;boweb;sips;web">
							<name>CustomerProfile</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Tax applies</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="0">
							<name>TaxApplies</name>
						</characteristic>
						<!-- <xsd:annotation> <xsd:documentation><p>Promotions applied on the cost</annotation> -->
						<characteristic  type="xsd:boolean" defaultValue="1">
							<name>PromotionsAppliedOnThePrice</name>
						</characteristic>

						<relations>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="Price" oCSTargetEntityType="Price" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>Price</scheme>
										</dn>
										<entityType>Price</entityType>
									</target>
								</item>
							</relation>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="TariffPlan" oCSTargetEntityType="TariffPlan" minOccurs="0" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>TariffPlan</scheme>
										</dn>
										<entityType>TariffPlan</entityType>
									</target>
								</item>
							</relation>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="Province" oCSTargetEntityType="Province" minOccurs="0" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>Province</scheme>
										</dn>
										<entityType>Province</entityType>
									</target>
								</item>
							</relation>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="Operation" oCSTargetEntityType="Operation" minOccurs="0" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>Operation</scheme>
										</dn>
										<entityType>Operation</entityType>
									</target>
								</item>
							</relation>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="ServiceProvider" oCSTargetEntityType="ServiceProvider" minOccurs="0" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>ServiceProvider</scheme>
										</dn>
										<entityType>ServiceProvider</entityType>
									</target>
								</item>
							</relation>
						</relations>
					</item>
				</complexCharacteristic>
				<complexCharacteristic minOccurs="0" individualItemUpdate="true">
					<name>ChargingModeGuiding</name>
					<item operation="A">
						<!-- <xsd:annotation> <xsd:documentation><p>Mode</annotation> -->
						<characteristic  type="xsd:string" enumValues="E;C">
							<name>Mode</name>
						</characteristic>

						<relations>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="ChargingMode" oCSTargetEntityType="ChargingMode" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>ChargingMode</scheme>
										</dn>
										<entityType>ChargingMode</entityType>
									</target>
								</item>
							</relation>
							<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
							<relation name="Operation" oCSTargetEntityType="Operation" minOccurs="0" maxOccurs="1" individualItemUpdate="false">
								<item>
									<target>
										<context>
											<valuePairs/>
											<scheme>String</scheme>
										</context>
										<dn>
											<valuePairs/>
											<scheme>Operation</scheme>
										</dn>
										<entityType>Operation</entityType>
									</target>
								</item>
							</relation>
						</relations>
					</item>
				</complexCharacteristic>
			</oCSProductSpecCharValueUse>
			<relations>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="RecurrentBoltOnSpecification" oCSTargetEntityType="RecurrentBoltOnSpecification" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>RecurrentBoltOnSpecification</scheme>
							</dn>
							<entityType>RecurrentBoltOnSpecification</entityType>
						</target>
					</item>
				</relation>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="PriceForCycleChange" oCSTargetEntityType="Price" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>Price</scheme>
							</dn>
							<entityType>PriceForCycleChange</entityType>
						</target>
					</item>
				</relation>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="ChargingModeForCycleChange" oCSTargetEntityType="ChargingMode" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>ChargingMode</scheme>
							</dn>
							<entityType>ChargingModeForCycleChange</entityType>
						</target>
					</item>
				</relation>
				<!-- <xsd:annotation> <xsd:documentation><p>null</annotation> -->
				<relation name="BoltOn" oCSTargetEntityType="BoltOn" minOccurs="1" maxOccurs="1" individualItemUpdate="false">
					<item>
						<target>
							<context>
								<valuePairs/>
								<scheme>String</scheme>
							</context>
							<dn>
								<valuePairs/>
								<scheme>BoltOn</scheme>
							</dn>
							<entityType>BoltOn</entityType>
						</target>
					</item>
				</relation>
			</relations>
		</oCSProductOffering>
</OCSEntityType>
