  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select
           itemattributecode,defaultvalue
        from
            cwpc_itemattribute_v
        where
            1=1
            {[0] and itemcode = ?}
        union
        select
            'description',description
        from
            cwpc_item
        where
             1=1
            {[0] and cwpc_item.ITEMCODE = ?}
        union
        select
            'name',name
        from
            cwpc_item
        where
             1=1
            {[0] and cwpc_item.ITEMCODE = ?}
      ]]></script>
    </method>
  </methodList>
