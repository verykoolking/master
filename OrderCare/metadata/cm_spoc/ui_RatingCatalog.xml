    <method name="onInit" type="action">
      <description><![CDATA[
        <p style="margin-top: 0">
          This notification method is called by the framework when this application is initialized
        </p>
      ]]></description>
      <script><![CDATA[
        this.cw$super_onInit();

        var curUser = Global.getUserId();
        var isAvail = UserProfile.isAvailable(curUser);

        if(isAvail){
              this.worklistImage = "/cwf/MenuIconTaskAvail.gif";
        }else{
              this.worklistImage = "/cwf/MenuIconTaskUnavail.gif";
        }
        //Global.startTimer(this);

        this.projectImage = "/cwt/images/24/folder.png";
        var proj = cwt_pc.getDefaultProject();
        if (proj)
              this.defaultProject = "Project : " + proj.name;
        else
              this.defaultProject = "Project : NONE";

        this.loginUserImage = "/cwt/images/24/user3.png"
        var curUserProfile = UserProfile.getUserProfile(curUser);
        if (curUserProfile) {
              this.loginUser = "Login User: " +curUserProfile.userName;
        } else {
              this.loginUser = "User Unknown";
        }

        var docUrl = "/cwf/r/help/index.html";
        var imgUrl = "/cwf/r/images/16/help.png";
        this.helpURL = "<html><body><a href=\"" + docUrl + "\" target=\"_blank\"><img src=\"" + imgUrl + "\" border=\"0\" width=\"18\" height=\"18\" /></a></body> </html>";

        var allow = UserProfile.getUserParticipants(UserProfile.userId());
        if (!allow || allow.length == 0)
              this.isWorklistParticipant = false;
        else
              this.isWorklistParticipant = true;

        return new cm_spoc.RatingHomePage(null, this);
      ]]></script>
