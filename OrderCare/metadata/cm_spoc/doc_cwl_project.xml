        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="onInit" type="action">
        <script><![CDATA[
          this.cw$super_onInit();
          this.hasPLM = false;
          if(this.model.projectId) {
              var ord = cm_plm.getPLMOrderFromProjectCode(this.model.projectCode);
              if(ord!=null) {
                  var orderequest = ord.change;
                  this.hasPLM = true;
                  var procFinder = new cwf_pm.ProcessFinder();
                  procFinder.searchDocument.ORDER_ID = orderequest.cwOrderId;
                  procFinder.search();
                  this.processForm = new cwf_pm.ProcessFinder.UserInterface(procFinder, this);
                  this.errorMessage = orderequest.errorList;
              }
          }
        ]]></script>
      </method>
      <method name="showPLMButton" type="permMethod">
        <script><![CDATA[return (this.model.projectId!=null && !this.hasPLM);]]></script>
      </method>
      <method name="showPLMFinder" type="permMethod">
        <script>return this.hasPLM;</script>
      </method>
      <method name="isStatusEditable" type="permMethod">
        <script><![CDATA[return this.detailEditable() && !this.hasPLM;]]></script>
      </method>
      <method name="startPLM" type="usrActionMethod">
        <script><![CDATA[
          if(cwa_epc.getDefaultProj() == null)
              Global.setSessionParameter("project", this.model);
          cm_plm.startPLM(this.model.projectCode, "GUI", false, false, true, true);
          this.onInit();
        ]]></script>
      </method>
      <method name="showError" type="permMethod">
        <script>return this.errorMessage != null;</script>
      </method>
      <method name="isWorkflowStatusEditable" type="permMethod">
        <script>return false;</script>
      </method>
    </methodList>
  </UserInterface>
</document>