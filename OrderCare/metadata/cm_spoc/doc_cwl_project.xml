  <variableList>
    <variable name="plmOrderStatus" type="leaf">
      <dontMap>true</dontMap>
      <valueType>dtype_cm_plm.plmOrderStatusType</valueType>
    </variable>
  </variableList>
  <methodList>
    <method name="getStatus" type="uimthd">
      <valueType>dtype_com.conceptwave.system.String</valueType>
      <script><![CDATA[
        if(this.plmOrderStatus != null)
            return Global.getCodeDescription('cm_plm.plmOrderStatusType', this.plmOrderStatus, null);
        else if(this.status != null)
            return Global.getCodeDescription('cwt_pclm.status', this.status, null);
        return null;
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
