                      </variablePathList>
                    </visible>
                  </element>
                  <element name="ErrorLayout" type="elgrd">
                    <alignment>left,center</alignment>
                    <colWidths>1%,99%</colWidths>
                    <fillSpace>false</fillSpace>
                    <numOfCols>2</numOfCols>
                    <visible type="varPath">
                      <variablePathList>
                        <variablePath>doc_cm_spoc.cwl_project/ui_UserInterface/permMethod_showError</variablePath>
                      </variablePathList>
                    </visible>
                    <width>100%</width>
                    <elementList>
                      <element name="errorIcon" type="elimg">
                        <columnSpan>1</columnSpan>
                        <imageUrl>/cwt/images/24/error.png</imageUrl>
                        <rowSpan>0</rowSpan>
                        <showLabel>false</showLabel>
                        <visible type="varPath">
                          <variablePathList>
                            <variablePath>doc_cm_spoc.cwl_project/ui_UserInterface/permMethod_showError</variablePath>
                          </variablePathList>
                        </visible>
                      </element>
                      <element name="errorMessage" type="ellab">
                        <columnSpan>1</columnSpan>
                        <rowSpan>0</rowSpan>
                        <style>cwErrorMessageText</style>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>doc_cwt_pc.cwt_project/ui_UserInterface/leaf_errorMessage</variablePath>
                          </variablePathList>
                        </variable>
                        <visible type="varPath">
                          <variablePathList>
                            <variablePath>doc_cm_spoc.cwl_project/ui_UserInterface/permMethod_showError</variablePath>
                          </variablePathList>
                        </visible>
                        <width>100%</width>
                      </element>
                    </elementList>
                  </element>
                  <element name="LayoutSpacer2" type="ellsp">
                    <height>10px</height>
                  </element>
                  <element name="PLMLayout" type="elvlt">
                    <height>100%</height>
                    <width>100%</width>
                    <elementList>
                      <element name="startPLM" type="elbtn">
                        <clickMethod type="varPath">
                          <variablePathList>
                            <variablePath>doc_cm_spoc.cwl_project/ui_UserInterface/usrActionMethod_startPLM</variablePath>
                          </variablePathList>
                        </clickMethod>
                        <label>Start PLM</label>
                        <visible type="varPath">
                          <variablePathList>
                            <variablePath>doc_cm_spoc.cwl_project/ui_UserInterface/permMethod_showPLMButton</variablePath>
                          </variablePathList>
                        </visible>
                      </element>
                      <element name="Processes" type="elovf">
                        <formReference type="varPath">
                          <variablePathList>
                            <variablePath>doc_cm_spoc.cwl_project/ui_UserInterface/leaf_processForm</variablePath>
                            <variablePath>findDoc_cwf_pm.ProcessFinder/ui_UserInterface/frmui_Forms</variablePath>
                            <variablePath>findDoc_cwf_pm.ProcessFinder/ui_UserInterface/frmui_Forms/vform_Result</variablePath>
                          </variablePathList>
                        </formReference>
                        <height>220px</height>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>doc_cm_spoc.cwl_project/ui_UserInterface/leaf_processForm</variablePath>
                          </variablePathList>
                        </variable>
                        <visible type="varPath">
                          <variablePathList>
                            <variablePath>doc_cm_spoc.cwl_project/ui_UserInterface/permMethod_showPLMFinder</variablePath>
                          </variablePathList>
                        </visible>
                        <width>100%</width>
                      </element>
                    </elementList>
                  </element>
                  <element name="LayoutSpacer3" type="ellsp">
                    <height>100%</height>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>doc_cwl_plm.cwl_project/ui_UserInterface/frmui_Forms/exov_Default</base>
            <extends>doc_cwl_plm.cwl_project/ui_UserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
          </overlay>
          <overlay name="Detail" type="exov">
            <base>doc_cwt_pc.cwt_project/ui_UserInterface/frmui_Forms/vform_Detail</base>
            <extends>doc_cwt_pc.cwt_project/ui_UserInterface/frmui_Forms/vform_Detail</extends>
            <label>Detail</label>
            <overrideList>
              <override name="GridLayout" type="elext">
                <elementList>
                  <element name="plmOrderStatusType" type="elslct">
                    <label>Order Status</label>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>doc_cm_spoc.cwl_project/ui_UserInterface/uivar_model</variablePath>
                        <variablePath>doc_cm_spoc.cwl_project/leaf_plmOrderStatus</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                </elementList>
              </override>
              <override name="label" type="eldel"/>
            </overrideList>
          </overlay>
          <overlay name="homePageView" type="vform">
            <label>homePageView</label>
          </overlay>
          <overlay name="workflowForm" type="exov">
            <base>doc_cwt_pc.cwt_project/ui_UserInterface/frmui_Forms/vform_workflowForm</base>
            <extends>doc_cwt_pc.cwt_project/ui_UserInterface/frmui_Forms/vform_workflowForm</extends>
