<?xml version="1.0" encoding="UTF-8" ?>
<script name="cm_spoc.posSwitchTop">
  <group>ui_scripts.UsageControl.AccumulationConditions.FinderHelpers</group>
  <label>posSwitchTop</label>
  <metaVersion>4</metaVersion>
  <parameterList>
    <parameter name="pos" type="rifp"/>
    <parameter name="array" type="rifp"/>
  </parameterList>
  <script><![CDATA[
    while(pos > 0){
    cm_spoc.posSwitchUp(pos, array);
    pos--;
    }
  ]]></script>
</script>