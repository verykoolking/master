<?xml version="1.0" encoding="UTF-8" ?>
<findScript name="cm_spoc.SDFinder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <group>ui_scripts.Calendars.Finders</group>
  <input>doc_cwt_pc.cwt_itemSearch</input>
  <label>SDFinder</label>
  <metaVersion>4</metaVersion>
  <output>dstruct_cm_spoc.data.SpecialDay</output>
  <type>script</type>
  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSel" type="action">
      <system>true</system>
      <script>return cm_spoc.getSpecialDays(this);</script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>dstruct_cm_spoc.data.SpecialDay</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_cwt_pc.cwt_itemSearch/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>dstruct_cm_spoc.data.SpecialDay/tree_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>300px</height>
                <width>652px</width>
                <elementList>
                  <element name="specialDayFinderResultTable" type="eltabl">
                    <doubleClickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findScript_cm_spoc.SDFinder/ui_UserInterface/uimthd_onDblClickSpecialDay</variablePath>
                      </variablePathList>
                    </doubleClickMethod>
                    <filterEditorHeight>1</filterEditorHeight>
                    <height>300px</height>
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <showAlternateStyle>true</showAlternateStyle>
                    <showFilter>true</showFilter>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findScript_cm_spoc.SDFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <width>652px</width>
                    <elementList>
                      <element name="SpecialDayType" type="ellab">
                        <cellAlignment>center,center</cellAlignment>
                        <label>Special Day Type</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findScript_cm_spoc.SDFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>dstruct_cm_spoc.data.SpecialDay/dstruct_SpecialDayType</variablePath>
                          </variablePathList>
                        </variable>
                        <width>125px</width>
                      </element>
                      <element name="TypeOfDay" type="ellab">
                        <cellAlignment>center,center</cellAlignment>
                        <label>Type Of Day</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findScript_cm_spoc.SDFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>dstruct_cm_spoc.data.SpecialDay/dstruct_TypeOfDay</variablePath>
                          </variablePathList>
                        </variable>
                        <width>125px</width>
                      </element>
                      <element name="Date" type="ellab">
                        <cellAlignment>center,center</cellAlignment>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findScript_cm_spoc.SDFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>dstruct_cm_spoc.data.SpecialDay/dstruct_Date</variablePath>
                          </variablePathList>
                        </variable>
                        <width>125px</width>
                      </element>
                      <element name="isAnyPlans" type="ellab">
                        <cellAlignment>center,center</cellAlignment>
                        <label>Any Plans</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findScript_cm_spoc.SDFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>dstruct_cm_spoc.data.SpecialDay/dstruct_isAnyPlans</variablePath>
                          </variablePathList>
                        </variable>
                        <width>125px</width>
                      </element>
                      <element name="isDefaultSP" type="ellab">
                        <cellAlignment>center,center</cellAlignment>
                        <label>Default SP</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findScript_cm_spoc.SDFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>dstruct_cm_spoc.data.SpecialDay/dstruct_isDefaultSP</variablePath>
                          </variablePathList>
                        </variable>
                        <width>125px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findScript_cm_spoc.SDFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findScript_cm_spoc.SDFinder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="onDblClickSpecialDay" type="uimthd">
        <script>cm_spoc.onDblClickSpecialDay(this);</script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="priceFinderView" type="findVView">
      <outputOverlay>findScript_cm_spoc.SDFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
    </view>
  </viewList>
</findScript>