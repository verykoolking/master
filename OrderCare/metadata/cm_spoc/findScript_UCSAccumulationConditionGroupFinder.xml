  <methodList>
    <method name="cwOnFinderSel" type="action">
      <system>true</system>
      <script><![CDATA[
        /*var itemFinder = new Finder("cwa_epc.catalogItemFinder");
        itemFinder.searchDocument.baseItemCode = "base_UsageControlApplicabilityCo";
        var result = itemFinder.search();
        return result;*/

        var UCAppType=this.parent.UCAccumulationTypeEnum;

        //var itemFinder = new Finder("cwa_epc.catalogItemFinder");
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode ="base_UsageControlApplicabilityCo";
        var result = itemFinder.search();

        //se non è specificato un tipo restiruisco il finder completo
        if (UCAppType==null) return result;

        //pulisco il valore attualmente salvato nella combo per conformità di comportamento della form
        //this.parent.UCAccumulationConditionGroupEnum=null;

        itemFinder.searchDocument.baseItemCode ="base_UsageControlApplicabilityCo";
        var result = itemFinder.search();
        /*
        //istanzio un finder sulle le relazioni di base_UsageControlSpecification
        var UCSRelationFinder = new Finder("cwt_pc.cwt_itemRelationFinder");
        var UCSRelationResult;
        */
        var currentResult = new DataObjectList();
        //ciclo su tutte le item del tipo base_UsageControlApplicabilityCo create nel catalog
        for ( var i = 0; i < result.length; i++){
            //istanzio il finder che cerca nel catalog gli attributi di una determinata item
            var attrFinder = new Finder ("cm_spoc.ItemCodeAttributeFinder");
            //specifico di quale item deve resituirmi gli attributi passandogli l'itemcode
            attrFinder.searchDocument.itemCode = result[i].itemCode;
            var itemSpec = epc.getItemSpecificationByKey(result[i].itemCode);
            var attrResult = attrFinder.search();

            for ( var j = 0; j < attrResult.length; j++){
                // cerco l'attributo richiesto e controllo se il valore dell'attributo è quello che si vuole mostrare
                //"attributeName" :  attrResult[j].itemAttribute, "defaultValue" :attrResult[j].defaultValue;
                if (attrResult[j].itemAttribute=="UnitType" && attrResult[j].defaultValue==UCAppType) {
                    // salvo il risultato nel dataobject da resituire alla combo
                    currentResult.add(i,result[i])
                    /*
                    UCSRelationFinder.searchDocument.itemCode = attrResult[j].itemCode;
                    UCSRelationResult = UCSRelationFinder.search();
                    for ( var y = 0; y < UCSRelationResult.length; y++){
                        currentResult.add(i,UCSRelationResult[y]);
                    }*/
                }
            }
        }
        return currentResult;
      ]]></script>
    </method>
  </methodList>
