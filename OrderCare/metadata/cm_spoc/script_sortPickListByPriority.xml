<?xml version="1.0" encoding="UTF-8" ?>
<script name="cm_spoc.sortPickListByPriority">
  <label>sortPickListByPriority</label>
  <metaVersion>4</metaVersion>
  <parameterList>
    <parameter name="item1" type="rifp">
      <mandatory>true</mandatory>
      <type>dstruct_cm_spoc.PickListDS</type>
    </parameter>
    <parameter name="item2" type="rifp">
      <mandatory>true</mandatory>
      <type>dstruct_cm_spoc.PickListDS</type>
    </parameter>
  </parameterList>
  <valueType>dtype_com.conceptwave.system.Integer</valueType>
  <script><![CDATA[
    if(item1.priority<item2.priority){
        return -1;
    }

    return 1;
  ]]></script>
</script>