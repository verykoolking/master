<?xml version="1.0" encoding="UTF-8" ?>
<script name="cm_spoc.loadConstraintByServiceProvider">
  <group>ui_scripts.Operation</group>
  <label>loadConstraintByServiceProvider</label>
  <metaVersion>4</metaVersion>
  <parameterList>
    <parameter name="array" type="rifp">
      <type>nmeta_com.conceptwave.system.Object</type>
    </parameter>
    <parameter name="iterator" type="rifp">
      <type>nmeta_com.conceptwave.system.Object</type>
    </parameter>
  </parameterList>
  <script><![CDATA[
    for(var i=0;i<array.length;i++)
    {
        cm_spoc.loadSubOperations(array[i], iterator[i])
    }
  ]]></script>
</script>