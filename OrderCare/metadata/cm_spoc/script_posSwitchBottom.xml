<?xml version="1.0" encoding="UTF-8" ?>
<script name="cm_spoc.posSwitchBottom">
  <group>ui_scripts.UsageControl.AccumulationConditions.FinderHelpers</group>
  <label>posSwitchBottom</label>
  <metaVersion>4</metaVersion>
  <parameterList>
    <parameter name="pos" type="rifp"/>
    <parameter name="array" type="rifp"/>
  </parameterList>
  <script><![CDATA[
    while(pos < array.List.length-1){
    cm_spoc.posSwitchDown(pos, array);
    pos++;
    }
  ]]></script>
</script>