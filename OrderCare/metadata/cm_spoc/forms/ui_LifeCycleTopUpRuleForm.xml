    <method name="setTPPickListModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.LCTURTariffPlanArrDS.List =null;
        this.parent.model.LCTURTariffPlanArrDS.ListDS =null;
        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.LCTURTariffPlanArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="setTUOPickListModel" type="uimthd">
