    <method name="onMaximumValidityChecked" type="permMethod">
      <script><![CDATA[
        if (this.isReadOnly)
            return false;
        if (this.isChecked){
            return true;
        }
        this.model.MaximumValidity = "";
        return false;
      ]]></script>
    </method>
    <method name="onSearchClickVisRule" type="permMethod">
      <script><![CDATA[
        var value;
        if(this.isClicked != null)
        {
            value = this.isClicked;
        }
        else
            value = false;

        return value;
      ]]></script>
    </method>
