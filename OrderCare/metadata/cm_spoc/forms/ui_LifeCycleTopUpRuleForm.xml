    <method name="cwLeafInitAction$SubscriberStatusPickList" type="laction">
      <path>ui_cm_spoc.forms.LifeCycleTopUpRuleForm/leaf_SubscriberStatusPickList</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.LifeCycleTopUpRuleForm/leaf_SubscriberStatusPickList</valueType>
      <script><![CDATA[
        //
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_LCTURStatus";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.LCTURStatusArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setStPickListModel;
        return pl;
      ]]></script>
    </method>
