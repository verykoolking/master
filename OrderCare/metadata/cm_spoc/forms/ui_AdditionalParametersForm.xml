  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.AdditionalParametersForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.AdditionalParametersForm/leaf_model</valueType>
      <script><![CDATA[
        if (!this.model.specificationCode){
            this.model = cm_spoc.initModelForCreation(this.model);
        }else{
           this.model = cm_spoc.initModelForModification(this.model);
        }

        cm_spoc.initDialingCodeListUI(this,this.model);
        cm_spoc.initAdditionalParametersUI(this);

        return this.model;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$LifeCycleSubStatusPickList" type="laction">
      <path>ui_cm_spoc.forms.AdditionalParametersForm/leaf_LifeCycleSubStatusPickList</path>
      <type>initializer</type>
      <script><![CDATA[
        var pl = new cm_spoc.PickList(null,this);
        pl.availableItems = new DataObjectList();

        var LifeCycleSubStatusFinder=new Finder("cm_spoc.lifeCycleSubStatusFinderForNotifyStatusRule");
        var result = LifeCycleSubStatusFinder.search();

        for(var i=0;i<result.length;i++){
            var item = new cm_spoc.PickListDS();
            item.id = result[i].itemCode;
            item.description = result[i].msgToDisp;
            pl.availableItems.addLast(item);
        }

        pl.modelCallback = this.setPickListModelLifeCycleSubStatus;
        return pl;
      ]]></script>
    </method>
