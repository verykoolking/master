  <methodList>
    <method name="cwLeafInitAction$TPGFBTTariffPlanArrPickListUI" type="laction">
      <path>ui_cm_spoc.forms.TariffPlanGroupForBalanceTrForm/leaf_TPGFBTTariffPlanArrPickListUI</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.TariffPlanGroupForBalanceTrForm/leaf_TPGFBTTariffPlanArrPickListUI</valueType>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_TariffPlan";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.TPGFBTTariffPlanArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setTPGFBTTariffPlanArrPLModel;
        return pl;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.TariffPlanGroupForBalanceTrForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.TariffPlanGroupForBalanceTrForm/leaf_model</valueType>
      <script><![CDATA[
        var m = cm_spoc.initTariffPlanGroupForBalanceTransferModel(this);
        return m;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$TPGFBTArrPickListUI" type="laction">
      <path>ui_cm_spoc.forms.TariffPlanGroupForBalanceTrForm/leaf_TPGFBTArrPickListUI</path>
      <type>initializer</type>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_TariffPlanGroupForBalanceTr";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.TPGFBTArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setTPGFBTArrPLModel;
        return pl;
      ]]></script>
    </method>
    <method name="ReadOnly" type="permMethod">
      <script>return this.model.readonly;</script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="setTPGFBTArrPLModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.TPGFBTArrDS.List=null;
        this.parent.model.TPGFBTArrDS.ListDS=null;
        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.TPGFBTArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="setTPGFBTTariffPlanArrPLModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.TPGFBTTariffPlanArrDS.List=null;
        this.parent.model.TPGFBTTariffPlanArrDS.ListDS=null;
        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.TPGFBTTariffPlanArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="permission_ShowTPGFBTArrPickListUI" type="permMethod">
      <script><![CDATA[
        //
        return (this.model.SendBalanceTransfers || this.model.ReceiveBalanceTransfers);
      ]]></script>
    </method>
    <method name="permission_ShowMinMaxAmount" type="permMethod">
      <script><![CDATA[
        //
        return (this.model.SendBalanceTransfers);
      ]]></script>
    </method>
    <method name="onClick_SendBalance" type="action">
      <script><![CDATA[
        //
            this.cw$super_onClick();
            if (this.model.ReceiveBalanceTransfers==true) {
                this.model.ReceiveBalanceTransfers=false;
            }
      ]]></script>
    </method>
    <method name="onClick_ReceiveBalance" type="action">
      <script><![CDATA[
        //
            this.cw$super_onClick();
            if (this.model.SendBalanceTransfers==true) {
                this.model.SendBalanceTransfers=false;
            }
      ]]></script>
    </method>
