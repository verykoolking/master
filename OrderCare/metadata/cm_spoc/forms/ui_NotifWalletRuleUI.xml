<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.NotifWalletRuleUI">
  <group>TariffPlan</group>
  <label>NotifWalletRuleUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="AllowedWalletsLabel" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="parentUI" type="leaf">
      <valueType>ui_cm_spoc.forms.SetOfWalletsForm</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.NotifWalletRule</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elgrd">
              <colWidths>50px</colWidths>
              <fixedColWidths>true</fixedColWidths>
              <numOfCols>20</numOfCols>
              <width>1000px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <columnSpan>1</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.NotifWalletRuleUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>30px</width>
                </element>
                <element name="Description" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>4</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.NotifWalletRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.NotifWalletRule/dstruct_Description</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="AllowedWalletsLabel" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>4</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                    </variablePathList>
                  </editable>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.NotifWalletRuleUI/leaf_AllowedWalletsLabel</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="WalletButton" type="elbtn">
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.NotifWalletRuleUI/usrActionMethod_WalletsPickListDialog</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <columnSpan>1</columnSpan>
                  <disabled type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.NotifWalletRuleUI/leaf_parentUI</variablePath>
                      <variablePath>ui_cm_spoc.forms.SetOfWalletsForm/permMethod_ReadOnly</variablePath>
                    </variablePathList>
                  </disabled>
                  <icon>/cwf/finder.gif</icon>
                  <width>22px</width>
                </element>
                <element name="MessageButton" type="elbtn">
                  <cellAlignment>center,center</cellAlignment>
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.NotifWalletRuleUI/usrActionMethod_MessagesDialog</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <columnSpan>3</columnSpan>
                  <icon>/cwt/images/16/funnel.png</icon>
                  <width>22px</width>
                </element>
                <element name="LargestExpirationDate" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>3</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.NotifWalletRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.NotifWalletRule/dstruct_LargestExpirationDate</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="TruncatedBalanceInformation" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>4</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.NotifWalletRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.NotifWalletRule/dstruct_TruncatedBalanceInformation</variablePath>
                    </variablePathList>
                  </variable>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="getParentUI" type="uimthd">
      <script>return this.parentUI;</script>
    </method>
    <method name="setParentUI" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <type>nmeta_com.conceptwave.system.Object</type>
        </parameter>
      </parameterList>
      <script>this.parentUI = ui;</script>
    </method>
    <method name="WalletsPickListDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <parameterList>
        <parameter name="dialog" type="rifp">
          <type>ui_com.conceptwave.system.Dialog</type>
        </parameter>
      </parameterList>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.forms.WalletsPickList(this.model,this);
        newDetail.pickList.setParentUI(this);

        return newDetail;
      ]]></script>
    </method>
    <method name="MessagesDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <parameterList>
        <parameter name="dialog" type="rifp">
          <type>ui_com.conceptwave.system.Dialog</type>
        </parameter>
      </parameterList>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var messagesPopUp = new cm_spoc.forms.SetOfWalletsMessagesUI(this.model,this);

        return messagesPopUp;
      ]]></script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
  </methodList>
</userInterface>