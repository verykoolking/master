  <methodList>
    <method name="cwLeafInitAction$cwShowHover" type="laction">
      <path>ui_com.conceptwave.system.UserInterface/leaf_cwShowHover</path>
      <type>initializer</type>
      <script>return false;</script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.LifeCycleSubStatusTransitioForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initLifeCycleSubstatusTransitio(this.model);
        return m;
      ]]></script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="LifeCycleTrigger" type="uimthd">
      <script>this.model.LifeCycle=this.model.LifeCycleEnum;</script>
    </method>
    <method name="LifeCycleInternalSubStatus1Trigger" type="uimthd">
      <script><![CDATA[
        this.model.TransIncludesADestSubStatus=this.model.TransIncludesADestSubStatusEnum;
        this.isLifeCycleInternalSubstatus2=true;
      ]]></script>
    </method>
    <method name="LifeCycleInternalSubStatus2Trigger" type="uimthd">
      <script>this.model.TransIncludesAnOrigSubStatus=this.model.TransIncludesAnOrigSubStatusEnum;</script>
    </method>
    <method name="deleteModelAction" type="usrActionMethod">
      <script><![CDATA[
        try{
                var project = cwa_epc.getDefaultProj();
                var response=cm_common.plmRemoveItem(this.model,project);
                if(response instanceof api_common.data.fault)
                     this.setModelStatus(response);
                else{
                    this.cancelAction();
                    this.parent.parent.model.searchDocument.specification.specificationCode=null;
                    this.setModelStatus(null,"CWA_EPC_REMOVE_002",true, this.model.metadataTypeLabel);
                    this.parent.parent.resultUI.result=this.parent.parent.model.search();
                }

        }catch(ex){
            var fault = api_common.createException(ex, "COM_ERR_0001");
            this.setModelStatus(fault);
        }
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
  </methodList>
