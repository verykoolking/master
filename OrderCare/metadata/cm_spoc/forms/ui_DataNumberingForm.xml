    <method name="ContentTypeTrigger" type="uimthd">
      <script><![CDATA[
        var btCode=this.model.ContentTypeEnum;
        this.model.ContentType = btCode;
      ]]></script>
    </method>
    <method name="setIsPrimaryKey" type="permMethod">
