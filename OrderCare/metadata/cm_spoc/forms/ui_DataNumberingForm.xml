  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.DataNumberingForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        cm_spoc.initDataNumbering(this.model);
        return this.model;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="referenceCallback" type="uimthd">
      <parameterList>
        <parameter name="itemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
        <parameter name="baseItemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        switch(baseItemCode){
            case "base_RatingGroup":
                this.model.RatingGroupInputFor =itemCode;
                break;
            case "base_APN":
                this.model.APNInputFor = itemCode;
                break;
            case "base_ServiceID":
                this.model.ServiceIDInputFor = itemCode;
                break;
            case "base_TariffPlan":
                this.model.TariffPlanDataInputFor = itemCode;
                break;
             case "base_DestinationType":
                this.model.DestinationTypeDataOutputOf = itemCode;
                break;

        }
      ]]></script>
    </method>
    <method name="FinderAPNReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_APN");
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
