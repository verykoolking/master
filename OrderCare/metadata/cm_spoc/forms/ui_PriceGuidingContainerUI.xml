    <method name="cwLeafInitAction$price" type="laction">
      <path>ui_cm_spoc.forms.PriceGuidingContainerUI/leaf_price</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.PriceGuidingContainerUI/leaf_price</valueType>
      <script><![CDATA[
        var m = cm_spoc.initPriceModel(this);

        return m;
      ]]></script>
    </method>
    <method name="onSelectPriceEventTrigger" type="uimthd">
      <script>cm_spoc.onSelectPriceEvent(this);</script>
    </method>
