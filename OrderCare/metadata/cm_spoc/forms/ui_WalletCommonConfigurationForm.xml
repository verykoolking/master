<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.WalletCommonConfigurationForm">
  <group>Wallets</group>
  <label>WalletCommonConfigurationForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.WalletCommonConfiguration</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>800px</width>
              <elementList>
                <element name="WalletCommonConfiguration" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.WalletCommonConfigurationForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.WalletCommonConfigurationForm/frmui_Forms/vform_CommonConfigurationForm</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Wallet Common Configuration</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="CommonConfigurationForm" type="vform">
          <label>CommonConfigurationForm</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="SectionStack" type="elsectnstck">
                <visibilityMode>multiple</visibilityMode>
                <width>750px</width>
                <elementList>
                  <element name="SectionWallets" type="elsect">
                    <removeLabelLine>true</removeLabelLine>
                    <elementList>
                      <element name="Wallets" type="elsecth">
                        <label>Wallets</label>
                      </element>
                      <element name="layoutCharacteristics" type="elgrd">
                        <alignment>right,center</alignment>
                        <numOfCols>2</numOfCols>
                        <width>300px</width>
                        <elementList>
                          <element name="DefaultWalletEnum" type="elslct">
                            <cellAlignment>left,center</cellAlignment>
                            <labeAlign>left</labeAlign>
                            <label>Default Wallet</label>
                            <labelOrientation>left</labelOrientation>
                            <multiselect>false</multiselect>
                            <onChangeMethod type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.WalletCommonConfigurationForm/uimthd_DefaultWalletTrigger</variablePath>
                              </variablePathList>
                            </onChangeMethod>
                            <optional type="varPath">
                              <variablePathList>
                                <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                              </variablePathList>
                            </optional>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.WalletCommonConfigurationForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.WalletCommonConfiguration/dstruct_DefaultWalletEnum</variablePath>
                              </variablePathList>
                            </variable>
                            <width>150px</width>
                          </element>
                          <element name="DefaultWalletForSOSTopUpEnum" type="elslct">
                            <cellAlignment>left,center</cellAlignment>
                            <labeAlign>left</labeAlign>
                            <label>Default Wallet for SOS Top Ups</label>
                            <labelOrientation>left</labelOrientation>
                            <multiselect>false</multiselect>
                            <onChangeMethod type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.WalletCommonConfigurationForm/uimthd_DefaultWalletForSOSTopUpTrigger</variablePath>
                              </variablePathList>
                            </onChangeMethod>
                            <optional type="varPath">
                              <variablePathList>
                                <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                              </variablePathList>
                            </optional>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.WalletCommonConfigurationForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.WalletCommonConfiguration/dstruct_DefaultWalletForSOSTopUpEnum</variablePath>
                              </variablePathList>
                            </variable>
                            <width>150px</width>
                          </element>
                          <element name="Walletforrechargingtrafficunits" type="elslct">
                            <cellAlignment>left,center</cellAlignment>
                            <labeAlign>left</labeAlign>
                            <label>Wallet for recharging traffic units</label>
                            <labelOrientation>left</labelOrientation>
                            <visible type="varPath">
                              <variablePathList>
                                <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                              </variablePathList>
                            </visible>
                            <width>150px</width>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                  <element name="SectionChargingModes" type="elsect">
                    <removeLabelLine>true</removeLabelLine>
                    <visible type="varPath">
                      <variablePathList>
                        <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                      </variablePathList>
                    </visible>
                    <elementList>
                      <element name="ChargingModes" type="elsecth">
                        <label>Charging Modes</label>
                      </element>
                      <element name="layoutCharacteristics1" type="elvlt">
                        <alignment>right,center</alignment>
                        <visible type="varPath">
                          <variablePathList>
                            <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                          </variablePathList>
                        </visible>
                        <elementList>
                          <element name="Chargingmodeforrechargingtrafficunits" type="elslct">
                            <labeAlign>left</labeAlign>
                            <label>Charging mode for recharging traffic units</label>
                            <labelOrientation>left</labelOrientation>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$cwShowHover" type="laction">
      <path>ui_com.conceptwave.system.UserInterface/leaf_cwShowHover</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initWalletCommonConfiguration(this.model);
        return m;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="WCCMaximumNumberofWalletsReadORule" type="permMethod">
      <script>return this.model.readonly</script>
    </method>
    <method name="DefaultWalletTrigger" type="uimthd">
      <script><![CDATA[
        var btCode = this.model.DefaultWalletEnum;
        this.model.DefaultWallet = btCode;
      ]]></script>
    </method>
    <method name="DefaultWalletForSOSTopUpTrigger" type="uimthd">
      <script><![CDATA[
        var btCode = this.model.DefaultWalletForSOSTopUpEnum;
        this.model.DefaultWalletForSOSTopUp = btCode;
      ]]></script>
    </method>
  </methodList>
</userInterface>