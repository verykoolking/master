<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.SetOfWalletsForm">
  <group>TariffPlan</group>
  <label>SetOfWalletsForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="SelectAllNotifWalletRule" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="NotifWalletRuleIterator" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.SetOfWallets</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="VerticalLayout" type="elvlt">
              <width>850px</width>
              <elementList>
                <element name="SectionStack1" type="elsectnstck">
                  <width>1000px</width>
                  <elementList>
                    <element name="Section" type="elsect">
                      <elementList>
                        <element name="SectionHeader" type="elsecth"/>
                        <element name="GridLayout" type="elgrd">
                          <colWidths>50px</colWidths>
                          <labeAlign>left</labeAlign>
                          <labelOrientation>left</labelOrientation>
                          <numOfCols>4</numOfCols>
                          <width>200px</width>
                          <elementList>
                            <element name="RowSpacer1" type="elrsp"/>
                            <element name="SetOfWalletsID" type="elfld">
                              <autoFocusNextField>false</autoFocusNextField>
                              <columnSpan>4</columnSpan>
                              <labeAlign>left</labeAlign>
                              <label>Identifier</label>
                              <labelOrientation>left</labelOrientation>
                              <optional type="varPath">
                                <variablePathList>
                                  <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                </variablePathList>
                              </optional>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.SetOfWalletsForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.SetOfWallets/dstruct_SetOfWalletsID</variablePath>
                                </variablePathList>
                              </variable>
                              <width>200px</width>
                            </element>
                            <element name="SetOfWalletsDescription" type="elfld">
                              <autoFocusNextField>false</autoFocusNextField>
                              <columnSpan>4</columnSpan>
                              <labeAlign>left</labeAlign>
                              <label>Description</label>
                              <labelOrientation>left</labelOrientation>
                              <optional type="varPath">
                                <variablePathList>
                                  <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                </variablePathList>
                              </optional>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.SetOfWalletsForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.SetOfWallets/dstruct_SetOfWalletsDescription</variablePath>
                                </variablePathList>
                              </variable>
                              <width>200px</width>
                            </element>
                            <element name="RowSpacer" type="elrsp"/>
                          </elementList>
                        </element>
                        <element name="buttonLayout" type="elhlt">
                          <elementList>
                            <element name="addButton" type="elmnu">
                              <clickMethod type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.SetOfWalletsForm/uimthd_addNotifWalletRule</variablePath>
                                </variablePathList>
                              </clickMethod>
                              <icon>/cwt/images/16/notebook_add.png</icon>
                            </element>
                            <element name="LayoutSpacer1" type="ellsp">
                              <width>10px</width>
                            </element>
                            <element name="deleteButton" type="elmnu">
                              <clickMethod type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.SetOfWalletsForm/uimthd_deleteNotifWalletRule</variablePath>
                                </variablePathList>
                              </clickMethod>
                              <icon>/cwt/images/16/delete.png</icon>
                            </element>
                          </elementList>
                        </element>
                        <element name="GridLayout1" type="elgrd">
                          <colWidths>50px</colWidths>
                          <fixedColWidths>true</fixedColWidths>
                          <numOfCols>20</numOfCols>
                          <width>1000px</width>
                          <elementList>
                            <element name="SelectAllNotifWalletRule" type="elchk">
                              <columnSpan>1</columnSpan>
                              <showLabel>false</showLabel>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.SetOfWalletsForm/leaf_SelectAllNotifWalletRule</variablePath>
                                </variablePathList>
                              </variable>
                              <width>30px</width>
                            </element>
                            <element name="Description" type="ellab">
                              <cellAlignment>center,center</cellAlignment>
                              <columnSpan>4</columnSpan>
                              <label>Description</label>
                            </element>
                            <element name="Wallets" type="ellab">
                              <cellAlignment>center,center</cellAlignment>
                              <columnSpan>4</columnSpan>
                              <label>Wallets</label>
                            </element>
                            <element name="CellSpacer" type="elcsp">
                              <columnSpan>1</columnSpan>
                            </element>
                            <element name="Messages" type="ellab">
                              <cellAlignment>center,center</cellAlignment>
                              <columnSpan>3</columnSpan>
                              <label>Messages</label>
                            </element>
                            <element name="LargestExpirationDate" type="ellab">
                              <cellAlignment>center,center</cellAlignment>
                              <columnSpan>3</columnSpan>
                              <label>Largest Expiration Date</label>
                            </element>
                            <element name="TruncateBalanceInformation" type="ellab">
                              <cellAlignment>center,center</cellAlignment>
                              <columnSpan>4</columnSpan>
                              <label>Truncate Balance Information</label>
                            </element>
                          </elementList>
                        </element>
                        <element name="notifWalletRuletHL" type="elvlt">
                          <height>200px</height>
                          <width>950px</width>
                          <elementList>
                            <element name="notifWalletRuleIterator" type="eliter">
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.SetOfWalletsForm/leaf_NotifWalletRuleIterator</variablePath>
                                </variablePathList>
                              </variable>
                              <elementList>
                                <element name="notifWalletRuleHL" type="elhlt">
                                  <elementList>
                                    <element name="FormFrame" type="elovf">
                                      <formReference type="varPath">
                                        <variablePathList>
                                          <variablePath>ui_cm_spoc.forms.SetOfWalletsForm/leaf_NotifWalletRuleIterator</variablePath>
                                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                        </variablePathList>
                                      </formReference>
                                      <width>950px</width>
                                    </element>
                                  </elementList>
                                </element>
                              </elementList>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafTriggerAction$SelectAllNotifWalletRule" type="laction">
      <path>ui_cm_spoc.forms.SetOfWalletsForm/leaf_SelectAllNotifWalletRule</path>
      <type>trigger</type>
      <script><![CDATA[
        for (var i = 0;i<this.NotifWalletRuleIterator.length; i++){
                this.NotifWalletRuleIterator[i].Selected = this.SelectAllNotifWalletRule;
            }
      ]]></script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.SetOfWalletsForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initSetOfWallets(this.model);
        cm_spoc.initNotifWalletRuleUI(this);
        return m;
      ]]></script>
    </method>
    <method name="addNotifWalletRule" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <type>nmeta_com.conceptwave.system.Object</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        var model=this.model.SOWNotifWalletRuleArrDS.ListDS;
        var len = model.length;

        cm_spoc.addListElement(this, model,"cm_spoc.forms.NotifWalletRuleUI","base_NotifWalletRule","NotifWalletRuleIterator");
      ]]></script>
    </method>
    <method name="deleteNotifWalletRule" type="uimthd">
      <script><![CDATA[
        var model = this.model.SOWNotifWalletRuleArrDS;
        cm_spoc.removeListElements(this , model ,"List", "NotifWalletRuleIterator");
        this.SelectAllNotifWalletRule = false;
      ]]></script>
    </method>
    <method name="ReadOnly" type="permMethod">
      <script>return this.model.readonly;</script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i=0;i<this.NotifWalletRuleIterator.length;i++){
            this.NotifWalletRuleIterator[i].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
  </methodList>
</userInterface>