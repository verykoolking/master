<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.RinForm">
  <group>Numbering</group>
  <label>RinForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.Rin</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>400px</width>
              <elementList>
                <element name="TabFrame" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RinForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.RinForm/frmui_Forms/vform_general</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                  <width>100px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="general" type="vform">
          <label>general</label>
          <root name="VerticalLayout" type="elvlt">
            <alignment>right,center</alignment>
            <elementList>
              <element name="Description" type="elfld">
                <labeAlign>left</labeAlign>
                <labelOrientation>left</labelOrientation>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_Description</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="VoiceServiceTax" type="elfld">
                <labeAlign>left</labeAlign>
                <label>Voice Service Tax</label>
                <labelOrientation>left</labelOrientation>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_VoiceServiceTax</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="ASLProvinceCode" type="elfld">
                <labeAlign>left</labeAlign>
                <label>ASL Province Code</label>
                <labelOrientation>left</labelOrientation>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_ASLProvinceCode</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="SMSServiceTax" type="elfld">
                <labeAlign>left</labeAlign>
                <label>SMS Service Tax</label>
                <labelOrientation>left</labelOrientation>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_SMSServiceTax</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="LongDistanceComponentTax" type="elfld">
                <labeAlign>left</labeAlign>
                <label>Long Distance Component Tax</label>
                <labelOrientation>left</labelOrientation>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_LongDistanceComponentTax</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="SubscribersCategoryTaxApplicatio" type="elfld">
                <labeAlign>left</labeAlign>
                <label>Subscribers Category Tax Application</label>
                <labelOrientation>left</labelOrientation>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_SubscribersCategoryTaxApplicatio</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="HexadecimalID" type="elfld">
                <labeAlign>left</labeAlign>
                <label>Hexadecimal ID</label>
                <labelOrientation>left</labelOrientation>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_HexadecimalID</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="RinCode" type="elfld">
                <editable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/permMethod_setIsPrimaryKey</variablePath>
                  </variablePathList>
                </editable>
                <labeAlign>left</labeAlign>
                <label>Province Code</label>
                <labelOrientation>left</labelOrientation>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_RinCode</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="TimeZoneEnum" type="elslct">
                <labeAlign>left</labeAlign>
                <label>Time Zone</label>
                <labelOrientation>left</labelOrientation>
                <multiselect>false</multiselect>
                <onChangeMethod type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/usrActionMethod_updateTimeZone</variablePath>
                  </variablePathList>
                </onChangeMethod>
                <onEnter type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/usrActionMethod_updateTimeZone</variablePath>
                  </variablePathList>
                </onEnter>
                <optional type="varPath">
                  <variablePathList>
                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                  </variablePathList>
                </optional>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_TimeZoneEnum</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="StateEnum" type="elslct">
                <labeAlign>left</labeAlign>
                <label>State</label>
                <labelOrientation>left</labelOrientation>
                <multiselect>false</multiselect>
                <onChangeMethod type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/usrActionMethod_updateState</variablePath>
                  </variablePathList>
                </onChangeMethod>
                <onEnter type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/usrActionMethod_updateState</variablePath>
                  </variablePathList>
                </onEnter>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_StateEnum</variablePath>
                  </variablePathList>
                </variable>
              </element>
              <element name="RegionEnum" type="elslct">
                <labeAlign>left</labeAlign>
                <label>Region</label>
                <labelOrientation>left</labelOrientation>
                <multiselect>false</multiselect>
                <onChangeMethod type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/usrActionMethod_updateRegion</variablePath>
                  </variablePathList>
                </onChangeMethod>
                <onEnter type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/usrActionMethod_updateRegion</variablePath>
                  </variablePathList>
                </onEnter>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RinForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Rin/dstruct_RegionEnum</variablePath>
                  </variablePathList>
                </variable>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$cwShowHover" type="laction">
      <path>ui_com.conceptwave.system.UserInterface/leaf_cwShowHover</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initRin(this.model);
        return m;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script>return true;</script>
    </method>
    <method name="updateTimeZone" type="usrActionMethod">
      <script><![CDATA[
        //this.parent.model.TimeZone = this.TimeZoneEnum;
        this.model.TimeZone=this.model.TimeZoneEnum;
      ]]></script>
    </method>
    <method name="updateState" type="usrActionMethod">
      <script><![CDATA[
        //this.parent.model.State = this.StateEnum;
        this.model.State=this.model.StateEnum;
      ]]></script>
    </method>
    <method name="updateRegion" type="usrActionMethod">
      <script><![CDATA[
        //this.parent.model.Region = this.RegionEnum;
        this.model.Region=this.model.RegionEnum;
      ]]></script>
    </method>
    <method name="setIsPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
  </methodList>
</userInterface>