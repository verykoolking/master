<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.BalanceTransferSettingIteratorUIForm">
  <group>TariffPlan</group>
  <label>BalanceTransferSettingIteratorUIForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.TariffPlanModality</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elgrd">
              <numOfCols>4</numOfCols>
              <width>460px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.BalanceTransferSettingIteratorUIForm/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>30px</width>
                </element>
                <element name="ModalityId" type="elslct">
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.BalanceTransferSettingIteratorUIForm/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.TariffPlanModality/dstruct_ModalityId</variablePath>
                    </variablePathList>
                  </variable>
                  <width>200px</width>
                </element>
                <element name="WalletList" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                    </variablePathList>
                  </editable>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.BalanceTransferSettingIteratorUIForm/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.TariffPlanModality/dstruct_WalletList</variablePath>
                    </variablePathList>
                  </variable>
                  <width>200px</width>
                </element>
                <element name="MenuItem" type="elmnu">
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.BalanceTransferSettingIteratorUIForm/usrActionMethod_showWalletPopUp</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <icon>/cwt/images/16/notebook_add.png</icon>
                  <width>30px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="isAllSelected" type="uimthd">
      <script><![CDATA[
        if (this.Selected == false)
            this.parent.SelectAll = false;
      ]]></script>
    </method>
    <method name="showWalletPopUp" type="usrActionMethod">
      <dialogHeight>378</dialogHeight>
      <dialogWidth>417</dialogWidth>
      <parameterList>
        <parameter name="dialog" type="rifp">
          <type>ui_com.conceptwave.system.Dialog</type>
        </parameter>
      </parameterList>
      <showInPopup>true</showInPopup>
      <script>return cm_spoc.openWalletPopUp(this);</script>
    </method>
  </methodList>
</userInterface>