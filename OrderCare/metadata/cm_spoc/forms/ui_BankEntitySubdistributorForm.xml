<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.BankEntitySubdistributorForm">
  <group>TopUp</group>
  <label>BankEntitySubdistributorForm</label>
  <metaVersion>4</metaVersion>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>750px</width>
              <elementList>
                <element name="generalParameters" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.BankEntitySubdistributorForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.BankEntitySubdistributorForm/frmui_Forms/vform_generalParameters</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                </element>
                <element name="Bank" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.BankEntitySubdistributorForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.BankEntitySubdistributorForm/frmui_Forms/vform_Bank</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Bank</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="generalParameters" type="vform">
          <label>generalParameters</label>
          <root name="VerticalLayout" type="elvlt">
            <alignment>left,center</alignment>
            <elementList>
              <element name="LayoutSpacer1" type="ellsp">
                <height>10px</height>
              </element>
              <element name="SubdistributorCode" type="elfld">
                <canSort>false</canSort>
                <cellAlignment>center,center</cellAlignment>
                <groupBy>true</groupBy>
                <labeAlign>left</labeAlign>
                <label>Code</label>
                <labelOrientation>left</labelOrientation>
              </element>
              <element name="LayoutSpacer" type="ellsp">
                <height>10px</height>
              </element>
              <element name="Description" type="elfld">
                <canSort>false</canSort>
                <cellAlignment>center,center</cellAlignment>
                <groupBy>true</groupBy>
                <label>Descripton</label>
                <labelOrientation>left</labelOrientation>
              </element>
              <element name="LayoutSpacer3" type="ellsp">
                <height>10px</height>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="Bank" type="vform">
          <label>Bank</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="LayoutSpacer3" type="ellsp">
                <height>10px</height>
              </element>
              <element name="VerticalLayout2" type="elvlt">
                <alignment>center,center</alignment>
                <width>500px</width>
                <elementList>
                  <element name="HorizontalLayout1" type="elhlt">
                    <elementList>
                      <element name="specificationCode" type="elslct">
                        <cellAlignment>center,center</cellAlignment>
                        <heightInLines>15</heightInLines>
                        <label>Available Bank</label>
                        <multiselect>false</multiselect>
                        <width>200px</width>
                      </element>
                      <element name="ButtonsVL" type="elvlt">
                        <edgeOffset>5</edgeOffset>
                        <elementList>
                          <element name="LayoutSpacer2" type="ellsp">
                            <height>30px</height>
                          </element>
                          <element name="ButtonRight" type="elbtn">
                            <icon>/cwt/images/16/navigate_right.png</icon>
                            <width>22px</width>
                          </element>
                          <element name="ButtonDoubleRight" type="elbtn">
                            <icon>/cwt/images/16/navigate_right2.png</icon>
                            <width>22px</width>
                          </element>
                          <element name="ButtonDoubleLeft" type="elbtn">
                            <icon>/cwt/images/16/navigate_left2.png</icon>
                            <width>22px</width>
                          </element>
                          <element name="ButtonLeft" type="elbtn">
                            <icon>/cwt/images/16/navigate_left.png</icon>
                            <width>22px</width>
                          </element>
                        </elementList>
                      </element>
                      <element name="AssociatedGREs" type="elslct">
                        <cellAlignment>center,center</cellAlignment>
                        <heightInLines>15</heightInLines>
                        <label>Selected Bank</label>
                        <multiselect>false</multiselect>
                        <width>200px</width>
                      </element>
                    </elementList>
                  </element>
                  <element name="ButtonsHL" type="elhlt">
                    <fillSpace>false</fillSpace>
                    <snapOnMouseOver>false</snapOnMouseOver>
                    <visible type="varPath">
                      <variablePathList>
                        <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                      </variablePathList>
                    </visible>
                    <elementList>
                      <element name="OK" type="elbtn">
                        <cellAlignment></cellAlignment>
                        <label>OK</label>
                      </element>
                      <element name="LayoutSpacer1" type="ellsp">
                        <width>50px</width>
                      </element>
                      <element name="Cancel" type="elbtn">
                        <canSort>false</canSort>
                        <cellAlignment>center,center</cellAlignment>
                        <label>Cancel</label>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$cwShowHover" type="laction">
      <path>ui_com.conceptwave.system.UserInterface/leaf_cwShowHover</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initBankEntity(this.model);
        return m;
      ]]></script>
    </method>
  </methodList>
</userInterface>