<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.LifeCyclePickList">
  <group>LifeCycles</group>
  <label>LifeCyclePickList</label>
  <metaVersion>4</metaVersion>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="VerticalLayout" type="elvlt">
              <alignment>center,center</alignment>
              <height>100%</height>
              <width>100%</width>
              <elementList>
                <element name="LayoutSpace" type="ellsp">
                  <height>20px</height>
                </element>
                <element name="HorizontalLayout" type="elhlt">
                  <alignment>center,center</alignment>
                  <edgeSize>2</edgeSize>
                  <height>90%</height>
                  <membersMargin>2</membersMargin>
                  <showEdge>true</showEdge>
                  <width>90%</width>
                  <elementList>
                    <element name="AvailableItems" type="eltabl">
                      <filterFullData>true</filterFullData>
                      <height>100%</height>
                      <noItemsMessage>No items to select</noItemsMessage>
                      <width>45%</width>
                      <elementList>
                        <element name="description1" type="elfld">
                          <height>100%</height>
                          <label>Available</label>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.PickList/leaf_availableItems</variablePath>
                              <variablePath>dstruct_cm_spoc.PickListDS/dstruct_description</variablePath>
                            </variablePathList>
                          </variable>
                          <width>100%</width>
                        </element>
                      </elementList>
                    </element>
                    <element name="ButtonsVL" type="elvlt">
                      <alignment>center,center</alignment>
                      <edgeOffset>5</edgeOffset>
                      <width>10%</width>
                      <elementList>
                        <element name="ButtonRight" type="elbtn">
                          <icon>/cwt/images/16/navigate_right.png</icon>
                          <width>22px</width>
                        </element>
                        <element name="ButtonDoubleRight" type="elbtn">
                          <icon>/cwt/images/16/navigate_right2.png</icon>
                          <width>22px</width>
                        </element>
                        <element name="ButtonDoubleLeft" type="elbtn">
                          <icon>/cwt/images/16/navigate_left2.png</icon>
                          <width>22px</width>
                        </element>
                        <element name="ButtonLeft" type="elbtn">
                          <icon>/cwt/images/16/navigate_left.png</icon>
                          <width>22px</width>
                        </element>
                      </elementList>
                    </element>
                    <element name="SelectedItems" type="eltabl">
                      <height>100%</height>
                      <noItemsMessage>No selected items</noItemsMessage>
                      <width>45%</width>
                      <elementList>
                        <element name="description" type="elfld">
                          <dynLabel type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.PickList/uimthd_DynamicLabelDescription</variablePath>
                            </variablePathList>
                          </dynLabel>
                          <height>100%</height>
                          <label>Selected</label>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.PickList/leaf_selectedItems</variablePath>
                              <variablePath>dstruct_cm_spoc.PickListDS/dstruct_description</variablePath>
                            </variablePathList>
                          </variable>
                          <width>100%</width>
                        </element>
                        <element name="priority" type="elfld">
                          <canFilter>false</canFilter>
                          <label>Priority</label>
                          <textAlign>center</textAlign>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.PickList/leaf_selectedItems</variablePath>
                              <variablePath>dstruct_cm_spoc.PickListDS/dstruct_priority</variablePath>
                            </variablePathList>
                          </variable>
                          <visible type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.PickList/permMethod_PriorityButtonsVisible</variablePath>
                            </variablePathList>
                          </visible>
                          <width>25%</width>
                        </element>
                      </elementList>
                    </element>
                    <element name="PriorityButtonsVL" type="elvlt">
                      <elementList>
                        <element name="ButtonUpDouble" type="elbtn">
                          <icon>/cwt/images/16/navigate_up2.png</icon>
                          <width>22px</width>
                        </element>
                        <element name="ButtonUp" type="elbtn">
                          <icon>/cwt/images/16/navigate_up.png</icon>
                          <width>22px</width>
                        </element>
                        <element name="HorizontalLayout1" type="elhlt">
                          <elementList>
                            <element name="ButtonRefresh" type="elbtn">
                              <icon>/cwt/images/16/refresh.png</icon>
                              <showLabel>false</showLabel>
                              <width>22px</width>
                            </element>
                            <element name="inputIndex" type="elfld">
                              <cellAlignment>center,center</cellAlignment>
                              <labelOrientation>right</labelOrientation>
                              <showLabel>false</showLabel>
                              <width>40px</width>
                            </element>
                          </elementList>
                        </element>
                        <element name="ButtonDown" type="elbtn">
                          <icon>/cwt/images/16/navigate_down.png</icon>
                          <width>22px</width>
                        </element>
                        <element name="ButtonDownDouble" type="elbtn">
                          <icon>/cwt/images/16/navigate_down2.png</icon>
                          <width>22px</width>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
                <element name="LayoutSpace2" type="ellsp">
                  <height>20px</height>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
</userInterface>