        if(this.btAssignment)
            return true;
        return false;
      ]]></script>
    </method>
    <method name="tpAssignmentVisRule" type="permMethod">
      <script><![CDATA[
        if(this.tpAssignment)
            return true;
        return false;
      ]]></script>
    </method>
    <method name="cycAssignmentVisRule" type="permMethod">
