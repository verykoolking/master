        if(this.cycAssignment)
            return true;
        return false;
      ]]></script>
    </method>
    <method name="setBoltOn" type="uimthd">
      <script>this.model.BoltOn = this.model.BoltOnEnum;</script>
    </method>
    <method name="setCyclicActionSpec" type="uimthd">
      <script>this.model.CyclicActionSpecification = this.model.CyclicActionSpecEnum;</script>
    </method>
