  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.PromotionUsingWindowWithResForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var model = this.model;
        if(model.BoltOn)
            this.btAssignment = true;
        if(model.CyclicActionSpecification)
            this.cycAssignment = true;
        if(model.PUWWRTUODestPairArrDS.ListDS.length > 0)
            this.tpAssignment = true;
        return;
      ]]></script>
    </method>
    <method name="TopUpOriginDestinationDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <parameterList>
        <parameter name="dialog" type="rifp">
          <type>ui_com.conceptwave.system.Dialog</type>
        </parameter>
      </parameterList>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var topUpDestinationPopUp = new cm_spoc.forms.TopUpOriginDestinationPairUI(this.model.PUWWRTUODestPairArrDS,this);
        topUpDestinationPopUp.TopUpOriginDestinationPairIterator = new Array();
        var model = topUpDestinationPopUp.model.ListDS;

        if(topUpDestinationPopUp.model.specificationCode){
           cm_spoc.loadListElements(topUpDestinationPopUp, model, "cm_spoc.forms.TopUpOriginDestinationPairIteratorUI", "TopUpOriginDestinationPairIterator");
        }
        return topUpDestinationPopUp;
      ]]></script>
    </method>
    <method name="selectTrigger" type="uimthd">
