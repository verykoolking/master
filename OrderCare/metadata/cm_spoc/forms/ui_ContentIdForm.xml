<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.ContentIdForm">
  <group>Numbering</group>
  <label>ContentIdForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.ContentId</valueType>
    </variable>
    <variable name="CntCodeIterator" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>400px</width>
              <elementList>
                <element name="TabFrame" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ContentIdForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.ContentIdForm/frmui_Forms/vform_general</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                  <width>100px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="general" type="vform">
          <label>general</label>
          <root name="VerticalLayout" type="elvlt">
            <alignment>left,center</alignment>
            <elementList>
              <element name="VerticalLayout1" type="elvlt">
                <alignment>right,center</alignment>
                <elementList>
                  <element name="ContentIdCode" type="elfld">
                    <editable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.ContentIdForm/permMethod_setIsPrimaryKey</variablePath>
                      </variablePathList>
                    </editable>
                    <labeAlign>left</labeAlign>
                    <label>ID</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.ContentIdForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.ContentId/dstruct_ContentIdCode</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="CntDescription" type="elfld">
                    <labeAlign>left</labeAlign>
                    <label>Description</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.ContentIdForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.ContentId/dstruct_CntDescription</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="CntXnt" type="elslct">
                    <labeAlign>left</labeAlign>
                    <label>XNT</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.ContentIdForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.ContentId/dstruct_CntXnt</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                </elementList>
              </element>
              <element name="SectionStack" type="elsectnstck">
                <visibilityMode>multiple</visibilityMode>
                <width>700px</width>
                <elementList>
                  <element name="ConditionsSection" type="elsect">
                    <removeLabelLine>true</removeLabelLine>
                    <elementList>
                      <element name="SectionHeader" type="elsecth">
                        <label>Code</label>
                      </element>
                      <element name="LayoutSpacer1" type="ellsp">
                        <height>4px</height>
                      </element>
                      <element name="buttonsLayout" type="elhlt">
                        <elementList>
                          <element name="addButton" type="elmnu">
                            <clickMethod type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.ContentIdForm/uimthd_addCntCode</variablePath>
                              </variablePathList>
                            </clickMethod>
                            <icon>/cwt/images/16/notebook_add.png</icon>
                            <width>40px</width>
                          </element>
                          <element name="deleteButton" type="elmnu">
                            <clickMethod type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.ContentIdForm/uimthd_deleteCntCode</variablePath>
                              </variablePathList>
                            </clickMethod>
                            <icon>/cwt/images/16/delete.png</icon>
                            <width>40px</width>
                          </element>
                        </elementList>
                      </element>
                      <element name="ConditionsHeaders" type="elgrd">
                        <colWidths>50px</colWidths>
                        <fixedColWidths>true</fixedColWidths>
                        <numOfCols>13</numOfCols>
                        <width>500px</width>
                        <elementList>
                          <element name="Select" type="ellab">
                            <columnSpan>1</columnSpan>
                            <width>30px</width>
                          </element>
                          <element name="CntCodeId" type="ellab">
                            <columnSpan>2</columnSpan>
                            <label>Code</label>
                            <width>100px</width>
                          </element>
                          <element name="CntCodeIds" type="ellab">
                            <columnSpan>2</columnSpan>
                            <label>Description</label>
                            <width>100px</width>
                          </element>
                        </elementList>
                      </element>
                      <element name="ConditionsVL" type="elvlt">
                        <height>200px</height>
                        <overflow>auto</overflow>
                        <width>500px</width>
                        <elementList>
                          <element name="ConditionsIterator" type="eliter">
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.ContentIdForm/leaf_CntCodeIterator</variablePath>
                              </variablePathList>
                            </variable>
                            <elementList>
                              <element name="ConditionsHL" type="elhlt">
                                <width>500px</width>
                                <elementList>
                                  <element name="ConditionsFormFrame" type="elovf">
                                    <formReference type="varPath">
                                      <variablePathList>
                                        <variablePath>ui_cm_spoc.forms.ContentIdForm/leaf_CntCodeIterator</variablePath>
                                        <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                        <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                      </variablePathList>
                                    </formReference>
                                  </element>
                                </elementList>
                              </element>
                            </elementList>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.ContentIdForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initCntCode(this.model);
        cm_spoc.initCntCodeUi(this);

        return m;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i=0;i<this.CntCodeIterator.length;i++){
            this.CntCodeIterator[i].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
    <method name="addCntCode" type="uimthd">
      <script><![CDATA[
        var model = this.model.CntCodeArrDS.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.CntCodeUI","base_CntCode","CntCodeIterator");
      ]]></script>
    </method>
    <method name="deleteCntCode" type="uimthd">
      <script><![CDATA[this.model.CntCodeArrDS= cm_spoc.removeListElements(this ,this.model.CntCodeArrDS,"List", "CntCodeIterator");]]></script>
    </method>
    <method name="setIsPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
  </methodList>
</userInterface>