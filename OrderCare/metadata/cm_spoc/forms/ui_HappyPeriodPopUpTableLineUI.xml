<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.HappyPeriodPopUpTableLineUI">
  <group>UsageControl</group>
  <label>HappyPeriodPopUpTableLineUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.UCSApConHappyPeriod</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="yearsSelected" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="monthsSelected" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="daysSelected" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="hoursSelected" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="weekDaysSelected" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="CopyGridLayout" type="elgrd">
              <cellBorderStyle>CgCellDark</cellBorderStyle>
              <cellPadding>10</cellPadding>
              <colWidths>50px</colWidths>
              <fixedColWidths>true</fixedColWidths>
              <numOfCols>12</numOfCols>
              <width>1200px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>1</columnSpan>
                  <labelAsTitle>false</labelAsTitle>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>25px</width>
                </element>
                <element name="yearSelected" type="ellab">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>2</columnSpan>
                  <label>Year</label>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_yearsSelected</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="monthsSelected" type="ellab">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>2</columnSpan>
                  <label>Month</label>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_monthsSelected</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="daysSelected" type="ellab">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>2</columnSpan>
                  <label>Day</label>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_daysSelected</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="hoursSelected" type="ellab">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>2</columnSpan>
                  <label>Hour</label>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_hoursSelected</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="weekDaysSelected" type="ellab">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>2</columnSpan>
                  <label>Week Day</label>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_weekDaysSelected</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="EditButton" type="elbtn">
                  <cellAlignment>center,center</cellAlignment>
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/uimthd_edit</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <columnSpan>1</columnSpan>
                  <label>Edit</label>
                  <width>50px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$yearsSelected" type="laction">
      <path>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_yearsSelected</path>
      <type>initializer</type>
      <script><![CDATA[return "None";]]></script>
    </method>
    <method name="cwLeafInitAction$hoursSelected" type="laction">
      <path>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_hoursSelected</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_hoursSelected</valueType>
      <script><![CDATA[return "None";]]></script>
    </method>
    <method name="cwLeafInitAction$weekDaysSelected" type="laction">
      <path>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_weekDaysSelected</path>
      <type>initializer</type>
      <script><![CDATA[return "None";]]></script>
    </method>
    <method name="cwLeafInitAction$daysSelected" type="laction">
      <path>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_daysSelected</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_daysSelected</valueType>
      <script><![CDATA[return "None";]]></script>
    </method>
    <method name="cwLeafInitAction$monthsSelected" type="laction">
      <path>ui_cm_spoc.forms.HappyPeriodPopUpTableLineUI/leaf_monthsSelected</path>
      <type>initializer</type>
      <script><![CDATA[return "None"]]></script>
    </method>
    <method name="edit" type="uimthd">
      <script>cm_spoc.initHappyPeriod(this.parent,this.model,true);</script>
    </method>
  </methodList>
</userInterface>