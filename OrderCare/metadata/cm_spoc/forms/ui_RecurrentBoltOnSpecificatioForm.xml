<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.RecurrentBoltOnSpecificatioForm">
  <group>CyclicActions</group>
  <label>RecurrentBoltOnSpecificatioForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>600px</width>
              <elementList>
                <element name="TabFrame1" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/frmui_Forms/vform_GeneralParameters</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                </element>
                <element name="TabFrame" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/frmui_Forms/vform_CycleChangeProperties</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Cycle Change Properties</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="CycleChangeProperties" type="vform">
          <label>CycleChangeProperties</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="VerticalLayout1" type="elvlt">
                <alignment>right,center</alignment>
                <elementList>
                  <element name="boltonAmount" type="elfld">
                    <autoFocusNextField>false</autoFocusNextField>
                    <label>Awarded Amount</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CABOCycleChngeSpecificationDS</variablePath>
                        <variablePath>dstruct_cm_spoc.data.CABOCycleChngeSpecification/dstruct_boltonAmount</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="calcBoltonAmountCode" type="elchk">
                    <label>It is a percentage of the awarded amount</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CABOCycleChngeSpecificationDS</variablePath>
                        <variablePath>dstruct_cm_spoc.data.CABOCycleChngeSpecification/dstruct_calcBoltonAmountCode</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="boltonMaxNumberOfSessions" type="elfld">
                    <autoFocusNextField>false</autoFocusNextField>
                    <label>Number of sessions</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CABOCycleChngeSpecificationDS</variablePath>
                        <variablePath>dstruct_cm_spoc.data.CABOCycleChngeSpecification/dstruct_boltonMaxNumberOfSessions</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="calcMaxNumberOfSessionsCode" type="elchk">
                    <label>It is a percentage of the number of sessions</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CABOCycleChngeSpecificationDS</variablePath>
                        <variablePath>dstruct_cm_spoc.data.CABOCycleChngeSpecification/dstruct_calcMaxNumberOfSessionsCode</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="MaximumUnitsPerSessionDuringACycleChange" type="elfld">
                    <autoFocusNextField>false</autoFocusNextField>
                    <label>Maximum units per session</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CABOCycleChngeSpecificationDS</variablePath>
                        <variablePath>dstruct_cm_spoc.data.CABOCycleChngeSpecification/dstruct_MaximumUnitsPerSessionDuringACycleChange</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="PercenMaxNumberUnitsPerSessions" type="elchk">
                    <label>It is a percentage of the maximum number of units per session</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CABOCycleChngeSpecificationDS</variablePath>
                        <variablePath>dstruct_cm_spoc.data.CABOCycleChngeSpecification/dstruct_PercenMaxNumberUnitsPerSessions</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="boltonLD" type="elfld">
                    <autoFocusNextField>false</autoFocusNextField>
                    <errorIconGap>true</errorIconGap>
                    <label>% reduction in long distance</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CABOCycleChngeSpecificationDS</variablePath>
                        <variablePath>dstruct_cm_spoc.data.CABOCycleChngeSpecification/dstruct_boltonLD</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="calcBoltonLDCode" type="elchk">
                    <label>It is a percentage of reduction in long distance</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CABOCycleChngeSpecificationDS</variablePath>
                        <variablePath>dstruct_cm_spoc.data.CABOCycleChngeSpecification/dstruct_calcBoltonLDCode</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="calcBoltonCostCode" type="elchk">
                    <label>It is a percentage of the cost of the recurrent bolt-on provision</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CABOCycleChngeSpecificationDS</variablePath>
                        <variablePath>dstruct_cm_spoc.data.CABOCycleChngeSpecification/dstruct_calcBoltonCostCode</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="GeneralParameters" type="vform">
          <label>GeneralParameters</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="CyclicActionSpecEnum" type="elslct">
                <label>Cyclic Action Specificaton</label>
                <labelOrientation>left</labelOrientation>
                <multiselect>false</multiselect>
                <onChangeMethod type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/uimthd_cyclicActionSpecSelected</variablePath>
                  </variablePathList>
                </onChangeMethod>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.RecurrentBoltOnSpecificatio/dstruct_CyclicActionSpecEnum</variablePath>
                  </variablePathList>
                </variable>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.RecurrentBoltOnSpecificatioForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initRecurrentBoltOnSpecification(this.model);
        if(m.CyclicActionSpecEnum == null){
            m.CyclicActionSpecEnum = m.CyclicActionSpecification;
        }
        return m;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script>return true;</script>
    </method>
    <method name="cyclicActionSpecSelected" type="uimthd">
      <script>this.model.CyclicActionSpecification = this.model.CyclicActionSpecEnum;</script>
    </method>
  </methodList>
</userInterface>