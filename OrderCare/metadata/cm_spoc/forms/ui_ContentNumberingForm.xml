        newDetail.setBaseItemCode("base_ContentId",null,null,"CntXnt",this.model.AnalysisTypeContent);
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="FinderTariffReferenceDialog" type="usrActionMethod">
