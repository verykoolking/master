        newDetail.setBaseItemCode("base_ContentType",null,null,"CntTypeXnt",this.model.AnalysisTypeContent);
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="FinderContentIdReferenceDialog" type="usrActionMethod">
