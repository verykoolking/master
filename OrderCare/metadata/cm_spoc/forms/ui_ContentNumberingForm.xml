    <method name="TonTrigger" type="uimthd">
      <script><![CDATA[
        var btCode=this.model.TonEnum;
        this.model.TonContentInputFor = btCode;
      ]]></script>
    </method>
    <method name="setIsPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
    <method name="initializeFields" type="uimthd">
      <script><![CDATA[
        this.model.ContentIdInputFor = null;
        this.model.ContentServiceProviderInputFor = null;
        this.model.ContentTypeInputFor = null;
      ]]></script>
    </method>
