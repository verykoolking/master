<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.PulseTrainUI">
  <group>Rate</group>
  <label>PulseTrainUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.RatePriceEventPhase</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elvlt">
              <width>750px</width>
              <elementList>
                <element name="SectionStack" type="elsectnstck">
                  <visibilityMode>multiple</visibilityMode>
                  <elementList>
                    <element name="Section" type="elsect">
                      <elementList>
                        <element name="SectionHeader" type="elsecth">
                          <label>Pulse Train</label>
                          <elementList>
                            <element name="PulseTrainLabel" type="ellab">
                              <cellAlignment>left,center</cellAlignment>
                              <labelOrientation>left</labelOrientation>
                              <textAlign>left</textAlign>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.PulseTrainUI/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.RatePriceEventPhase/dstruct_PulseTrainLabel</variablePath>
                                </variablePathList>
                              </variable>
                            </element>
                            <element name="LayoutSpacer3" type="ellsp">
                              <width>530px</width>
                            </element>
                          </elementList>
                        </element>
                        <element name="HorizontalLayout1" type="elhlt">
                          <alignment>right,center</alignment>
                          <elementList>
                            <element name="RatePriceDesc" type="elfld">
                              <autoFocusNextField>false</autoFocusNextField>
                              <columnSpan>3</columnSpan>
                              <labeAlign>left</labeAlign>
                              <label>Description</label>
                              <labelOrientation>left</labelOrientation>
                              <removeLabelLine>true</removeLabelLine>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.PulseTrainUI/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.RatePriceEventPhase/dstruct_RatePriceDS</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.RatePrice/dstruct_RatePriceDesc</variablePath>
                                </variablePathList>
                              </variable>
                              <width>200px</width>
                            </element>
                          </elementList>
                        </element>
                        <element name="HorizontalLayout" type="elhlt">
                          <alignment>right,center</alignment>
                          <elementList>
                            <element name="Price" type="elfld">
                              <autoFocusNextField>false</autoFocusNextField>
                              <columnSpan>3</columnSpan>
                              <labeAlign>left</labeAlign>
                              <label>Cost</label>
                              <labelOrientation>left</labelOrientation>
                              <removeLabelLine>true</removeLabelLine>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.PulseTrainUI/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.RatePriceEventPhase/dstruct_RatePriceDS</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.RatePrice/dstruct_Price</variablePath>
                                </variablePathList>
                              </variable>
                              <width>100px</width>
                            </element>
                            <element name="CentsUnits" type="ellab">
                              <columnSpan>3</columnSpan>
                              <label>cents</label>
                              <removeLabelLine>true</removeLabelLine>
                              <showLabel>false</showLabel>
                              <width>40px</width>
                            </element>
                            <element name="LayoutSpacer2" type="ellsp">
                              <width>0px</width>
                            </element>
                            <element name="Denominator" type="elfld">
                              <autoFocusNextField>false</autoFocusNextField>
                              <columnSpan>3</columnSpan>
                              <labeAlign>left</labeAlign>
                              <label>per</label>
                              <labelOrientation>left</labelOrientation>
                              <removeLabelLine>true</removeLabelLine>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.PulseTrainUI/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.RatePriceEventPhase/dstruct_RatePriceDS</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.RatePrice/dstruct_Denominator</variablePath>
                                </variablePathList>
                              </variable>
                              <width>70px</width>
                            </element>
                            <element name="Seconds2" type="ellab">
                              <columnSpan>3</columnSpan>
                              <label>seconds</label>
                              <removeLabelLine>true</removeLabelLine>
                              <showLabel>false</showLabel>
                              <width>100px</width>
                            </element>
                          </elementList>
                        </element>
                        <element name="layout" type="elvlt">
                          <alignment>right,center</alignment>
                          <elementList>
                            <element name="HorizontalLayout2" type="elhlt">
                              <alignment>right,center</alignment>
                              <elementList>
                                <element name="StartUnit" type="elfld">
                                  <autoFocusNextField>false</autoFocusNextField>
                                  <columnSpan>3</columnSpan>
                                  <labeAlign>right</labeAlign>
                                  <label>Time Period from</label>
                                  <labelOrientation>left</labelOrientation>
                                  <removeLabelLine>true</removeLabelLine>
                                  <variable type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.PulseTrainUI/leaf_model</variablePath>
                                      <variablePath>dstruct_cm_spoc.data.RatePriceEventPhase/dstruct_StartUnit</variablePath>
                                    </variablePathList>
                                  </variable>
                                  <width>100px</width>
                                </element>
                                <element name="LayoutSpacer" type="ellsp">
                                  <width>20px</width>
                                </element>
                                <element name="MinutesSecondsEnum" type="elslct">
                                  <columnSpan>3</columnSpan>
                                  <onChangeMethod type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.PulseTrainUI/uimthd_setMinuteSecond</variablePath>
                                    </variablePathList>
                                  </onChangeMethod>
                                  <removeLabelLine>true</removeLabelLine>
                                  <showLabel>false</showLabel>
                                  <variable type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.PulseTrainUI/leaf_model</variablePath>
                                      <variablePath>dstruct_cm_spoc.data.RatePriceEventPhase/dstruct_MinutesSecondsEnum</variablePath>
                                    </variablePathList>
                                  </variable>
                                  <width>100px</width>
                                </element>
                              </elementList>
                            </element>
                            <element name="HorizontalLayout3" type="elhlt">
                              <alignment>right,center</alignment>
                              <elementList>
                                <element name="unitOfMeasure" type="elfld">
                                  <autoFocusNextField>false</autoFocusNextField>
                                  <columnSpan>3</columnSpan>
                                  <labeAlign>right</labeAlign>
                                  <label>Charge Period</label>
                                  <labelOrientation>left</labelOrientation>
                                  <removeLabelLine>true</removeLabelLine>
                                  <variable type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.PulseTrainUI/leaf_model</variablePath>
                                      <variablePath>dstruct_cm_spoc.data.RatePriceEventPhase/dstruct_RatePriceDS</variablePath>
                                      <variablePath>dstruct_cm_spoc.data.RatePrice/dstruct_unitOfMeasure</variablePath>
                                    </variablePathList>
                                  </variable>
                                  <width>100px</width>
                                </element>
                                <element name="LayoutSpacer1" type="ellsp">
                                  <width>20px</width>
                                </element>
                                <element name="Seconds" type="ellab">
                                  <columnSpan>3</columnSpan>
                                  <label>seconds</label>
                                  <removeLabelLine>true</removeLabelLine>
                                  <showLabel>false</showLabel>
                                  <width>100px</width>
                                </element>
                              </elementList>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                    <element name="Sectiondummy" type="elsect"/>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="setReadOnly" type="uimthd">
      <script>return;</script>
    </method>
    <method name="setMinuteSecond" type="uimthd">
      <script><![CDATA[
        if (this.model.MinutesSecondsEnum == "min") {
           this.model.StartUnit = this.model.StartUnit/60;
        }
        else{
           this.model.StartUnit = this.model.StartUnit*60;
        }
      ]]></script>
    </method>
  </methodList>
</userInterface>