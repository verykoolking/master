<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.NumberOfExecutionApplicabilForm">
  <group>Promotion</group>
  <label>NumberOfExecutionApplicabilForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.NumberOfExecutionApplicabil</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="SectionStack" type="elsectnstck">
              <width>750px</width>
              <elementList>
                <element name="Section" type="elsect">
                  <elementList>
                    <element name="SectionHeader" type="elsecth">
                      <label>Number of Excecution</label>
                    </element>
                    <element name="VerticalLayout" type="elvlt">
                      <width>750px</width>
                      <elementList>
                        <element name="HorizontalLayout" type="elhlt">
                          <elementList>
                            <element name="TotalExecutionsApplicability" type="elchk">
                              <label>Total Executions</label>
                              <labelOrientation>left</labelOrientation>
                              <onChangeMethod type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/uimthd_selectTotalNumber</variablePath>
                                </variablePathList>
                              </onChangeMethod>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.NumberOfExecutionApplicabil/dstruct_TotalExecutionsApplicability</variablePath>
                                </variablePathList>
                              </variable>
                            </element>
                          </elementList>
                        </element>
                        <element name="TotalNumberOfExecutions" type="elfld">
                          <labeAlign>left</labeAlign>
                          <label> Total Number of Executions</label>
                          <labelOrientation>left</labelOrientation>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</variablePath>
                              <variablePath>dstruct_cm_spoc.data.NumberOfExecutionApplicabil/dstruct_TotalNumberOfExecutions</variablePath>
                            </variablePathList>
                          </variable>
                          <visible type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/permMethod_NOEATotalNumberOfExecutionsVisRule</variablePath>
                            </variablePathList>
                          </visible>
                        </element>
                        <element name="ExecutionsInAPeriodApplicability" type="elchk">
                          <label>Execution in a period of time</label>
                          <labelOrientation>left</labelOrientation>
                          <onChangeMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/uimthd_selectTrigger</variablePath>
                            </variablePathList>
                          </onChangeMethod>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</variablePath>
                              <variablePath>dstruct_cm_spoc.data.NumberOfExecutionApplicabil/dstruct_ExecutionsInAPeriodApplicability</variablePath>
                            </variablePathList>
                          </variable>
                        </element>
                        <element name="NumberOfExecType" type="elrgrp">
                          <labelOrientation>left</labelOrientation>
                          <onChangeMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/uimthd_selectRelativeAbsolute</variablePath>
                            </variablePathList>
                          </onChangeMethod>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</variablePath>
                              <variablePath>dstruct_cm_spoc.data.NumberOfExecutionApplicabil/dstruct_NumberOfExecType</variablePath>
                            </variablePathList>
                          </variable>
                          <visible type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/permMethod_NOEANumberOfExecTypeVisRule</variablePath>
                            </variablePathList>
                          </visible>
                        </element>
                        <element name="HorizontalLayout1" type="elhlt">
                          <width>750px</width>
                          <elementList>
                            <element name="RelativeNumberOfTimes" type="elfld">
                              <labeAlign>left</labeAlign>
                              <labelOrientation>left</labelOrientation>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.NumberOfExecutionApplicabil/dstruct_RelativeNumberOfTimes</variablePath>
                                </variablePathList>
                              </variable>
                              <visible type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/permMethod_NOEARelativeNumberOfTimesVisRule</variablePath>
                                </variablePathList>
                              </visible>
                            </element>
                            <element name="RelativeLastNumber" type="elfld">
                              <labeAlign>left</labeAlign>
                              <label>times in the last</label>
                              <labelOrientation>left</labelOrientation>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.NumberOfExecutionApplicabil/dstruct_RelativeLastNumber</variablePath>
                                </variablePathList>
                              </variable>
                              <visible type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/permMethod_NOEARelativeLastNumberVisRule</variablePath>
                                </variablePathList>
                              </visible>
                            </element>
                            <element name="RelativeUnitTime" type="elslct">
                              <labeAlign>left</labeAlign>
                              <labelOrientation>left</labelOrientation>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.NumberOfExecutionApplicabil/dstruct_RelativeUnitTime</variablePath>
                                </variablePathList>
                              </variable>
                              <visible type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/permMethod_NOEARelativeUnitTimeVisRule</variablePath>
                                </variablePathList>
                              </visible>
                            </element>
                          </elementList>
                        </element>
                        <element name="HorizontalLayout2" type="elhlt">
                          <width>750px</width>
                          <elementList>
                            <element name="AbsoluteNumberOfTimes" type="elfld">
                              <labeAlign>left</labeAlign>
                              <labelOrientation>left</labelOrientation>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.NumberOfExecutionApplicabil/dstruct_AbsoluteNumberOfTimes</variablePath>
                                </variablePathList>
                              </variable>
                              <visible type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/permMethod_NOEAAbsoluteNumberOfTimesVisRule</variablePath>
                                </variablePathList>
                              </visible>
                            </element>
                            <element name="CurrentUnitTime" type="elslct">
                              <labeAlign>left</labeAlign>
                              <label>times in the current</label>
                              <labelOrientation>left</labelOrientation>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.NumberOfExecutionApplicabil/dstruct_CurrentUnitTime</variablePath>
                                </variablePathList>
                              </variable>
                              <visible type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/permMethod_NOEACurrentUnitTimeVisRule</variablePath>
                                </variablePathList>
                              </visible>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.NumberOfExecutionApplicabilForm/leaf_model</path>
      <type>initializer</type>
      <script>return cm_spoc.initNumberOfExecutionApplicabilityUI(this.model);</script>
    </method>
    <method name="NOEATotalNumberOfExecutionsVisRule" type="permMethod">
      <script><![CDATA[
        var vis = this.model.TotalExecutionsApplicability;
        if (vis) {
            return true;
        }
        else {
            return false;
        }
      ]]></script>
    </method>
    <method name="selectTrigger" type="uimthd">
      <script>return;</script>
    </method>
    <method name="NOEANumberOfExecTypeVisRule" type="permMethod">
      <script><![CDATA[
        var vis = this.model.ExecutionsInAPeriodApplicability;
        if (vis) {
            return true;
        }
        else {
            this.model.NumberOfExecType = null;
            return false;
        }
      ]]></script>
    </method>
    <method name="NOEARelativeNumberOfTimesVisRule" type="permMethod">
      <script><![CDATA[
        var vis = this.model.NumberOfExecType;
        if (vis=='R') {
               // this.model.RelativeNumberOfTimes = null;
            return true;
        }
        else {
            return false;
        }
      ]]></script>
    </method>
    <method name="NOEARelativeLastNumberVisRule" type="permMethod">
      <script><![CDATA[
        var vis = this.model.NumberOfExecType;
        if (vis=='R') {
               // this.model.RelativeLastNumber = null;
            return true;
        }
        else {

            return false;
        }
      ]]></script>
    </method>
    <method name="NOEARelativeUnitTimeVisRule" type="permMethod">
      <script><![CDATA[
        var vis = this.model.NumberOfExecType;
        if (vis=='R') {
              //  this.model.RelativeUnitTime = null;
            return true;
        }
        else {

            return false;
        }
      ]]></script>
    </method>
    <method name="NOEAAbsoluteNumberOfTimesVisRule" type="permMethod">
      <script><![CDATA[
        var vis = this.model.NumberOfExecType;
        if (vis=='A') {
              //  this.model.AbsoluteNumberOfTimes = null;
            return true;
        }
        else {

            return false;
        }
      ]]></script>
    </method>
    <method name="NOEACurrentUnitTimeVisRule" type="permMethod">
      <script><![CDATA[
        var vis = this.model.NumberOfExecType;
        if (vis=='A') {
             //   this.model.CurrentUnitTime = null;
            return true;
        }
        else {

            return false;
        }
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="selectTotalNumber" type="uimthd">
      <script><![CDATA[
        var val = this.model.TotalExecutionsApplicability;

        if (!val) {
            this.model.TotalNumberOfExecutions=null;
        }
      ]]></script>
    </method>
    <method name="selectRelativeAbsolute" type="uimthd">
      <script><![CDATA[
        var val = this.model.NumberOfExecType;

        if (val=='A') {
            this.model.RelativeNumberOfTimes=null;
            this.model.RelativeLastNumber=null;
            this.model.RelativeUnitTime=null;
        }
        else {
            this.model.AbsoluteNumberOfTimes=null;
            this.model.CurrentUnitTime=null;
        }
      ]]></script>
    </method>
  </methodList>
</userInterface>