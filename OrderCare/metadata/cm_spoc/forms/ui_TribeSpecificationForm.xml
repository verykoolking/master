<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.TribeSpecificationForm">
  <group>Tribes</group>
  <label>TribeSpecificationForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.TribeSpecification</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>750px</width>
              <elementList>
                <element name="TribeSpecification" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/frmui_Forms/vform_Tribes</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Tribe Specification</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="Tribes" type="vform">
          <label>Tribes</label>
          <root name="VerticalLayout" type="elvlt">
            <width>750px</width>
            <elementList>
              <element name="SectionStack" type="elsectnstck">
                <width>800px</width>
                <elementList>
                  <element name="Section" type="elsect">
                    <elementList>
                      <element name="SectionHeader" type="elsecth">
                        <label>Tribes</label>
                      </element>
                      <element name="VerticalLayout1" type="elvlt">
                        <elementList>
                          <element name="GridLayout1" type="elgrd">
                            <alignment>right,center</alignment>
                            <elementList>
                              <element name="TribePriority" type="elfld">
                                <labeAlign>left</labeAlign>
                                <label>Tribe Priority</label>
                                <labelOrientation>left</labelOrientation>
                                <textAlign>left</textAlign>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_TribePriority</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>150px</width>
                              </element>
                              <element name="MaxMembersInTheTribe" type="elfld">
                                <labeAlign>left</labeAlign>
                                <label>Maximum members in the Tribe</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_MaxMembersInTheTribe</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>150px</width>
                              </element>
                              <element name="MaxExternalMembersTribe" type="elfld">
                                <labeAlign>left</labeAlign>
                                <label>Maximum external members in the Tribe</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_MaxExternalMembersTribe</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>150px</width>
                              </element>
                              <element name="RegExCode" type="elfld">
                                <editable type="varPath">
                                  <variablePathList>
                                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                  </variablePathList>
                                </editable>
                                <labeAlign>left</labeAlign>
                                <label>Regular Expression Code</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_RegExCode</variablePath>
                                  </variablePathList>
                                </variable>
                                <visible type="varPath">
                                  <variablePathList>
                                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                  </variablePathList>
                                </visible>
                                <width>150px</width>
                              </element>
                              <element name="RegExDescription" type="elfld">
                                <editable type="varPath">
                                  <variablePathList>
                                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                  </variablePathList>
                                </editable>
                                <labeAlign>left</labeAlign>
                                <label>Regular Expression Description</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_RegExDescription</variablePath>
                                  </variablePathList>
                                </variable>
                                <visible type="varPath">
                                  <variablePathList>
                                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                  </variablePathList>
                                </visible>
                                <width>150px</width>
                              </element>
                              <element name="RegExString" type="eltxtArea">
                                <height>70px</height>
                                <labeAlign>left</labeAlign>
                                <label>Regular Expression of the Tribe members</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_RegExString</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>200px</width>
                              </element>
                              <element name="ImpSubsAllFromExToOwn" type="elchk">
                                <label>Imported subscribers are allowed to pass from external members to owned members</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_ImpSubsAllFromExToOwn</variablePath>
                                  </variablePathList>
                                </variable>
                              </element>
                              <element name="ExpSubsAllFromOwnToExt" type="elchk">
                                <label>Exported subscribers are allowed to pass from owned members to external members</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_ExpSubsAllFromOwnToExt</variablePath>
                                  </variablePathList>
                                </variable>
                              </element>
                            </elementList>
                          </element>
                          <element name="GridLayout2" type="elgrd">
                            <alignment>left,center</alignment>
                            <labelOrientation>left</labelOrientation>
                            <numOfCols>7</numOfCols>
                            <elementList>
                              <element name="InfiniteDate" type="elchk">
                                <label>Infinite Date</label>
                                <labelOrientation>left</labelOrientation>
                                <onChangeMethod type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/uimthd_resettrigger</variablePath>
                                  </variablePathList>
                                </onChangeMethod>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_InfiniteDate</variablePath>
                                  </variablePathList>
                                </variable>
                              </element>
                              <element name="StartDate" type="eldate">
                                <label>Start date</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_StartDate</variablePath>
                                  </variablePathList>
                                </variable>
                                <visible type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/permMethod_TSStartDateVisRule</variablePath>
                                  </variablePathList>
                                </visible>
                                <width>125px</width>
                              </element>
                              <element name="EndDate" type="eldate">
                                <label>End date</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_EndDate</variablePath>
                                  </variablePathList>
                                </variable>
                                <visible type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/permMethod_TSEndDateVisRule</variablePath>
                                  </variablePathList>
                                </visible>
                                <width>125px</width>
                              </element>
                            </elementList>
                          </element>
                          <element name="GridLayout5" type="elgrd">
                            <colWidths>300px</colWidths>
                            <edgeShowCenter>true</edgeShowCenter>
                            <edgeSize>2</edgeSize>
                            <labeAlign>center</labeAlign>
                            <numOfCols>6</numOfCols>
                            <width>600px</width>
                            <elementList>
                              <element name="EndAllowedSubscriptionDate" type="eldate">
                                <labeAlign>left</labeAlign>
                                <label>End Allowed Subscription Date</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_EndAllowedSubscriptionDate</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>125px</width>
                              </element>
                            </elementList>
                          </element>
                          <element name="SectionStack1" type="elsectnstck">
                            <width>800px</width>
                            <elementList>
                              <element name="Section1" type="elsect">
                                <elementList>
                                  <element name="SectionHeader1" type="elsecth">
                                    <label>Applicability</label>
                                  </element>
                                  <element name="GridLayout4" type="elgrd">
                                    <colWidths>160px</colWidths>
                                    <edgeShowCenter>true</edgeShowCenter>
                                    <edgeSize>2</edgeSize>
                                    <labeAlign>center</labeAlign>
                                    <numOfCols>2</numOfCols>
                                    <width>600px</width>
                                    <elementList>
                                      <element name="Label3" type="ellab">
                                        <cellAlignment>left,center</cellAlignment>
                                        <fontProperties type="pfnt">
                                          <color>#003168</color>
                                          <family>Arial</family>
                                          <size>11</size>
                                        </fontProperties>
                                        <label>Voice</label>
                                        <width>250px</width>
                                      </element>
                                      <element name="VoiceTribe1" type="elchk">
                                        <cellAlignment>left,center</cellAlignment>
                                        <labelAsTitle>false</labelAsTitle>
                                        <showLabel>false</showLabel>
                                        <variable type="varPath">
                                          <variablePathList>
                                            <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                            <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_VoiceTribe</variablePath>
                                          </variablePathList>
                                        </variable>
                                      </element>
                                      <element name="Label4" type="ellab">
                                        <cellAlignment>left,center</cellAlignment>
                                        <fontProperties type="pfnt">
                                          <color>#003168</color>
                                          <family>Arial</family>
                                          <size>11</size>
                                        </fontProperties>
                                        <label>SMS</label>
                                        <width>250px</width>
                                      </element>
                                      <element name="SMSTribe1" type="elchk">
                                        <cellAlignment>left,center</cellAlignment>
                                        <labelAsTitle>false</labelAsTitle>
                                        <showLabel>false</showLabel>
                                        <variable type="varPath">
                                          <variablePathList>
                                            <variablePath>ui_cm_spoc.forms.TribeSpecificationForm/leaf_model</variablePath>
                                            <variablePath>dstruct_cm_spoc.data.TribeSpecification/dstruct_SMSTribe</variablePath>
                                          </variablePathList>
                                        </variable>
                                      </element>
                                    </elementList>
                                  </element>
                                </elementList>
                              </element>
                            </elementList>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$cwShowHover" type="laction">
      <path>ui_com.conceptwave.system.UserInterface/leaf_cwShowHover</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initTribesSpecification(this.model);
        return m;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="TSStartDateVisRule" type="permMethod">
      <script><![CDATA[
        var value =this.model.InfiniteDate;
        if(value== false ) return true;
        else return false;
      ]]></script>
    </method>
    <method name="resettrigger" type="uimthd">
      <script><![CDATA[
        var value =this.model.InfiniteDate;
        if(value== true )
        {
            this.model.StartDate=null;
            this.model.EndDate=null;
        }
        return;
      ]]></script>
    </method>
    <method name="TSEndDateVisRule" type="permMethod">
      <script><![CDATA[
        var value =this.model.InfiniteDate;
        if(value== false ) return true;
        else return false;
      ]]></script>
    </method>
  </methodList>
</userInterface>