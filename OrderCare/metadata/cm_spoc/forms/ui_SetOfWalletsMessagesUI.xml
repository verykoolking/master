<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.SetOfWalletsMessagesUI">
  <group>TariffPlan</group>
  <label>SetOfWalletsMessagesUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.NotifWalletRule</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="VerticalLayout" type="elvlt">
              <alignment>center,center</alignment>
              <elementList>
                <element name="Header" type="elhdr">
                  <label>Set Of Wallets Notifications</label>
                </element>
                <element name="GridLayout" type="elgrd">
                  <alignment>center,center</alignment>
                  <colWidths>50px</colWidths>
                  <numOfCols>6</numOfCols>
                  <width>200px</width>
                  <elementList>
                    <element name="RowSpacer" type="elrsp"/>
                    <element name="BalanceMessage" type="ellab">
                      <cellAlignment>left,center</cellAlignment>
                      <columnSpan>6</columnSpan>
                      <label>Balance Message</label>
                    </element>
                    <element name="CellSpacer" type="elcsp">
                      <columnSpan>1</columnSpan>
                    </element>
                    <element name="BalanceMsgEnum" type="elslct">
                      <columnSpan>4</columnSpan>
                      <multiselect>false</multiselect>
                      <onChangeMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/uimthd_setBalanceMsg</variablePath>
                        </variablePathList>
                      </onChangeMethod>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.NotifWalletRule/dstruct_BalanceMsgEnum</variablePath>
                        </variablePathList>
                      </variable>
                    </element>
                    <element name="SearchMessage" type="elbtn">
                      <columnSpan>1</columnSpan>
                      <icon>/cwt/images/16/find.png</icon>
                      <iconHeight>15</iconHeight>
                      <iconWidth>15</iconWidth>
                      <width>22px</width>
                    </element>
                    <element name="RowSpacer1" type="elrsp"/>
                    <element name="ExpirationMessage" type="ellab">
                      <cellAlignment>left,center</cellAlignment>
                      <columnSpan>6</columnSpan>
                      <label>Expiration Message</label>
                    </element>
                    <element name="CellSpacer1" type="elcsp">
                      <columnSpan>1</columnSpan>
                    </element>
                    <element name="ExpirationMsgEnum" type="elslct">
                      <columnSpan>4</columnSpan>
                      <multiselect>false</multiselect>
                      <onChangeMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/uimthd_setExpirationMsg</variablePath>
                        </variablePathList>
                      </onChangeMethod>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.NotifWalletRule/dstruct_ExpirationMsgEnum</variablePath>
                        </variablePathList>
                      </variable>
                    </element>
                    <element name="SearchMessage1" type="elbtn">
                      <columnSpan>1</columnSpan>
                      <icon>/cwt/images/16/find.png</icon>
                      <width>22px</width>
                    </element>
                    <element name="RowSpacer2" type="elrsp"/>
                    <element name="ConsumptionMessage" type="ellab">
                      <cellAlignment>left,center</cellAlignment>
                      <columnSpan>6</columnSpan>
                      <label>Consumption Message</label>
                    </element>
                    <element name="CellSpacer2" type="elcsp">
                      <columnSpan>1</columnSpan>
                    </element>
                    <element name="ConsumptionMsgEnum" type="elslct">
                      <columnSpan>4</columnSpan>
                      <multiselect>false</multiselect>
                      <onChangeMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/uimthd_setConsumptionMsg</variablePath>
                        </variablePathList>
                      </onChangeMethod>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.NotifWalletRule/dstruct_ConsumptionMsgEnum</variablePath>
                        </variablePathList>
                      </variable>
                    </element>
                    <element name="SearchMessage2" type="elbtn">
                      <columnSpan>1</columnSpan>
                      <icon>/cwt/images/16/find.png</icon>
                      <width>22px</width>
                    </element>
                    <element name="RowSpacer3" type="elrsp"/>
                    <element name="ExhaustedBalanceMessage" type="ellab">
                      <cellAlignment>left,center</cellAlignment>
                      <columnSpan>6</columnSpan>
                      <label>Exhausted Balance Message</label>
                    </element>
                    <element name="CellSpacer3" type="elcsp">
                      <columnSpan>1</columnSpan>
                    </element>
                    <element name="ExhaustedMsgEnum" type="elslct">
                      <columnSpan>4</columnSpan>
                      <multiselect>false</multiselect>
                      <onChangeMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/uimthd_setExhaustedMsg</variablePath>
                        </variablePathList>
                      </onChangeMethod>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.NotifWalletRule/dstruct_ExhaustedMsgEnum</variablePath>
                        </variablePathList>
                      </variable>
                    </element>
                    <element name="SearchMessage3" type="elbtn">
                      <columnSpan>1</columnSpan>
                      <icon>/cwt/images/16/find.png</icon>
                      <width>22px</width>
                    </element>
                    <element name="RowSpacer4" type="elrsp">
                      <height>60px</height>
                      <rowSpan>60px</rowSpan>
                    </element>
                  </elementList>
                </element>
                <element name="ButtonsHL" type="elhlt">
                  <alignment>center,center</alignment>
                  <fillSpace>false</fillSpace>
                  <snapOnMouseOver>false</snapOnMouseOver>
                  <width>500px</width>
                  <elementList>
                    <element name="LayoutSpacer" type="ellsp">
                      <width>55px</width>
                    </element>
                    <element name="OK" type="elbtn">
                      <cellAlignment></cellAlignment>
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/uimthd_onClickOk</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Accept</label>
                    </element>
                    <element name="LayoutSpacer1" type="ellsp">
                      <width>50px</width>
                    </element>
                    <element name="Cancel" type="elbtn">
                      <canSort>false</canSort>
                      <cellAlignment>center,center</cellAlignment>
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.SetOfWalletsMessagesUI/uimthd_onClickCancel</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Cancel</label>
                    </element>
                    <element name="LayoutSpacer2" type="ellsp">
                      <height>50px</height>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="onClickOk" type="uimthd">
      <script>this.parent.cwDialogClose();</script>
    </method>
    <method name="onClickCancel" type="uimthd">
      <script>this.parent.cwDialogClose();</script>
    </method>
    <method name="setBalanceMsg" type="uimthd">
      <script>this.model.BalanceMsg = this.model.BalanceMsgEnum;</script>
    </method>
    <method name="setExpirationMsg" type="uimthd">
      <script>this.model.ExpirationMsg = this.model.ExpirationMsgEnum;</script>
    </method>
    <method name="setConsumptionMsg" type="uimthd">
      <script>this.model.ConsumptionMsg = this.model.ConsumptionMsgEnum;</script>
    </method>
    <method name="setExhaustedMsg" type="uimthd">
      <script>this.model.ExhaustedMsg = this.model.ExhaustedMsgEnum;</script>
    </method>
  </methodList>
</userInterface>