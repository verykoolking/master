<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.PromotionUsingWindowRuleIteratorUI">
  <group>Promotion</group>
  <label>PromotionUsingWindowRuleIteratorUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.PromotionUsingWindowRule</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elgrd">
              <colWidths>50px</colWidths>
              <fixedColWidths>true</fixedColWidths>
              <numOfCols>15</numOfCols>
              <width>750px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <cellAlignment>left,center</cellAlignment>
                  <columnSpan>1</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionUsingWindowRuleIteratorUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>30px</width>
                </element>
                <element name="AccumulatedTopUpAmountThreshold" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>5</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionUsingWindowRuleIteratorUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PromotionUsingWindowRule/dstruct_AccumulatedTopUpAmountThreshold</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="AmountAwardedByThePromotion" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>3</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionUsingWindowRuleIteratorUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PromotionUsingWindowRule/dstruct_AmountAwardedByThePromotion</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="Percentage" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>2</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionUsingWindowRuleIteratorUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PromotionUsingWindowRule/dstruct_Percentage</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="Button" type="elbtn">
                  <cellAlignment>center,center</cellAlignment>
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionUsingWindowRuleIteratorUI/usrActionMethod_TopUpOriginDestinationDialog</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <columnSpan>4</columnSpan>
                  <label>Top Up Origin-Destination</label>
                  <width>150px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="TopUpOriginDestinationDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <parameterList>
        <parameter name="dialog" type="rifp">
          <type>ui_com.conceptwave.system.Dialog</type>
        </parameter>
      </parameterList>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var topUpDestinationPopUp = new cm_spoc.forms.TopUpOriginDestinationPairUI(this.model.PUWRTUODestPairArrDS,this);
        topUpDestinationPopUp.TopUpOriginDestinationPairIterator = new Array();
        var model = topUpDestinationPopUp.model.ListDS;

        if(topUpDestinationPopUp.model.specificationCode){
           cm_spoc.loadListElements(topUpDestinationPopUp, model, "cm_spoc.forms.TopUpOriginDestinationPairIteratorUI", "TopUpOriginDestinationPairIterator");
        }
        return topUpDestinationPopUp;
      ]]></script>
    </method>
  </methodList>
</userInterface>