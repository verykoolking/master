<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.WeekDayDefinitionForm">
  <group>Calendars</group>
  <label>WeekDayDefinitionForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.WeekDayDefinition</valueType>
    </variable>
    <variable name="DayDefinitionIterator" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
    <variable name="PickList" type="leaf">
      <valueType>ui_cm_spoc.PickList</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="WeekDayDefinitionVL" type="elvlt">
              <height>700px</height>
              <overflow>auto</overflow>
              <width>656px</width>
              <elementList>
                <element name="SectionStack" type="elsectnstck">
                  <visibilityMode>multiple</visibilityMode>
                  <width>655px</width>
                  <elementList>
                    <element name="WeekDayDefinitionSection" type="elsect">
                      <removeLabelLine>true</removeLabelLine>
                      <elementList>
                        <element name="WeekDayDefinitionHeader" type="elsecth">
                          <label>Service Provider</label>
                        </element>
                        <element name="DayDefinitionVL" type="elvlt">
                          <width>652px</width>
                          <elementList>
                            <element name="FormFrame" type="elovf">
                              <cellAlignment>center,center</cellAlignment>
                              <formReference type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WeekDayDefinitionForm/leaf_PickList</variablePath>
                                  <variablePath>ui_cm_spoc.PickList/frmui_Forms</variablePath>
                                  <variablePath>ui_cm_spoc.PickList/frmui_Forms/exov_Default</variablePath>
                                </variablePathList>
                              </formReference>
                              <height>325px</height>
                              <padding>0</padding>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WeekDayDefinitionForm/leaf_PickList</variablePath>
                                </variablePathList>
                              </variable>
                            </element>
                            <element name="SectionStack1" type="elsectnstck">
                              <visibilityMode>multiple</visibilityMode>
                              <width>654px</width>
                              <elementList>
                                <element name="WeekDayDefinitionSection1" type="elsect">
                                  <removeLabelLine>true</removeLabelLine>
                                  <elementList>
                                    <element name="WeekDayDefinitionHeader1" type="elsecth">
                                      <label>Week Day Definition</label>
                                    </element>
                                    <element name="DayDefinitionVL1" type="elvlt">
                                      <height>321px</height>
                                      <width>652px</width>
                                      <elementList>
                                        <element name="DayDefinitionIterator1" type="eliter">
                                          <variable type="varPath">
                                            <variablePathList>
                                              <variablePath>ui_cm_spoc.forms.WeekDayDefinitionForm/leaf_DayDefinitionIterator</variablePath>
                                            </variablePathList>
                                          </variable>
                                          <elementList>
                                            <element name="DayDefinitionHL1" type="elhlt">
                                              <width>652px</width>
                                              <elementList>
                                                <element name="DayDefinitionFormFrame1" type="elovf">
                                                  <formReference type="varPath">
                                                    <variablePathList>
                                                      <variablePath>ui_cm_spoc.forms.WeekDayDefinitionForm/leaf_DayDefinitionIterator</variablePath>
                                                      <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                                      <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                                    </variablePathList>
                                                  </formReference>
                                                </element>
                                              </elementList>
                                            </element>
                                          </elementList>
                                        </element>
                                      </elementList>
                                    </element>
                                  </elementList>
                                </element>
                              </elementList>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.WeekDayDefinitionForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.WeekDayDefinitionForm/leaf_model</valueType>
      <script><![CDATA[
        var m = cm_spoc.initWeekDayDefinition(this.model);
        cm_spoc.initWeekDayDefinitionUi(this);
        return m;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$PickList" type="laction">
      <path>ui_cm_spoc.forms.WeekDayDefinitionForm/leaf_PickList</path>
      <type>initializer</type>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_ServiceProvider";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.WDDServiceProviderArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setPickListModel;
        return pl;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i = 0; i < this.DayDefinitionIterator.length; i++){
            this.DayDefinitionIterator[i].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
    <method name="setPickListModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <array>true</array>
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.WDDServiceProviderArrDS.List =null;
        this.parent.model.WDDServiceProviderArrDS.ListDS =null;
        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.WDDServiceProviderArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
  </methodList>
</userInterface>