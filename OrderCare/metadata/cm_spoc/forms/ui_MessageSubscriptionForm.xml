<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.MessageSubscriptionForm">
  <group>Messages</group>
  <label>MessageSubscriptionForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.MessageSubscription</valueType>
    </variable>
    <variable name="MessageSubscriptionIterator" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="VerticalLayout" type="elvlt">
              <alignment>left,center</alignment>
              <elementList>
                <element name="VerticalLayout2" type="elvlt">
                  <alignment>left,center</alignment>
                  <elementList>
                    <element name="SendAdditional" type="elfld">
                      <labeAlign>left</labeAlign>
                      <label>Send Additional</label>
                      <labelOrientation>left</labelOrientation>
                      <onChangeMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.MessageSubscriptionForm/usrActionMethod_checkSendAdditional</variablePath>
                        </variablePathList>
                      </onChangeMethod>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.MessageSubscriptionForm/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.MessageSubscription/dstruct_SendAdditional</variablePath>
                        </variablePathList>
                      </variable>
                    </element>
                    <element name="HorizontalLayout" type="elhlt">
                      <elementList>
                        <element name="MessageSubscriptionSpecification" type="elfld">
                          <editable type="varPath">
                            <variablePathList>
                              <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                            </variablePathList>
                          </editable>
                          <labeAlign>left</labeAlign>
                          <label>&lt;b&gt;Message Subscription Specification *&lt;/b&gt;</label>
                          <labelOrientation>left</labelOrientation>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.MessageSubscriptionForm/leaf_model</variablePath>
                              <variablePath>dstruct_cm_spoc.data.MessageSubscription/dstruct_MessageSubscriptionSpecific</variablePath>
                            </variablePathList>
                          </variable>
                        </element>
                        <element name="OperationButton" type="elbtn">
                          <clickMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.MessageSubscriptionForm/usrActionMethod_FinderMessageReferenceDialog</variablePath>
                            </variablePathList>
                          </clickMethod>
                          <icon>/cwf/finder.gif</icon>
                          <width>22px</width>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
                <element name="VerticalLayout1" type="elvlt">
                  <elementList>
                    <element name="SectionStack" type="elsectnstck">
                      <visibilityMode>multiple</visibilityMode>
                      <width>1050px</width>
                      <elementList>
                        <element name="transformationRules" type="elsect">
                          <removeLabelLine>true</removeLabelLine>
                          <elementList>
                            <element name="SectionHeader" type="elsecth"/>
                            <element name="buttonsLayout" type="elhlt">
                              <elementList>
                                <element name="addButton" type="elmnu">
                                  <clickMethod type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.MessageSubscriptionForm/uimthd_addMessageSubscriptionPriceGui</variablePath>
                                    </variablePathList>
                                  </clickMethod>
                                  <icon>/cwt/images/16/notebook_add.png</icon>
                                  <width>40px</width>
                                </element>
                                <element name="deleteButton" type="elmnu">
                                  <clickMethod type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.MessageSubscriptionForm/uimthd_deleteMessageSubscriptionPriceGui</variablePath>
                                    </variablePathList>
                                  </clickMethod>
                                  <icon>/cwt/images/16/delete.png</icon>
                                  <width>40px</width>
                                </element>
                              </elementList>
                            </element>
                            <element name="TransformationRulesHeaders" type="elgrd">
                              <colWidths>50px</colWidths>
                              <fixedColWidths>true</fixedColWidths>
                              <numOfCols>11</numOfCols>
                              <width>1000px</width>
                              <elementList>
                                <element name="Select" type="ellab">
                                  <columnSpan>1</columnSpan>
                                  <width>30px</width>
                                </element>
                                <element name="techcode" type="ellab">
                                  <canSort>false</canSort>
                                  <columnSpan>1</columnSpan>
                                  <label>Technology Code</label>
                                  <width>100px</width>
                                </element>
                                <element name="price" type="ellab">
                                  <columnSpan>1</columnSpan>
                                  <fontProperties type="pfnt"/>
                                  <label>Price *</label>
                                  <width>100px</width>
                                </element>
                              </elementList>
                            </element>
                            <element name="ConditionsVL" type="elvlt">
                              <height>200px</height>
                              <overflow>auto</overflow>
                              <width>1050px</width>
                              <elementList>
                                <element name="ConditionsIterator" type="eliter">
                                  <variable type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.MessageSubscriptionForm/leaf_MessageSubscriptionIterator</variablePath>
                                    </variablePathList>
                                  </variable>
                                  <elementList>
                                    <element name="ConditionsHL" type="elhlt">
                                      <width>1050px</width>
                                      <elementList>
                                        <element name="ConditionsFormFrame" type="elovf">
                                          <formReference type="varPath">
                                            <variablePathList>
                                              <variablePath>ui_cm_spoc.forms.MessageSubscriptionForm/leaf_MessageSubscriptionIterator</variablePath>
                                              <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                              <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                            </variablePathList>
                                          </formReference>
                                          <showResizeBar>true</showResizeBar>
                                          <width>1050px</width>
                                        </element>
                                      </elementList>
                                    </element>
                                  </elementList>
                                </element>
                              </elementList>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.MessageSubscriptionForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initMessageSubscription(this.model);
        cm_spoc.initMessageSubscriptionUi(this);
        return m;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i=0;i<this.MessageSubscriptionIterator.length;i++){
            this.MessageSubscriptionIterator[i].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
    <method name="addMessageSubscriptionPriceGui" type="uimthd">
      <script><![CDATA[
        if(this.model.MSMSPriceGuidingArrDS.ListDS.length==0){
            var MSMSPriceGuidingArr = cm_spoc.initInnerDS("base_MSMSPriceGuidingArr");
            this.model.MSMSPriceGuidingArrDS = MSMSPriceGuidingArr;
        }

        var model = this.model.MSMSPriceGuidingArrDS.ListDS;
        if(model.length < 2)
            cm_spoc.addListElement(this, model,"cm_spoc.forms.MessageSubscriptionUI","base_MessageSubscriptionPriceGui","MessageSubscriptionIterator");
        else
            Global.showUserMessage("Maximum size is 2");
      ]]></script>
    </method>
    <method name="deleteMessageSubscriptionPriceGui" type="uimthd">
      <script><![CDATA[cm_spoc.removeListElements(this ,this.model.MSMSPriceGuidingArrDS,"List", "MessageSubscriptionIterator");]]></script>
    </method>
    <method name="FinderMessageReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_MessageSubscriptionSpecific");
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="referenceCallback" type="uimthd">
      <parameterList>
        <parameter name="itemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
        <parameter name="baseItemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        if(this.model)
        {
            this.model.MessageSubscriptionSpecific=itemCode;
        }
      ]]></script>
    </method>
    <method name="checkSendAdditional" type="usrActionMethod">
      <script><![CDATA[
        if(this.model.SendAdditional < 0 || this.model.SendAdditional == null)
            this.model.SendAdditional = 0;
      ]]></script>
    </method>
  </methodList>
</userInterface>