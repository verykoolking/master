<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.CMTariffPlanGroupRuleUI">
  <group>Common.ChargingMode</group>
  <label>CMTariffPlanGroupRuleUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.ChargingModeByPlanGroup</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="HorizontalLayout" type="elhlt">
              <alignment>center,center</alignment>
              <width>120px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>0</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.CMTariffPlanGroupRuleUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <labelOrientation>left</labelOrientation>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.CMTariffPlanGroupRuleUI/uimthd_selectTariffPlanGroup</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.CMTariffPlanGroupRuleUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>20px</width>
                </element>
                <element name="TariffPlanGroup" type="ellab">
                  <columnSpan>0</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                    </variablePathList>
                  </editable>
                  <labelOrientation>left</labelOrientation>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.CMTariffPlanGroupRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.ChargingModeByPlanGroup/dstruct_TariffPlanGroup</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="operationSelectChange" type="uimthd">
      <script>this.model.Operation = this.model.OperationEnum;</script>
    </method>
    <method name="chargingModeSelectChange" type="uimthd">
      <script>this.model.ChargingMode = this.model.ChargingModeEnum;</script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="selectTariffPlanGroup" type="uimthd">
      <script>this.parent.refreshSelectedTariffPlanGroup(this);</script>
    </method>
  </methodList>
</userInterface>