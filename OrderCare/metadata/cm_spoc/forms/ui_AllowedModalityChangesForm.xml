<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.AllowedModalityChangesForm">
  <group>TariffPlan</group>
  <label>AllowedModalityChangesForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.AllowedModalityChanges</valueType>
    </variable>
    <variable name="ModalityDestinationListFormArr" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>ui_cm_spoc.forms.ModalityDestinationListForm</valueType>
    </variable>
    <variable name="SelectAll" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="optionBetweenSameTechnology" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="VerticalLayout5" type="elvlt">
              <fillSpace>false</fillSpace>
              <height>100%</height>
              <width>100%</width>
              <elementList>
                <element name="BetweenSameTechnology" type="elrgrp">
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_com.conceptwave.system.UserInterface/action_onClick</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedModalityChangesForm/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.AllowedModalityChanges/dstruct_BetweenSameTechnology</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="LayoutSpacer2" type="ellsp">
                  <height>10px</height>
                </element>
                <element name="HorizontalLayout1" type="elhlt">
                  <elementList>
                    <element name="SectionStack" type="elsectnstck">
                      <width>750px</width>
                      <elementList>
                        <element name="Section" type="elsect">
                          <fillSpace>false</fillSpace>
                          <elementList>
                            <element name="SectionHeader" type="elsecth">
                              <label>Modality Destinations</label>
                              <elementList>
                                <element name="ToolBarHorizontalLayout" type="elhlt">
                                  <edgeSize>1</edgeSize>
                                  <width>100%</width>
                                  <elementList>
                                    <element name="AddImg" type="elimg">
                                      <clickMethod type="varPath">
                                        <variablePathList>
                                          <variablePath>ui_cm_spoc.forms.AllowedModalityChangesForm/uimthd_addModalityDestination</variablePath>
                                        </variablePathList>
                                      </clickMethod>
                                      <imageType>normal</imageType>
                                      <imageUrl>/cwt/images/16/table_row_add.png</imageUrl>
                                      <labelOrientation>right</labelOrientation>
                                      <padding>5</padding>
                                      <showLabel>false</showLabel>
                                    </element>
                                    <element name="LayoutSpacer3" type="ellsp">
                                      <width>5px</width>
                                    </element>
                                    <element name="AddLabel" type="ellab">
                                      <label>Add</label>
                                      <width>60px</width>
                                    </element>
                                    <element name="RemoveImg" type="elimg">
                                      <cellAlignment>right,center</cellAlignment>
                                      <clickMethod type="varPath">
                                        <variablePathList>
                                          <variablePath>ui_cm_spoc.forms.AllowedModalityChangesForm/uimthd_removeModalityDestination</variablePath>
                                        </variablePathList>
                                      </clickMethod>
                                      <imageType>normal</imageType>
                                      <imageUrl>/cwt/images/16/table_row_delete.png</imageUrl>
                                      <labelOrientation>right</labelOrientation>
                                      <padding>5</padding>
                                      <showLabel>false</showLabel>
                                    </element>
                                    <element name="LayoutSpacer4" type="ellsp">
                                      <width>5px</width>
                                    </element>
                                    <element name="RemoveLabel" type="ellab">
                                      <label>Remove</label>
                                      <width>60px</width>
                                    </element>
                                  </elementList>
                                </element>
                              </elementList>
                            </element>
                            <element name="IteratorHeader" type="elhlt">
                              <width>100%</width>
                              <elementList>
                                <element name="LayoutSpacer11" type="ellsp">
                                  <width>0px</width>
                                </element>
                                <element name="GridLayout" type="elgrd">
                                  <colWidths>100px</colWidths>
                                  <numOfCols>8</numOfCols>
                                  <width>450px</width>
                                  <elementList>
                                    <element name="CellSpacer" type="elcsp">
                                      <width>2px</width>
                                    </element>
                                    <element name="SelectAll" type="elchk">
                                      <onChangeMethod type="varPath">
                                        <variablePathList>
                                          <variablePath>ui_cm_spoc.forms.AllowedModalityChangesForm/uimthd_selectAllMDRows</variablePath>
                                        </variablePathList>
                                      </onChangeMethod>
                                      <removeLabelLine>true</removeLabelLine>
                                      <showLabel>false</showLabel>
                                      <variable type="varPath">
                                        <variablePathList>
                                          <variablePath>ui_cm_spoc.forms.AllowedModalityChangesForm/leaf_SelectAll</variablePath>
                                        </variablePathList>
                                      </variable>
                                      <width>32px</width>
                                    </element>
                                    <element name="DesModalityIDEnum" type="ellab">
                                      <columnSpan>2</columnSpan>
                                      <label>Destination Modality</label>
                                    </element>
                                    <element name="CustomerProfileEnum" type="ellab">
                                      <columnSpan>2</columnSpan>
                                      <label>Customer Profile</label>
                                    </element>
                                    <element name="CauseEnum" type="ellab">
                                      <columnSpan>2</columnSpan>
                                      <label>Cause</label>
                                      <visible type="varPath">
                                        <variablePathList>
                                          <variablePath>ui_cm_spoc.forms.AllowedModalityChangesForm/permMethod_Visibility_CauseLabel</variablePath>
                                        </variablePathList>
                                      </visible>
                                    </element>
                                  </elementList>
                                </element>
                              </elementList>
                            </element>
                            <element name="VerticalLayout" type="elvlt">
                              <elementList>
                                <element name="Iterator" type="eliter">
                                  <variable type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.AllowedModalityChangesForm/leaf_ModalityDestinationListFormArr</variablePath>
                                    </variablePathList>
                                  </variable>
                                  <elementList>
                                    <element name="ModalityDestinationListFormFrame" type="elovf">
                                      <formReference type="varPath">
                                        <variablePathList>
                                          <variablePath>ui_cm_spoc.forms.AllowedModalityChangesForm/leaf_ModalityDestinationListFormArr</variablePath>
                                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                        </variablePathList>
                                      </formReference>
                                      <height>32px</height>
                                      <width>800%</width>
                                    </element>
                                  </elementList>
                                </element>
                              </elementList>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.AllowedModalityChangesForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        //
        this.optionBetweenSameTechnology=this.model.BetweenSameTechnology;

        return cm_spoc.initAllowedModalityChangesForm(this);
      ]]></script>
    </method>
    <method name="ReadOnly" type="permMethod">
      <script>return this.model.readonly;</script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="addModalityDestination" type="uimthd">
      <script><![CDATA[
        var IteratorVariableName="ModalityDestinationListFormArr";
        var ArrDS=this.model.AllowedModalityChangesArrDS.ListDS;
        var uiArrForm="cm_spoc.forms.ModalityDestinationListForm";
        var itemCode = "base_ModalityDestinationList";

        cm_spoc.addListElement(this, ArrDS,uiArrForm, itemCode, IteratorVariableName);
        this.SelectAll = false;
      ]]></script>
    </method>
    <method name="removeModalityDestination" type="uimthd">
      <script><![CDATA[
        var IteratorVariableName="ModalityDestinationListFormArr";
        var ArrDS=this.model.AllowedModalityChangesArrDS;
        cm_spoc.removeListElements(this, ArrDS,"List", IteratorVariableName);
      ]]></script>
    </method>
    <method name="selectAllMDRows" type="uimthd">
      <script><![CDATA[
        //
        var UIList= this.ModalityDestinationListFormArr;
        /*
        if (!this.SelectAll) return;
        var l = UIList.length;
        for (var i=0;i<l; i++){
            UIList[i].Selected = true;
        }*/
        var l = UIList.length;
        for (var i=0;i<l; i++){
            UIList[i].Selected = this.SelectAll;
        }
      ]]></script>
    </method>
    <method name="Visibility_CauseLabel" type="permMethod">
      <script><![CDATA[
        //
        this.optionBetweenSameTechnology=this.model.BetweenSameTechnology;
        return this.optionBetweenSameTechnology;
      ]]></script>
    </method>
  </methodList>
</userInterface>