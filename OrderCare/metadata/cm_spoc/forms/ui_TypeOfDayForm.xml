              <alignment>right,center</alignment>
              <elementList>
                <element name="HorizontalLayout_1" type="elhlt">
                  <alignment>right,center</alignment>
                  <elementList>
                    <element name="LayoutSpacer21111121" type="ellsp">
                      <width>10px</width>
