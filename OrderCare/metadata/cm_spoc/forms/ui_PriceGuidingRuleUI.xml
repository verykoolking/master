<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.PriceGuidingRuleUI">
  <group>Common.Price</group>
  <label>PriceGuidingRuleUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.PriceGuiding</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="RateHeaders" type="elgrd">
              <alignment>center,center</alignment>
              <colWidths>120px</colWidths>
              <fixedColWidths>true</fixedColWidths>
              <numOfCols>9</numOfCols>
              <width>1260px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <columnSpan>1</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="TaxApplies" type="elchk">
                  <columnSpan>1</columnSpan>
                  <label>Tax Applies</label>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PriceGuiding/dstruct_TaxApplies</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="PromotionsAppliedOnThePrice" type="elchk">
                  <columnSpan>2</columnSpan>
                  <label>Promotions Applied On The Price </label>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PriceGuiding/dstruct_PromotionsAppliedOnThePrice</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="CustomerProfile" type="elslct">
                  <columnSpan>1</columnSpan>
                  <label>User Profile</label>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PriceGuiding/dstruct_CustomerProfile</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="EventPriceEnum" type="elslct">
                  <columnSpan>1</columnSpan>
                  <label>Price</label>
                  <multiselect>false</multiselect>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/uimthd_onSelectPriceTrigger</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PriceGuiding/dstruct_EventPriceEnum</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="TariffPlanEnum" type="elslct">
                  <columnSpan>1</columnSpan>
                  <multiselect>false</multiselect>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/uimthd_onSelectTariffPlanTrigger</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PriceGuiding/dstruct_TariffPlanEnum</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="ProvinceEnum" type="elslct">
                  <columnSpan>1</columnSpan>
                  <multiselect>false</multiselect>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/uimthd_onSelectProvinceTrigger</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PriceGuiding/dstruct_ProvinceEnum</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="ServiceProviderEnum" type="elslct">
                  <columnSpan>1</columnSpan>
                  <multiselect>false</multiselect>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/uimthd_onSelectServiceProviderTrigger</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PriceGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.PriceGuiding/dstruct_ServiceProviderEnum</variablePath>
                    </variablePathList>
                  </variable>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="GPRSRateSpecificationVisRule" type="permMethod">
      <script><![CDATA[
        var value=this.BoltOnType.BoltOnTypeModality;
        if (value=="ME" || value=="MI") return true;
        else return false;
      ]]></script>
    </method>
    <method name="onSelectPriceTrigger" type="uimthd">
      <script>cm_spoc.onSelectPrice(this);</script>
    </method>
    <method name="onSelectProvinceTrigger" type="uimthd">
      <script>cm_spoc.onSelectProvince(this);</script>
    </method>
    <method name="onSelectTariffPlanTrigger" type="uimthd">
      <script>cm_spoc.onSelectTariffPlan(this);</script>
    </method>
    <method name="onSelectOperationTrigger" type="uimthd">
      <script>cm_spoc.onSelectOperation(this);</script>
    </method>
    <method name="onSelectServiceProviderTrigger" type="uimthd">
      <script>cm_spoc.onSelectServiceProvider(this);</script>
    </method>
  </methodList>
</userInterface>