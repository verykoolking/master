<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.AutomaticTopUpModeForm">
  <group>TopUp</group>
  <label>AutomaticTopUpModeForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.AutomaticTopUpMode</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>750px</width>
              <elementList>
                <element name="generalParameters" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/frmui_Forms/vform_generalParameters</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="generalParameters" type="vform">
          <label>generalParameters</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="VerticalLayout2" type="elgrd">
                <numOfCols>2</numOfCols>
                <elementList>
                  <element name="AutomaticTopUpModalityCode" type="elfld">
                    <cellAlignment>left,center</cellAlignment>
                    <editable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/permMethod_setIsPrimaryKey</variablePath>
                      </variablePathList>
                    </editable>
                    <labeAlign>left</labeAlign>
                    <label>Automatic Top Up Modality Code</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.AutomaticTopUpMode/dstruct_AutomaticTopUpModalityCode</variablePath>
                      </variablePathList>
                    </variable>
                    <width>180px</width>
                  </element>
                  <element name="AutomaticTopUpModalityDescriptio" type="elfld">
                    <canSort>false</canSort>
                    <cellAlignment>left,center</cellAlignment>
                    <groupBy>true</groupBy>
                    <labeAlign>left</labeAlign>
                    <label>Automatic Top Up Modality Description</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.AutomaticTopUpMode/dstruct_AutomaticTopUpModalityDescriptio</variablePath>
                      </variablePathList>
                    </variable>
                    <width>180px</width>
                  </element>
                  <element name="PromotionalProfileEnum" type="elslct">
                    <canSort>false</canSort>
                    <cellAlignment>left,center</cellAlignment>
                    <groupBy>true</groupBy>
                    <labeAlign>left</labeAlign>
                    <label>Promotional Profile assigned to the subscriber in subscription time	</label>
                    <labelOrientation>left</labelOrientation>
                    <multiselect>false</multiselect>
                    <onChangeMethod type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/uimthd_PromotionalProfileTrigger</variablePath>
                      </variablePathList>
                    </onChangeMethod>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.AutomaticTopUpMode/dstruct_PromotionalProfileEnum</variablePath>
                      </variablePathList>
                    </variable>
                    <width>180px</width>
                  </element>
                  <element name="BalanceThresThrowingAutoTU" type="elslct">
                    <canSort>false</canSort>
                    <cellAlignment>left,center</cellAlignment>
                    <groupBy>true</groupBy>
                    <labeAlign>left</labeAlign>
                    <label>Balance threshold which throw the automatic top up	</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.AutomaticTopUpMode/dstruct_BalanceThresThrowingAutoTU</variablePath>
                      </variablePathList>
                    </variable>
                    <width>180px</width>
                  </element>
                  <element name="NumberTimesThresholdEventSent" type="elfld">
                    <canSort>false</canSort>
                    <cellAlignment>left,center</cellAlignment>
                    <groupBy>true</groupBy>
                    <labeAlign>left</labeAlign>
                    <label>Number of times it is sent the threshold event</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.AutomaticTopUpMode/dstruct_NumberTimesThresholdEventSent</variablePath>
                      </variablePathList>
                    </variable>
                    <width>180px</width>
                  </element>
                </elementList>
              </element>
              <element name="VerticalLayout1" type="elvlt">
                <elementList>
                  <element name="SectionStack" type="elsectnstck">
                    <visibilityMode>multiple</visibilityMode>
                    <width>750px</width>
                    <elementList>
                      <element name="SectionProperties" type="elsect">
                        <removeLabelLine>true</removeLabelLine>
                        <elementList>
                          <element name="AutomaticTopUpproperties" type="elsecth">
                            <label>Automatic Top Up properties</label>
                          </element>
                          <element name="layoutCharacteristics" type="elgrd">
                            <alignment>left,center</alignment>
                            <colWidths>100px</colWidths>
                            <numOfCols>2</numOfCols>
                            <width>200px</width>
                            <elementList>
                              <element name="TopUpOriginEnum" type="elslct">
                                <cellAlignment>left,center</cellAlignment>
                                <labeAlign>left</labeAlign>
                                <label>Top Up Origin where the top up will be automatically provisioned</label>
                                <labelOrientation>left</labelOrientation>
                                <multiselect>false</multiselect>
                                <onChangeMethod type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/uimthd_TopUpOriginTrigger</variablePath>
                                  </variablePathList>
                                </onChangeMethod>
                                <optional type="varPath">
                                  <variablePathList>
                                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                  </variablePathList>
                                </optional>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.AutomaticTopUpMode/dstruct_TopUpOriginEnum</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>180px</width>
                              </element>
                              <element name="MinAmountAutomaticTUModality" type="elfld">
                                <cellAlignment>left,center</cellAlignment>
                                <labeAlign>left</labeAlign>
                                <label>Minimum amount of the automatic top up modality</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.AutomaticTopUpMode/dstruct_MinAmountAutomaticTUModality</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>180px</width>
                              </element>
                              <element name="MaxAmountAutomaticTUModality" type="elfld">
                                <cellAlignment>left,center</cellAlignment>
                                <labeAlign>left</labeAlign>
                                <label>Maximum amount of the automatic top up modality</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.AutomaticTopUpModeForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.AutomaticTopUpMode/dstruct_MaxAmountAutomaticTUModality</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>180px</width>
                              </element>
                            </elementList>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.AutomaticTopUpModeForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initAutomaticTopUp(this.model);
        return m;
      ]]></script>
    </method>
    <method name="TopUpOriginTrigger" type="uimthd">
      <script><![CDATA[
        var btCode = this.model.TopUpOriginEnum;
        this.model.TopUpOrigin = btCode;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="PromotionalProfileTrigger" type="uimthd">
      <script><![CDATA[
        var btCode = this.model.PromotionalProfileEnum;
        this.model.PromotionalProfile = btCode;
      ]]></script>
    </method>
    <method name="setIsPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
  </methodList>
</userInterface>