  <methodList>
    <method name="cwLeafTriggerAction$SelectAll" type="laction">
      <path>ui_cm_spoc.forms.TopUpOriginDestinationPairUI/leaf_SelectAll</path>
      <type>trigger</type>
      <script><![CDATA[
        for (var i = 0;i<this.TopUpOriginDestinationPairIterator.length; i++){
                this.TopUpOriginDestinationPairIterator[i].Selected = this.SelectAll;
            }
      ]]></script>
    </method>
    <method name="addTopUpDesitonationPair" type="uimthd">
      <script><![CDATA[
        var model=this.model.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.TopUpOriginDestinationPairIteratorUI","base_TopUpOriginDestinationPair","TopUpOriginDestinationPairIterator");
      ]]></script>
    </method>
    <method name="deleteTopUpDestinationPair" type="uimthd">
      <script><![CDATA[
        var model = this.model;
        cm_spoc.removeListElements(this , model ,"List", "TopUpOriginDestinationPairIterator");
        this.SelectAll = false;
      ]]></script>
    </method>
    <method name="onOkClick" type="uimthd">
      <script>return this.parent.cwDialogClose();</script>
    </method>
  </methodList>
