<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.RateUI">
  <group>BoltOnType</group>
  <label>RateUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="TemporalAppConditionEnum" type="leaf">
      <valueType>dtype_cm_spoc.data.TemporalAppConditionEnum</valueType>
    </variable>
    <variable name="parentUI" type="leaf">
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.RateSpecification</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="RateHeaders" type="elhlt">
              <width>550px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>1</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RateUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RateUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>10%</width>
                </element>
                <element name="TmpAppCondition" type="elhlt">
                  <alignment>center,center</alignment>
                  <width>45%</width>
                  <elementList>
                    <element name="TemporalAppCondition" type="elfld">
                      <autoFocusNextField>false</autoFocusNextField>
                      <cellAlignment>right,center</cellAlignment>
                      <columnSpan>3</columnSpan>
                      <disablePaste>true</disablePaste>
                      <editable type="varPath">
                        <variablePathList>
                          <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                        </variablePathList>
                      </editable>
                      <labelOrientation>left</labelOrientation>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.RateUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.RateSpecification/dstruct_TemporalAppCondition</variablePath>
                        </variablePathList>
                      </variable>
                      <width>200px</width>
                    </element>
                    <element name="Button1" type="elbtn">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.RateUI/usrActionMethod_RefTmpAppConditionDialog</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <disabled type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.RateUI/permMethod_buttonsDisable</variablePath>
                        </variablePathList>
                      </disabled>
                      <icon>/cwf/finder.gif</icon>
                      <width>22px</width>
                    </element>
                  </elementList>
                </element>
                <element name="HorizontalLayout" type="elhlt">
                  <width>45%</width>
                  <elementList>
                    <element name="Rate" type="elfld">
                      <autoFocusNextField>false</autoFocusNextField>
                      <cellAlignment>right,center</cellAlignment>
                      <columnSpan>3</columnSpan>
                      <disablePaste>true</disablePaste>
                      <editable type="varPath">
                        <variablePathList>
                          <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                        </variablePathList>
                      </editable>
                      <labelOrientation>left</labelOrientation>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.RateUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.RateSpecification/dstruct_Rate</variablePath>
                        </variablePathList>
                      </variable>
                      <width>200px</width>
                    </element>
                    <element name="Button" type="elbtn">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.RateUI/usrActionMethod_FinderSelectReferenceDialog</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <disabled type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.RateUI/permMethod_buttonsDisable</variablePath>
                        </variablePathList>
                      </disabled>
                      <icon>/cwf/finder.gif</icon>
                      <width>22px</width>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="readOnly" type="permMethod">
      <script>return !this.parentUI.model.readonly;</script>
    </method>
    <method name="setParentUI" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <mandatory>true</mandatory>
          <type>ui_com.conceptwave.system.UserInterface</type>
        </parameter>
      </parameterList>
      <script>this.parentUI=ui;</script>
    </method>
    <method name="getParentUI" type="uimthd">
      <script>return this.parentUI;</script>
    </method>
    <method name="temporalAppConditionTrigger" type="uimthd">
      <script>this.model.TemporalAppCondition = this.model.TemporalAppConditionEnum;</script>
    </method>
    <method name="FinderSelectReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_Rate");
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="referenceCallback" type="uimthd">
      <parameterList>
        <parameter name="itemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
        <parameter name="baseItemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        switch(baseItemCode){
            case "base_TemporalAppCondition":
                this.model.TemporalAppCondition=itemCode;
                break;
            case "base_Rate":
                this.model.Rate = itemCode;
                break;
        }
      ]]></script>
    </method>
    <method name="RefTmpAppConditionDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_TemporalAppCondition");
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="buttonsDisable" type="permMethod">
      <script>return this.parentUI.model.readonly;</script>
    </method>
  </methodList>
</userInterface>