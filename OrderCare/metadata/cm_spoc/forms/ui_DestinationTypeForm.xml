<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.DestinationTypeForm">
  <group>Numbering</group>
  <label>DestinationTypeForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.DestinationType</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>400px</width>
              <elementList>
                <element name="TabFrame" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.DestinationTypeForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.DestinationTypeForm/frmui_Forms/vform_general</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                  <width>100px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="general" type="vform">
          <label>general</label>
          <root name="GridLayout" type="elvlt">
            <elementList>
              <element name="VerticalLayout" type="elvlt">
                <alignment>right,center</alignment>
                <elementList>
                  <element name="DestTypeId" type="elfld">
                    <editable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/permMethod_setIsPrimaryKey</variablePath>
                      </variablePathList>
                    </editable>
                    <labeAlign>left</labeAlign>
                    <label>Destination Type ID</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.DestinationType/dstruct_DestTypeId</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="DestTypeDesc" type="elfld">
                    <labeAlign>left</labeAlign>
                    <label>Description</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.DestinationType/dstruct_DestTypeDesc</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                </elementList>
              </element>
              <element name="VerticalLayout1" type="elvlt">
                <alignment>left,center</alignment>
                <elementList>
                  <element name="DestTypeAvPartPay" type="elchk">
                    <label>Avoid Partial Payment</label>
                    <labelOrientation>right</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.DestinationType/dstruct_DestTypeAvPartPay</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="DestTypeMinBalance" type="elchk">
                    <label>Minimum Balance</label>
                    <labelOrientation>right</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.DestinationType/dstruct_DestTypeMinBalance</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                </elementList>
              </element>
              <element name="VerticalLayout2" type="elvlt">
                <alignment>right,center</alignment>
                <elementList>
                  <element name="DestTypeTypeZoneDest" type="elfld">
                    <labeAlign>left</labeAlign>
                    <label>Type Zone Destination</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.DestinationType/dstruct_DestTypeTypeZoneDest</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                </elementList>
              </element>
              <element name="VerticalLayout3" type="elvlt">
                <alignment>left,center</alignment>
                <elementList>
                  <element name="DestTypeDestLiPort" type="elchk">
                    <label>Destination List Port</label>
                    <labelOrientation>right</labelOrientation>
                    <onChangeMethod type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/uimthd_DesLiPortRuntrigger</variablePath>
                      </variablePathList>
                    </onChangeMethod>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.DestinationType/dstruct_DestTypeDestLiPort</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                </elementList>
              </element>
              <element name="GridLayout2" type="elhlt">
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.DestinationTypeForm/permMethod_DTGDestTypeGroupIdVisRule</variablePath>
                  </variablePathList>
                </visible>
                <elementList>
                  <element name="DestinationTypeGroup" type="elfld">
                    <editable type="varPath">
                      <variablePathList>
                        <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                      </variablePathList>
                    </editable>
                    <label>Destination Type Group</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.DestinationType/dstruct_DestinationTypeGroup</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="Button" type="elbtn">
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.DestinationTypeForm/usrActionMethod_FinderSelectReferenceDialog</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <icon>/cwf/finder.gif</icon>
                    <width>22px</width>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="referenceCallback" type="uimthd">
      <parameterList>
        <parameter name="itemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
        <parameter name="baseItemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        switch(baseItemCode){
            case "base_DestinationTypeGroup":
                this.model.DestinationTypeGroup = itemCode;
                break;
        }
      ]]></script>
    </method>
    <method name="FinderSelectReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_DestinationTypeGroup");
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="DTGDestTypeGroupIdVisRule" type="permMethod">
      <script><![CDATA[
        var value=this.model.DestTypeDestLiPort;
        if (value == true) return true;
        else
        return false;
      ]]></script>
    </method>
    <method name="DesLiPortRuntrigger" type="uimthd">
      <script><![CDATA[
        if (this.model.DestTypeDestLiPort==false){
            this.model.DestinationTypeGroup=null;
        }
        return;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script>return true;</script>
    </method>
    <method name="setIsPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
  </methodList>
</userInterface>