<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.TemporalAppConditionForm">
  <group>Common.ApplicabilityCondition</group>
  <label>TemporalAppConditionForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="TemporalApplicabilityIterator" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.TemporalAppCondition</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="SectionStack" type="elsectnstck">
              <visibilityMode>multiple</visibilityMode>
              <width>750px</width>
              <elementList>
                <element name="Section" type="elsect">
                  <removeLabelLine>true</removeLabelLine>
                  <elementList>
                    <element name="TempApplicabilityCondHeader" type="elsecth">
                      <label>Temporal Applicability Conditions</label>
                    </element>
                    <element name="Description1" type="elfld">
                      <labeAlign>left</labeAlign>
                      <labelOrientation>left</labelOrientation>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.TemporalAppConditionForm/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.TemporalAppCondition/dstruct_Description</variablePath>
                        </variablePathList>
                      </variable>
                    </element>
                    <element name="LayoutSpacer2" type="ellsp">
                      <height>50px</height>
                    </element>
                    <element name="ButtonHorizontalLayout" type="elhlt">
                      <elementList>
                        <element name="AddButton" type="elmnu">
                          <clickMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.TemporalAppConditionForm/uimthd_addSimpleTemporalAppCondition</variablePath>
                            </variablePathList>
                          </clickMethod>
                          <icon>/cwt/images/16/notebook_add.png</icon>
                          <width>40px</width>
                        </element>
                        <element name="DeleteButton" type="elmnu">
                          <clickMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.TemporalAppConditionForm/uimthd_deleteSimpleTemporalAppCondition</variablePath>
                            </variablePathList>
                          </clickMethod>
                          <icon>/cwt/images/16/delete.png</icon>
                          <width>40px</width>
                        </element>
                      </elementList>
                    </element>
                    <element name="GridLayout" type="elgrd">
                      <colWidths>50px</colWidths>
                      <numOfCols>10</numOfCols>
                      <width>750px</width>
                      <elementList>
                        <element name="CellSpacer" type="elcsp">
                          <width>50px</width>
                        </element>
                        <element name="TypeOfDayLabel" type="ellab">
                          <columnSpan>3</columnSpan>
                          <label>Type of Day</label>
                          <rowSpan>0</rowSpan>
                        </element>
                        <element name="StartHourLabel" type="ellab">
                          <columnSpan>3</columnSpan>
                          <label>Start Hour</label>
                          <rowSpan>0</rowSpan>
                        </element>
                        <element name="EndHourLabel" type="ellab">
                          <columnSpan>3</columnSpan>
                          <label>End Hour</label>
                          <rowSpan>0</rowSpan>
                        </element>
                      </elementList>
                    </element>
                    <element name="TemporalApplicabilityListVL" type="elvlt">
                      <elementList>
                        <element name="TemporalApplicabilityIterator" type="eliter">
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.TemporalAppConditionForm/leaf_TemporalApplicabilityIterator</variablePath>
                            </variablePathList>
                          </variable>
                          <elementList>
                            <element name="FormFrame" type="elovf">
                              <formReference type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.TemporalAppConditionForm/leaf_TemporalApplicabilityIterator</variablePath>
                                  <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                  <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                </variablePathList>
                              </formReference>
                              <width>750px</width>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                    <element name="LayoutSpacer1" type="ellsp">
                      <height>20px</height>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.TemporalAppConditionForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initTemporalAppCondition(this.model);
        cm_spoc.initTemporalAppConditionUI(this);
        return m;
      ]]></script>
    </method>
    <method name="addSimpleTemporalAppCondition" type="uimthd">
      <script><![CDATA[
        //var model=this.model.SubscriptionOfferingDS.SubscriptionLimitArrayDS.ListDS;
        var model=this.model.SimpleTemporalAppCondArrDS.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.TemporalApplicabilityIteratorUI","base_SimpleTemporalAppCondition","TemporalApplicabilityIterator");
        model[model.length-1].EndHourEnum="00:59";
        model[model.length-1].StartHourEnum="00:00";
        model[model.length-1].Hours = "100000000000000000000000";
      ]]></script>
    </method>
    <method name="deleteSimpleTemporalAppCondition" type="uimthd">
      <script><![CDATA[
        cm_spoc.removeListElements(this ,this.model.SimpleTemporalAppCondArrDS,"List", "TemporalApplicabilityIterator");
      ]]></script>
    </method>
    <method name="ReadOnly" type="permMethod"/>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i=0;i<this.TemporalApplicabilityIterator.length;i++){
            this.TemporalApplicabilityIterator[i].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
  </methodList>
</userInterface>