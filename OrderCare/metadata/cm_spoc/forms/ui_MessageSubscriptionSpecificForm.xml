<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.MessageSubscriptionSpecificForm">
  <group>Messages</group>
  <label>MessageSubscriptionSpecificForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.MessageSubscriptionSpecific</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="VerticalLayout2" type="elvlt">
              <alignment>right,center</alignment>
              <elementList>
                <element name="HorizontalLayout" type="elhlt">
                  <elementList>
                    <element name="MessageTechnicalSpec" type="elfld">
                      <editable type="varPath">
                        <variablePathList>
                          <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                        </variablePathList>
                      </editable>
                      <labeAlign>left</labeAlign>
                      <label>Message Technical Specification</label>
                      <labelOrientation>left</labelOrientation>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.MessageSubscriptionSpecificForm/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.MessageSubscriptionSpecific/dstruct_MessageTechnicalSpec</variablePath>
                        </variablePathList>
                      </variable>
                    </element>
                    <element name="MessageButton" type="elbtn">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.MessageSubscriptionSpecificForm/usrActionMethod_FinderMessageReferenceDialog</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <icon>/cwf/finder.gif</icon>
                      <width>22px</width>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="FinderMessageReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_MessageTechnicalSpec");
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="referenceCallback" type="uimthd">
      <parameterList>
        <parameter name="itemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
        <parameter name="baseItemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        if(this.model)
        {
            this.model.MessageTechnicalSpec=itemCode;
        }
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script>return true;</script>
    </method>
  </methodList>
</userInterface>