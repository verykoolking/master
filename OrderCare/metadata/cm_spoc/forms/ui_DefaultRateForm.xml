<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.DefaultRateForm">
  <group>Rate</group>
  <label>DefaultRateForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.DefaultRate</valueType>
    </variable>
    <variable name="isReadOnly" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="TonsPickList" type="leaf">
      <valueType>ui_cm_spoc.PickList</valueType>
    </variable>
    <variable name="RatesPickList" type="leaf">
      <valueType>ui_cm_spoc.PickList</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>750px</width>
              <elementList>
                <element name="GeneralParameters" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.DefaultRateForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.DefaultRateForm/frmui_Forms/vform_generalParameters</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                </element>
                <element name="Tons" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.DefaultRateForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.DefaultRateForm/frmui_Forms/vform_TonsForm</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Tons</label>
                </element>
                <element name="Rates" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.DefaultRateForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.DefaultRateForm/frmui_Forms/vform_RatesForm</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Rates</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="RatesForm" type="vform">
          <label>RatesForm</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="SectionStack" type="elsectnstck">
                <visibilityMode>multiple</visibilityMode>
                <width>750px</width>
                <elementList>
                  <element name="LCParameters" type="elsect">
                    <removeLabelLine>true</removeLabelLine>
                    <elementList>
                      <element name="LCParametersHeader" type="elsecth">
                        <label>Rate Specifications</label>
                      </element>
                      <element name="layoutCharacteristics" type="elvlt">
                        <alignment>center,center</alignment>
                        <width>100%</width>
                        <elementList>
                          <element name="FormFrame" type="elovf">
                            <formReference type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.DefaultRateForm/leaf_RatesPickList</variablePath>
                                <variablePath>ui_cm_spoc.PickList/frmui_Forms</variablePath>
                                <variablePath>ui_cm_spoc.PickList/frmui_Forms/exov_Default</variablePath>
                              </variablePathList>
                            </formReference>
                            <height>350px</height>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.DefaultRateForm/leaf_RatesPickList</variablePath>
                              </variablePathList>
                            </variable>
                            <width>550px</width>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="TonsForm" type="vform">
          <label>TonsForm</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="SectionStack" type="elsectnstck">
                <visibilityMode>multiple</visibilityMode>
                <width>750px</width>
                <elementList>
                  <element name="LCParameters" type="elsect">
                    <removeLabelLine>true</removeLabelLine>
                    <elementList>
                      <element name="TPParametersHeader" type="elsecth">
                        <label>Tons</label>
                      </element>
                      <element name="layoutCharacteristics" type="elvlt">
                        <alignment>center,center</alignment>
                        <width>100%</width>
                        <elementList>
                          <element name="FormFrame" type="elovf">
                            <formReference type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.DefaultRateForm/leaf_TonsPickList</variablePath>
                                <variablePath>ui_cm_spoc.PickList/frmui_Forms</variablePath>
                                <variablePath>ui_cm_spoc.PickList/frmui_Forms/exov_Default</variablePath>
                              </variablePathList>
                            </formReference>
                            <height>350px</height>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.DefaultRateForm/leaf_TonsPickList</variablePath>
                              </variablePathList>
                            </variable>
                            <width>550px</width>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="generalParameters" type="vform">
          <label>generalParameters</label>
          <root name="VerticalLayout" type="elvlt">
            <height>400px</height>
            <width>750px</width>
            <elementList>
              <element name="SectionStack" type="elsectnstck">
                <height>400px</height>
                <width>750px</width>
                <elementList>
                  <element name="Section" type="elsect">
                    <elementList>
                      <element name="SectionHeader" type="elsecth">
                        <label>General Parameters</label>
                      </element>
                      <element name="VerticalLayout1" type="elvlt">
                        <alignment>right,center</alignment>
                        <elementList>
                          <element name="Description" type="elfld">
                            <autoFocusNextField>false</autoFocusNextField>
                            <label>Description</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.DefaultRateForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.DefaultRate/dstruct_Description</variablePath>
                              </variablePathList>
                            </variable>
                          </element>
                          <element name="NetworkIndicator" type="elfld">
                            <autoFocusNextField>false</autoFocusNextField>
                            <label>Network Indicator</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.DefaultRateForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.DefaultRate/dstruct_NetworkIndicator</variablePath>
                              </variablePathList>
                            </variable>
                          </element>
                          <element name="Service" type="elslct">
                            <label>Service</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.DefaultRateForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.DefaultRate/dstruct_Service</variablePath>
                              </variablePathList>
                            </variable>
                          </element>
                          <element name="ExtendedLogicTechnicalPropeEnum" type="elslct">
                            <cellAlignment></cellAlignment>
                            <labeAlign>left</labeAlign>
                            <label>Extended Logic Technical Properties</label>
                            <labelOrientation>left</labelOrientation>
                            <multiselect>false</multiselect>
                            <onChangeMethod type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.DefaultRateForm/uimthd_setExtendedLTPropertiesTrigger</variablePath>
                              </variablePathList>
                            </onChangeMethod>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.DefaultRateForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.DefaultRate/dstruct_ExtendedLogicTechnicalPropeEnum</variablePath>
                              </variablePathList>
                            </variable>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.DefaultRateForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.DefaultRateForm/leaf_model</valueType>
      <script><![CDATA[
        var m = cm_spoc.initDefaultRate(this.model);
        return m;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$RatesPickList" type="laction">
      <path>ui_cm_spoc.forms.DefaultRateForm/leaf_RatesPickList</path>
      <type>initializer</type>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_RateSpecification";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.DRRateSpecificationArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setRatesPickListModel;
        return pl;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$TonsPickList" type="laction">
      <path>ui_cm_spoc.forms.DefaultRateForm/leaf_TonsPickList</path>
      <type>initializer</type>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_Ton";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.DRTonArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setTonsPickListModel;
        return pl;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        /*if (this.isReadOnly ==null)
            this.isReadOnly = true;
        else

        this.isReadOnly = !(this.isReadOnly);
        */
      ]]></script>
    </method>
    <method name="setRatesPickListModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <array>true</array>
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.DRRateSpecificationArrDS.List = null;
        this.parent.model.DRRateSpecificationArrDS.ListDS = null;

        for(var i=0;i<selectedItems.length;i++)
        {
            this.parent.model.DRRateSpecificationArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="setTonsPickListModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <array>true</array>
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.DRTonArrDS.List = null;
        this.parent.model.DRTonArrDS.ListDS = null;

        for(var i=0; i<selectedItems.length; i++)
        {
            this.parent.model.DRTonArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="setExtendedLTPropertiesTrigger" type="uimthd">
      <script>this.model.ExtendedLogicTechnicalPrope = this.model.ExtendedLogicTechnicalPropeEnum;</script>
    </method>
  </methodList>
</userInterface>