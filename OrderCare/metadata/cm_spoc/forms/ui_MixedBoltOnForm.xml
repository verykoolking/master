                <height>10px</height>
              </element>
              <element name="SectionStack" type="elsectnstck">
                <visibilityMode>multiple</visibilityMode>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/permMethod_IVRBODIVRBoltOnDescriptionsVisRule</variablePath>
                  </variablePathList>
                </visible>
                <width>750px</width>
                <elementList>
                  <element name="languagueSection" type="elsect">
