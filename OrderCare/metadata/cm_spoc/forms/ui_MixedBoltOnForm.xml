  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_com.conceptwave.system.UserInterface/uivar_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var model = cm_spoc.initMixedBolton(this.model);
        cm_spoc.initMixedBoltonUI(this);
        return model;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$pickList" type="laction">
      <path>ui_cm_spoc.forms.MixedBoltOnForm/leaf_pickList</path>
      <type>initializer</type>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_BoltOn";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.MixOrderedBoltOnArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setModel;
        return pl;
      ]]></script>
    </method>
    <method name="IVRBODIVRBoltOnDescriptionsVisRule" type="permMethod">
      <script><![CDATA[
        var value=this.model.IVRBoltOnCode;
        if (value==null || value=="") return false;
        else return true;
      ]]></script>
    </method>
    <method name="selectTrigger" type="uimthd">
      <script><![CDATA[
        /*this.model.IVRBoltOnDescriptionArrDS.ListDS.clear();
        this.languageIterator=new Array();*/

        return;
      ]]></script>
    </method>
    <method name="setModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <array>true</array>
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.MixOrderedBoltOnArrDS.List=null;
        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.MixOrderedBoltOnArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="deleteLanguage" type="uimthd">
      <script><![CDATA[
        cm_spoc.removeListElements(this, this.model.IVRBoltOnDescriptionArrDS, "List", "languageIterator");
        this.showLanguageAddButton=true;
      ]]></script>
    </method>
    <method name="setParentUI" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <mandatory>true</mandatory>
          <type>ui_com.conceptwave.system.UserInterface</type>
        </parameter>
      </parameterList>
      <script>this.parentUI = ui;</script>
    </method>
    <method name="getParentUI" type="uimthd">
      <valueType>ui_cm_spoc.forms.BoltOnForm</valueType>
      <script>return this.parentUI;</script>
    </method>
    <method name="addLenguage" type="uimthd">
      <script><![CDATA[
        var listDS = this.model.IVRBoltOnDescriptionArrDS.ListDS;
        cm_spoc.addListElement(this, listDS,"cm_spoc.forms.LanguageUI","base_IVRBoltOnDescription","languageIterator");
      ]]></script>
    </method>
    <method name="MessageTrigger" type="uimthd">
      <script><![CDATA[
        this.model.Message=this.model.MessageEnum;
        return;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="NotReadOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="setIsPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
    <method name="maxLanguage" type="permMethod">
      <script><![CDATA[
        if(this.model.IVRBoltOnDescriptionArrDS.ListDS.length>2)return false;
        return true;
      ]]></script>
    </method>
    <method name="addNewPrice" type="usrActionMethod">
      <parameterList>
        <parameter name="dialog" type="rifp">
          <type>ui_com.conceptwave.system.Dialog</type>
        </parameter>
      </parameterList>
      <showInPopup>true</showInPopup>
      <script>return cm_spoc.mixedBoltonPriceDialog(this);</script>
    </method>
    <method name="selectExistingPrice" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_MixedBoltOnPriceEvent");
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="referenceCallback" type="uimthd">
      <parameterList>
        <parameter name="itemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
        <parameter name="baseItemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
      </parameterList>
      <script>this.model.Price = itemCode;</script>
    </method>
  </methodList>
</userInterface>