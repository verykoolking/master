                    <elementList>
                      <element name="SectionHeader" type="elsecth"/>
                      <element name="buttonsLayout" type="elhlt">
                        <elementList>
                          <element name="addButton" type="elmnu">
                            <menuAccel type="mAccelP"/>
                            <canFocus>false</canFocus>
                            <clickMethod type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/uimthd_addLenguage</variablePath>
                              </variablePathList>
                            </clickMethod>
                            <icon>/cwt/images/16/notebook_add.png</icon>
                            <visible type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/permMethod_maxLanguage</variablePath>
                              </variablePathList>
                            </visible>
                            <width>40px</width>
                          </element>
                          <element name="deleteButton" type="elmnu">
                            <menuAccel type="mAccelP"/>
                            <clickMethod type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/uimthd_deleteLanguage</variablePath>
                              </variablePathList>
                            </clickMethod>
                            <icon>/cwt/images/16/delete.png</icon>
                            <width>40px</width>
                          </element>
                        </elementList>
                      </element>
                      <element name="HorizontalLayout" type="elhlt">
                        <width>700px</width>
                        <elementList>
                          <element name="Select2" type="ellab">
                            <columnSpan>1</columnSpan>
                            <width>20px</width>
                          </element>
                          <element name="language" type="ellab">
                            <canSort>false</canSort>
                            <columnSpan>4</columnSpan>
                            <label>Language</label>
                            <textAlign>center</textAlign>
                            <width>340px</width>
                          </element>
                          <element name="description" type="ellab">
                            <columnSpan>2</columnSpan>
                            <label>Description</label>
                            <textAlign>center</textAlign>
                            <width>340px</width>
                          </element>
                        </elementList>
                      </element>
                      <element name="LanguageVL" type="elvlt">
                        <height>200px</height>
                        <overflow>auto</overflow>
                        <width>750px</width>
                        <elementList>
                          <element name="LanguageIterator" type="eliter">
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/leaf_languageIterator</variablePath>
                              </variablePathList>
                            </variable>
                            <elementList>
                              <element name="languageHL" type="elhlt">
                                <width>750px</width>
                                <elementList>
                                  <element name="languageFormFrame" type="elovf">
                                    <formReference type="varPath">
                                      <variablePathList>
                                        <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/leaf_languageIterator</variablePath>
