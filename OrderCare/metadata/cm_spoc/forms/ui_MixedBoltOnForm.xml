                      </element>
                      <element name="Header" type="elvlt">
                        <alignment>left,center</alignment>
                        <elementList>
                          <element name="GridLayout" type="elgrd">
                            <labelOrientation>left</labelOrientation>
                            <width>400px</width>
                          </element>
                          <element name="GridLayout3" type="elhlt">
                            <alignment>right,center</alignment>
                            <elementList>
                              <element name="Price" type="elfld">
                                <editable type="varPath">
                                  <variablePathList>
                                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                  </variablePathList>
                                </editable>
                                <label>Price</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.MixedBoltOn/dstruct_Price</variablePath>
                                  </variablePathList>
                                </variable>
                              </element>
                              <element name="Button" type="elbtn">
                                <clickMethod type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/usrActionMethod_selectExistingPrice</variablePath>
                                  </variablePathList>
                                </clickMethod>
                                <icon>/cwf/finder.gif</icon>
                                <width>22px</width>
                              </element>
                            </elementList>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="IVR" type="vform">
          <label>IVR</label>
          <root name="VerticalLayout" type="elvlt">
            <alignment>left,center</alignment>
            <elementList>
              <element name="IVRBoltOnCode" type="elfld">
                <autoFocusNextField>false</autoFocusNextField>
                <labeAlign>left</labeAlign>
                <label>IVR Bolt On Code</label>
                <labelOrientation>left</labelOrientation>
                <onChangeMethod type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/uimthd_selectTrigger</variablePath>
                  </variablePathList>
                </onChangeMethod>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.MixedBoltOnForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.MixedBoltOn/dstruct_IVRBoltOnCode</variablePath>
                  </variablePathList>
                </variable>
              </element>
