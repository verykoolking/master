  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.UsageControlBoltOnAssociatiForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.UsageControlBoltOnAssociatiForm/leaf_model</valueType>
      <script><![CDATA[
        var m = cm_spoc.initUsageControlBoltOnAssociatiForm(this.model);
        this.BoltOnTypeModalityFinder = cm_spoc.initUCBOTAFinder(this);
        return m;
      ]]></script>
    </method>
    <method name="cwLeafTriggerAction$SelectAll" type="laction">
      <path>ui_cm_spoc.forms.UsageControlBoltOnAssociatiForm/leaf_SelectAll</path>
      <type>trigger</type>
      <script><![CDATA[
        var result = this.BoltOnTypeModalityFinder.result;
        for (var i = 0; i < result.length; i++){
         result[i].selectItem = this.SelectAll;

        }
        this.BoltOnTypeModalityFinder.model.updateUI();
      ]]></script>
    </method>
    <method name="addBoltOnAssociation" type="uimthd">
      <script><![CDATA[cm_spoc.initUCBoltOnTypeAssociationUI(this,"addNew");]]></script>
    </method>
    <method name="deleteBoltOnAssociation" type="uimthd">
      <script><![CDATA[
        cm_spoc.removeSelect(this);
        this.SelectAll = false;
        //this.BoltOnTypeModalityFinder = null;
      ]]></script>
    </method>
    <method name="ReadOnly" type="permMethod">
      <script>return this.model.readonly;</script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script>//cm_spoc.setRead(this);</script>
    </method>
    <method name="checkBoltOnTypeAssociationIdentifier" type="usrActionMethod">
      <script><![CDATA[
        if(this.model.BoltOnTypeAssociationIdentifier < 0 || this.model.BoltOnTypeAssociationIdentifier == null){
            this.model.BoltOnTypeAssociationIdentifier = null;
            Global.showUserMessage("Bolt On Type Association Identifier inserted is invalid");
        }
      ]]></script>
    </method>
  </methodList>
</userInterface>