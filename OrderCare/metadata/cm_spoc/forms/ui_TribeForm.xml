<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.TribeForm">
  <group>Tribes</group>
  <label>TribeForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="PriceGuidingContainerUI" type="leaf">
      <valueType>ui_cm_spoc.forms.PriceGuidingContainerUI</valueType>
    </variable>
    <variable name="ChargingModeGuidingContainerUI" type="leaf">
      <valueType>ui_cm_spoc.forms.ChargingModeGuidingContainerUI</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.Tribe</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>750px</width>
              <elementList>
                <element name="GeneralParameters" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TribeForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.TribeForm/frmui_Forms/vform_TribeProductOffering</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                </element>
                <element name="price" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TribeForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.TribeForm/frmui_Forms/vform_price</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Price</label>
                </element>
                <element name="ChargingMode" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TribeForm/leaf_ChargingModeGuidingContainerUI</variablePath>
                      <variablePath>ui_cm_spoc.forms.ChargingModeGuidingContainerUI/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.ChargingModeGuidingContainerUI/frmui_Forms/exov_Default</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Charging Mode</label>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TribeForm/leaf_ChargingModeGuidingContainerUI</variablePath>
                    </variablePathList>
                  </variable>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="TribeProductOffering" type="vform">
          <label>TribeProductOffering</label>
          <root name="VerticalLayout" type="elvlt">
            <width>750px</width>
            <elementList>
              <element name="SectionStack" type="elsectnstck">
                <elementList>
                  <element name="Section" type="elsect">
                    <elementList>
                      <element name="VerticalLayout1" type="elvlt">
                        <elementList>
                          <element name="GridLayout1" type="elgrd">
                            <alignment>right,center</alignment>
                            <numOfCols>2</numOfCols>
                            <elementList>
                              <element name="TribeId" type="elfld">
                                <displayFormat>[0-9]{4}</displayFormat>
                                <editable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeForm/permMethod_isPrimaryKey</variablePath>
                                  </variablePathList>
                                </editable>
                                <labeAlign>left</labeAlign>
                                <label>Tribe Code</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.Tribe/dstruct_TribeId</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>150px</width>
                              </element>
                              <element name="TribeDescription" type="elfld">
                                <labeAlign>left</labeAlign>
                                <label>Tribe Description</label>
                                <labelOrientation>left</labelOrientation>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.Tribe/dstruct_TribeDescription</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>150px</width>
                              </element>
                              <element name="TribeSpecificationEnum" type="elslct">
                                <labeAlign>left</labeAlign>
                                <label>Tribe Specification</label>
                                <labelOrientation>left</labelOrientation>
                                <multiselect>false</multiselect>
                                <onChangeMethod type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeForm/uimthd_TribeSpecificationTrigger</variablePath>
                                  </variablePathList>
                                </onChangeMethod>
                                <optional type="varPath">
                                  <variablePathList>
                                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                  </variablePathList>
                                </optional>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.Tribe/dstruct_TribeSpecificationEnum</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>150px</width>
                              </element>
                              <element name="CategoryEnum" type="elslct">
                                <labeAlign>left</labeAlign>
                                <label>Category</label>
                                <labelOrientation>left</labelOrientation>
                                <multiselect>false</multiselect>
                                <onChangeMethod type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeForm/uimthd_TribeCategoryTrigger</variablePath>
                                  </variablePathList>
                                </onChangeMethod>
                                <optional type="varPath">
                                  <variablePathList>
                                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                  </variablePathList>
                                </optional>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.Tribe/dstruct_CategoryEnum</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>150px</width>
                              </element>
                              <element name="MessageEnum" type="elslct">
                                <labeAlign>left</labeAlign>
                                <label>Tribe subscription text message</label>
                                <labelOrientation>left</labelOrientation>
                                <multiselect>false</multiselect>
                                <onChangeMethod type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeForm/uimthd_MessageTrigger</variablePath>
                                  </variablePathList>
                                </onChangeMethod>
                                <optional type="varPath">
                                  <variablePathList>
                                    <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                                  </variablePathList>
                                </optional>
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.TribeForm/leaf_model</variablePath>
                                    <variablePath>dstruct_cm_spoc.data.Tribe/dstruct_MessageEnum</variablePath>
                                  </variablePathList>
                                </variable>
                                <width>150px</width>
                              </element>
                            </elementList>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="chargingMode" type="vform">
          <label>chargingMode</label>
          <root name="VerticalLayout" type="elvlt">
            <alignment>right,center</alignment>
            <elementList>
              <element name="ChargingMode" type="elovf">
                <height>400px</height>
                <width>100%</width>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="price" type="vform">
          <label>price</label>
          <root name="VerticalLayout" type="elvlt">
            <alignment>left,center</alignment>
            <elementList>
              <element name="PriceEventEnum" type="elslct">
                <canSort>false</canSort>
                <label>Price Event</label>
                <multiselect>false</multiselect>
                <onChangeMethod type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.TribeForm/uimthd_onSelectPriceEventTrigger</variablePath>
                  </variablePathList>
                </onChangeMethod>
                <showEmpty>false</showEmpty>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.TribeForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.Tribe/dstruct_TPriceGuidingArrDS</variablePath>
                    <variablePath>dstruct_cm_spoc.data.TPriceGuidingArr/dstruct_PriceEventEnum</variablePath>
                  </variablePathList>
                </variable>
                <width>400px</width>
              </element>
              <element name="Price" type="elovf">
                <fillSpace>false</fillSpace>
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.TribeForm/leaf_PriceGuidingContainerUI</variablePath>
                    <variablePath>ui_cm_spoc.forms.PriceGuidingContainerUI/frmui_Forms</variablePath>
                    <variablePath>ui_cm_spoc.forms.PriceGuidingContainerUI/frmui_Forms/exov_Default</variablePath>
                  </variablePathList>
                </formReference>
                <height>400px</height>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.TribeForm/leaf_PriceGuidingContainerUI</variablePath>
                  </variablePathList>
                </variable>
                <width>100%</width>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$PriceGuidingContainerUI" type="laction">
      <path>ui_cm_spoc.forms.TribeForm/leaf_PriceGuidingContainerUI</path>
      <type>initializer</type>
      <script>return cm_spoc.initPriceGuidingContainerUI(this);</script>
    </method>
    <method name="cwLeafInitAction$ChargingModeGuidingContainerUI" type="laction">
      <path>ui_cm_spoc.forms.TribeForm/leaf_ChargingModeGuidingContainerUI</path>
      <type>initializer</type>
      <script>return cm_spoc.initChargingModeGuidingContainerUI(this);</script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.TribeForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initTribesOffering(this.model);
        return m;
      ]]></script>
    </method>
    <method name="TribeSpecificationTrigger" type="uimthd">
      <script><![CDATA[
        var btCode = this.model.TribeSpecificationEnum;
        this.model.TribeSpecification = btCode;
      ]]></script>
    </method>
    <method name="TribeCategoryTrigger" type="uimthd">
      <script><![CDATA[
        var btCode = this.model.CategoryEnum;
        this.model.TribeCategory = btCode;
      ]]></script>
    </method>
    <method name="MessageTrigger" type="uimthd">
      <script><![CDATA[
        var btCode = this.model.MessageEnum;
        this.model.Message = btCode;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i=0;i<this.ChargingModeGuidingContainerUI.ChargingModeGuidingUI.ChargingModeGuidingIterator.length;i++){
            this.ChargingModeGuidingContainerUI.ChargingModeGuidingUI.ChargingModeGuidingIterator[i].model.readonly = this.model.readonly;
        }

        for(var i=0;i<this.PriceGuidingContainerUI.PriceGuidingUI.PriceGuidingIterator.length;i++){
            this.PriceGuidingContainerUI.PriceGuidingUI.PriceGuidingIterator[i].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
    <method name="onSelectPriceEventTrigger" type="uimthd">
      <script>return cm_spoc.onSelectPriceEvent(this);</script>
    </method>
    <method name="isPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
  </methodList>
</userInterface>