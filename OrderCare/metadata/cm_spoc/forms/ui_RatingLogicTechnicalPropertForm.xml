  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.RatingLogicTechnicalPropertForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.RatingLogicTechnicalPropertForm/leaf_model</valueType>
      <script><![CDATA[
        var m = cm_spoc.initRatingLogicTechnicalPropert(this.model);
        cm_spoc.initRatingLogicTechnicalPropertUi(this);
        return m;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$finalDestinationPickList" type="laction">
      <path>ui_cm_spoc.forms.RatingLogicTechnicalPropertForm/leaf_finalDestinationPickList</path>
      <type>initializer</type>
      <script><![CDATA[
        var pl = new cm_spoc.PickList(null,this);
        pl.availableItems = new DataObjectList();

        var destinationTypeFinder=new Finder("cm_spoc.DestinationsTypeFinder");
           var result = destinationTypeFinder.search();

        for(var i=0;i<result.length;i++){
            var item = new cm_spoc.PickListDS();
            item.id = result[i].itemCode;
            item.description = result[i].itemCode;
            pl.availableItems.addLast(item);
        }

        pl.modelCallback = this.setPickListModel;
        return pl;
      ]]></script>
    </method>
    <method name="onInit" type="action">
      <valueType>nmeta_com.conceptwave.system.Object</valueType>
      <script>return this.cw$super_onInit();</script>
    </method>
    <method name="checkAllMethod" type="usrActionMethod">
      <script><![CDATA[
        for(var i=0; i<this.testIterator.length; i++){
            if(this.checkAll)
                this.testIterator[i].Selected=true;
            else
                this.testIterator[i].Selected=false;
        }
      ]]></script>
    </method>
    <method name="setPickListModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.RLTPDestinationTypeArrDS.List = null;
        this.parent.model.RLTPDestinationTypeArrDS.ListDS = null;
        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.RLTPDestinationTypeArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
  </methodList>
