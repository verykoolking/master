<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.AwardedPromotionGroupingForm">
  <group>Promotion</group>
  <label>AwardedPromotionGroupingForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.AwardedPromotionGrouping</valueType>
    </variable>
    <variable name="PickList" type="leaf">
      <valueType>ui_cm_spoc.PickList</valueType>
    </variable>
    <variable name="AwardedPromotionSegmentIterator" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="AwPrGroupingParameters" type="vform">
          <label>AwPrGroupingParameters</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="SectionStack" type="elsectnstck">
                <visibilityMode>multiple</visibilityMode>
                <width>850px</width>
                <elementList>
                  <element name="Section" type="elsect">
                    <elementList>
                      <element name="SectionHeader" type="elsecth">
                        <label>Status Properties</label>
                      </element>
                      <element name="GridLayout2" type="elgrd">
                        <colWidths>300px</colWidths>
                        <edgeShowCenter>true</edgeShowCenter>
                        <edgeSize>2</edgeSize>
                        <labeAlign>center</labeAlign>
                        <numOfCols>2</numOfCols>
                        <width>600px</width>
                        <elementList>
                          <element name="CellSpacer" type="elcsp">
                            <height>5px</height>
                            <rowSpan>5px</rowSpan>
                          </element>
                          <element name="AwPrGroupingCode" type="elfld">
                            <autoFocusNextField>false</autoFocusNextField>
                            <cellAlignment>left,center</cellAlignment>
                            <editable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.AwardedPromotionGroupingForm/permMethod_setIsPrimaryKey</variablePath>
                              </variablePathList>
                            </editable>
                            <labeAlign>left</labeAlign>
                            <label>Grouping Code</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.AwardedPromotionGroupingForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.AwardedPromotionGrouping/dstruct_AwPrGroupingCode</variablePath>
                              </variablePathList>
                            </variable>
                            <width>150px</width>
                          </element>
                          <element name="AwPrGroupingDescription" type="elfld">
                            <autoFocusNextField>false</autoFocusNextField>
                            <cellAlignment>left,center</cellAlignment>
                            <labeAlign>left</labeAlign>
                            <label>Grouping Description</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.AwardedPromotionGroupingForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.AwardedPromotionGrouping/dstruct_AwPrGroupingDescription</variablePath>
                              </variablePathList>
                            </variable>
                            <width>300px</width>
                          </element>
                          <element name="AwPrTypology" type="elslct">
                            <labeAlign>left</labeAlign>
                            <label>Typology</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.AwardedPromotionGroupingForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.AwardedPromotionGrouping/dstruct_AwPrTypology</variablePath>
                              </variablePathList>
                            </variable>
                          </element>
                          <element name="CellSpacer2" type="elcsp">
                            <height>5px</height>
                            <rowSpan>5px</rowSpan>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                  <element name="AwardedPromotionSegmentSection" type="elsect">
                    <removeLabelLine>true</removeLabelLine>
                    <elementList>
                      <element name="SectionHeader1" type="elsecth">
                        <label>AwardedPromotionSegment</label>
                      </element>
                      <element name="FormFrame" type="elovf">
                        <cellAlignment>center,center</cellAlignment>
                        <formReference type="varPath">
                          <variablePathList>
                            <variablePath>ui_cm_spoc.forms.AwardedPromotionGroupingForm/leaf_PickList</variablePath>
                            <variablePath>ui_cm_spoc.PickList/frmui_Forms</variablePath>
                            <variablePath>ui_cm_spoc.PickList/frmui_Forms/exov_Default</variablePath>
                          </variablePathList>
                        </formReference>
                        <height>350px</height>
                        <padding>0</padding>
                        <showResizeBar>true</showResizeBar>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>ui_cm_spoc.forms.AwardedPromotionGroupingForm/leaf_PickList</variablePath>
                          </variablePathList>
                        </variable>
                        <width>750px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>850px</width>
              <elementList>
                <element name="TabFrame" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AwardedPromotionGroupingForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.AwardedPromotionGroupingForm/frmui_Forms/vform_AwPrGroupingParameters</variablePath>
                    </variablePathList>
                  </formReference>
