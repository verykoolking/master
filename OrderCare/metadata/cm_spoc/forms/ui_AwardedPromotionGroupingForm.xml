                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.AwardedPromotionGroupingForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initAwardedPromotionGrouping(this.model);
        //cm_spoc.initAwardedPromotionSegmentUI(this);
        return m;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$PickList" type="laction">
      <path>ui_cm_spoc.forms.AwardedPromotionGroupingForm/leaf_PickList</path>
      <type>initializer</type>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_AwardedPromotionSegment";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.APGAwPromSegmArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setPickListModel;
        return pl;
      ]]></script>
    </method>
    <method name="addAwardedPromotionSegmentIterator" type="uimthd">
      <parameterList>
        <parameter name="itemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        /*if(this.model.APGAwPromSegmArrDS.ListDS.length==0){
            var apGAwPromSegmArr = cm_spoc.initInnerDS("base_APGAwPromSegmArr");
            this.model.APGAwPromSegmArrDS = apGAwPromSegmArr;
        }
        var model=this.model.APGAwPromSegmArrDS.List;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.AwardedPromotionSegmentUI","base_AwardedPromotionSegment","AwardedPromotionSegmentIterator");
        */
        var apsUI = cm_spoc.forms.AwardedPromotionSegmentUI();
        apsUI.AwPrSegmentCode = itemCode;
        this.AwardedPromotionSegmentIterator.push(apsUI);
      ]]></script>
    </method>
    <method name="deleteAwardedPromotionSegmentIterator" type="uimthd">
      <script><![CDATA[
        cm_spoc.removeListElements(this, this.model.APGAwPromSegmArrDS,"List", "AwardedPromotionSegmentIterator");
        if(this.model.APGAwPromSegmArrDS.ListDS.length==0)
        {
            this.model.APGAwPromSegmArrDS=null;
        }
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="setIsPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
    <method name="FinderAwPrSegmentSelectReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        //this.model.SourceStatusID = " ";
        var newDetail = new cm_spoc.FinderSelectReference(null, this);
        newDetail.setBaseItemCode("base_AwardedPromotionSegment");
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="referenceCallback" type="uimthd">
      <parameterList>
        <parameter name="itemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
        <parameter name="baseItemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        switch(baseItemCode){
            case "base_AwardedPromotionSegment":
                this.addAwardedPromotionSegmentIterator(itemCode);
                break;
        }
      ]]></script>
    </method>
    <method name="setPickListModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <array>true</array>
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.APGAwPromSegmArrDS.List = null;
        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.APGAwPromSegmArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
  </methodList>
</userInterface>