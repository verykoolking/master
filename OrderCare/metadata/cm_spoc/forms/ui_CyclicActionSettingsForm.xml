<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.CyclicActionSettingsForm">
  <group>CyclicActions</group>
  <label>CyclicActionSettingsForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.CyclicActionSettings</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Settings" type="vform">
          <label>Settings</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="SectionStack" type="elsectnstck">
                <width>750px</width>
                <elementList>
                  <element name="Section" type="elsect">
                    <elementList>
                      <element name="SectionHeader" type="elsecth">
                        <label>Cyclic Action Settings</label>
                      </element>
                      <element name="GridLayout2" type="elgrd">
                        <colWidths>300px</colWidths>
                        <edgeShowCenter>true</edgeShowCenter>
                        <edgeSize>2</edgeSize>
                        <labeAlign>center</labeAlign>
                        <numOfCols>2</numOfCols>
                        <width>600px</width>
                        <elementList>
                          <element name="CommonGracePeriodCyclicBoltOns" type="elfld">
                            <cellAlignment>left,center</cellAlignment>
                            <labeAlign>left</labeAlign>
                            <label>Common Grace Period Cyclic Bolt Ons (Days)</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.CyclicActionSettingsForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.CyclicActionSettings/dstruct_CommonGracePeriodCyclicBoltOns</variablePath>
                              </variablePathList>
                            </variable>
                            <width>150px</width>
                          </element>
                          <element name="RecordsBlockSizeRetriesProcess" type="elfld">
                            <cellAlignment>left,center</cellAlignment>
                            <labeAlign>left</labeAlign>
                            <label>Records block size that will be treates by the cyclic actions retries process</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.CyclicActionSettingsForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.CyclicActionSettings/dstruct_RecordsBlockSizeRetriesProcess</variablePath>
                              </variablePathList>
                            </variable>
                            <width>150px</width>
                          </element>
                          <element name="TimeBetweenRetriesExecution" type="elfld">
                            <cellAlignment>left,center</cellAlignment>
                            <labeAlign>left</labeAlign>
                            <label>Time (in minutes) between each new execution of the retries process</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.CyclicActionSettingsForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.CyclicActionSettings/dstruct_TimeBetweenRetriesExecution</variablePath>
                              </variablePathList>
                            </variable>
                            <width>150px</width>
                          </element>
                          <element name="SpareTime" type="elfld">
                            <cellAlignment>left,center</cellAlignment>
                            <labeAlign>left</labeAlign>
                            <label>Spare time, in seconds, for validity time</label>
                            <labelOrientation>left</labelOrientation>
                            <variable type="varPath">
                              <variablePathList>
                                <variablePath>ui_cm_spoc.forms.CyclicActionSettingsForm/leaf_model</variablePath>
                                <variablePath>dstruct_cm_spoc.data.CyclicActionSettings/dstruct_SpareTime</variablePath>
                              </variablePathList>
                            </variable>
                            <width>150px</width>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>750px</width>
              <elementList>
                <element name="TabFrame" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.CyclicActionSettingsForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.CyclicActionSettingsForm/frmui_Forms/vform_Settings</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Settings</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="setReadOnly" type="uimthd"/>
  </methodList>
</userInterface>