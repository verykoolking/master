<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.TopUpOriginDestinationPairIteratorUI">
  <group>Promotion</group>
  <label>TopUpOriginDestinationPairIteratorUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.TopUpOriginDestinationPair</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elgrd">
              <colWidths>50px</colWidths>
              <numOfCols>6</numOfCols>
              <width>300px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <cellAlignment>left,center</cellAlignment>
                  <columnSpan>1</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TopUpOriginDestinationPairIteratorUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>30px</width>
                </element>
                <element name="TopUpOriginEnum" type="elslct">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>2</columnSpan>
                  <multiselect>false</multiselect>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TopUpOriginDestinationPairIteratorUI/uimthd_setTopUpOrigin</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TopUpOriginDestinationPairIteratorUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.TopUpOriginDestinationPair/dstruct_TopUpOriginEnum</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="TopUpDestinationEnum" type="elslct">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>3</columnSpan>
                  <multiselect>false</multiselect>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TopUpOriginDestinationPairIteratorUI/uimthd_setTopUpDestination</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.TopUpOriginDestinationPairIteratorUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.TopUpOriginDestinationPair/dstruct_TopUpDestinationEnum</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="setTopUpOrigin" type="uimthd">
      <script>this.model.TopUpOrigin = this.model.TopUpOriginEnum;</script>
    </method>
    <method name="setTopUpDestination" type="uimthd">
      <script>this.model.TopUpDestination = this.model.TopUpDestinationEnum;</script>
    </method>
  </methodList>
</userInterface>