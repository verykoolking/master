  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.FreeAndBarredDestinationForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initFreeAndBarredDestination(this.model);
        cm_spoc.initFreeAndBarredDestinationUi(this);
        return m;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i=0;i<this.SimpleDestinationRuleIterator.length;i++){
            this.SimpleDestinationRuleIterator[i].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
    <method name="addSimpleDestination" type="uimthd">
      <script><![CDATA[
        var model = this.model.SimpleDestinationRuleArrDS.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.SimpleDestinationRuleUI","base_SimpleDestinationRule","SimpleDestinationRuleIterator");
      ]]></script>
    </method>
    <method name="deleteSimpleDestination" type="uimthd">
      <script><![CDATA[
        this.model.SimpleDestinationRuleArrDS = cm_spoc.removeListElements(this ,this.model.SimpleDestinationRuleArrDS,"List", "SimpleDestinationRuleIterator");
      ]]></script>
    </method>
    <method name="setIsPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
  </methodList>
</userInterface>