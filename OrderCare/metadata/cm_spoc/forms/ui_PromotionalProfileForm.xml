<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.PromotionalProfileForm">
  <group>Promotion</group>
  <label>PromotionalProfileForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.PromotionalProfile</valueType>
    </variable>
    <variable name="PriceGuidingContainerUI" type="leaf">
      <valueType>ui_cm_spoc.forms.PriceGuidingContainerUI</valueType>
    </variable>
    <variable name="ChargingModeGuidingContainerUI" type="leaf">
      <valueType>ui_cm_spoc.forms.ChargingModeGuidingContainerUI</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>1000px</width>
              <elementList>
                <element name="GeneralParameters" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/frmui_Forms/vform_GeneralParameters</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                </element>
                <element name="Price" type="eltab">
                  <fillSpace>false</fillSpace>
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/frmui_Forms/vform_price</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Price</label>
                </element>
                <element name="ChargingMode" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/frmui_Forms/vform_chargingMode</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>Charge Mode</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="GeneralParameters" type="vform">
          <label>GeneralParameters</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="VerticalLayout1" type="elvlt">
                <alignment>right,center</alignment>
                <elementList>
                  <element name="ProfileCode" type="elfld">
                    <autoFocusNextField>false</autoFocusNextField>
                    <editable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/permMethod_setIsPrimaryKey</variablePath>
                      </variablePathList>
                    </editable>
                    <label>Profile code</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.PromotionalProfile/dstruct_ProfileCode</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="ProfileDescription" type="elfld">
                    <autoFocusNextField>false</autoFocusNextField>
                    <label>Profile Description</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.PromotionalProfile/dstruct_ProfileDescription</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="HorizontalLayout" type="elhlt">
                    <elementList>
                      <element name="PromotionalProfileSpecification" type="elfld">
                        <autoFocusNextField>false</autoFocusNextField>
                        <label>Promotional Profile Specification</label>
                        <labelOrientation>left</labelOrientation>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/leaf_model</variablePath>
                            <variablePath>dstruct_cm_spoc.data.PromotionalProfile/dstruct_PromotionalProfileSpecifica</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="PromotionalProfileSpecificaButton" type="elbtn">
                        <clickMethod type="varPath">
                          <variablePathList>
                            <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/usrActionMethod_FinderPromotionalProfileSpecificaReferenceDialog</variablePath>
                          </variablePathList>
                        </clickMethod>
                        <icon>/cwf/finder.gif</icon>
                        <showLabel>false</showLabel>
                        <visible type="varPath">
                          <variablePathList>
                            <variablePath>dtype_com.conceptwave.system.Boolean/leaf_TRUE</variablePath>
                          </variablePathList>
                        </visible>
                        <width>22px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="price" type="vform">
          <label>price</label>
          <root name="VerticalLayout" type="elvlt">
            <alignment>left,center</alignment>
            <elementList>
              <element name="PriceEventEnum" type="elslct">
                <canSort>false</canSort>
                <label>Price Event</label>
                <multiselect>false</multiselect>
                <onChangeMethod type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/uimthd_onSelectPriceEventTrigger</variablePath>
                  </variablePathList>
                </onChangeMethod>
                <showEmpty>false</showEmpty>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.PromotionalProfile/dstruct_PPPriceGuidingArrDS</variablePath>
                    <variablePath>dstruct_cm_spoc.data.PPPriceGuidingArr/dstruct_PriceEventEnum</variablePath>
                  </variablePathList>
                </variable>
                <width>400px</width>
              </element>
              <element name="Price" type="elovf">
                <fillSpace>false</fillSpace>
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/leaf_PriceGuidingContainerUI</variablePath>
                    <variablePath>ui_cm_spoc.forms.PriceGuidingContainerUI/frmui_Forms</variablePath>
                    <variablePath>ui_cm_spoc.forms.PriceGuidingContainerUI/frmui_Forms/exov_Default</variablePath>
                  </variablePathList>
                </formReference>
                <height>400px</height>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/leaf_PriceGuidingContainerUI</variablePath>
                  </variablePathList>
                </variable>
                <width>100%</width>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="chargingMode" type="vform">
          <label>chargingMode</label>
          <root name="VerticalLayout" type="elvlt">
            <alignment>right,center</alignment>
            <elementList>
              <element name="ChargingMode" type="elovf">
                <fillSpace>false</fillSpace>
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/leaf_ChargingModeGuidingContainerUI</variablePath>
                    <variablePath>ui_cm_spoc.forms.ChargingModeGuidingContainerUI/frmui_Forms</variablePath>
                    <variablePath>ui_cm_spoc.forms.ChargingModeGuidingContainerUI/frmui_Forms/exov_Default</variablePath>
                  </variablePathList>
                </formReference>
                <height>400px</height>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.PromotionalProfileForm/leaf_ChargingModeGuidingContainerUI</variablePath>
                  </variablePathList>
                </variable>
                <width>100%</width>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$PriceGuidingContainerUI" type="laction">
      <path>ui_cm_spoc.forms.PromotionalProfileForm/leaf_PriceGuidingContainerUI</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.BoltOnForm/leaf_PriceGuidingContainerUI</valueType>
      <script>return cm_spoc.initPriceGuidingContainerUI(this);</script>
    </method>
    <method name="cwLeafInitAction$ChargingModeGuidingContainerUI" type="laction">
      <path>ui_cm_spoc.forms.PromotionalProfileForm/leaf_ChargingModeGuidingContainerUI</path>
      <type>initializer</type>
      <script>return cm_spoc.initChargingModeGuidingContainerUI(this);</script>
    </method>
    <method name="onSelectPriceEventTrigger" type="uimthd">
      <script>return cm_spoc.onSelectPriceEvent(this);</script>
    </method>
    <method name="setIsPrimaryKey" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="setPromotionalProfileSpecifica" type="uimthd">
      <parameterList>
        <parameter name="selected" type="rifp">
          <mandatory>true</mandatory>
          <type>nmeta_com.conceptwave.system.Object</type>
        </parameter>
      </parameterList>
      <script>this.parentUI.model.PromotionalProfileSpecifica = selected.itemCode;</script>
    </method>
    <method name="FinderPromotionalProfileSpecificaReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_PromotionalProfileSpecifica");
        newDetail.parentUI = this;
        newDetail.modelCallback = this.setPromotionalProfileSpecifica;
        return newDetail;
      ]]></script>
    </method>
  </methodList>
</userInterface>