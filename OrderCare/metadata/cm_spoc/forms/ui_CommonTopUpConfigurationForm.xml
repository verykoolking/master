    <method name="cwLeafTriggerAction$SelectAllFixedLimitsIterator" type="laction">
      <path>ui_cm_spoc.forms.CommonTopUpConfigurationForm/leaf_SelectAllFixedLimitsIterator</path>
      <type>trigger</type>
      <script><![CDATA[
        if(this.SelectAllFixedLimits){

        for (var i = 0;i<this.WalletUsageRuleIterator.length; i++){
           // if(!this.WalletUsageRuleIterator[i].Selected)
                this.WalletUsageRuleIterator[i].Selected = true;
        }
        }
        else {
            for (var i = 0;i<this.WalletUsageRuleIterator.length; i++){
           // if(!this.WalletUsageRuleIterator[i].Selected)
                this.WalletUsageRuleIterator[i].Selected = false;
            }
        }
      ]]></script>
    </method>
