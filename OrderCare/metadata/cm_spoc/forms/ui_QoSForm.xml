<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.QoSForm">
  <group>QoS</group>
  <label>QoSForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="SyQoSRuleFinder" type="leaf">
      <valueType>findScript_cm_spoc.SyQoSRuleFinder/ui_UserInterface</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <elementList>
                <element name="Rx" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.QoSForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.QoSForm/frmui_Forms/vform_RxForm</variablePath>
                    </variablePathList>
                  </formReference>
                </element>
                <element name="Sy" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.QoSForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.QoSForm/frmui_Forms/vform_SyForm</variablePath>
                    </variablePathList>
                  </formReference>
                </element>
                <element name="CommonConfiguration" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.QoSForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.QoSForm/frmui_Forms/vform_CommonConfigurationForm</variablePath>
                    </variablePathList>
                  </formReference>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="RxForm" type="vform">
          <label>RxForm</label>
        </overlay>
        <overlay name="SyForm" type="vform">
          <label>SyForm</label>
          <root name="SectionStack" type="elsectnstck">
            <width>750px</width>
            <elementList>
              <element name="Section" type="elsect">
                <elementList>
                  <element name="GridLayout" type="elgrd">
                    <numOfCols>3</numOfCols>
                    <width>420px</width>
                    <elementList>
                      <element name="CellSpacer" type="elcsp">
                        <width>20px</width>
                      </element>
                      <element name="Label" type="ellab">
                        <label>Policy Counter ID</label>
                        <width>160px</width>
                      </element>
                      <element name="TextField" type="elfld">
                        <width>230px</width>
                      </element>
                      <element name="CellSpacer1" type="elcsp"/>
                      <element name="Label1" type="ellab">
                        <label>Tariff Plans</label>
                      </element>
                      <element name="TextField1" type="elfld">
                        <width>230px</width>
                      </element>
                      <element name="CellSpacer2" type="elcsp"/>
                      <element name="Label2" type="ellab">
                        <label>Policy Counter Rule Description</label>
                      </element>
                      <element name="TextField2" type="elfld">
                        <width>230px</width>
                      </element>
                      <element name="RowSpacer" type="elrsp">
                        <height>25px</height>
                        <rowSpan>25px</rowSpan>
                      </element>
                    </elementList>
                  </element>
                  <element name="HorizontalLayout" type="elhlt">
                    <elementList>
                      <element name="Checkbox" type="elchk"/>
                      <element name="MenuItem" type="elmnu">
                        <icon>/cwt/images/16/notebook_add.png</icon>
                      </element>
                      <element name="MenuItem1" type="elmnu">
                        <icon>/cwt/images/16/delete.png</icon>
                      </element>
                    </elementList>
                  </element>
                  <element name="FormFrame" type="elovf">
                    <formReference type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.QoSForm/leaf_SyQoSRuleFinder</variablePath>
                        <variablePath>findScript_cm_spoc.SyQoSRuleFinder/ui_UserInterface/frmui_Forms</variablePath>
                        <variablePath>findScript_cm_spoc.SyQoSRuleFinder/ui_UserInterface/frmui_Forms/exov_Default</variablePath>
                      </variablePathList>
                    </formReference>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.QoSForm/leaf_SyQoSRuleFinder</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="CommonConfigurationForm" type="vform">
          <label>CommonConfigurationForm</label>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
</userInterface>