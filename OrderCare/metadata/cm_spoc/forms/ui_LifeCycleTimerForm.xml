<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.LifeCycleTimerForm">
  <group>LifeCycles</group>
  <label>LifeCycleTimerForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.LifeCycleTimer</valueType>
    </variable>
    <variable name="LifeCyclePickList" type="leaf">
      <valueType>ui_cm_spoc.PickList</valueType>
    </variable>
    <variable name="ExitingPickList" type="leaf">
      <valueType>ui_cm_spoc.PickList</valueType>
    </variable>
    <variable name="PlansGroupPickList" type="leaf">
      <valueType>ui_cm_spoc.PickList</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="SectionStack" type="elsectnstck">
              <width>750px</width>
              <elementList>
                <element name="Section" type="elsect">
                  <elementList>
                    <element name="SectionHeader" type="elsecth">
                      <label>Life Cycle Timer</label>
                    </element>
                    <element name="HorizontalLayout" type="elhlt">
                      <width>750px</width>
                      <elementList>
                        <element name="GridLayout" type="elgrd">
                          <colWidths>100px</colWidths>
                          <numOfCols>2</numOfCols>
                          <width>350px</width>
                          <elementList>
                            <element name="Label" type="ellab">
                              <label>Timer</label>
                            </element>
                            <element name="TimerCodeEnum" type="elslct">
                              <multiselect>false</multiselect>
                              <onChangeMethod type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/uimthd_setTimerCode</variablePath>
                                </variablePathList>
                              </onChangeMethod>
                              <showLabel>false</showLabel>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.LifeCycleTimer/dstruct_TimerCodeEnum</variablePath>
                                </variablePathList>
                              </variable>
                              <width>350px</width>
                            </element>
                            <element name="Label3" type="ellab">
                              <label>Validity (Hours)</label>
                            </element>
                            <element name="Validity" type="elfld">
                              <autoFocusNextField>false</autoFocusNextField>
                              <showLabel>false</showLabel>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.LifeCycleTimer/dstruct_Validity</variablePath>
                                </variablePathList>
                              </variable>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                    <element name="LayoutSpacer1" type="ellsp">
                      <height>10px</height>
                    </element>
                    <element name="Tabset" type="eltset">
                      <elementList>
                        <element name="LifeCycleTab" type="eltab">
                          <formReference type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/frmui_Forms</variablePath>
                              <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/frmui_Forms/vform_LifeCyclesForm</variablePath>
                            </variablePathList>
                          </formReference>
                          <label>Life Cycles</label>
                        </element>
                        <element name="PlansTab" type="eltab">
                          <formReference type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/frmui_Forms</variablePath>
                              <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/frmui_Forms/vform_PlansForm</variablePath>
                            </variablePathList>
                          </formReference>
                          <label>Plans</label>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="LifeCyclesForm" type="vform">
          <label>LifeCyclesForm</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="FormFrame" type="elovf">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_LifeCyclePickList</variablePath>
                    <variablePath>ui_cm_spoc.PickList/frmui_Forms</variablePath>
                    <variablePath>ui_cm_spoc.PickList/frmui_Forms/exov_Default</variablePath>
                  </variablePathList>
                </formReference>
                <height>350px</height>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_LifeCyclePickList</variablePath>
                  </variablePathList>
                </variable>
                <width>550px</width>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="PlansForm" type="vform">
          <label>PlansForm</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="FormFrame" type="elovf">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_ExitingPickList</variablePath>
                    <variablePath>ui_cm_spoc.PickList/frmui_Forms</variablePath>
                    <variablePath>ui_cm_spoc.PickList/frmui_Forms/exov_Default</variablePath>
                  </variablePathList>
                </formReference>
                <height>350px</height>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_ExitingPickList</variablePath>
                  </variablePathList>
                </variable>
                <width>550px</width>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_model</valueType>
      <script><![CDATA[
        //
        var m = cm_spoc.initLifeCycleTimer(this.model);
        cm_spoc.initLifeCycleTimerUI(this);
        //this.EvaluationOrderList =  cm_spoc.initLifeCycleTimerFinder(this); //new cm_spoc.LifeCycleTimerFinder.UserInterface(null,this);

        return m;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$LifeCyclePickList" type="laction">
      <path>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_LifeCyclePickList</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_LifeCyclePickList</valueType>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_LifeCycle";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.LCTimerLifeCycleArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setLifeCyclePLModel;
        return pl;
        ////
        /*
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_TariffPlan";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.SpDayTariffPlanArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setPickListModelTariffPlan;
        return pl;
        */
      ]]></script>
    </method>
    <method name="cwLeafInitAction$ExitingPickList" type="laction">
      <path>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_ExitingPickList</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_ExitingPickList</valueType>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_TariffPlan";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.LCTTariffPlanArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setTariffPlanPLModel;
        return pl;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$PlansGroupPickList" type="laction">
      <path>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_PlansGroupPickList</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.LifeCycleTimerForm/leaf_PlansGroupPickList</valueType>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_TariffPlan";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.LCTTariffPlanArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setTariffPlanPLModel;
        return pl;
      ]]></script>
    </method>
    <method name="setTimerCode" type="uimthd">
      <script>this.model.TimerCode = this.model.TimerCodeEnum;</script>
    </method>
    <method name="setLifeCyclePLModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.LCTimerLifeCycleArrDS.List =null;
        this.parent.model.LCTimerLifeCycleArrDS.ListDS =null;

        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.LCTimerLifeCycleArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="setTariffPlanPLModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.LCTTariffPlanArrDS.List =null;
        this.parent.model.LCTTariffPlanArrDS.ListDS =null;

        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.LCTTariffPlanArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
  </methodList>
</userInterface>