    <method name="onSettingSMSInfoOfBalanceRetrievedAfterTU" type="uimthd">
      <script><![CDATA[
        var value = this.model.SMSInfoOfBalanceRetrievedAfterTU;
        if(value==true)
            this.model.RecoveredBalanceMessageEnum= null;
        return;
      ]]></script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="SaveBalanceMessageTrigger" type="uimthd">
      <script>this.model.SaveBalanceMessage=this.model.SaveBalanceMessageEnum;</script>
    </method>
    <method name="SaveTotalBalanceMessageTrigger" type="uimthd">
      <script>this.model.SaveTotalBalanceMessage = this.model.SaveTotalBalanceMessageEnum;</script>
    </method>
    <method name="RecoveredBalanceMessageTrigger" type="uimthd">
      <script>this.model.RecoveredBalanceMessage= this.model.RecoveredBalanceMessageEnum;</script>
    </method>
    <method name="onSettingOutStandingTrigger" type="uimthd">
      <script><![CDATA[
        cm_spoc.onSettingOutStandingTrigger(this);
        /*
        var value = this.model.OutstandingStatRetrievedTUWallet;//SMSInfoOfBalanceRetrievedAfterTU;
        if(value==true)
            this.model.RecoveredBalanceMessageEnum= null;
        return;
        */
      ]]></script>
    </method>
    <method name="onSettingSMSInforOfStoredBalanceOfWallet" type="uimthd">
