        var value = this.model.SMSInforOfStoredBalanceOfWallet;
        if(value==true)
            this.model.SaveBalanceMessageEnum = null;
        return;
      ]]></script>
    </method>
    <method name="onSettingSMSInforTotalStoredBalanceWallet" type="uimthd">
