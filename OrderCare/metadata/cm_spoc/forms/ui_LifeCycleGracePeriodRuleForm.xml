  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.LifeCycleGracePeriodRuleForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initLifeCycle(this.model);
        cm_spoc.initLifeCycleUi(this);
        return m;
      ]]></script>
    </method>
    <method name="LCGPRMessageVisRule1" type="permMethod">
      <script><![CDATA[
        var value = this.model.SMSInfoOfBalanceRetrievedAfterTU;
        if(value==true) return true;
        else return false;
      ]]></script>
    </method>
    <method name="LCGPRMessageVisRule2" type="permMethod">
