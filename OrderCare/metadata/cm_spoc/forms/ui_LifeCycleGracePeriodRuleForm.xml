        return;
      ]]></script>
    </method>
    <method name="addItemsList" type="uimthd">
      <script><![CDATA[
        var model = this.model.RecoveryConditionArrDS.ListDS;

        cm_spoc.addListElement(this, model ,"cm_spoc.forms.RecoveryConditionForm","base_RecoveryCondition","RecoveryConditionIterator");
      ]]></script>
    </method>
    <method name="deleteItemsList" type="uimthd">
      <script><![CDATA[
        this.model.RecoveryConditionArrDS= cm_spoc.removeListElements(this ,this.model.RecoveryConditionArrDS,"List", "RecoveryConditionIterator");
      ]]></script>
    </method>
    <method name="LCGPROutstandingStatRetrievedTUWalletVisRule" type="permMethod">
      <script><![CDATA[
        //var listArrayDS=this.parent.model.RecoveryConditionArrDS.ListDS;
        var listArrayDS=this.model.RecoveryConditionArrDS.ListDS;
         var esito = false;
         var n = listArrayDS.length;
         for (var i=0; i<n; i++){
             if (listArrayDS[i].RetrievablePC==true)
             {
             //    this.parent.Checked = listArrayDS[i].RetrievablePC;
                 this.Checked = listArrayDS[i].RetrievablePC;
                 esito = true;
                 break;
             }
         }

         return esito;
      ]]></script>
    </method>
