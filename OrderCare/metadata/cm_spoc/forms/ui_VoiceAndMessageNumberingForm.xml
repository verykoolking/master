    <method name="PatternOfNumberUpdate" type="usrActionMethod">
      <script><![CDATA[
        this.model.PatternOfNumber = this.model.PatternOfNumberOptionEnum == 'Begins' ? this.model.PatternOfNumberTemp + '*' : this.model.PatternOfNumberTemp;
      ]]></script>
    </method>
    <method name="ServiceProviderUpdate" type="usrActionMethod">
      <script>this.model.ServiceProviderVoiceInputFor = this.model.ServiceProviderEnum;</script>
    </method>
    <method name="NpiUpdate" type="usrActionMethod">
