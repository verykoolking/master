    <method name="RunAppliesDaylightSavingTime" type="uimthd">
      <script><![CDATA[
        this.model.AppliesOffset=false;
        this.model.SecondGMTHourOffset="";
        this.model.SecondGMTMinuteOffset="";
        this.model.StartingDaylightMonth="March";
        this.model.StartingDaylightWeek="Last Week";
        this.model.StartingDaylightWeekDay="";
        this.model.EndingDaylightMonth="";
        this.model.EndingDaylightWeek="";
        this.model.EndingDaylightWeekDay="";
        this.model.StartingDaylightHour="";
        this.model.StartingDaylightMin="";
        this.model.StartingDaylightSec="";
        this.model.EndingDaylightHour="";
        this.model.EndingDaylightMin="";
        this.model.EndingDaylightSec="";

        if (this.model.AppliesDaylightSavingTime==true && this.model.AppliesOffset==false && this.model.GMTHourOffset!=null){
            this.model.SecondGMTHourOffset=this.model.GMTHourOffset+1;
            this.model.SecondGMTMinuteOffset=this.model.GMTMinuteOffset;
            if (this.model.GMTHourOffset=12){
                this.model.SecondGMTHourOffset==-11;
            }
        }
        else{
        this.model.SecondGMTHourOffset="";
        this.model.SecondGMTMinuteOffset="";
        }
        if (this.model.AppliesDaylightSavingTime==false){
            this.model.StartingDaylightMonth="";
            this.model.StartingDaylightWeek="";
        }
        return;
      ]]></script>
    </method>
    <method name="SecondHourTrigger" type="uimthd">
      <script><![CDATA[
        var value=this.model.AppliesOffset;
        var GTMHour=this.model.GMTHourOffset;

        if(this.model.AppliesDaylightSavingTime==true &&  value==false && GTMHour!=null){
            this.model.SecondGMTHourOffset=GTMHour + 1;
            if (this.model.GMTHourOffset==12){
                this.model.SecondGMTHourOffset=-11;
            }
        }
        else
            this.model.SecondGMTHourOffset="";
        return;
      ]]></script>
    </method>
    <method name="RunAppliesOffSet" type="uimthd">
      <script><![CDATA[
        var value=this.model.AppliesOffset;
        var GTMHour=this.model.GMTHourOffset;

        if(value==false && GTMHour!=null)
            this.model.SecondGMTHourOffset=GTMHour + 1;
        else{
            this.model.SecondGMTHourOffset="";
            this.model.SecondGMTMinuteOffset="";
        }
        if (this.model.AppliesDaylightSavingTime==true && this.model.AppliesOffset==false && this.model.GMTHourOffset!=null){
            this.model.SecondGMTHourOffset=this.model.GMTHourOffset+1;
            this.model.SecondGMTMinuteOffset=this.model.GMTMinuteOffset;
            if (this.model.GMTHourOffset==12){
                this.model.SecondGMTHourOffset=-11;
            }
        }
        else{
        this.model.SecondGMTHourOffset="";
        this.model.SecondGMTMinuteOffset="";
        }
        return;
      ]]></script>
    </method>
