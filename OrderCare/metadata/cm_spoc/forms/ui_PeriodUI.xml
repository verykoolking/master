<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.PeriodUI">
  <group>BoltOnType</group>
  <label>PeriodUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="parentUI" type="leaf">
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.HappyBoltOnPeriod</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elgrd">
              <colWidths>50px</colWidths>
              <fixedColWidths>true</fixedColWidths>
              <numOfCols>13</numOfCols>
              <width>750px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <columnSpan>1</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>30px</width>
                </element>
                <element name="Year" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>2</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.HappyBoltOnPeriod/dstruct_Year</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="Month" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>2</columnSpan>
                  <displayFormat>[0-1]{12}</displayFormat>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/uimthd_completMonth</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.HappyBoltOnPeriod/dstruct_Month</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="Day" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>2</columnSpan>
                  <displayFormat>[0-1]{31}</displayFormat>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <label>day</label>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/uimthd_completDay</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.HappyBoltOnPeriod/dstruct_Day</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="Hour" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>2</columnSpan>
                  <displayFormat>[0-1]{24}</displayFormat>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <label>hour</label>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/uimthd_completHour</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.HappyBoltOnPeriod/dstruct_Hour</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="WeekDay" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>2</columnSpan>
                  <displayFormat>[0-1]{7}</displayFormat>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <label>week day</label>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/uimthd_completWeekDay</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.HappyBoltOnPeriod/dstruct_WeekDay</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="Duration" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>2</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PeriodUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.HappyBoltOnPeriod/dstruct_Duration</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="readOnly" type="permMethod">
      <script>return !this.parentUI.model.readonly;</script>
    </method>
    <method name="setParentUI" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <mandatory>true</mandatory>
          <type>ui_com.conceptwave.system.UserInterface</type>
        </parameter>
      </parameterList>
      <script>this.parentUI=ui;</script>
    </method>
    <method name="getParentUI" type="uimthd">
      <script>return this.parentUI;</script>
    </method>
    <method name="completMonth" type="uimthd">
      <script><![CDATA[
        var value=this.model.Month;
        if(value.length!=12)
            while(value.length<12)
            {
                value=value+0;
            }
        this.model.Month=value;
        return;
      ]]></script>
    </method>
    <method name="completDay" type="uimthd">
      <script><![CDATA[
        var value=this.model.Day;
        if(value.length!=31)
            while(value.length<31)
            {
                value=value+0;
            }
        this.model.Day=value;
        return;
      ]]></script>
    </method>
    <method name="completHour" type="uimthd">
      <script><![CDATA[
        var value=this.model.Hour;
        if(value.length!=24)
            while(value.length<24)
            {
                value=value+0;
            }
        this.model.Hour=value;
        return;
      ]]></script>
    </method>
    <method name="completWeekDay" type="uimthd">
      <script><![CDATA[
        var value=this.model.WeekDay;
        if(value.length!=7)
            while(value.length<7)
            {
                value=value+0;
            }
        this.model.WeekDay=value;
        return;
      ]]></script>
    </method>
  </methodList>
</userInterface>