<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.RatePrioritizationRuleForm">
  <group>Rate</group>
  <label>RatePrioritizationRuleForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="PickList" type="leaf">
      <valueType>ui_cm_spoc.PickList</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.RatePrioritizationRule</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="Tabset" type="eltset">
              <width>750px</width>
              <elementList>
                <element name="generalParameters" type="eltab">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RatePrioritizationRuleForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.RatePrioritizationRuleForm/frmui_Forms/vform_generalParameters</variablePath>
                    </variablePathList>
                  </formReference>
                  <label>General Parameters</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="generalParameters" type="vform">
          <label>generalParameters</label>
          <root name="VerticalLayout" type="elvlt">
            <elementList>
              <element name="layout" type="elvlt">
                <alignment>right,center</alignment>
                <elementList>
                  <element name="Description" type="elfld">
                    <autoFocusNextField>false</autoFocusNextField>
                    <label>Description</label>
                    <labelOrientation>left</labelOrientation>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RatePrioritizationRuleForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RatePrioritizationRule/dstruct_Description</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="ApplicabilityConditionEnum" type="elslct">
                    <labeAlign>left</labeAlign>
                    <label>Applicability Conditions</label>
                    <labelOrientation>left</labelOrientation>
                    <multiselect>false</multiselect>
                    <onChangeMethod type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RatePrioritizationRuleForm/uimthd_setAppConditionSelected</variablePath>
                      </variablePathList>
                    </onChangeMethod>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RatePrioritizationRuleForm/leaf_model</variablePath>
                        <variablePath>dstruct_cm_spoc.data.RatePrioritizationRule/dstruct_ApplicabilityConditionEnum</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="FormFrame" type="elovf">
                    <formReference type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RatePrioritizationRuleForm/leaf_PickList</variablePath>
                        <variablePath>ui_cm_spoc.PickList/frmui_Forms</variablePath>
                        <variablePath>ui_cm_spoc.PickList/frmui_Forms/exov_Default</variablePath>
                      </variablePathList>
                    </formReference>
                    <height>175px</height>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RatePrioritizationRuleForm/leaf_PickList</variablePath>
                      </variablePathList>
                    </variable>
                    <width>500px</width>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$PickList" type="laction">
      <path>ui_cm_spoc.forms.RatePrioritizationRuleForm/leaf_PickList</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.RatePrioritizationRuleForm/leaf_PickList</valueType>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_RatePrioritizationRule";
        var result = itemFinder.search();
        var pl = cm_spoc.initPickList(this,result,this.model.RPRRateArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setPickListModel;
        return pl;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.RatePrioritizationRuleForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initRatePrioritizationRule(this.model);
        cm_spoc.initRatePrioritizationRuleUI(this, m);

        return m;
      ]]></script>
    </method>
    <method name="FinderSelectReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_ApplicabilityCondition");
        newDetail.showButton=true;
        newDetail.parentUI = this;
        //newDetail.modelCallback=this.referenceCallback;
        return newDetail;
      ]]></script>
    </method>
    <method name="CurrencyTrigger" type="uimthd">
      <script>this.model.Currency  = this.model.CurrencyEnum;</script>
    </method>
    <method name="referenceCallback" type="uimthd">
      <parameterList>
        <parameter name="selected" type="rifp">
          <type>dstruct_cm_spoc.FinderSelectReferenceDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        if(selected != null){
            this.model.ApplicabilityCondition = selected;
        }
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script>return;</script>
    </method>
    <method name="setAppConditionSelected" type="uimthd">
      <script>this.model.ApplicabilityCondition=this.model.ApplicabilityConditionEnum;</script>
    </method>
    <method name="setPickListModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItem" type="rifp">
          <array>true</array>
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.RPRRateArrDS.List = null;
        this.parent.model.RPRRateArrDS.ListDS =null;
        for(var i=0;i<selectedItem.length;i++){
            this.parent.model.RPRRateArrDS.List[i] = selectedItem[i].id;
        }
      ]]></script>
    </method>
  </methodList>
</userInterface>