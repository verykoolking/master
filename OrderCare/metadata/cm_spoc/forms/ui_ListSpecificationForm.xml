      <path>ui_cm_spoc.forms.ListSpecificationForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initListSpecification(this.model);
        cm_spoc.initListSpecificationUI(this,m);
        //m.InfiniteDate=true;
        //this.model.DuoDS.ApplyShortDialing=true;
        return m;
      ]]></script>
    </method>
    <method name="addItemsList" type="uimthd">
      <script><![CDATA[
        var model = this.model.LSDestGrAndNumSpecArrDS.ListDS;

        cm_spoc.addListElement(this, model ,"cm_spoc.forms.RestrictionsItemsListUI","base_DestGrAndNumSpec","restrictionIterator");
      ]]></script>
    </method>
    <method name="deleteItemsList" type="uimthd">
      <script><![CDATA[
        this.model.LSDestGrAndNumSpecArrDS = cm_spoc.removeListElements(this ,this.model.LSDestGrAndNumSpecArrDS,"List", "restrictionIterator");
      ]]></script>
    </method>
    <method name="visibleDate" type="permMethod">
      <script><![CDATA[
