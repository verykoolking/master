  <methodList>
    <method name="cwLeafInitAction$ServiceTariffPlanPL" type="laction">
      <path>ui_cm_spoc.forms.ServiceTariffPlanGroupForm/leaf_ServiceTariffPlanPL</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.ServiceTariffPlanGroupForm/leaf_ServiceTariffPlanPL</valueType>
      <script><![CDATA[
        var itemFinder = new Finder("cwa_epc.catalogItemFinder");
        itemFinder.searchDocument.baseItemCode = "base_ServiceTariffPlan";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.STPGServiceTPArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setTariffPlanPLModel;
        return pl;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.ServiceTariffPlanGroupForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initServiceTariffPlanGroup(this.model);
        return m;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$BoltOnPL" type="laction">
      <path>ui_cm_spoc.forms.ServiceTariffPlanGroupForm/leaf_BoltOnPL</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.ServiceTariffPlanGroupForm/leaf_BoltOnPL</valueType>
      <script><![CDATA[
        var itemFinder = new Finder("cwa_epc.catalogItemFinder");
        itemFinder.searchDocument.baseItemCode = "base_BoltOnType";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.DefaultBoltOnTypeListDS.DBOTLBoltOnTypeArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setBoltOnPLModel;
        return pl;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="setTariffPlanPLModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.STPGServiceTPArrDS.List =null;
        this.parent.model.STPGServiceTPArrDS.ListDS =null;

        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.STPGServiceTPArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="setBoltOnPLModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.DefaultBoltOnTypeListDS.DBOTLBoltOnTypeArrDS.List =null;
        this.parent.model.DefaultBoltOnTypeListDS.DBOTLBoltOnTypeArrDS.ListDS =null;

        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.DefaultBoltOnTypeListDS.DBOTLBoltOnTypeArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
  </methodList>
