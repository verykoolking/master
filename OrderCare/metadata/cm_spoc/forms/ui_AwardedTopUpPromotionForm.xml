    <method name="AwardedByLastTimeReceivedAttributeSectionVisRule" type="permMethod">
      <script><![CDATA[
        if(!this.model.AwardedTopUpPromotionType)
        {
            return false;
        }
        else if(this.model.AwardedTopUpPromotionType == "LT")
        {
            return true;
        }
        else
        {
            return false;
        }
      ]]></script>
    </method>
