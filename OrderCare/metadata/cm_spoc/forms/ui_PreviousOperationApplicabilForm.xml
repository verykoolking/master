<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.PreviousOperationApplicabilForm">
  <group>Promotion</group>
  <label>PreviousOperationApplicabilForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="simpleIterator" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.PreviousOperationApplicabil</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="SectionStack" type="elsectnstck">
              <width>750px</width>
              <elementList>
                <element name="Section" type="elsect">
                  <elementList>
                    <element name="SectionHeader" type="elsecth">
                      <label>Previous Operations</label>
                    </element>
                    <element name="VerticalLayout2" type="elvlt">
                      <width>750px</width>
                      <elementList>
                        <element name="HorizontalLayout1" type="elhlt">
                          <width>750px</width>
                          <elementList>
                            <element name="addButton" type="elmnu">
                              <clickMethod type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.PreviousOperationApplicabilForm/uimthd_addItemsList</variablePath>
                                </variablePathList>
                              </clickMethod>
                              <icon>/cwt/images/16/notebook_add.png</icon>
                              <width>40px</width>
                            </element>
                            <element name="deleteButton" type="elmnu">
                              <icon>/cwt/images/16/delete.png</icon>
                              <width>40px</width>
                            </element>
                          </elementList>
                        </element>
                        <element name="ItemsListHeaders" type="elgrd">
                          <colWidths>50px</colWidths>
                          <fixedColWidths>true</fixedColWidths>
                          <numOfCols>10</numOfCols>
                          <width>750px</width>
                          <elementList>
                            <element name="Select" type="ellab">
                              <columnSpan>1</columnSpan>
                              <width>30px</width>
                            </element>
                            <element name="DestinationType" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>Operation</label>
                              <width>150px</width>
                            </element>
                            <element name="Numbering" type="ellab">
                              <columnSpan>3</columnSpan>
                              <label>Interval</label>
                              <width>150px</width>
                            </element>
                            <element name="MaximumMembers" type="ellab">
                              <columnSpan>3</columnSpan>
                              <label>Interval Unit</label>
                              <width>150px</width>
                            </element>
                          </elementList>
                        </element>
                        <element name="ItemsListIterator" type="eliter">
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.PreviousOperationApplicabilForm/leaf_simpleIterator</variablePath>
                            </variablePathList>
                          </variable>
                          <elementList>
                            <element name="Items" type="elhlt">
                              <width>750px</width>
                              <elementList>
                                <element name="ItemsFormFrame" type="elovf">
                                  <fillSpace>false</fillSpace>
                                  <formReference type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.PreviousOperationApplicabilForm/leaf_simpleIterator</variablePath>
                                      <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                      <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                    </variablePathList>
                                  </formReference>
                                </element>
                              </elementList>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.PreviousOperationApplicabilForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        //return cm_spoc.initPreviousOperationApplicability(this.model);

        var m = cm_spoc.initPreviousOperationApplicability(this.model);
        cm_spoc.initPreviousOperationUI(this,m);
        return m;
      ]]></script>
    </method>
    <method name="addItemsList" type="uimthd">
      <script><![CDATA[
        var model = this.model.POASimpleOpAppArrDS.ListDS;

        cm_spoc.addListElement(this, model ,"cm_spoc.forms.PreviousOperationIteratorUI","base_SimpleOperationApplicability","simpleIterator");
      ]]></script>
    </method>
  </methodList>
</userInterface>