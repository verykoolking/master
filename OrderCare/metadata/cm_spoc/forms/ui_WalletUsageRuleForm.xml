<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.WalletUsageRuleForm">
  <group>Wallets</group>
  <label>WalletUsageRuleForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="SimpleWalletUsageRuleIterator" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.WalletUsageRule</valueType>
    </variable>
    <variable name="SelectAll" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="VerticalLayout" type="elvlt">
              <width>1050px</width>
              <elementList>
                <element name="SectionStack" type="elsectnstck">
                  <width>1100px</width>
                  <elementList>
                    <element name="WalletUsageRuleSection" type="elsect">
                      <elementList>
                        <element name="WalletUsageRuleHeader" type="elsecth"/>
                        <element name="VerticalLayout1" type="elvlt">
                          <alignment>right,center</alignment>
                          <elementList>
                            <element name="RuleIdentifier" type="elfld">
                              <editable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WalletUsageRuleForm/permMethod_enabledPrimaryKeyFields</variablePath>
                                </variablePathList>
                              </editable>
                              <labeAlign>left</labeAlign>
                              <label>Rule Identifier</label>
                              <labelOrientation>left</labelOrientation>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WalletUsageRuleForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.WalletUsageRule/dstruct_RuleIdentifier</variablePath>
                                </variablePathList>
                              </variable>
                            </element>
                            <element name="RuleDescription" type="elfld">
                              <labeAlign>left</labeAlign>
                              <label>Rule Description</label>
                              <labelOrientation>left</labelOrientation>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WalletUsageRuleForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.WalletUsageRule/dstruct_RuleDescription</variablePath>
                                </variablePathList>
                              </variable>
                            </element>
                            <element name="ConditionType" type="elslct">
                              <labeAlign>left</labeAlign>
                              <label>Condition Type</label>
                              <labelOrientation>left</labelOrientation>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WalletUsageRuleForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.WalletUsageRule/dstruct_ConditionType</variablePath>
                                </variablePathList>
                              </variable>
                            </element>
                            <element name="LayoutSpacer" type="ellsp">
                              <height>15px</height>
                            </element>
                          </elementList>
                        </element>
                        <element name="buttonLayout" type="elhlt">
                          <elementList>
                            <element name="addButton" type="elmnu">
                              <clickMethod type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WalletUsageRuleForm/uimthd_addSimpleWalletUsageRule</variablePath>
                                </variablePathList>
                              </clickMethod>
                              <icon>/cwt/images/16/notebook_add.png</icon>
                            </element>
                            <element name="deleteButton" type="elmnu">
                              <clickMethod type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WalletUsageRuleForm/uimthd_deleteSimpleWalletUsageRule</variablePath>
                                </variablePathList>
                              </clickMethod>
                              <icon>/cwt/images/16/notebook_delete.png</icon>
                            </element>
                          </elementList>
                        </element>
                        <element name="GridLayout" type="elgrd">
                          <colWidths>50px</colWidths>
                          <fixedColWidths>true</fixedColWidths>
                          <numOfCols>22</numOfCols>
                          <width>1100px</width>
                          <elementList>
                            <element name="SelectAll" type="elchk">
                              <columnSpan>1</columnSpan>
                              <showLabel>false</showLabel>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WalletUsageRuleForm/leaf_SelectAll</variablePath>
                                </variablePathList>
                              </variable>
                              <width>30px</width>
                            </element>
                            <element name="TribeCategory" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>Tribe Category</label>
                              <width>100px</width>
                            </element>
                            <element name="List" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>List</label>
                              <width>100px</width>
                            </element>
                            <element name="LongDistance" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>Long Distance</label>
                              <width>100px</width>
                            </element>
                            <element name="Operation" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>Operation</label>
                              <width>100px</width>
                            </element>
                            <element name="TariffPlan" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>Tariff Plan</label>
                              <width>100px</width>
                            </element>
                            <element name="Processes" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>Processes</label>
                              <width>100px</width>
                            </element>
                            <element name="RoamingZone" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>Roaming Zone</label>
                              <width>100px</width>
                            </element>
                            <element name="DestinationType" type="ellab">
                              <columnSpan>3</columnSpan>
                              <label>Destination Type</label>
                              <width>150px</width>
                            </element>
                            <element name="UnitsType" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>Units Type</label>
                              <width>100px</width>
                            </element>
                            <element name="Tribe" type="ellab">
                              <columnSpan>2</columnSpan>
                              <label>Tribe</label>
                              <width>100px</width>
                            </element>
                          </elementList>
                        </element>
                        <element name="UsageRuleVL" type="elvlt">
                          <height>200px</height>
                          <width>1100px</width>
                          <elementList>
                            <element name="UsageRuleIterator" type="eliter">
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.WalletUsageRuleForm/leaf_SimpleWalletUsageRuleIterator</variablePath>
                                </variablePathList>
                              </variable>
                              <elementList>
                                <element name="UsageRuleHL" type="elhlt">
                                  <width>1100px</width>
                                  <elementList>
                                    <element name="UsageRuleFormFrame" type="elovf">
                                      <formReference type="varPath">
                                        <variablePathList>
                                          <variablePath>ui_cm_spoc.forms.WalletUsageRuleForm/leaf_SimpleWalletUsageRuleIterator</variablePath>
                                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                        </variablePathList>
                                      </formReference>
                                    </element>
                                  </elementList>
                                </element>
                              </elementList>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafTriggerAction$SelectAll" type="laction">
      <path>ui_cm_spoc.forms.WalletUsageRuleForm/leaf_SelectAll</path>
      <type>trigger</type>
      <script><![CDATA[
        for (var i = 0;i<this.SimpleWalletUsageRuleIterator.length; i++){
                this.SimpleWalletUsageRuleIterator[i].Selected = this.SelectAll;
            }
      ]]></script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.WalletUsageRuleForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.WalletUsageRuleForm/leaf_model</valueType>
      <script><![CDATA[
        var m = cm_spoc.initWalletUsageRule(this.model);
        cm_spoc.initWalletUsageRuleUI(this);
        return m;
      ]]></script>
    </method>
    <method name="addSimpleWalletUsageRule" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <type>ui_com.conceptwave.system.UserInterface</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        var model=this.model.WURSWalletUsageRuleArrDS.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.SimpleWalletUsageRuleIteratorUI","base_SimpleWalletUsageRule","SimpleWalletUsageRuleIterator");
      ]]></script>
    </method>
    <method name="deleteSimpleWalletUsageRule" type="uimthd">
      <script><![CDATA[
        var model = this.model.WURSWalletUsageRuleArrDS;
        cm_spoc.removeListElements(this , model ,"List", "SimpleWalletUsageRuleIterator");
        this.SelectAll = false;
      ]]></script>
    </method>
    <method name="ReadOnly" type="permMethod">
      <script>return this.model.readonly;</script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i=0;i<this.SimpleWalletUsageRuleIterator.length;i++){
            this.SimpleWalletUsageRuleIterator[i].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
    <method name="enabledPrimaryKeyFields" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
  </methodList>
</userInterface>