        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$tmpPromotionUsingWindowWithRes" type="laction">
      <path>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_tmpPromotionUsingWindowWithRes</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_tmpPromotionUsingWindowWithRes</valueType>
      <script>return this.model.PromotionUsingWindowWithResDS.copy();</script>
    </method>
    <method name="cwLeafInitAction$tmpPromotionUsingWindowWithout" type="laction">
      <path>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_tmpPromotionUsingWindowWithout</path>
      <type>initializer</type>
      <script>return this.model.PromotionUsingWindowWithoutDS.copy();</script>
    </method>
    <method name="PUWWithoutResetVisRule" type="permMethod">
      <script><![CDATA[
        if(!this.model.AccumulationWindowReset){
            this.model.PromotionUsingWindowWithResDS = null ;

           if(this.PromotionUsingWindowWithoutResetForm == null){
                 this.model.PromotionUsingWindowWithoutDS = this.tmpPromotionUsingWindowWithout.copy();
                this.PromotionUsingWindowWithoutResetForm = new cm_spoc.forms.PromotionUsingWindowWithoutForm(this.model.PromotionUsingWindowWithoutDS,this);
           }
            return true;
        }
        this.PromotionUsingWindowWithoutResetForm = null;
        return false;
      ]]></script>
    </method>
    <method name="selectTrigger" type="uimthd">
      <script>return;</script>
    </method>
    <method name="PUWWithResetVisRule" type="permMethod">
      <script><![CDATA[
        if(this.model.AccumulationWindowReset){
            this.model.PromotionUsingWindowWithoutDS = null;

            if(this.PromotionUsingWindowWithResForm == null){
                this.model.PromotionUsingWindowWithResDS = this.tmpPromotionUsingWindowWithRes.copy();
                this.PromotionUsingWindowWithResForm = new cm_spoc.forms.PromotionUsingWindowWithResForm(this.model.PromotionUsingWindowWithResDS,this);
            }
            return true;
        }
        this.PromotionUsingWindowWithResForm = null;
        return false;
      ]]></script>
    </method>
  </methodList>
</userInterface>