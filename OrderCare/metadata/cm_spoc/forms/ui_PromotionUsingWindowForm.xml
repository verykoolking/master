                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_model</variablePath>
                              <variablePath>dstruct_cm_spoc.data.PromotionUsingWindow/dstruct_SizeOfTheAccWindowNumber</variablePath>
                            </variablePathList>
