                    <element name="GridLayout7" type="elgrd">
                      <colWidths>40px</colWidths>
                      <numOfCols>6</numOfCols>
                      <width>240px</width>
                      <elementList>
                        <element name="CellSpacer12" type="elcsp">
                          <columnSpan>1</columnSpan>
                          <width>40px</width>
                        </element>
                        <element name="AccumulationWindowReset" type="elchk">
                          <label>The accumulation window will be reset each time the threshold is reached</label>
                          <labelOrientation>right</labelOrientation>
                          <onChangeMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/uimthd_selectTrigger</variablePath>
                            </variablePathList>
                          </onChangeMethod>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_model</variablePath>
                              <variablePath>dstruct_cm_spoc.data.PromotionUsingWindow/dstruct_AccumulationWindowReset</variablePath>
                            </variablePathList>
                          </variable>
                        </element>
                        <element name="CellSpacer13" type="elcsp">
                          <columnSpan>1</columnSpan>
                          <startRow>true</startRow>
                          <width>40px</width>
                        </element>
                      </elementList>
                    </element>
                    <element name="LayoutSpacer5" type="ellsp">
                      <height>10px</height>
                    </element>
                  </elementList>
                </element>
                <element name="VerticalLayout2" type="elvlt">
                  <alignment>center,center</alignment>
                  <width>1000px</width>
                  <elementList>
                    <element name="SectionStack2" type="elsectnstck">
                      <height>100px</height>
                      <width>600px</width>
                      <elementList>
                        <element name="Section2" type="elsect">
                          <elementList>
                            <element name="SectionHeader1" type="elsecth">
                              <label>It will be counted in top ups accumulation window top ups of type</label>
                            </element>
                            <element name="LayoutSpacer6" type="ellsp">
                              <height>10px</height>
                            </element>
                            <element name="GridLayout8" type="elgrd">
                              <colWidths>20px</colWidths>
                              <numOfCols>6</numOfCols>
                              <width>120px</width>
                              <elementList>
                                <element name="CellSpacer14" type="elcsp">
                                  <columnSpan>1</columnSpan>
                                  <width>20px</width>
                                </element>
                                <element name="NormalTopUpsRC" type="elchk">
                                  <label>Normal top ups (RC)</label>
                                  <labelOrientation>right</labelOrientation>
                                  <variable type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_model</variablePath>
                                      <variablePath>dstruct_cm_spoc.data.PromotionUsingWindow/dstruct_NormalTopUpsRC</variablePath>
                                    </variablePathList>
                                  </variable>
                                </element>
                                <element name="CellSpacer15" type="elcsp">
                                  <columnSpan>1</columnSpan>
                                  <startRow>true</startRow>
                                  <width>20px</width>
                                </element>
                                <element name="MultipleTopUpsRA" type="elchk">
                                  <label>Multiple top ups (RA)</label>
                                  <labelOrientation>right</labelOrientation>
                                  <variable type="varPath">
                                    <variablePathList>
                                      <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_model</variablePath>
                                      <variablePath>dstruct_cm_spoc.data.PromotionUsingWindow/dstruct_MultipleTopUpsRA</variablePath>
                                    </variablePathList>
                                  </variable>
                                </element>
                              </elementList>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                    <element name="LayoutSpacer11" type="ellsp">
                      <height>10px</height>
                    </element>
                  </elementList>
                </element>
                <element name="PromotionUsingWindowWithRes" type="elvlt">
                  <visible type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/permMethod_PUWWithResetVisRule</variablePath>
                    </variablePathList>
                  </visible>
                  <width>1100px</width>
                  <elementList>
                    <element name="LayoutSpacer1" type="ellsp">
                      <height>10px</height>
                    </element>
                    <element name="FormFrame1" type="elovf">
                      <formReference type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_PromotionUsingWindowWithResForm</variablePath>
                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                        </variablePathList>
                      </formReference>
                      <height>200px</height>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_PromotionUsingWindowWithResForm</variablePath>
                        </variablePathList>
                      </variable>
                    </element>
                  </elementList>
                </element>
                <element name="PromotionUsingWindowWithout" type="elvlt">
                  <alignment>center,center</alignment>
                  <visible type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/permMethod_PUWWithoutResetVisRule</variablePath>
                    </variablePathList>
                  </visible>
                  <width>1100px</width>
                  <elementList>
                    <element name="LayoutSpacer" type="ellsp">
                      <height>10px</height>
                    </element>
                    <element name="FormFrame2" type="elovf">
                      <formReference type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_PromotionUsingWindowWithoutResetForm</variablePath>
                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                        </variablePathList>
                      </formReference>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.PromotionUsingWindowForm/leaf_PromotionUsingWindowWithoutResetForm</variablePath>
