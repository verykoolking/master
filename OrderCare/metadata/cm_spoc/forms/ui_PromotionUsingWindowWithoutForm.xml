  <methodList>
    <method name="cwLeafTriggerAction$SelectAll" type="laction">
      <path>ui_cm_spoc.forms.PromotionUsingWindowWithoutForm/leaf_SelectAll</path>
      <type>trigger</type>
      <script><![CDATA[
        for(var i = 0; i<this.PromotionUsingWindowRuleIterator.length;i++){
            this.PromotionUsingWindowRuleIterator[i].Selected = this.SelectAll;
        }
      ]]></script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.PromotionUsingWindowWithoutForm/leaf_model</path>
      <type>initializer</type>
      <script>return cm_spoc.initPromotionUsingWindowWithoutUI(this);</script>
    </method>
    <method name="addPromotionUsingWindowRule" type="uimthd">
      <script><![CDATA[
        var model=this.model.PUWWRPromUsingWRuleArrDS.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.PromotionUsingWindowRuleIteratorUI","base_PromotionUsingWindowRule","PromotionUsingWindowRuleIterator");
      ]]></script>
    </method>
    <method name="deletePromotionUsingWindowRule" type="uimthd">
      <script><![CDATA[
        var model = this.model.PUWWRPromUsingWRuleArrDS;
        cm_spoc.removeListElements(this , model ,"List", "PromotionUsingWindowRuleIterator");
        this.SelectAll = false;
      ]]></script>
    </method>
  </methodList>
</userInterface>