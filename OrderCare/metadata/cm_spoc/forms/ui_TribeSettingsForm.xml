<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.TribeSettingsForm">
  <group>Tribes</group>
  <label>TribeSettingsForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.TribeSettings</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="VerticalLayout" type="elvlt">
              <elementList>
                <element name="SectionStack" type="elsectnstck">
                  <visibilityMode>multiple</visibilityMode>
                  <width>750px</width>
                  <elementList>
                    <element name="SectionCharacteristics" type="elsect">
                      <removeLabelLine>true</removeLabelLine>
                      <elementList>
                        <element name="Tribes" type="elsecth">
                          <label>Tribes</label>
                        </element>
                        <element name="layoutCharacteristics" type="elvlt">
                          <alignment>right,center</alignment>
                          <elementList>
                            <element name="LayoutSpacer" type="ellsp">
                              <height>5px</height>
                            </element>
                            <element name="MaxTribesPerSubscriber" type="elfld">
                              <displayFormat>[1-4]{1}</displayFormat>
                              <labeAlign>left</labeAlign>
                              <label>Maximum Tribes per subscriber</label>
                              <labelOrientation>left</labelOrientation>
                              <textAlign>left</textAlign>
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.TribeSettingsForm/leaf_model</variablePath>
                                  <variablePath>dstruct_cm_spoc.data.TribeSettings/dstruct_MaxTribesPerSubscriber</variablePath>
                                </variablePathList>
                              </variable>
                            </element>
                            <element name="LayoutSpacer2" type="ellsp">
                              <height>5px</height>
                            </element>
                          </elementList>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.TribeSettingsForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        //var m = cm_spoc.initTribeSettings(this.model);
        //return m;
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
  </methodList>
</userInterface>