  <methodList>
    <method name="cwLeafInitAction$PriceEvent" type="laction">
      <path>ui_cm_spoc.forms.RecurrentChargeForm/leaf_PriceEvent</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.RecurrentBoltOnForm/leaf_PriceEvent</valueType>
      <script><![CDATA[return "RecurrentPhase";]]></script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.RecurrentChargeForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initRecurrentCharge(this.model);
        cm_spoc.initRecurrentChargePhaseIteratorUI(this);
        if(m.RecurrentChargeSpecEnum == null){
          m.RecurrentChargeSpecEnum = m.RecurrentChargeSpecificatio;
        }
        return m;
      ]]></script>
    </method>
    <method name="addRecurrentChargePhase" type="uimthd">
      <script><![CDATA[
        var model=this.model.RecurrentChargePhaseArrDS.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.RecurrentChargePhaseIteratorUI",'base_RecurrentChargePhase',"RecurrentChargePhaseIterator");
      ]]></script>
    </method>
    <method name="deleteRecurrentChargePhase" type="uimthd">
      <script><![CDATA[
        this.model.RecurrentChargePhaseArrDS = cm_spoc.removeListElements(this ,this.model.RecurrentChargePhaseArrDS,"List","RecurrentChargePhaseIterator");
      ]]></script>
    </method>
    <method name="FinderChargingModeReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderSelectReference(null,this);
        newDetail.setBaseItemCode("base_ChargingMode");
        newDetail.parentUI = this;
        return newDetail;
      ]]></script>
    </method>
    <method name="referenceCallback" type="uimthd">
      <parameterList>
        <parameter name="itemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
        <parameter name="baseItemCode" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.String</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        switch(baseItemCode){
            case "base_ChargingMode":
                this.model.ChargingModeForCycleChange = itemCode;
                break;
        }
      ]]></script>
    </method>
    <method name="onSelectPriceEventTrigger" type="uimthd">
      <script>return cm_spoc.onSelectPriceEvent(this);</script>
    </method>
    <method name="specSelected" type="usrActionMethod">
      <script>this.model.RecurrentChargeSpecificatio  = this.model.RecurrentChargeSpecEnum;</script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script>return true;</script>
    </method>
    <method name="cwLeafInitAction$PriceGuidingContainerUI" type="laction">
      <path>ui_cm_spoc.forms.RecurrentChargeForm/leaf_PriceGuidingContainerUI</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.BoltOnForm/leaf_PriceGuidingContainerUI</valueType>
      <script>return cm_spoc.initPriceGuidingContainerUI(this);</script>
    </method>
    <method name="cwLeafInitAction$ChargingModeGuidingContainerUI" type="laction">
      <path>ui_cm_spoc.forms.RecurrentChargeForm/leaf_ChargingModeGuidingContainerUI</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.RecurrentTopUpForm/leaf_ChargingModeGuidingContainerUI</valueType>
      <script>return cm_spoc.initChargingModeGuidingContainerUI(this);</script>
    </method>
    <method name="FinderPriceReferenceDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.FinderRecPhasePriceSelectReference(null,this);
        newDetail.setBaseItemCode("RecurrentPhaseP");
        newDetail.parentUI = this;
        newDetail.modelCallback = this.setPrice;
        return newDetail;
      ]]></script>
    </method>
    <method name="setPrice" type="uimthd">
      <parameterList>
        <parameter name="selected" type="rifp">
          <mandatory>true</mandatory>
          <type>nmeta_com.conceptwave.system.Object</type>
        </parameter>
      </parameterList>
      <script>this.parentUI.model.PriceForCycleChange = selected.itemCode;</script>
    </method>
    <method name="openPriceDialog" type="usrActionMethod">
      <dialogHeight>400</dialogHeight>
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <dialogWidth>600</dialogWidth>
      <showInPopup>true</showInPopup>
      <script>return cm_spoc.priceFinderDialog(this);</script>
    </method>
  </methodList>
</userInterface>