                                    <width>800px</width>
                                    <elementList>
                                      <element name="ConditionsFormFrame" type="elovf">
                                        <fillSpace>false</fillSpace>
                                        <formReference type="varPath">
                                          <variablePathList>
                                            <variablePath>ui_cm_spoc.forms.RecurrentChargeForm/leaf_RecurrentChargePhaseIterator</variablePath>
                                            <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                            <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                          </variablePathList>
                                        </formReference>
                                        <width>800px</width>
                                      </element>
                                    </elementList>
                                  </element>
                                </elementList>
                              </element>
                            </elementList>
                          </element>
                        </elementList>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </element>
            </elementList>
          </root>
        </overlay>
        <overlay name="CycleChangeProperties" type="vform">
          <label>CycleChangeProperties</label>
          <root name="VerticalLayout1" type="elvlt">
            <alignment>right,center</alignment>
            <elementList>
              <element name="VerticalLayout" type="elvlt">
                <elementList>
                  <element name="priceButton1" type="elbtn">
                    <canSort>false</canSort>
                    <cellAlignment>center,center</cellAlignment>
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>ui_cm_spoc.forms.RecurrentChargeForm/usrActionMethod_openPriceDialog</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <iconHeight>0</iconHeight>
                    <label>Price</label>
                    <width>70px</width>
