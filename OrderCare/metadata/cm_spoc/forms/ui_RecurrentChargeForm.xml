              <element name="PriceEventEnum" type="elslct">
                <canSort>false</canSort>
                <label>Price Event</label>
                <multiselect>false</multiselect>
                <onChangeMethod type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RecurrentChargeForm/uimthd_onSelectPriceEventTrigger</variablePath>
                  </variablePathList>
                </onChangeMethod>
                <showEmpty>false</showEmpty>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>ui_cm_spoc.forms.RecurrentChargeForm/leaf_model</variablePath>
                    <variablePath>dstruct_cm_spoc.data.RecurrentCharge/dstruct_PGRecurrentChargeArrDS</variablePath>
                    <variablePath>dstruct_cm_spoc.data.PGRecurrentChargeArr/dstruct_PriceEventEnum</variablePath>
