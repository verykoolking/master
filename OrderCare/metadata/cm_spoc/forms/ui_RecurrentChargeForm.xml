                          <element name="ConditionsVL" type="elvlt">
                            <height>200px</height>
                            <overflow>auto</overflow>
                            <width>800px</width>
                            <elementList>
                              <element name="ConditionsIterator" type="eliter">
                                <variable type="varPath">
                                  <variablePathList>
                                    <variablePath>ui_cm_spoc.forms.RecurrentChargeForm/leaf_RecurrentChargePhaseIterator</variablePath>
                                  </variablePathList>
                                </variable>
                                <elementList>
