    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.RecurrentBoltOnForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.RecurrentBoltOnForm/leaf_model</valueType>
      <script><![CDATA[
        var m = cm_spoc.initRecurrentBoltOn(this.model);
        cm_spoc.initBoltOnInPhasesIteratiorUI(this);
        if(m.RecurrentBoltOnSpecEnum == null)
        {
            m.RecurrentBoltOnSpecEnum = m.RecurrentBoltOnSpecificatio;
        }
        if(m.BoltOnOrMixedBoltOnEnum == null)
        {
            if(m.BoltOn != null){
               m.BoltOnOrMixedBoltOnEnum = "BoltOn";
               m.BoltOnEnum = m.BoltOn;
            }else{
                if(m.MixedBoltOn != null){
                   m.BoltOnOrMixedBoltOnEnum = "MixedBoltOn";
                   m.MixedBoltOnEnum = m.MixedBoltOn;
                }
            }
        }
        return m;
      ]]></script>
    </method>
