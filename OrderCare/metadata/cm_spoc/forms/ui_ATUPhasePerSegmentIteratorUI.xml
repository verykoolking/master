<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.ATUPhasePerSegmentIteratorUI">
  <group>Promotion</group>
  <label>ATUPhasePerSegmentIteratorUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.ATUPhasePerSegment</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="ATUPhasePerSegmentIteratorUIVL" type="elvlt">
              <alignment>center,center</alignment>
              <width>1000px</width>
              <elementList>
                <element name="ATUPhasePerSegmentIteratorGridLayout" type="elgrd">
                  <alignment>center,center</alignment>
                  <colWidths>50px</colWidths>
                  <fixedColWidths>true</fixedColWidths>
                  <numOfCols>14</numOfCols>
                  <width>1000px</width>
                  <elementList>
                    <element name="Selected" type="elchk">
                      <columnSpan>1</columnSpan>
                      <editable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUPhasePerSegmentIteratorUI/permMethod_readOnly</variablePath>
                        </variablePathList>
                      </editable>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUPhasePerSegmentIteratorUI/leaf_Selected</variablePath>
                        </variablePathList>
                      </variable>
                      <width>30px</width>
                    </element>
                    <element name="AwardedPromotionSegmentEnum" type="elslct">
                      <columnSpan>2</columnSpan>
                      <label>Min Top Up Threshold</label>
                      <multiselect>false</multiselect>
                      <removeLabelLine>true</removeLabelLine>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUPhasePerSegmentIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUPhasePerSegment/dstruct_AwardedPromotionSegmentEnum</variablePath>
                        </variablePathList>
                      </variable>
                      <width>100px</width>
                    </element>
                    <element name="RPMinTopUpThreshold" type="elfld">
                      <autoFocusNextField>false</autoFocusNextField>
                      <columnSpan>2</columnSpan>
                      <label>Min Top Up Threshold</label>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUPhasePerSegmentIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUPhasePerSegment/dstruct_RPMinTopUpThreshold</variablePath>
                        </variablePathList>
                      </variable>
                      <width>100px</width>
                    </element>
                    <element name="RPMaxTopUpThreshold" type="elfld">
                      <autoFocusNextField>false</autoFocusNextField>
                      <columnSpan>2</columnSpan>
                      <label>Max Top Up Threshold</label>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUPhasePerSegmentIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUPhasePerSegment/dstruct_RPMaxTopUpThreshold</variablePath>
                        </variablePathList>
                      </variable>
                      <width>100px</width>
                    </element>
                    <element name="RPTopUpAmountEachExecution" type="elfld">
                      <autoFocusNextField>false</autoFocusNextField>
                      <columnSpan>2</columnSpan>
                      <label>Top Up Amount In Each Execution</label>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUPhasePerSegmentIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUPhasePerSegment/dstruct_RPTopUpAmountEachExecution</variablePath>
                        </variablePathList>
                      </variable>
                      <width>100px</width>
                    </element>
                    <element name="RPPercentageOverTopUpAmount" type="elchk">
                      <cellAlignment>center,center</cellAlignment>
                      <columnSpan>2</columnSpan>
                      <label>Percentage Over Top Up Amount</label>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUPhasePerSegmentIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUPhasePerSegment/dstruct_RPPercentageOverTopUpAmount</variablePath>
                        </variablePathList>
                      </variable>
                      <width>100px</width>
                    </element>
                    <element name="BoltOnEnum" type="elslct">
                      <columnSpan>1</columnSpan>
                      <label>Bolt On</label>
                      <multiselect>false</multiselect>
                      <removeLabelLine>true</removeLabelLine>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUPhasePerSegmentIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUPhasePerSegment/dstruct_BoltOnEnum</variablePath>
                        </variablePathList>
                      </variable>
                      <width>100px</width>
                    </element>
                    <element name="OriginDestination" type="elbtn">
                      <columnSpan>2</columnSpan>
                      <iconHeight>0</iconHeight>
                      <iconWidth>0</iconWidth>
                      <label>Origin - Destination</label>
                      <width>100px</width>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="setBoltOnTrigger" type="uimthd">
      <script>this.model.BoltOn = this.model.BoltOnEnum;</script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="setAwardedPromotionSegmentTrigger" type="uimthd">
      <script>this.model.AwardedPromotionSegment = this.model.AwardedPromotionSegmentEnum;</script>
    </method>
  </methodList>
</userInterface>