  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.LifeCycleInternalSubstatusForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initLifeCycleInternalSubstatus(this.model);
        //cm_spoc.initCALifeCycleUI(this);
        return m;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$LifeCyclePickList" type="laction">
      <path>ui_cm_spoc.forms.LifeCycleInternalSubstatusForm/leaf_LifeCyclePickList</path>
      <type>initializer</type>
      <script><![CDATA[
        var pl = new cm_spoc.PickList(null,this);
        pl.availableItems = new DataObjectList();

        var itemFinder = new Finder("cm_spoc.lifeCycleFinder");
        var result = itemFinder.search();

        for(var i=0;i<result.length;i++){
            var item = new cm_spoc.PickListDS();
            item.id = result[i].itemCode;
            item.description = result[i].itemCode;
            pl.availableItems.addLast(item);
        }

        pl.modelCallback = this.setPickListModel;
        return pl;
      ]]></script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="setPickListModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.LCSSLifeCycleArrDS.List = null;
        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.LCSSLifeCycleArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="LifeCycleSubStatusTrigger" type="uimthd">
      <script>this.model.LifeCycleSubStatus=this.model.LifeCycleSubStatusEnum;</script>
    </method>
    <method name="deleteModelAction" type="usrActionMethod">
      <script><![CDATA[
        try{
                var project = cwa_epc.getDefaultProj();
                var response=cm_common.plmRemoveItem(this.model,project);
                if(response instanceof api_common.data.fault)
                     this.setModelStatus(response);
                else{
                    this.cancelAction();
                    this.parent.parent.model.searchDocument.specification.specificationCode=null;
                    this.setModelStatus(null,"CWA_EPC_REMOVE_002",true, this.model.metadataTypeLabel);
                    this.parent.parent.resultUI.result=this.parent.parent.model.search();
                }

        }catch(ex){
            var fault = api_common.createException(ex, "COM_ERR_0001");
            this.setModelStatus(fault);
        }
      ]]></script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
  </methodList>
</userInterface>