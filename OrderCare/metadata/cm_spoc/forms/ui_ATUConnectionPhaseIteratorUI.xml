<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.ATUConnectionPhaseIteratorUI">
  <group>Promotion</group>
  <label>ATUConnectionPhaseIteratorUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.ATUConnectionPhase</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="ATUConnectionPhaseIteratorUIVL" type="elvlt">
              <alignment>center,center</alignment>
              <width>1000px</width>
              <elementList>
                <element name="ATUConnectionPhaseIteratorGridLayout" type="elgrd">
                  <alignment>center,center</alignment>
                  <colWidths>50px</colWidths>
                  <fixedColWidths>true</fixedColWidths>
                  <numOfCols>12</numOfCols>
                  <width>1000px</width>
                  <elementList>
                    <element name="Selected" type="elchk">
                      <columnSpan>1</columnSpan>
                      <editable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUConnectionPhaseIteratorUI/permMethod_readOnly</variablePath>
                        </variablePathList>
                      </editable>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUConnectionPhaseIteratorUI/leaf_Selected</variablePath>
                        </variablePathList>
                      </variable>
                      <width>30px</width>
                    </element>
                    <element name="CPMinTopUpThreshold" type="elfld">
                      <autoFocusNextField>false</autoFocusNextField>
                      <columnSpan>2</columnSpan>
                      <label>Min Top Up Threshold</label>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUConnectionPhaseIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUConnectionPhase/dstruct_CPMinTopUpThreshold</variablePath>
                        </variablePathList>
                      </variable>
                      <width>140px</width>
                    </element>
                    <element name="CPMaxTopUpThreshold" type="elfld">
                      <autoFocusNextField>false</autoFocusNextField>
                      <columnSpan>2</columnSpan>
                      <label>Max Top Up Threshold</label>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUConnectionPhaseIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUConnectionPhase/dstruct_CPMaxTopUpThreshold</variablePath>
                        </variablePathList>
                      </variable>
                      <width>140px</width>
                    </element>
                    <element name="CPTopUpAmountEachExecution" type="elfld">
                      <autoFocusNextField>false</autoFocusNextField>
                      <columnSpan>2</columnSpan>
                      <label>Top Up Amount In Each Execution</label>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUConnectionPhaseIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUConnectionPhase/dstruct_CPTopUpAmountEachExecution</variablePath>
                        </variablePathList>
                      </variable>
                      <width>190px</width>
                    </element>
                    <element name="CPPercentageOverTopUpAmount" type="elchk">
                      <cellAlignment>center,center</cellAlignment>
                      <columnSpan>2</columnSpan>
                      <label>Percentage Over Top Up Amount</label>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUConnectionPhaseIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUConnectionPhase/dstruct_CPPercentageOverTopUpAmount</variablePath>
                        </variablePathList>
                      </variable>
                      <width>190px</width>
                    </element>
                    <element name="BoltOnEnum" type="elslct">
                      <columnSpan>1</columnSpan>
                      <label>Bolt On</label>
                      <multiselect>false</multiselect>
                      <removeLabelLine>true</removeLabelLine>
                      <showLabel>false</showLabel>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUConnectionPhaseIteratorUI/leaf_model</variablePath>
                          <variablePath>dstruct_cm_spoc.data.ATUConnectionPhase/dstruct_BoltOnEnum</variablePath>
                        </variablePathList>
                      </variable>
                      <width>80px</width>
                    </element>
                    <element name="OriginDestination" type="elbtn">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_cm_spoc.forms.ATUConnectionPhaseIteratorUI/usrActionMethod_openTopUpOriginDestinationPairContainerUI</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <columnSpan>2</columnSpan>
                      <iconHeight>0</iconHeight>
                      <iconWidth>0</iconWidth>
                      <label>Top Up Origin - Top Up Destination</label>
                      <width>190px</width>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="setDestinationType" type="uimthd">
      <script>this.model.BoltOn = this.model.BoltOnEnum;</script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="openTopUpOriginDestinationPairContainerUI" type="usrActionMethod">
      <dialogHeight>400</dialogHeight>
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <dialogWidth>600</dialogWidth>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var TopUpOriginDestinationPairContainerUI = new cm_spoc.forms.TopUpOriginDestinationPairContainerUI(this.model.ATUCPhaseTUODestPairArrDS,this);

        return TopUpOriginDestinationPairContainerUI;
      ]]></script>
    </method>
  </methodList>
</userInterface>