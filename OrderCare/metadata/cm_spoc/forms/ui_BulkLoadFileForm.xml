  <methodList>
    <method name="cwLeafInitAction$cwShowHover" type="laction">
      <path>ui_com.conceptwave.system.UserInterface/leaf_cwShowHover</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initBulkLoadFiles(this.model);
        cm_spoc.initBulkLoadFilesUi(this);
        return m;
      ]]></script>
    </method>
    <method name="TariffPlanEnumTrigger" type="uimthd">
      <script>this.model.TariffPlan = this.model.TariffPlanEnum;</script>
    </method>
    <method name="ReSubscriberProfileCodeTrigger" type="uimthd">
      <script>this.model.ReSubscriberProfileCode = this.model.ReSubscriberProfileCodeEnum;</script>
    </method>
    <method name="SaleModalityCodeTrigger" type="uimthd">
      <script>this.model.SaleModalityCode = this.model.SaleModalityCodeEnum;</script>
    </method>
    <method name="SubscriptionTypeTrigger" type="uimthd">
      <script>this.model.SubscriptionType = this.model.SubscriptionTypeEnum;</script>
    </method>
    <method name="SubstatusCodeTrigger" type="uimthd">
      <script>this.model.SubstatusCode = this.model.SubstatusCodeEnum;</script>
    </method>
    <method name="ActivationFlagVisibility" type="permMethod">
      <script><![CDATA[
        var value = this.model.UnblokFlag;
        if(value==true) return true;
        else return false;
      ]]></script>
    </method>
    <method name="onSettingUnblokFlag" type="uimthd">
      <script><![CDATA[
        var value = this.model.UnblokFlag;
        if(value==true)
           this.model.ActivationFlag = null;
        return;
      ]]></script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
