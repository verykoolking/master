<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.SubscriptionLimitsUI">
  <group>BoltOn</group>
  <label>SubscriptionLimitsUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.SubscriptionLimit</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elgrd">
              <colWidths>50px</colWidths>
              <fixedColWidths>true</fixedColWidths>
              <numOfCols>15</numOfCols>
              <width>850px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <columnSpan>1</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>30px</width>
                </element>
                <element name="MaxSubscriptions" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>2</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.SubscriptionLimit/dstruct_MaxSubscriptions</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="Period" type="elslct">
                  <columnSpan>2</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <removeLabelLine>true</removeLabelLine>
                  <showEmpty>false</showEmpty>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.SubscriptionLimit/dstruct_Period</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="NaturalPeriodEnum" type="elslct">
                  <columnSpan>2</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <label>Period Type</label>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/uimthd_setNaturalPeriod</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <removeLabelLine>true</removeLabelLine>
                  <showEmpty>false</showEmpty>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.SubscriptionLimit/dstruct_NaturalPeriodEnum</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="CountingOnlyActivesEnum" type="elslct">
                  <columnSpan>3</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <label>Activity flag</label>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/uimthd_setCountingOnlyActives</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <removeLabelLine>true</removeLabelLine>
                  <showEmpty>false</showEmpty>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.SubscriptionLimit/dstruct_CountingOnlyActivesEnum</variablePath>
                    </variablePathList>
                  </variable>
                  <width>150px</width>
                </element>
                <element name="OperationalProfile" type="elslct">
                  <columnSpan>3</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <label>Operational Profile</label>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.SubscriptionLimit/dstruct_CustomerProfile</variablePath>
                    </variablePathList>
                  </variable>
                  <width>150px</width>
                </element>
                <element name="SubscriptionEach" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>2</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/uimthd_selectTrigger</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.SubscriptionLimit/dstruct_SubscriptionEach</variablePath>
                    </variablePathList>
                  </variable>
                  <visible type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SubscriptionLimitsUI/permMethod_SLSubscriptionEachValueVisRule</variablePath>
                    </variablePathList>
                  </visible>
                  <width>100px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="setNaturalPeriod" type="uimthd">
      <script><![CDATA[
        if(this.model.NaturalPeriodEnum=="true")
            this.model.NaturalPeriod=true;
        else  this.model.NaturalPeriod=false;
        this.model.SubscriptionEach="";
      ]]></script>
    </method>
    <method name="setCountingOnlyActives" type="uimthd">
      <script><![CDATA[
        if(this.model.CountingOnlyActivesEnum=="true") this.model.CountingOnlyActives=true;
        else this.model.CountingOnlyActives=false;
        return;
      ]]></script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="SLSubscriptionEachValueVisRule" type="permMethod">
      <script><![CDATA[
        var value=this.model.NaturalPeriod;
        if(value==false) return true;
        return false;
      ]]></script>
    </method>
    <method name="selectTrigger" type="uimthd">
      <script>return;</script>
    </method>
  </methodList>
</userInterface>