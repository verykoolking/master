<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.ChargingModeGuidingRuleUI">
  <group>Common.ChargingModeGuiding</group>
  <label>ChargingModeGuidingRuleUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.ChargingModeGuiding</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="HorizontalLayout" type="elgrd">
              <alignment>center,center</alignment>
              <colWidths>100px</colWidths>
              <numOfCols>8</numOfCols>
              <elementList>
                <element name="Selected" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>0</columnSpan>
                  <labelOrientation>top</labelOrientation>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ChargingModeGuidingRuleUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="Mode" type="elslct">
                  <columnSpan>0</columnSpan>
                  <label>Mode</label>
                  <showEmpty>false</showEmpty>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ChargingModeGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.ChargingModeGuiding/dstruct_Mode</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="OperationEnum" type="elslct">
                  <columnSpan>0</columnSpan>
                  <label>Operation</label>
                  <multiselect>false</multiselect>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ChargingModeGuidingRuleUI/uimthd_operationSelectChange</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ChargingModeGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.ChargingModeGuiding/dstruct_OperationEnum</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="ChargingModeEnum" type="elslct">
                  <columnSpan>0</columnSpan>
                  <label>chargingMode</label>
                  <multiselect>false</multiselect>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ChargingModeGuidingRuleUI/uimthd_chargingModeSelectChange</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ChargingModeGuidingRuleUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.ChargingModeGuiding/dstruct_ChargingModeEnum</variablePath>
                    </variablePathList>
                  </variable>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="operationSelectChange" type="uimthd">
      <script>this.model.Operation = this.model.OperationEnum;</script>
    </method>
    <method name="chargingModeSelectChange" type="uimthd">
      <script>this.model.ChargingMode = this.model.ChargingModeEnum;</script>
    </method>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
  </methodList>
</userInterface>