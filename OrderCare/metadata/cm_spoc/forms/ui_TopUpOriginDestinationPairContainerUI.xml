    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="TopUpOriginDestinationPairVL" type="elvlt">
              <fillSpace>false</fillSpace>
              <width>500px</width>
              <elementList>
                <element name="TopUpOriginDestinationPairSectionStack" type="elsectnstck">
                  <visibilityMode>multiple</visibilityMode>
                  <width>500px</width>
                  <elementList>
                    <element name="TopUpOriginDestinationPairSection" type="elsect">
                      <removeLabelLine>true</removeLabelLine>
                      <elementList>
                        <element name="TopUpOriginDestinationPairHeader" type="elsecth">
                          <label>Top Up Origin - Top Up Destination Pair</label>
                        </element>
                        <element name="TopUpOriginDestinationPairLayout" type="elhlt">
                          <width>500px</width>
                          <elementList>
                            <element name="addButton1" type="elmnu">
                              <menuAccel type="mAccelP"/>
                              <clickMethod type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.TopUpOriginDestinationPairContainerUI/uimthd_addTopUpOriginDestinationIteratorRule</variablePath>
                                </variablePathList>
                              </clickMethod>
                              <icon>/cwt/images/16/notebook_add.png</icon>
                              <width>40px</width>
                            </element>
                            <element name="deleteButton1" type="elmnu">
                              <clickMethod type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.TopUpOriginDestinationPairContainerUI/uimthd_deleteTopUpOriginDestinationIteratorRule</variablePath>
                                </variablePathList>
                              </clickMethod>
                              <icon>/cwt/images/16/delete.png</icon>
                              <width>40px</width>
