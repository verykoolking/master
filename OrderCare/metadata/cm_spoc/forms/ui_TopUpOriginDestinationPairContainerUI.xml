                        <element name="TopUpOriginDestinationPairGridLayout" type="elgrd">
                          <alignment>center,center</alignment>
                          <colWidths>50px</colWidths>
                          <fixedColWidths>true</fixedColWidths>
                          <numOfCols>3</numOfCols>
                          <width>500px</width>
                          <elementList>
                            <element name="Select" type="ellab">
                              <label>Select</label>
                              <width>80px</width>
                            </element>
                            <element name="TopUpOrigin" type="ellab">
                              <label>Top Up Origin</label>
                              <tooltipWidth>10</tooltipWidth>
                              <width>100px</width>
                            </element>
                            <element name="TopUpDestination" type="ellab">
                              <columnSpan>0</columnSpan>
                              <label>Top Up Destination</label>
                              <width>100px</width>
                            </element>
                          </elementList>
                        </element>
                        <element name="TopUpOriginDestinationIteratorVL" type="elvlt">
                          <width>500px</width>
                          <elementList>
                            <element name="TopUpOriginDestinationIterator" type="eliter">
                              <variable type="varPath">
                                <variablePathList>
                                  <variablePath>ui_cm_spoc.forms.TopUpOriginDestinationPairContainerUI/leaf_TopUpOriginDestinationIterator</variablePath>
                                </variablePathList>
                              </variable>
                              <elementList>
                                <element name="TopUpOriginDestinationIteratorHL" type="elhlt">
                                  <width>500px</width>
                                  <elementList>
                                    <element name="TopUpOriginDestinationIteratorFormFrame" type="elovf">
                                      <fillSpace>false</fillSpace>
                                      <formReference type="varPath">
                                        <variablePathList>
                                          <variablePath>ui_cm_spoc.forms.TopUpOriginDestinationPairContainerUI/leaf_TopUpOriginDestinationIterator</variablePath>
                                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms</variablePath>
                                          <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</variablePath>
                                        </variablePathList>
