                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="deleteTopUpOriginDestinationIteratorRule" type="uimthd">
      <script><![CDATA[
        var model = this.model;
        cm_spoc.removeListElements(this,model,"List","TopUpOriginDestinationIterator");
      ]]></script>
    </method>
    <method name="addTopUpOriginDestinationIteratorRule" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <type>ui_com.conceptwave.system.UserInterface</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        var model = this.model.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.TopUpOriginDestinationIteratorUI","base_ATUConnectionPhase","TopUpOriginDestinationIterator");
      ]]></script>
    </method>
    <method name="ReadOnly" type="permMethod">
      <script>return this.model.readonly;</script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i=0; i<this.TopUpOriginDestinationIterator.length; i++)
        {
            this.TopUpOriginDestinationIterator[i].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
  </methodList>
</userInterface>