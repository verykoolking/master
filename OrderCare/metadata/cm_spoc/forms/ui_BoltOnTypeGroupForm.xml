  <methodList>
    <method name="cwLeafInitAction$BoltOnTypePL" type="laction">
      <path>ui_cm_spoc.forms.BoltOnTypeGroupForm/leaf_BoltOnTypePL</path>
      <type>initializer</type>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        itemFinder.searchDocument.baseItemCode = "base_BoltOnType";
        var result = itemFinder.search();

        var pl = cm_spoc.initPickList(this,result,this.model.BOTGBoltOnTypeArrDS.List,"itemCode","itemCode");
        pl.modelCallback = this.setBoltOnTypePL;
        return pl;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.BoltOnTypeGroupForm/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.BoltOnTypeGroupForm/leaf_model</valueType>
      <script><![CDATA[
        var m = cm_spoc.initBoltOnTypeGroup(this.model);
        return m;
      ]]></script>
    </method>
    <method name="setBoltOnTypePL" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.BOTGBoltOnTypeArrDS.List = null;
        this.parent.model.BOTGBoltOnTypeArrDS.ListDS = null;
        for(var i=0;i<selectedItems.length;i++){
            this.parent.model.BOTGBoltOnTypeArrDS.List[i] = selectedItems[i].id;
        }
      ]]></script>
    </method>
    <method name="ReadOnly" type="permMethod">
      <script>return this.model.readonly;</script>
