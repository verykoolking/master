<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.RatingTreeForm">
  <group>RatingTree</group>
  <label>RatingTreeForm</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="RTFinder" type="leaf">
      <valueType>findScript_cm_spoc.RTFinder/ui_UserInterface</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.RatingTree</valueType>
    </variable>
    <variable name="table" type="leaf">
      <valueType>ui_com.conceptwave.system.UserInterface</valueType>
    </variable>
    <variable name="nodes" type="leaf">
      <flags enum-type="metadataFlag">ARRAY</flags>
      <valueType>doc_cm_spoc.RatingTreeNodeTypes</valueType>
    </variable>
    <variable name="branchForm" type="leaf">
      <valueType>ui_cm_spoc.forms.RatingTreeBranchForm</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="VerticalLayout" type="elvlt">
              <width>750px</width>
              <elementList>
                <element name="Table_FormFrame" type="elovf">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RatingTreeForm/leaf_RTFinder</variablePath>
                      <variablePath>findScript_cm_spoc.RTFinder/ui_UserInterface/frmui_Forms</variablePath>
                      <variablePath>findScript_cm_spoc.RTFinder/ui_UserInterface/frmui_Forms/exov_Default</variablePath>
                    </variablePathList>
                  </formReference>
                  <height>200px</height>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RatingTreeForm/leaf_RTFinder</variablePath>
                    </variablePathList>
                  </variable>
                  <width>750px</width>
                </element>
                <element name="LayoutSpacer" type="ellsp">
                  <height>40px</height>
                </element>
                <element name="HorizontalLayout" type="elhlt">
                  <elementList>
                    <element name="GridLayout" type="elgrd">
                      <alignment>left,center</alignment>
                      <numOfCols>4</numOfCols>
                      <elementList>
                        <element name="Create" type="elbtn">
                          <clickMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.RatingTreeForm/uimthd_onClickCreate</variablePath>
                            </variablePathList>
                          </clickMethod>
                          <label>Create</label>
                        </element>
                        <element name="Copy" type="elbtn">
                          <clickMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.RatingTreeForm/uimthd_onClickCopy</variablePath>
                            </variablePathList>
                          </clickMethod>
                          <label>Copy</label>
                        </element>
                        <element name="Delete" type="elbtn">
                          <clickMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.RatingTreeForm/uimthd_onClickDelete</variablePath>
                            </variablePathList>
                          </clickMethod>
                          <label>Delete</label>
                        </element>
                      </elementList>
                    </element>
                    <element name="GridLayout1" type="elgrd">
                      <alignment>right,center</alignment>
                      <elementList>
                        <element name="DefaultTariffClass" type="elbtn">
                          <clickMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_cm_spoc.forms.RatingTreeForm/usrActionMethod_openDefaultTariffClassDialog</variablePath>
                            </variablePathList>
                          </clickMethod>
                          <label>Default Tariff Class</label>
                        </element>
                      </elementList>
                    </element>
                  </elementList>
                </element>
                <element name="LayoutSpacer2" type="ellsp">
                  <height>40px</height>
                </element>
                <element name="Branch_FormFrame" type="elovf">
                  <formReference type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RatingTreeForm/leaf_branchForm</variablePath>
                      <variablePath>ui_cm_spoc.forms.RatingTreeBranchForm/frmui_Forms</variablePath>
                      <variablePath>ui_cm_spoc.forms.RatingTreeBranchForm/frmui_Forms/exov_Default</variablePath>
                    </variablePathList>
                  </formReference>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RatingTreeForm/leaf_branchForm</variablePath>
                    </variablePathList>
                  </variable>
                  <width>750px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.RatingTreeForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initRatingTree(this.model);
        cm_spoc.initRatingTreeUI(this,m);

        this.RTFinder = cm_spoc.initRTFinder(this);

        return m;
      ]]></script>
    </method>
    <method name="cwLeafInitAction$nodes" type="laction">
      <path>ui_cm_spoc.forms.RatingTreeForm/leaf_nodes</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.RatingTreeForm/leaf_nodes</valueType>
      <script>return cm_spoc.getAllNodesTypes();</script>
    </method>
    <method name="onClickCreate" type="uimthd">
      <script>cm_spoc.initBranchForm(this);</script>
    </method>
    <method name="setReadOnly" type="uimthd"/>
    <method name="onClickCopy" type="uimthd">
      <script>cm_spoc.copyBranch(this);</script>
    </method>
    <method name="openDefaultTariffClassDialog" type="usrActionMethod">
      <dialogHeight>600</dialogHeight>
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <dialogWidth>460</dialogWidth>
      <showInPopup>true</showInPopup>
      <script>return cm_spoc.editDefaultTariffClassDialog(this);</script>
    </method>
    <method name="onClickDelete" type="uimthd">
      <script>cm_spoc.deleteBranch(this);</script>
    </method>
  </methodList>
</userInterface>