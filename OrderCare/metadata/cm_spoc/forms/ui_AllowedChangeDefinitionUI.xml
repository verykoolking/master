<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.AllowedChangeDefinitionUI">
  <group>TariffPlan</group>
  <label>AllowedChangeDefinitionUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="parentUI" type="leaf">
      <valueType>ui_cm_spoc.forms.TariffPlanForm</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.AllowedChangeDefinition</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elgrd">
              <colWidths>50px</colWidths>
              <numOfCols>20</numOfCols>
              <width>1000px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <columnSpan>1</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>30px</width>
                </element>
                <element name="AllowedThroughIVR" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>3</columnSpan>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/uimthd_selectTrigger</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.AllowedChangeDefinition/dstruct_AllowedThroughIVR</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="AllowedThroughMS" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>3</columnSpan>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/uimthd_selectTrigger</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.AllowedChangeDefinition/dstruct_AllowedThroughMS</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="ScriptHLR" type="elslct">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>3</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/permMethod_OACDScriptHLRVisRule</variablePath>
                    </variablePathList>
                  </editable>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.AllowedChangeDefinition/dstruct_ScriptHLR</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="IsSameTechnology" type="elchk">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>3</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.AllowedChangeDefinition/dstruct_IsSameTechnology</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="TariffPlanAllowedPer" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>3</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                    </variablePathList>
                  </editable>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.AllowedChangeDefinition/dstruct_TariffPlanAllowedPer</variablePath>
                    </variablePathList>
                  </variable>
                </element>
                <element name="Button" type="elbtn">
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/usrActionMethod_TariffPlanPickListDialog</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <columnSpan>1</columnSpan>
                  <disabled type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/leaf_parentUI</variablePath>
                      <variablePath>ui_cm_spoc.forms.TariffPlanForm/permMethod_ReadOnly</variablePath>
                    </variablePathList>
                  </disabled>
                  <icon>/cwf/finder.gif</icon>
                  <width>20px</width>
                </element>
                <element name="CustomerProfile" type="elslct">
                  <cellAlignment>center,center</cellAlignment>
                  <columnSpan>3</columnSpan>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.AllowedChangeDefinitionUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.AllowedChangeDefinition/dstruct_CustomerProfile</variablePath>
                    </variablePathList>
                  </variable>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
    <method name="TariffPlanPickListDialog" type="usrActionMethod">
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <parameterList>
        <parameter name="dialog" type="rifp">
          <type>ui_com.conceptwave.system.Dialog</type>
        </parameter>
      </parameterList>
      <showInPopup>true</showInPopup>
      <script><![CDATA[
        var newDetail = new cm_spoc.forms.TariffPlanAllowedPickList(null,this);
        newDetail.pickList.setParentUI(this);
        newDetail.model=this.model;

        var currentUsedTariffPlan = new Array();

        var list = this.parentUI.model.TPAChangeDefinitionArrDS.ListDS;
        for(var i=0;i<list.length;i++){
            var code = list[i].TariffPlanAllowedPer;

            for(var j=0;j<newDetail.pickList.availableItems.length;j++){
                if(newDetail.pickList.availableItems[j].id.equals(code)){
                    newDetail.pickList.availableItems.remove(newDetail.pickList.availableItems[j]);
                    break;
                }
            }
        }
        newDetail.pickList.selectedItems = new DataObjectList();

        if(this.model.TariffPlanAllowedPer!=null){
            var item = new cm_spoc.PickListDS();
            item.id = this.model.TariffPlanAllowedPer;
            item.description = this.model.TariffPlanAllowedPer;
            newDetail.pickList.selectedItems.addLast(item);
        }
        return newDetail;
      ]]></script>
    </method>
    <method name="setParentUI" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <type>nmeta_com.conceptwave.system.Object</type>
        </parameter>
      </parameterList>
      <script>this.parentUI = ui;</script>
    </method>
    <method name="getParentUI" type="uimthd">
      <script>return this.parentUI;</script>
    </method>
    <method name="OACDScriptHLRVisRule" type="permMethod">
      <script><![CDATA[
        if ( this.model.AllowedThroughMS && this.model.AllowedThroughIVR )
            return true;
        return false;
      ]]></script>
    </method>
    <method name="selectTrigger" type="uimthd">
      <script>return;</script>
    </method>
  </methodList>
</userInterface>