    <method name="setLCPickListModel" type="uimthd">
      <parameterList>
        <parameter name="selectedItems" type="rifp">
          <array>true</array>
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        this.parent.model.LCAdjRuleLifeCycleArrDS.List = null;
        this.parent.model.LCAdjRuleLifeCycleArrDS.ListDS = null;

