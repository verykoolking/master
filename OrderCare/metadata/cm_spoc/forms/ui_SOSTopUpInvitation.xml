    <method name="onSaveButtonClick" type="uimthd">
      <script><![CDATA[
        //
        this.model.SOSTUISOSTUInvTimerArrDS=cm_spoc.initInnerDS("base_SOSTUISOSTUInvTimerArr");

        var s = new epc.data.Specification();
        s.project =  cwa_epc.getDefaultProj();


        for (var i=0; i<this.EventsIterator.length;i++){
            this.model.SOSTUISOSTUInvTimerArrDS.List[i]=this.EventsIterator[i].model.SOSTimerEnum;
             s.specificationCode = this.EventsIterator[i].model.SOSTimerEnum;
             var r = epc.getSpecificationByKey(s);
             if(!api_common.isFault(r)){
                this.model.SOSTUISOSTUInvTimerArrDS.ListDS[i]==r;
             }

            //save SubOperation
            //var s = new epc.data.Specification();
            //s.project =  cwa_epc.getDefaultProj();
            if (this.EventsIterator[i].model.OperationEnumTopUp){
                s.specificationCode = this.EventsIterator[i].model.OperationEnumTopUp;
                this.EventsIterator[i].SubOperation = epc.getSpecificationByKey(s);
                if(api_common.isFault(this.EventsIterator[i].SubOperation))
                    continue;
                //
                this.EventsIterator[i].SubOperation.SOSTopUpInvitationTimer = this.EventsIterator[i].model.SOSTimerEnum;
                cm_spoc.initModelForModification(this.EventsIterator[i].SubOperation);

                cm_common.cm_common.plmUpdateItem(this.EventsIterator[i].SubOperation, s.project);
            }

        }



        /* var project = cwa_epc.getDefaultProj();
         * cm_common.setSpecificationByValue(model, project);
         *
         *
        if (this.model.Message){
            s.specificationCode = this.model.Message;
            var r = epc.getSpecificationByKey(s);
            if(!api_common.isFault(r)){
                this.model.MessageDS=r;
            }
        }else
            this.model.MessageDS = null;
        */
        this.parent.owner.model.TopUpOriginSOSPropertiesDS.SOSTopUpInvitationDS = this.model;
        return this.parent.cwDialogClose();
      ]]></script>
    </method>
