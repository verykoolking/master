    <method name="loadItemsSelectedPreviously" type="uimthd">
      <parameterList>
        <parameter name="pl" type="rifp">
          <type>nmeta_com.conceptwave.system.Object</type>
        </parameter>
        <parameter name="avaiableItems" type="rifp">
          <type>nmeta_com.conceptwave.system.Object</type>
        </parameter>
        <parameter name="selectedItems" type="rifp">
          <type>nmeta_com.conceptwave.system.Object</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        var itemSelected = false;
        for(var i=0;i<avaiableItems.length;i++){
            var item = new cm_spoc.PickListDS();
            item.id = avaiableItems[i].itemCode;
            item.description = avaiableItems[i].itemCode;
            itemSelected = false;
            if(selectedItems.length!=0){
                for(var j=0;j<selectedItems.length;j++)
                {
                    if(selectedItems[j] == item.id){
                        pl.selectedItems.addLast(item);
                        itemSelected = true;
                        break;
                    }
                }
            }
            if(itemSelected == false){
                pl.availableItems.addLast(item);
            }
        }
      ]]></script>
    </method>
