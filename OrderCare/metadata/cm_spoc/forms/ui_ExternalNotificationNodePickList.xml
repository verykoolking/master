    <method name="addToSelected" type="uimthd">
      <script><![CDATA[
        var selected = this.availableItems.selected;
        var priority = this.selectedItems.length;
        if(selected != null){
            for(var i=0;i<selected.length;i++){
                selected[i].priority=this.selectedItems.length+1;
                this.selectedItems.addLast(selected[i]);
                this.selectedItemsNoFilter.addLast(selected[i]);
                this.availableItems.remove(selected[i]);
                this.availableItemsNoFilter.remove(selected[i]);
            }
            this.selectedItems.updateList;
        }
        this.availableItems.updateList;

        if(this.modelCallback) this.modelCallback(this.selectedItems);
      ]]></script>
    </method>
    <method name="removeFromSelected" type="uimthd">
      <script><![CDATA[
        var selected = this.selectedItems.selected;
        if(selected != null){
            for(var i=0;i<selected.length;i++){
                this.availableItems.addLast(selected[i]);
                this.availableItemsNoFilter.addLast(selected[i]);
                this.selectedItems.remove(selected[i]);
                this.selectedItemsNoFilter.remove(selected[i]);
            }

            this.availableItems.updateList;
            this.selectedItems.updateList;
        }
        this.refreshPriorityValues();
        if(this.modelCallback) this.modelCallback(this.selectedItems);
      ]]></script>
    </method>
    <method name="addAllToSelected" type="uimthd">
      <script><![CDATA[
        this.availableItems.selectAllRecords();
        this.addToSelected();
        this.refreshPriorityValues();
      ]]></script>
    </method>
    <method name="removeAllFromSelected" type="uimthd">
      <script><![CDATA[
        this.selectedItems.selectAllRecords();
        this.removeFromSelected();
      ]]></script>
    </method>
    <method name="PriorityButtonsVisible" type="permMethod">
      <script><![CDATA[
        if(this.priorityButtonsEnabled==true){
            return true;
        }
        // false - if it is null or false
        return false;
      ]]></script>
    </method>
    <method name="clickArrowUp" type="uimthd">
      <script><![CDATA[
        var pos = this.getSingleSelectedIndex(); // Index of the selected item

        if(pos>0){
            this.changeItemPos(this.selectedItems[pos], pos-1);
            this.changeItemPosNoFilter(this.selectedItemsNoFilter[pos], pos-1);
        }
      ]]></script>
    </method>
    <method name="clickArrowUpTop" type="uimthd">
      <script><![CDATA[
        var pos = this.getSingleSelectedIndex(); // Index of the selected item

        if(pos>0){
            this.changeItemPos(this.selectedItems[pos], 0);
            this.changeItemPosNoFilter(this.selectedItemsNoFilter[pos], 0);
        }
      ]]></script>
    </method>
    <method name="clickArrowDownBottom" type="uimthd">
      <script><![CDATA[
        var pos = this.getSingleSelectedIndex(); // Index of the selected item

        if(pos>=0 && pos<this.selectedItems.length-1){
            this.changeItemPos(this.selectedItems[pos], this.selectedItems.length-1);
            this.changeItemPosNoFilter(this.selectedItemsNoFilter[pos], this.selectedItemsNoFilter.length-1);
        }
      ]]></script>
    </method>
    <method name="clickArrowDown" type="uimthd">
      <script><![CDATA[
        var pos = this.getSingleSelectedIndex(); // Index of the selected item

        if(pos>=0 && pos<this.selectedItems.length-1){
            this.changeItemPos(this.selectedItems[pos], pos+1);
            this.changeItemPosNoFilter(this.selectedItemsNoFilter[pos], pos+1);
        }
      ]]></script>
    </method>
    <method name="PriorityButtonsDisable" type="permMethod">
      <script><![CDATA[
        // Priority buttons must be enable only if one row is selected.
        return this.selectedItems.selected==null || this.selectedItems.selected.length!=1;
      ]]></script>
    </method>
    <method name="getSingleSelectedIndex" type="uimthd">
      <script><![CDATA[
        var selected = this.selectedItems.selected;

        if(selected!=null && selected.length==1){
            for(var i=0; i<this.selectedItems.length;i++){
                if(selected[0] == this.selectedItems[i]){
                    return i;
                }
            }
        }

        return -1;
      ]]></script>
    </method>
    <method name="changeItemPos" type="uimthd">
      <parameterList>
        <parameter name="item" type="rifp">
          <mandatory>true</mandatory>
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
        <parameter name="newPos" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.Integer</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        if(newPos>this.selectedItems.length-1){
            newPos=this.selectedItems.length-1;
        }else if(newPos<0){
            newPos = 0;
        }

        this.inputIndex=newPos;
        this.selectedItems.remove(item);
        this.selectedItems.add(newPos,item);
        this.selectedItems.setSelected(item);
        this.selectedItems.updateList;
        this.refreshPriorityValues();
        if(this.modelCallback) this.modelCallback(this.selectedItems);
