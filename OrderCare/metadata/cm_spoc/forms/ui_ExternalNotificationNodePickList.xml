    <method name="orderByPriority" type="uimthd">
      <script><![CDATA[
        if(this.selectedItems.length>0){
            var i=0;
            do{
                var item = this.selectedItems[i];
                if(item.priority!=i){
                    this.selectedItems.remove(item);
                    this.selectedItems.add(item.priority,item);
                }else{
                    i++;
                }
            }while(i<this.selectedItems.length);
        }
      ]]></script>
    </method>
    <method name="filter_applyOnAvailable" type="uimthd">
      <script><![CDATA[
        var textFilter='';
        var textTmp='';
        var pattern=null;
        var id='';
        var description='';

        if(this.TextFilterAvailable==null || this.TextFilterAvailable=='') {
            textFilter = '%'
        } else {
            textFilter = this.TextFilterAvailable;
        }

        for(var i=0;i<textFilter.length;i++){
            if(textFilter[i]=='%'){
                textTmp=textTmp+'.*'; //the % is used like * by the user
            }else {
                textTmp=textTmp + textFilter[i]; //escaping char for each char of the search string
            }
        }

        //clear list
        while(this.availableItems.length>0){
            this.availableItems.delete(0);
        }

        pattern= new RegExp('^'+ textTmp +'$','i'); //casi insensitive match
        for(var i=0;i<this.availableItemsNoFilter.length;i++){
            id = this.availableItemsNoFilter[i].id;
            description = this.availableItemsNoFilter[i].description;
            if(description.match(pattern)!=null) {
                this.availableItems.addLast(this.availableItemsNoFilter[i]);
            }
        }
        this.availableItems.updateList;

        //test external data object list filter
        var dbl = new DataObjectList;
        dbl = cm_spoc.getFilteredDataObject(this.availableItemsNoFilter, 'description', this.TextFilterAvailable);
        dbl=dbl;
      ]]></script>
    </method>
    <method name="filter_clearOnAvailable" type="uimthd">
      <script><![CDATA[
        //clear list
        while(this.availableItems.length>0){
            this.availableItems.delete(0);
        }
        for(var i=0;i<this.availableItemsNoFilter.length;i++){
            this.availableItems.addLast(this.availableItemsNoFilter[i]);
        }
        this.TextFilterAvailable = "";
        this.availableItems.updateList;
      ]]></script>
    </method>
