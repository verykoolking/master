    <method name="changeItemPosNoFilter" type="uimthd">
      <parameterList>
        <parameter name="item" type="rifp">
          <mandatory>true</mandatory>
          <type>dstruct_cm_spoc.PickListDS</type>
        </parameter>
        <parameter name="newPos" type="rifp">
          <mandatory>true</mandatory>
          <type>dtype_com.conceptwave.system.Integer</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        if(newPos>this.selectedItemsNoFilter.length-1){
            newPos=this.selectedItemsNoFilter.length-1;
        }else if(newPos<0){
            newPos = 0;
        }

        this.inputIndex=newPos;
        this.selectedItemsNoFilter.remove(item);
        this.selectedItemsNoFilter.add(newPos,item);
        this.selectedItemsNoFilter.setSelected(item);
        this.selectedItemsNoFilter.updateList;
        this.refreshPriorityValues();
        if(this.modelCallback) this.modelCallback(this.selectedItemsNoFilter);
      ]]></script>
    </method>
