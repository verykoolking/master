<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.ctNttNodeUI">
  <group>RatingTree.NodesUI</group>
  <label>ctNttNodeUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.RatingTreeNodeOutput</valueType>
    </variable>
    <variable name="code" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="nodeType" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="nodeTypeValue" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="description" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="values" type="leaf">
      <valueType>dtype_com.conceptwave.system.String</valueType>
    </variable>
    <variable name="position" type="leaf">
      <valueType>dtype_cwf_up.Integer3</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="HorizontalLayout" type="elhlt">
              <alignment>right,center</alignment>
              <elementList>
                <element name="values" type="elfld">
                  <dynLabel type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ctNttNodeUI/leaf_description</variablePath>
                    </variablePathList>
                  </dynLabel>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                    </variablePathList>
                  </editable>
                  <labeAlign>left</labeAlign>
                  <labelOrientation>left</labelOrientation>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ctNttNodeUI/leaf_values</variablePath>
                    </variablePathList>
                  </variable>
                  <width>200px</width>
                </element>
                <element name="LayoutSpacer" type="ellsp">
                  <width>20px</width>
                </element>
                <element name="edit" type="elimg">
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ctNttNodeUI/usrActionMethod_openEditNodeDialog</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <imageUrl>/cwf/MenuIconEdit.gif</imageUrl>
                  <tooltip>Edit Node</tooltip>
                </element>
                <element name="LayoutSpacer2" type="ellsp">
                  <width>20px</width>
                </element>
                <element name="delete" type="elimg">
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ctNttNodeUI/uimthd_onClickDelete</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <imageUrl>/cwf/MenuIconDelete.gif</imageUrl>
                  <tooltip>Delete Node</tooltip>
                </element>
                <element name="LayoutSpacer1" type="ellsp">
                  <width>20px</width>
                </element>
                <element name="moveup" type="elimg">
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ctNttNodeUI/uimthd_onClickMoveUp</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <imageUrl>/cwf/up.gif</imageUrl>
                </element>
                <element name="movedown" type="elimg">
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.ctNttNodeUI/uimthd_onClickMoveDown</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <imageUrl>/cwf/down.gif</imageUrl>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="onClickDelete" type="uimthd">
      <script>cm_spoc.deleteNode(this);</script>
    </method>
    <method name="onClickEdit" type="uimthd">
      <script>cm_spoc.editNode(this);</script>
    </method>
    <method name="openEditNodeDialog" type="usrActionMethod">
      <dialogHeight>400</dialogHeight>
      <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
      <dialogWidth>600</dialogWidth>
      <showInPopup>true</showInPopup>
      <script>return cm_spoc.editNodeDialog(this);</script>
    </method>
    <method name="onClickMoveUp" type="uimthd">
      <script>cm_spoc.moveUpNode(this);</script>
    </method>
    <method name="onClickMoveDown" type="uimthd">
      <script>cm_spoc.moveDownNode(this);</script>
    </method>
  </methodList>
</userInterface>