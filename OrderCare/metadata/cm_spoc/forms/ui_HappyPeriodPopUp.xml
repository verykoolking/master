    <method name="removeLine" type="uimthd">
      <script><![CDATA[
        var removeFlag= false;
        for(var i=0;i< this.tableIterator.length; i++){
            if(this.tableIterator[i].Selected ==true){ removeFlag= true;}
        }
            if(removeFlag){
                cm_spoc.removeLine(this);

                cm_spoc.flush(this);
            }else{
                //nothing to remove
            }
      ]]></script>
    </method>
