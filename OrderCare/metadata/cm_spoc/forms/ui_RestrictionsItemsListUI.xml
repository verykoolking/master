<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.RestrictionsItemsListUI">
  <group>Lists</group>
  <label>RestrictionsItemsListUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.DestGrAndNumSpec</valueType>
    </variable>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elgrd">
              <colWidths>50px</colWidths>
              <fixedColWidths>true</fixedColWidths>
              <numOfCols>10</numOfCols>
              <width>750px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <columnSpan>1</columnSpan>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RestrictionsItemsListUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>30px</width>
                </element>
                <element name="DestinationTypeEnum" type="elslct">
                  <columnSpan>2</columnSpan>
                  <label>Destination Group Type</label>
                  <multiselect>false</multiselect>
                  <onChangeMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RestrictionsItemsListUI/uimthd_DestTypeGroupIdTrigger</variablePath>
                    </variablePathList>
                  </onChangeMethod>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RestrictionsItemsListUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.DestGrAndNumSpec/dstruct_DestinationGroupTypeEnum</variablePath>
                    </variablePathList>
                  </variable>
                  <width>150px</width>
                </element>
                <element name="Numbering" type="elslct">
                  <columnSpan>3</columnSpan>
                  <label>Numbering</label>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RestrictionsItemsListUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.DestGrAndNumSpec/dstruct_Numbering</variablePath>
                    </variablePathList>
                  </variable>
                  <width>150px</width>
                </element>
                <element name="MaxMembersListWithRestriction" type="elfld">
                  <autoFocusNextField>false</autoFocusNextField>
                  <columnSpan>3</columnSpan>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.RestrictionsItemsListUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.DestGrAndNumSpec/dstruct_MaxMembersListWithRestriction</variablePath>
                    </variablePathList>
                  </variable>
                  <width>150px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="DestTypeGroupIdTrigger" type="uimthd">
      <script>this.model.DestinationTypeGroup = this.model.DestinationGroupTypeEnum;</script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script>return;</script>
    </method>
  </methodList>
</userInterface>