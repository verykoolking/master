  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.AwardedByLastTimeReceivedUI/leaf_model</path>
      <type>initializer</type>
      <valueType>ui_cm_spoc.forms.AwardedByLastTimeReceivedUI/leaf_model</valueType>
      <script><![CDATA[
        var m = cm_spoc.initAwardedByLastTimeReceived(this.model);
        cm_spoc.initAwardedByLastTimeReceivedUI(this);
        return m;
      ]]></script>
    </method>
    <method name="addATUConnectionPhaseIteratorRule" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <type>ui_com.conceptwave.system.UserInterface</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        var model = this.model.ABLTRATUConnectionPhaseArrDS.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.ATUConnectionPhaseIteratorUI","base_ATUConnectionPhase","ATUConnectionPhaseIterator");
      ]]></script>
    </method>
    <method name="deleteATUConnectionPhaseIteratorRule" type="uimthd">
      <script><![CDATA[
        var model = this.model.ABLTRATUConnectionPhaseArrDS;
        cm_spoc.removeListElements(this,model,"List","ATUConnectionPhaseIterator");
      ]]></script>
    </method>
    <method name="ReadOnly" type="permMethod">
      <script>return this.model.readonly;</script>
    </method>
    <method name="setReadOnly" type="uimthd">
      <script><![CDATA[
        for(var i=0; i<this.ATUConnectionPhaseIterator.length; i++)
        {
            this.ATUConnectionPhaseIterator[i].model.readonly = this.model.readonly;
        }

        for(var j=0; j<this.ATUPhasePerSegmentIterator.length; j++)
        {
            this.ATUPhasePerSegmentIterator[j].model.readonly = this.model.readonly;
        }
      ]]></script>
    </method>
    <method name="enabledPrimaryKeyFields" type="permMethod">
      <script>return !this.parent.parentFinderUI.isStored;</script>
    </method>
    <method name="addATUPhasePerSegmentIteratorRule" type="uimthd">
      <parameterList>
        <parameter name="ui" type="rifp">
          <type>ui_com.conceptwave.system.UserInterface</type>
        </parameter>
      </parameterList>
      <script><![CDATA[
        var model = this.model.ABLTRATUPhasePerSegmArrDS.ListDS;
        cm_spoc.addListElement(this, model,"cm_spoc.forms.ATUPhasePerSegmentIteratorUI","base_ATUPhasePerSegment","ATUPhasePerSegmentIterator");
      ]]></script>
    </method>
    <method name="deleteATUPhasePerSegmentIteratorRule" type="uimthd">
      <script><![CDATA[
        var model = this.model.ABLTRATUPhasePerSegmArrDS;
        cm_spoc.removeListElements(this,model,"List","ATUPhasePerSegmentIterator");
      ]]></script>
    </method>
    <method name="AwardedByLastTimeReceivedSectionVisRule" type="permMethod">
      <script><![CDATA[
        if(!this.parent.model.AwardedTopUpPromotionType)
        {
            return false;
        }
        else if(this.parent.model.AwardedTopUpPromotionType == "LT")
        {
            return true;
        }
        else
        {
            return false;
        }
      ]]></script>
    </method>
  </methodList>
