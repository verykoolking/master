  <methodList>
    <method name="cwLeafInitAction$model" type="laction">
      <path>ui_cm_spoc.forms.ConstraintsForm/leaf_model</path>
      <type>initializer</type>
      <script><![CDATA[
        var m = cm_spoc.initOperationSettings(this.model);

        m = cm_spoc.initIncompatibility(m);
        cm_spoc.initIncompatibilityIteratorUI(this);

        m = cm_spoc.initNAbyStatus(m);
        cm_spoc.initNAbyStatusIteratorUI(this);

        m = cm_spoc.initNAbyPlanorTechnology(m);
        cm_spoc.initNAbyPlanorTechnologyIteratorUI(this);

        m = cm_spoc.initNAbyPromotionalProfile(m);
        cm_spoc.initNAbyPromotionalProfileIteratorUI(this);

        m = cm_spoc.initNAbyServiceProvider(m);
        cm_spoc.initNAbyServiceProviderIteratorUI(this);

        var ConstraintByStatus = m.ConstraintsCByStatusArrDS.ListDS;
        cm_spoc.loadConstraintByStatus(ConstraintByStatus, this.NAbyStatusIterator);

        var ConstraintByPlanOrTechnolog = m.ConstraintsCByPlanOrTArrDS.ListDS;
        cm_spoc.loadConstraintByPlanOrTechnolog(ConstraintByPlanOrTechnolog, this.NAbyPlanorTechnologyIterator);

        var ConstraintByPromotionalProfile = m.ConstraintsCByPPArrDS.ListDS;
        cm_spoc.loadConstraintByPromotionalProfile(ConstraintByPromotionalProfile, this.NAbyPromotionalProfileIterator);

        var ConstraintByServiceProvider = m.ConstraintsCBySPArrDS.ListDS;
        cm_spoc.loadConstraintByServiceProvider(ConstraintByServiceProvider, this.NAbyServiceProviderIterator);

        return m;
      ]]></script>
    </method>
