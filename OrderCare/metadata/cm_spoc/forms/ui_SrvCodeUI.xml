<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="cm_spoc.forms.SrvCodeUI">
  <group>Numbering</group>
  <label>SrvCodeUI</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="Selected" type="leaf">
      <valueType>dtype_com.conceptwave.system.Boolean</valueType>
    </variable>
    <variable name="model" type="leaf">
      <valueType>dstruct_cm_spoc.data.SrvCode</valueType>
    </variable>
  </variableList>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</base>
          <extends>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="GridLayout" type="elgrd">
              <colWidths>50px</colWidths>
              <fixedColWidths>true</fixedColWidths>
              <numOfCols>13</numOfCols>
              <width>300px</width>
              <elementList>
                <element name="Selected" type="elchk">
                  <columnSpan>1</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SrvCodeUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SrvCodeUI/leaf_Selected</variablePath>
                    </variablePathList>
                  </variable>
                  <width>30px</width>
                </element>
                <element name="SrvId" type="elfld">
                  <columnSpan>2</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SrvCodeUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <label>Code</label>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SrvCodeUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.SrvCode/dstruct_SrvId</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
                <element name="SrvDesc" type="elfld">
                  <columnSpan>2</columnSpan>
                  <editable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SrvCodeUI/permMethod_readOnly</variablePath>
                    </variablePathList>
                  </editable>
                  <label>Code</label>
                  <removeLabelLine>true</removeLabelLine>
                  <showLabel>false</showLabel>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>ui_cm_spoc.forms.SrvCodeUI/leaf_model</variablePath>
                      <variablePath>dstruct_cm_spoc.data.SrvCode/dstruct_SrvDesc</variablePath>
                    </variablePathList>
                  </variable>
                  <width>100px</width>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="readOnly" type="permMethod">
      <script>return !this.model.readonly;</script>
    </method>
  </methodList>
</userInterface>