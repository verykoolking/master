                            <variablePath>findScript_cm_spoc.BoltOnTypeModalityFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cm_spoc.UCBOTAssociationDoc/leaf_secondMsgToDisp</variablePath>
