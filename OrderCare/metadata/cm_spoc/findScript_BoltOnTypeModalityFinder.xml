<?xml version="1.0" encoding="UTF-8" ?>
<findScript name="cm_spoc.BoltOnTypeModalityFinder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <group>ui_scripts.UsageControl.UCBoltOnTypeAssociation</group>
  <input>doc_cwt_pc.cwt_itemSearch</input>
  <label>BoltOnTypeModalityFinder</label>
  <metaVersion>4</metaVersion>
  <output>doc_cm_spoc.UCBOTAssociationDoc</output>
  <type>script</type>
  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSel" type="action">
      <system>true</system>
      <script><![CDATA[
        var itemFinder = new Finder("cm_spoc.catalogItemFilteredFinder");
        var attributeList = ["BoltOnTypeID" , "BoltOnTypeModality" , "BoltOnTypeDescription"];
        var attributeFinder = new Finder("cwt_pc:cwt_itemAttributeFinder");
        itemFinder.searchDocument.baseItemCode = "base_BoltOnType";
        var result = itemFinder.search();
        var attributeResult;
        var currentResult = new DataObjectList();


        for ( var i = 0; i < result.length; i++){
            var str = "";
            var strResult = "";
            attributeFinder.searchDocument.itemCode = result[i].itemCode;
            attributeResult = attributeFinder.search();

            for ( var k = 0; k < attributeList.length; k++ ) {
                for ( var j = 0; j < attributeResult.length; j++ ) {
                    if ( ( attributeResult[j].name == attributeList[k] ) && attributeResult[j].defaultValue ) {
                        if( str.length > 0 )
                            str = str + ";";
                        str = str + attributeResult[j].defaultValue;
                        break;
                    }
                }
            }
            var res = str.split(";");
            strResult = strResult + "[" + res[0] + "]" + "(" + res[1] + ") " + res[2];

            if(attributeResult.length > 0 ){
                var doc = new Document ("cm_spoc.UCBOTAssociationDoc");
                doc.msgToDisp = strResult;
                doc.selectItem = false;
                doc.itemCode = result[i].itemCode;
                currentResult.add(i,doc);
            }

        }
        return currentResult;
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="selectItem" type="leaf">
        <valueType>doc_cm_spoc.data.cwt_itemForCommon</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_cm_spoc.UCBOTAssociationDoc</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_cwt_pc.cwt_itemSearch/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_cm_spoc.UCBOTAssociationDoc/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>850px</width>
                <elementList>
                  <element name="Table" type="eltabl">
                    <doubleClickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findScript_cm_spoc.BoltOnTypeModalityFinder/ui_UserInterface/uimthd_onClickDetails</variablePath>
                      </variablePathList>
                    </doubleClickMethod>
                    <editable type="varPath">
                      <variablePathList>
                        <variablePath>findScript_cm_spoc.BoltOnTypeModalityFinder/ui_UserInterface/permMethod_readOnly</variablePath>
                      </variablePathList>
                    </editable>
                    <hideHeader>true</hideHeader>
                    <showDetailColumn>false</showDetailColumn>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findScript_cm_spoc.BoltOnTypeModalityFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <width>850px</width>
                    <elementList>
                      <element name="selectItem" type="elchk">
                        <columnSpan>1</columnSpan>
                        <editable type="varPath">
                          <variablePathList>
                            <variablePath>dtype_com.conceptwave.system.Boolean/leaf_TRUE</variablePath>
                          </variablePathList>
                        </editable>
                        <showLabel>false</showLabel>
                        <toggleEdit>true</toggleEdit>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findScript_cm_spoc.BoltOnTypeModalityFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cm_spoc.UCBOTAssociationDoc/leaf_selectItem</variablePath>
