      </method>
      <method name="onClickDetails" type="uimthd">
        <script>cm_spoc.loadDetails(this);</script>
      </method>
      <method name="readOnly" type="permMethod">
        <script>return !this.parent.model.readonly;</script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="BoltOnTypeModalityFinderView" type="findVView">
      <outputOverlay>findScript_cm_spoc.BoltOnTypeModalityFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
    </view>
  </viewList>
</findScript>