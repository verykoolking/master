    <methodList>
      <method name="onInit" type="action">
        <valueType>nmeta_com.conceptwave.system.Object</valueType>
        <script><![CDATA[
          if(this.model.isStored) {
              var contrlPhaseFinder = new cm_plm.plmControlPhaseFinder();
              contrlPhaseFinder.searchDocument.projectCode = this.model.projectCode;
              contrlPhaseFinder.search();
              this.controlPhases = new cm_plm.plmControlPhaseFinder.UserInterface(contrlPhaseFinder);

              var messageLogFinder = new cwl_plm.msgLogFinder();
              messageLogFinder.searchDocument.userData = this.model.projectCode;
              messageLogFinder.searchDocument.fromTime = Order.getOrderInstance(this.model.cwOrderId).creationDate;
              messageLogFinder.search();
              this.messageLog = new cwl_plm.msgLogFinder.UserInterface(messageLogFinder);
          }

          var user = UserProfile.userProfile();
          var isUserInMarketing = UserProfile.hasPrivilege("marketingPriv");
          var isUserInRevision = UserProfile.hasPrivilege("revisionPriv");

          //reglas delos campos isComercialConfRequired y isTechnicalConfRequired
          this.editIsComercialConfRequired = !this.model.isStored;
          this.editIsTechnicalConfRequired = !this.model.isStored;

          if(this.model.formNameParant == null){
              this.formName = 'main';
          }else{
              this.formName = this.model.formNameParant;
          }

          var workListItem = null;
          var workItemDoc = null;
          var procDoc = null;

          if(this.model.parent != null && this.model.parent.parent != null){
              workListItem = this.model.parent.parent;
              workItemDoc = workListItem.workItemDocument;
              if(workItemDoc != null){
                  procDoc = Process.getProcessDocument(workItemDoc.SenderId);
              }
          }

          if(procDoc != null){
              this.model.errorMsg = procDoc.caReturnMessage;
          }
        ]]></script>
      </method>
      <method name="showLogs" type="permMethod">
        <script>return this.model.isStored;</script>
      </method>
      <method name="permIsComercialConfRequired" type="permMethod">
        <script>return this.editIsComercialConfRequired;</script>
      </method>
      <method name="permIsTechnicalConfRequired" type="permMethod">
        <script>return this.editIsTechnicalConfRequired;</script>
      </method>
      <method name="popUpContinue" type="usrActionMethod">
        <script><![CDATA[
          var ord = Order.getOrderById(this.model.cwOrderId);
          this.addToOrderNote(ord);
          this.updatePopUpFields(ord);
          ord.save();

          if (this.validateForm()){
          Global.runContentDisplayScript(this, "displayScript");
          Global.confirmOK();
          }
        ]]></script>
      </method>
      <method name="popUpCancel" type="usrActionMethod">
        <script><![CDATA[
          Global.runContentDisplayScript(this, "displayScript");
          return Global.STOP_UI;
        ]]></script>
      </method>
      <method name="addToOrderNote" type="uimthd">
        <parameterList>
          <parameter name="ord" type="rifp">
            <type>nmeta_com.conceptwave.system.Object</type>
          </parameter>
        </parameterList>
        <script><![CDATA[
          if(!this.amendReason || this.amendReason.trim() == "")
              return;

          var ordNotes = ord.notes;

          if(ordNotes.notes)
              ordNotes.notes += new Date() + " - " + UserProfile.userId() + ":\n";
          else
              ordNotes.notes = new Date() + " - " + UserProfile.userId() + ":\n";

          ordNotes.notes += this.amendReason + "\n";
          ordNotes.notes += "------------------------------------------" + "\n";

          //reset the new Amend Reason
          this.amendReason = "";
        ]]></script>
      </method>
      <method name="updatePopUpFields" type="uimthd">
        <parameterList>
          <parameter name="ord" type="rifp">
            <type>nmeta_com.conceptwave.system.Object</type>
          </parameter>
        </parameterList>
        <script><![CDATA[
          var ordPlmControl = ord.plmControl;

          ordPlmControl.isComercialConfRequired = this.model.isComercialConfRequired;
          ordPlmControl.isTechnicalConfRequired = this.model.isTechnicalConfRequired;
        ]]></script>
      </method>
      <method name="validateForm" type="uimthd">
        <valueType>dtype_com.conceptwave.system.Boolean</valueType>
        <script>return this.model.isComercialConfRequired || this.model.isTechnicalConfRequired;</script>
      </method>
      <method name="dynLabelisComercialConfTecnicalConf" type="uimthd">
        <valueType>dtype_com.conceptwave.system.String</valueType>
        <script><![CDATA[
          if (!this.validateForm())
              return "cwErrorMessage";
          else
              return "";
        ]]></script>
      </method>
      <method name="showErrorTab" type="permMethod">
        <script>return this.model.errorMsg != null;</script>
      </method>
      <method name="permIsAdmin" type="permMethod">
        <privilegesList>
