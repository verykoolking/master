    <method name="cwOnDocInit" type="action">
      <system>true</system>
      <script><![CDATA[
        this.cw$super_cwOnDocInit();

        //se hay sido gravado antes no se borra los valores
        if(!this.isStored){
            //variables globales config
            var tst_req = Global.getConfigVariable("plmTestingRequired");
            var rev_req = Global.getConfigVariable("plmReviewRequired");
            this.isReviewRequired        = (rev_req == null) ? true : rev_req;
            this.isTestingRequired       = (tst_req == null) ? true : tst_req;

            var user = UserProfile.userProfile();
            var isUserInMarketing = UserProfile.hasPrivilege("marketingPriv");
            var isUserInTechnical = UserProfile.hasPrivilege("technicalPriv");
            var isUserInRevision = UserProfile.hasPrivilege("revisionPriv");

            if(isUserInMarketing){
                this.isComercialConfRequired = true;
                this.isTechnicalConfRequired = false;
            }else if(isUserInTechnical){
                this.isComercialConfRequired = false;
                this.isTechnicalConfRequired = true;
            }
        }
      ]]></script>
    </method>
  </methodList>
