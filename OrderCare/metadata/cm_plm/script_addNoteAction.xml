<?xml version="1.0" encoding="UTF-8" ?>
<script name="cm_plm.addNoteAction">
  <label>addNoteAction</label>
  <metaVersion>4</metaVersion>
  <parameterList>
    <parameter name="notesDoc" type="rifp">
      <mandatory>true</mandatory>
      <type>doc_cm_plm.plmNotes</type>
    </parameter>
    <parameter name="worklistDocument" type="rifp">
      <type>doc_com.conceptwave.system.Document</type>
    </parameter>
  </parameterList>
  <script>worklistDocument.processOrder.notes.setNotes(notesDoc.getNotes());</script>
</script>