<?xml version="1.0" encoding="UTF-8" ?>
<script name="cm_plm.setProcessOrderState">
  <label>setProcessOrderState</label>
  <metaVersion>4</metaVersion>
  <parameterList>
    <parameter name="process" type="rifp">
      <type>nmeta_com.conceptwave.system.Object</type>
    </parameter>
    <parameter name="state" type="rifp">
      <type>dtype_com.conceptwave.system.String</type>
    </parameter>
  </parameterList>
  <script><![CDATA[
    process.processOrder.state=state;
    process.processOrder.change.orderStatus = state;
    process.processOrder.save();
  ]]></script>
</script>