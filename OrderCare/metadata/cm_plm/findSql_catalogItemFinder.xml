  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select distinct {+i+} from CWPC_ITEM i {[5], cwpc_itemrelation_v r} where 1=1
        {[13] and i.ITEMTYPE != ?}
        {[14] and i.ITEMTYPE != ?}
        {[15] and i.ITEMTYPE != ?}
        {[16] and i.ITEMTYPE != ?}
        {[0] and i.ITEMCODE like ? || '%'}
        {[1] and}
        {[12] (}
        {[1] i.BASEITEMCODE = ?}
        {[:topItemCode] and i.TOPITEMCODE = ?}
        {[12] or i.BASEITEMCODE in ?}
        {[12] )}
        {[2] and i.ITEMTYPE = ?}
        {[3] and i.name = ?}
        {[4] and i.STATUS = ?}
        {[5] and i.itemcode = r.itemrelationtarget and r.itemrelationtarget = ?}
        {[6] and i.name like ? || '%'}
        {[10] and i.ITEMCODE = ? }
        {[7] and i.itemcode in (select catalogobjectcode from cwpc_projectCommand where projectid = ? and objecttype = 'cwt_pc:cwt_item')}
        and ((
        {[9] 1 = ? and } i.ORDERABLE = 1
        ) or (
        {[9] 0 = ? and } (i.ORDERABLE = 0 or i.ORDERABLE is null)
        ) or (
        {[9] 2 = ? and } (i.ORDERABLE = 0 or i.ORDERABLE = 1 or i.ORDERABLE is null)
        ))
      ]]></script>
    </method>
    <method name="cwOnFinderSQLGet" type="action">
      <system>true</system>
      <script>select {+*+} from CWPC_ITEM where ITEMCODE = ?</script>
    </method>
    <method name="editablePerm" type="permMethod">
      <script><![CDATA[
        if (this.refFinderDocument!=null) {
              return true;
        } else {
              return this.cw$super_editablePerm($psCondition);
        }
      ]]></script>
    </method>
    <method name="cwOnFilter" type="action">
      <system>true</system>
      <script>return cwt_pc.domainsFilter( input );</script>
    </method>
    <method name="cwOnFinderCompleteSearch" type="action">
      <system>true</system>
      <script><![CDATA[
        if (this.controllerOwner && this.controllerOwner.parent && this.controllerOwner.parent instanceof cwt_pc.itemTree) {

            this.controllerOwner.parent.numberOfItemsFound = this.list.length;

            for (var i=this.list.length-1; i>=this.searchDocument.numberOfResults; i--)
               this.list.remove(this.list[i]);
        }
      ]]></script>
    </method>
    <method name="cwOnInitSearch" type="uimthd">
      <script><![CDATA[
        this.cw$super_cwOnInitSearch();

        if (this.searchDocument.allDescendants == true && this.searchDocument.baseItemCode) {
              var arr = new Array();
              arr.push(this.searchDocument.baseItemCode);
              cwt_pc.getListOfBaseItemCodes(arr);
              var searchString = convertSearchString(arr);
              this.searchDocument.baseItemList = searchString;

        } else if (!this.searchDocument.allDescendants) {
              this.searchDocument.baseItemList = null;

        }

        function convertSearchString(arr) {
              var str = "";
              for (var i=0; i<arr.length; i++) {
                    if (i > 0)
                          str = str + ",";
                    str = str + "'" + arr[i] + "'";
              }
              return str; //"(" + str + ")";
        }
      ]]></script>
    </method>
  </methodList>
