      <script><![CDATA[
        select {+*+} from EPC_ITEMDOC
        {[*]where 1=1}
        {[:cwOrderId] and CWORDERID = ?}
        {[:itemCode] and ITEMCODE = ?}
        {[:publishedStatus] and PUBLISHEDSTATUS = ?}
        {[:configurationEntity] and CONFIGURATIONENTITY = ?}
        {[:projectCode] and PROJECTCODE = ?}
        {[:itemStatus] and ITEMSTATUS = ?}
