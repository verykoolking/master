<?xml version="1.0" encoding="UTF-8" ?>
<findOrder name="cm_plm.plmOrderFinder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cm_plm.plmOrderSearch</input>
  <itemTypeList>
    <itemType>order_cm_plm.plmOrder</itemType>
  </itemTypeList>
  <label>plmOrderFinder</label>
  <metaVersion>4</metaVersion>
  <output>doc_cm_plm.plmOrderSearch</output>
  <type>order</type>
  <vDetailForm>tree_com.conceptwave.system.OrderUserInterface/frmui_Forms/exov_Default</vDetailForm>
  <searchList>
