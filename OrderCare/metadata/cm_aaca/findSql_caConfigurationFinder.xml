<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="cm_aaca.caConfigurationFinder">
  <dbSchema>CATALOG</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cm_aaca.caConfigurationFinderData</input>
  <label>caConfigurationFinder</label>
  <metaVersion>4</metaVersion>
  <output>doc_cm_aaca.caConfigurationFinderData</output>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select distinct
            itemtoconf.itemCode as itemCode,
            paramOperation.defaultValue as operation,
            paramNode.defaultValue as nodeType,
            paramProc.defaultValue as ProcedureName,
            paramSequence.defaultValue as Sequence,
            confType.defaultValue as ConfigurationMode
        from
            cwpc_itemattribute_v paramOperation,
            cwpc_itemattribute_v paramSequence,
            cwpc_itemattribute_v paramNode,
            cwpc_itemattribute_v paramProc,
            cwpc_itemrelation_v adatoparamsOperation,
            cwpc_itemrelation_v adatoparamsSequence,
            cwpc_itemrelation_v adatoparamsNode,
            cwpc_itemrelation_v adatoparamsProc,
            cwpc_itemrelation_v conftoadat,
            epc_itemdoc itemtoconf,
            cwpc_itemattribute_v confType
        where
            paramOperation.itemAttributeCode='Operation' and
            paramSequence.itemAttributeCode='Sequence' and
            paramNode.itemAttributeCode='NodeType' and
            paramProc.itemAttributeCode='ProcedureName' and
            paramOperation.itemCode=paramSequence.itemCode and
            paramSequence.itemCode=paramNode.itemCode and
            paramNode.itemCode=paramProc.itemCode and
            paramOperation.itemCode = adatoparamsOperation.itemrelationtarget and
            paramSequence.itemCode = adatoparamsSequence.itemrelationtarget and
            paramNode.itemCode = adatoparamsNode.itemrelationtarget and
            paramProc.itemCode = adatoparamsProc.itemrelationtarget and
            adatoparamsOperation.associationtype='OCSAdapterConfigParameter' and
            adatoparamsSequence.associationtype='OCSAdapterConfigParameter' and
            adatoparamsNode.associationtype='OCSAdapterConfigParameter' and
            adatoparamsProc.associationtype='OCSAdapterConfigParameter' and
            adatoparamsOperation.itemCode=conftoadat.itemrelationtarget and
            adatoparamsSequence.itemCode=conftoadat.itemrelationtarget and
            adatoparamsNode.itemCode=conftoadat.itemrelationtarget and
            adatoparamsProc.itemCode=conftoadat.itemrelationtarget and
            conftoadat.associationtype='OCSAdapterConfigurationSpec' and
            conftoadat.itemCode=itemtoconf.configurationEntity and
            confType.itemCode=conftoadat.itemrelationtarget and
            confType.itemAttributeCode='configurationMode'
            {[0] and itemtoconf.itemCode = ?}
            {[1] and paramOperation.defaultValue like ('%'||?||'%')}
            {[2] and (paramNode.defaultValue = 'B' OR paramNode.defaultValue = ?)}
        order by
            paramSequence.defaultValue asc
      ]]></script>
    </method>
  </methodList>
