<?xml version="1.0" encoding="UTF-8" ?>
<script name="cm_aaca.addItem">
  <group>ApplicabilityCondition.helpers</group>
  <label>addItem</label>
  <metaVersion>4</metaVersion>
  <parameterList>
    <parameter name="parent" type="rifp">
      <type>nmeta_com.conceptwave.system.Object</type>
    </parameter>
    <parameter name="name" type="rifp">
      <type>dtype_com.conceptwave.system.String</type>
    </parameter>
  </parameterList>
  <script><![CDATA[
    var index = parent.AAItem.length;
    parent.AAItem[index].name = name;
    return parent.AAItem[index];
  ]]></script>
</script>