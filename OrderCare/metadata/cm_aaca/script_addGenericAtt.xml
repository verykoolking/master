<?xml version="1.0" encoding="UTF-8" ?>
<script name="cm_aaca.addGenericAtt">
  <group>ApplicabilityCondition.helpers</group>
  <label>addGenericAtt</label>
  <metaVersion>4</metaVersion>
  <parameterList>
    <parameter name="item" type="rifp">
      <type>dstruct_cm_aaca.AAItemType</type>
    </parameter>
    <parameter name="name" type="rifp">
      <type>dtype_com.conceptwave.system.String</type>
    </parameter>
    <parameter name="value" type="rifp">
      <type>dtype_com.conceptwave.system.String</type>
    </parameter>
  </parameterList>
  <script><![CDATA[
    var index = item.AAAttributeGeneric.length;
    item.AAAttributeGeneric[index].name = name;
    item.AAAttributeGeneric[index].string = value;
    return item;
  ]]></script>
</script>