<?xml version="1.0" encoding="UTF-8" ?>
<script name="cm_aaca.createRow">
  <group>ApplicabilityCondition.helpers</group>
  <label>createRow</label>
  <metaVersion>4</metaVersion>
  <parameterList>
    <parameter name="position" type="rifp">
      <type>dtype_com.conceptwave.system.Integer</type>
    </parameter>
    <parameter name="ArrayOperation" type="rifp">
      <type>dtype_com.conceptwave.system.String</type>
    </parameter>
  </parameterList>
  <script><![CDATA[
    var row = new cm_aaca.AAArrayRowType();
    row.pos = position;
    row.ArrayOperation = ArrayOperation;
    return row;
  ]]></script>
</script>