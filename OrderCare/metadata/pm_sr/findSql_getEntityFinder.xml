<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="pm_sr.getEntityFinder">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_pm_sr.selectInput</input>
  <label>getEntityFinder</label>
  <metaVersion>4</metaVersion>
  <output>doc_pm_sr.selectOutput</output>
  <type>SQL</type>
  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        SELECT
            subscriberId, offerType, productSerialNumber, opId, productIdAA1, productIdAA2, productIdAA3, productNumber, productOfferingId, xml, validfromdate
        FROM (
                        SELECT rownum rnum, subscriberId, offerType, productSerialNumber, opId, productIdAA1, productIdAA2, productIdAA3, productNumber, productOfferingId, xml, validfromdate  FROM
                        (
                                        SELECT
                                          entityapplicationcontext subscriberId
                                        , entitytype               offerType
                                        , entitydn                 productSerialNumber
                                        , MAX(DECODE(characteristic,  'opId',              valueString)) opId
                                        , MAX(DECODE(characteristic,  'productIdAA1',      valueString)) productIdAA1
                                        , MAX(DECODE(characteristic,  'productIdAA2',      valueString)) productIdAA2
                                        , MAX(DECODE(characteristic,  'productIdAA3',      valueString)) productIdAA3
                                        , MAX(DECODE( characteristic, 'productNumber',     valueString)) productNumber
                                        , MAX(DECODE( characteristic, 'productOfferingId', valueString)) productOfferingId
                                        , ( LISTAGG( ( CASE WHEN CHARACTERISTIC LIKE 'XML_%' THEN VALUESTRING END), '') WITHIN GROUP (ORDER BY CHARACTERISTIC) ) xml
                                        , validfromdate

                                        FROM
                                        {[11,B]  cwt_sr_entityvalueact }
                                        {[12,B]  cwt_sr_entityvalue    }

                                        WHERE 1=1
                                        {[0] and ENTITYAPPLICATIONCONTEXT = ?}
                                        {[1] and entitytype = ?}
                                        {[2] and entitydn = ?}
                                        GROUP BY entityapplicationcontext, entitytype, entitydn, validfromdate
                                        ORDER BY entityapplicationcontext, validfromdate DESC
                       )
                       WHERE 1=1
                       {[3] and opId = ?}
                       {[4] and productIdAA1 = ?}
                       {[5] and productIdAA2 = ?}
                       {[6] and productIdAA3 = ?}
                       {[7] and productNumber = ?}
                       {[8] and productOfferingId = ?}
                       {[9] and validfromdate >= ?}
                       {[10] and validfromdate <= ?}
        )
        {[13] WHERE rnum >= ? }
        {[14] AND rnum < ? }
      ]]></script>
    </method>
  </methodList>
