  <methodList>
    <method name="cwOnFinderSQLGet" type="action">
      <system>true</system>
      <script>select {+*+} from CWPC_ITEMRELATION_V where ITEMRELATIONVERSIONID = ?</script>
    </method>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select distinct {+r+}
        from CWPC_ITEMRELATION_V r, CWPC_PROJECTCOMMAND rp, CWPC_PROJECT p
        where r.ITEMRELATIONVERSIONID = rp.CATALOGOBJECTID
            and rp.PROJECTID = p.PROJECTID
            {[:itemCode] and r.ITEMCODE = ?}
            {[:status] and r.STATUS = ?}
            {[:projectCode] and p.PROJECTCODE = ?}
            {[:deleteMode] and rp.ENDDATE<=SYSDATE}
      ]]></script>
    </method>
    <method name="cwOnFinderCompleteSearch" type="action">
      <system>true</system>
      <script><![CDATA[
        for (var i=0; i<this.list.length; i++) {
            this.list[i].isValidTargetCodeProperty = true;
        }
      ]]></script>
    </method>
    <method name="cwOnFilter" type="action">
      <system>true</system>
      <script><![CDATA[
        if (input.type == 'CwTreeItemLink' || input.type == 'CwTreeNodeLink' || input.type == 'CwAvailabilityItemInclude' || input.type == 'CwAvailabilityItemExclude' || input.type == 'CwAvailabilityNodeLink') {
            return false;
        } else {
            return true;
        }
      ]]></script>
    </method>
  </methodList>
