        select distinct {+ia+}
        from cwpc_project p, cwpc_projectcommand pc, cwpc_itemattribute_v ia
        where p.projectid = pc.projectid
          and pc.catalogobjectid = ia.itemAttrVersionId
          and pc.objecttype like '%cwt_itemAttribute'
        {[:projectCode] and p.projectcode=?};
