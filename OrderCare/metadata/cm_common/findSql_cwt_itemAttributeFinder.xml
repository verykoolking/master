  <methodList>
    <method name="cwOnFinderCompleteSearch" type="action">
      <system>true</system>
      <script><![CDATA[
        for (var i=0; i<this.list.length; i++) {
              this.list[i].attributeType = this.list[i].attributeCode.reference.type;
              this.list[i].isValidAttributeCodeProperty = true;
        }
      ]]></script>
    </method>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        SELECT {+ia+}
        FROM cwpc_itemattribute_v ia
        WHERE EXISTS (
          SELECT 1 FROM (
            SELECT a.itemCode, a.itemattributecode, MAX(a.startdate) as startdate
            FROM cwpc_itemattribute_v a, cwpc_projectcommand c
            WHERE a.itemattrversionid = c.catalogobjectid
                and a.startdate<=sysdate
                and (a.enddate is null or a.enddate>sysdate)
                and (c.enddate is null or c.enddate>sysdate)
               {[:attributeCode] and a.attributeCode=?}
               {[:catalogObjectType] and a.catalogObjectType=?}
               {[:isProperty] and a.isProperty=?}
               {[:itemAttributeCode] and a.itemAttributeCode=?}
               {[:itemCode] and a.itemCode=?}
               {[:itemRelationCode] and a.itemRelationCode=?}
               {[:name] and a.name like ?}
               {[:status] and a.status=?}
            GROUP BY a.itemCode, a.itemattributecode
          ) g
          WHERE ia.itemCode=g.itemCode and
                ia.itemattributecode=g.itemattributecode and
                ia.startdate=g.startdate
          )
          {[:attributeCode] and ia.attributeCode=?}
          {[:catalogObjectType] and ia.catalogObjectType=?}
          {[:isProperty] and ia.isProperty=?}
          {[:itemAttributeCode] and ia.itemAttributeCode=?}
          {[:itemCode] and ia.itemCode=?}
          {[:itemRelationCode] and ia.itemRelationCode=?}
          {[:name] and ia.name like ?}
          {[:status] and ia.status=?}
      ]]></script>
    </method>
  </methodList>
