<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="pm_wm.externalSystemErrorFinder">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_pm_wm.pmErrorConversionMap</input>
  <label>externalSystemErrorFinder</label>
  <metaVersion>4</metaVersion>
  <output>doc_pm_wm.pmErrorCode</output>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
        from pmErrorConversionMap CV, pmErrorCode errorCode
        where CV.EXCEPTIONID = errorcode.exceptionid
        {[1] and CV.internalaaexceptionid = ?}
        {[2] and CV.category = ?}
