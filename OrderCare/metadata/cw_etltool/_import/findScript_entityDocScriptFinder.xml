  <methodList>
    <method name="cwOnFinderSel" type="action">
      <system>true</system>
      <script><![CDATA[
        var result = new DataObjectList();

        if(searchData.cwOrderId == null || searchData.cwOrderId == "" || searchData.FileID == null    || searchData.FileID == "" )
            return result
        else{
            var order =this.parent;//getScriptObject();
            if((order instanceof Order))
            {

                for(var f = 0; order.files && f < order.files.length; f++){

                    if(order.files[f].fileDoc.FileID == searchData.FileID){
                        var entities = order.files[f].entities;
                        for(var e=0; entities && e < entities.length; e++ ){
                            result.addLast(entities[e].entityDoc);
                        }
                    }
                }
            }
        }
        return result;
      ]]></script>
    </method>
