<findScript name="cw_etltool._import.fileBrowserFinder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cw_etltool._import.filesPathSearch</input>
  <label>File Browser </label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>4</metaVersion>
  <output>doc_cw_etltool._import.filesPath</output>
  <type>script</type>
  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSel" type="action">
      <system>true</system>
      <script><![CDATA[
        if( searchData.isFileBrowser)
        {
            objectList = new DataObjectList();
            if(searchData.fileDirectoryPath){
                var filesList =  FileReader.listFiles(searchData.fileDirectoryPath);
                for( var i=0; filesList && i < filesList.length; i++){

                    var doc = new Document("cw_etltool._import.filesPath");
                    doc.FileName = filesList[i];
                    doc.FilePath = searchData.fileDirectoryPath +"/"+ doc.FileName ;

                    if(doc.FileName.endsWith(".xml"))
                        objectList.addLast(doc);
                }
            }else{

                Global.showUserMessage("Please enter file directory path....");
            }
        }

        return objectList;
      ]]></script>
    </method>
  </methodList>
