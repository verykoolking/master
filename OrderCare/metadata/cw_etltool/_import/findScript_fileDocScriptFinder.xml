    <method name="cwOnFinderSel" type="action">
      <system>true</system>
      <script><![CDATA[
        var result =  new DataObjectList();
        if(searchData.cwOrderId == null || searchData.cwOrderId == "" )
            return result;
        else{
            var order = this.parent;//this.getScriptObject();
            if((order instanceof Order))
            {
                for(var f = 0; order.files && f < order.files.length; f++){
                    result.addLast(order.files[f].fileDoc);
                }
            }

        }
        return result;
      ]]></script>
    </method>
    <method name="cwOnFinderCompleteSearch" type="action">
      <system>true</system>
      <script><![CDATA[
        /*
        if(this.list.length > 0){
        this.selected = this.list[0];
        this.updateUI();
        }
        */
      ]]></script>
    </method>
