      <method name="loadFileDetails" type="uimthd">
        <script><![CDATA[
          if(this.result.selected != null)
              this.parent.loadFileDetail(this.result.selected[0]);
        ]]></script>
      </method>
      <method name="OnSelChanged" type="action">
        <script><![CDATA[
          this.cw$super_OnSelChanged();
          this.loadFileDetails();
        ]]></script>
      </method>
