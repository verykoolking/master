  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSel" type="action">
      <system>true</system>
      <script><![CDATA[
        var result = new DataObjectList();
        var finder = new Finder("cw_etltool._import.importReportFinder");
        searchData.mapTo(finder.searchDocument);
        var list = finder.search();
        for(var i = 0; list && i < list.length; i++){

            var doc = new Document("cw_etltool._import.reportDoc");
            list[i].mapTo(doc);
            result.addLast(doc);
        }
        return result;
      ]]></script>
    </method>
  </methodList>
