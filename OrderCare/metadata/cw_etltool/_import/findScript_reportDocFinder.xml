    <methodList>
      <method name="cwLeafInitAction$isViewReportButtonVisible" type="laction">
        <path>findScript_cw_etltool._import.reportDocFinder/ui_UserInterface/leaf_isViewReportButtonVisible</path>
        <type>initializer</type>
        <script>return true;</script>
      </method>
      <method name="viewReportButtonVisible" type="permMethod">
        <script>return !this.detailVisible($psCondition);</script>
      </method>
      <method name="viewReport" type="uimthd">
        <script>this.showDetailAction();</script>
      </method>
      <method name="showDetailAction" type="action">
        <script><![CDATA[
          //if this finder is being displayed as a reference finder, return the currently selected
          //document instead of displaying the detail document.
          var leafName = this.model.parentLeafName;
          if(this.result.selected==null){
              this.setReferenceData(null);
          }else{
              if(this.model.refFinderDocument!=null && !this.model.refFinderDocument.isTranslation(leafName)){
                  this.setReferenceData(this.result.selected);
              }else if (this.result.selected != null && this.result.selected.length == 1 && this.detailForm!=null){
                  this.detail = new this.result.selected[0].UserInterface(this.result.selected[0], this);
                  this.detail.parent = this;
                  //this.detail.loadFilesList();
                  var order = Order.getOrderById(this.detail.model.cwOrderId);
                  this.detail.importOrderUI.load(order);
                  this.detail.initOrderUI(order);
                  if(cw_etltool.common.getConfig().isDefaultReportView)
                      this.detailForm = "orderUI";
                  else
                  this.detailForm = "order";// "ImportReport";

                  //this.detail.OrderUserInterface.model = Order.getOrderById(this.detail.model.cwOrderId);
                  this.isSearchButtonVisible = false;
                  this.isViewReportButtonVisible = false;
              } else {
                  this.detail = null;
              }
          }

          if(this.cwSelectCallback!=null){
              var args = new Array();
              this.parent.owner[this.cwSelectCallback](this.result.selected, this.result.selected!=null?this.result.selected[0]:null);
              this.parent.parent.cwChildDialogClose();
          }
        ]]></script>
      </method>
