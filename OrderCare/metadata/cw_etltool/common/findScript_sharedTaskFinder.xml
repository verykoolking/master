    <methodList>
      <method name="assignTaskToCurrentUser" type="uimthd">
        <script><![CDATA[
          if(this.result.selected == null)
                return;

          var sel = this.result.selected[0];
          if(!sel)
                return;

          var selUser = Global.getUserId();
          var finder = Global.getWorkListFinder();
          if(finder==null)
                finder = this.model;
          if(finder!=null)
                finder.changeWLItem(this.result.selected, selUser, null, null);


          this.searchAction();
          this.parent.worklistFinderUI.searchAction();
          this.model.search();
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="sharedTaskFinderView" type="findVView">
