<?xml version="1.0" encoding="UTF-8" ?>
<findScript name="cw_etltool.common.sharedTaskFinder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cw_etltool.common.worklistSearch</input>
  <label>Shared Task Finder</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>4</metaVersion>
  <output>doc_cwf_pm.BaseWorklist</output>
  <type>script</type>
  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSel" type="action">
      <system>true</system>
      <script><![CDATA[
        var find = new Finder("cwf_pm.BaseWorklistFinder");
        find.searchDocument.userId = null;
        var list = find.search();
        var array = list.toArray();

        var result = new DataObjectList();

        for(var i=0; array && i < array.length; i++){
            var doc = array[i];

            if( (doc.userId == "" || doc.userId == null) && doc.Participant.startsWith("cw_etltool") && isUserParticpant(doc.Participant) &&
                doc.disable == false && doc.distributionType == 1 && (doc.Flags == 0 || doc.Flags == 2 )){ //Shared Task Filter

                    result.addLast(doc);


                }
        }
        return result;

        function isUserParticpant(partcipant){
            var flag =false;
            var userParticipants = UserProfile.getUserParticipants(UserProfile.userId());

            for(var p=0; p<userParticipants.length;p++){
                if(userParticipants[p] == partcipant ){
                    flag=true;
                    break;
                }
            }

            return flag;
        }
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
