  <methodList>
    <method name="uploadFiles" type="usrActionMethod">
      <form>ui_com.conceptwave.system.DocumentUserInterface/frmui_Forms/exov_Default</form>
      <userInterface>doc_cw_etltool._import.uploadDoc</userInterface>
      <valueType>doc_cw_etltool._import.uploadDoc</valueType>
    </method>
    <method name="createOrder" type="usrActionMethod">
      <form>order_cw_etltool._export.exportOrder/tree_UserInterface/frmui_Forms/exov_Default</form>
      <userInterface>order_cw_etltool._export.exportOrder</userInterface>
      <valueType>order_cw_etltool._export.exportOrder</valueType>
    </method>
    <method name="etlConfig" type="usrActionMethod">
      <allowInvalidObject>true</allowInvalidObject>
      <autosave>true</autosave>
      <dialogHeight>300</dialogHeight>
      <dialogWidth>600</dialogWidth>
      <showInPopup>true</showInPopup>
      <validate>true</validate>
      <script>return cw_etltool.common.getConfig().copyDocument(true);</script>
    </method>
    <method name="startProcess" type="usrActionMethod">
      <script><![CDATA[
        var ord = new Order("cw_etltool._import.importOrder");
        ord.save();
        Process.startProcess("cw_etltool._import.importProcess",ord.id);
        Global.showUserMessage("Process Created!");
      ]]></script>
    </method>
    <method name="report" type="usrActionMethod">
      <userInterface>findScript_cw_etltool._import.reportDocFinder</userInterface>
      <valueType>findScript_cw_etltool._import.reportDocFinder</valueType>
    </method>
    <method name="onInit" type="action">
      <description><![CDATA[
        <p style="margin-top: 0">
          This notification method is called by the framework when this application is initialized
        </p>
      ]]></description>
      <script><![CDATA[
        this.cw$super_onInit();
        this.userName =  UserProfile.userProfile().userName;
        this.etlToolDashbord = new cw_etltool.presentation.etlToolDashbord(null, this);
        return this.etlToolDashbord;
      ]]></script>
    </method>
    <method name="etlHome" type="usrActionMethod">
      <form>ui_cw_etltool.presentation.etlToolDashbord/frmui_Forms/exov_Default</form>
      <userInterface>ui_cw_etltool.presentation.etlToolDashbord</userInterface>
      <valueType>ui_cw_etltool.presentation.etlToolDashbord</valueType>
      <script>return this.etlToolDashbord.refresh();</script>
    </method>
    <method name="processManager" type="usrActionMethod">
      <form>findDoc_cwf_pm.ProcessFinder/ui_UserInterface/frmui_Forms/exov_Default</form>
      <userInterface>findDoc_cwf_pm.ProcessFinder</userInterface>
      <valueType>findDoc_cwf_pm.ProcessFinder</valueType>
    </method>
    <method name="eventLog" type="usrActionMethod">
      <script><![CDATA[return new Finder("cwf_oe:EventLogFinder");]]></script>
    </method>
    <method name="selectApp" type="usrActionMethod">
      <userInterface>doc_cwf_oe.SelectApp</userInterface>
      <valueType>doc_cwf_oe.SelectApp</valueType>
    </method>
    <method name="logout" type="usrActionMethod">
      <form>doc_cwf_oe.login/ui_UserInterface/frmui_Forms/exov_Default</form>
      <userInterface>doc_cwf_oe.login</userInterface>
      <valueType>doc_cwf_oe.login</valueType>
      <script>UserProfile.logout();</script>
    </method>
    <method name="menuVisible" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>ETLAppPriv</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
    </method>
    <method name="exportConfigurationIstances" type="usrActionMethod">
      <form>doc_cw_etltool._export.ConfigurationInstancesExport/ui_UserInterface/frmui_Forms/exov_Default</form>
      <userInterface>doc_cw_etltool._export.ConfigurationInstancesExport</userInterface>
      <valueType>doc_cw_etltool._export.ConfigurationInstancesExport</valueType>
    </method>
  </methodList>
</userInterface>