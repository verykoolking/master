<?xml version="1.0" encoding="UTF-8" ?>
<externalService name="pm_pws.services.OCSProvisioningService">
  <label>OCSProvisioningService</label>
  <metaVersion>4</metaVersion>
  <portList>
    <port name="OCSProvisioningPort" type="port">
      <binding>bind_pm_pws.services.OCSProvisioningSOAPBinding</binding>
      <label>OCSProvisioningPort</label>
      <listener>true</listener>
      <recover>5</recover>
    </port>
    <port name="OCSProvisioningJSPort" type="port">
      <binding>bind_pm_pws.services.OCSProvisioningJSBinding</binding>
      <label>OCSProvisioningJSPort</label>
      <recover>5</recover>
    </port>
  </portList>
</externalService>